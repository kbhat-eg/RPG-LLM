CREATE OR REPLACE TABLE ZSKTST(
  Instans          FOR COLUMN ZTINST CHAR(10) CCSID 277 NOT NULL,
  Firma            FOR COLUMN ZTFIRM NUMERIC(3, 0) NOT NULL,
  Avdelingsgruppe  FOR COLUMN ZTAVDG NUMERIC(4, 0) NOT NULL,
  Kundegruppe      FOR COLUMN ZTKUGR CHAR(10) CCSID 277 NOT NULL,
  Kundekjopstype   FOR COLUMN ZTKKTY CHAR(10) CCSID 277 NOT NULL,
  OmsetningFra     FOR COLUMN ZTOMSF NUMERIC(11, 0) NOT NULL,
  OmsetningTil     FOR COLUMN ZTOMST NUMERIC(11, 0) NOT NULL,
  Timestamp        FOR COLUMN ZTTMST TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
  OpprettetDato    FOR COLUMN ZTODAT DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl      FOR COLUMN ZTOTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv      FOR COLUMN ZTOUSR CHAR(10) NOT NULL DEFAULT ' ',
  EndretDato       FOR COLUMN ZTEDAT DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretTid        FOR COLUMN ZTETIM TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv         FOR COLUMN ZTEUSR CHAR(10) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

