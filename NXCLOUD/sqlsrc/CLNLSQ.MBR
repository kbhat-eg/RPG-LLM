CREATE OR REPLACE TABLE CLNLST(
  Firma              FOR COLUMN CLNLFI  NUMERIC(3, 0) NOT NULL DEFAULT 0,
  Nobbkategori       FOR COLUMN CLNLKA  NUMERIC(4, 0) NOT NULL DEFAULT 0,
  Leverandor         FOR COLUMN CLNLLV  NUMERIC(6, 0) NOT NULL DEFAULT 0,
  Kjedekobling       FOR COLUMN CLNLKJ  CHAR(2) NOT NULL DEFAULT ' ',
  OpprettetDato      FOR COLUMN CLNLOD DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretDato         FOR COLUMN CLNLED DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretKl           FOR COLUMN CLNLET TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv           FOR COLUMN CLNLEU CHAR(10) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

CREATE UNIQUE INDEX CLNLIU on CLNLST(CLNLFI,CLNLKA,CLNLLV,CLNLKJ);
CREATE INDEX CLNLIR on CLNLST(CLNLFI,CLNLKA,CLNLLV,CLNLKJ);
CREATE INDEX CLNLI1 on CLNLST(CLNLFI,CLNLKA,CLNLLV,CLNLKJ);

LABEL ON TABLE CLNLST IS 'Nobbkategori hovedleverand√∏rer';
