CREATE OR REPLACE TABLE VGIMST(
  UID              FOR COLUMN VGLUID NUMERIC(18, 0) NOT NULL
                   GENERATED ALWAYS AS IDENTITY
                   (START WITH 1 INCREMENT BY 1 CYCLE),
  Varenummer       FOR COLUMN VIVARE CHAR(15) NOT NULL DEFAULT ' ',
  Enhet            FOR COLUMN VIENHE CHAR(3) NOT NULL DEFAULT ' ',
  Kostpris         FOR COLUMN VICOST NUMERIC(9, 2) NOT NULL)
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE VGIMST IS 'Import kostpris fil';

CREATE INDEX VGIMI1 ON VGIMST(VIVARE);

