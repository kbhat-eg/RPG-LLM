
CREATE OR REPLACE TABLE XREFST
(
   UID              BIGINT    NOT NULL GENERATED
   ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
   NAVN             VARCHAR(255),
   STORRELSE        INTEGER,
   FET              SMALLINT,
   KURSIV           SMALLINT,
   RAPPORT_ID       BIGINT,
   FONT_TYPE        INTEGER,
   PRIMARY KEY(UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XREFST IS 'RA OEDFont';
