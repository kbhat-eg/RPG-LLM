
CREATE OR REPLACE TABLE XRERST
(
   UID              BIGINT    NOT NULL GENERATED
   ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
   ID_NAVN          VARCHAR(255),
   NAVN             VARCHAR(255),
   NUMMER           VARCHAR(255),
   RAD_TYPE         INTEGER,
   SNU_FORTEGN      SMALLINT,
   RAPPORT_ID       BIGINT,
   PARENT_ID        BIGINT,
   BEGRENSNING_ID   BIGINT,
   RAD_INDEX        INTEGER,
   PRIMARY KEY(UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XRERST IS 'RA OEDRad';
