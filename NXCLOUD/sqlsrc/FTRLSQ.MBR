CREATE OR REPLACE TABLE TRANSPORT_WHAREHOUSE FOR SYSTEM NAME FTRLST
(
  UID              FOR COLUMN FTLUID NUMERIC(18, 0) NOT NULL
                   GENERATED ALWAYS AS IDENTITY
                   (START WITH 1 INCREMENT BY 1 CYCLE),
  Firmanummer      FOR COLUMN FTLFIR NUMERIC(3, 0) NOT NULL,
  Lager            FOR COLUMN FTLLAG NUMERIC(2, 0) NOT NULL,
  Flatestyrt       FOR COLUMN FTLFLT NUMERIC(1, 0) NOT NULL,
  TransportID      FOR COLUMN FTLTID CHAR(35) NOT NULL DEFAULT ' ',
  EndretDato       FOR COLUMN FTLEDA DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretKl         FOR COLUMN FTLETI TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv         FOR COLUMN FTLEUS CHAR(10) NOT NULL DEFAULT ' ',
  OpprettetDato    FOR COLUMN FTLODA DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl      FOR COLUMN FTLOTI TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv      FOR COLUMN FTLOUS CHAR(10) NOT NULL DEFAULT ' ')

ON REPLACE PRESERVE ALL ROWS;

  CREATE INDEX FTRLIU
   ON FTRLST (FTLFIR, FTLLAG);

  CREATE INDEX FTRLIR
   ON FTRLST (FTLFIR, FTLLAG);

  CREATE INDEX FTRLI1
   ON FTRLST (FTLFIR, FTLLAG);

LABEL ON INDEX FTRLIU IS 'Transport information and wharehouse- firm/lager';
LABEL ON INDEX FTRLIR IS 'Transport information and wharehouse- firm/lager';
LABEL ON INDEX FTRLI1 IS 'Transport information and wharehouse- firm/lager';
LABEL ON TABLE FTRLST IS 'Transport information and wharehouse';
