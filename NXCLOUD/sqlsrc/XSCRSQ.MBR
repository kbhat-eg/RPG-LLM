
CREATE OR REPLACE TABLE XSCRST
(
   UID              BIGINT    NOT NULL GENERATED
   ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
   TITTEL           VARCHAR(255),
   BESKRIVELSE      VARCHAR(255),
   RAPPORT_TYPE     INTEGER,
   RAPPORT_ID       BIGINT,
   EIER             VARCHAR(255),
   INTERVALLTYPE    INTEGER,
   INTERVALL        VARCHAR(255),
   SISTKJORT        DATE,
   PRIMARY KEY(UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XSCRST IS 'RA ScheduledReport';
