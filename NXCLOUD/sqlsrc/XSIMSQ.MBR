
CREATE OR REPLACE TABLE XSIMST
(
   UID BIGINT    NOT NULL GENERATED ALWAYS AS IDENTITY
                (START WITH 1, INCREMENT BY 1),
   FIRMA              INTEGER,
   AVDELING           INTEGER,
   AAR                INTEGER,
   MND                INTEGER,
   ANT_DAGER_KREDITT  INTEGER,
   ANT_DAGER_KONTANT  INTEGER,
   KOMMENTAR     VARCHAR(255),
   PRIMARY KEY(UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XSIMST IS 'RA Salgsdager i mnd';
