CREATE OR REPLACE TABLE VGKEST(
  UID              FOR COLUMN VGLUID NUMERIC(18, 0) NOT NULL
                   GENERATED ALWAYS AS IDENTITY
                   (START WITH 1 INCREMENT BY 1 CYCLE),
  Firma            FOR COLUMN VEFIRM NUMERIC(3, 0) NOT NULL,
  Lager            FOR COLUMN VELAGE NUMERIC(2, 0) NOT NULL,
  Transdato        FOR COLUMN VETDAT DATE NOT NULL DEFAULT CURRENT_DATE,
  Bestilling       FOR COLUMN VEBEST NUMERIC(8, 0) NOT NULL,
  Suffix           FOR COLUMN VESUFF NUMERIC(2, 0) NOT NULL,
  Leverandor       FOR COLUMN VELDOR NUMERIC(6, 0) NOT NULL,
  Varenummer       FOR COLUMN VEVARE CHAR(15) NOT NULL DEFAULT ' ',
  Lagerenhet       FOR COLUMN VEENHE CHAR(3) NOT NULL,
  Bestenh          FOR COLUMN VEENHB CHAR(3) NOT NULL,
  Antbest          FOR COLUMN VEANTB NUMERIC(11, 3) NOT NULL,
  Bestpris         FOR COLUMN VEINKP NUMERIC(9, 2) NOT NULL,
  Omrfaktor        FOR COLUMN VEOMRF NUMERIC(12, 6) NOT NULL,
  KalkKostPris     FOR COLUMN VEKALP NUMERIC(9, 2) NOT NULL,
  SumBest          FOR COLUMN VESUMB NUMERIC(9, 2) NOT NULL,
  SumHist          FOR COLUMN VESUMH NUMERIC(9, 2) NOT NULL,
  HistBeholdning   FOR COLUMN VEBEHL NUMERIC(11, 3) NOT NULL,
  ProsentVolum     FOR COLUMN VEAVPV NUMERIC(3, 1) NOT NULL,
  ProsentPris      FOR COLUMN VEAVPP NUMERIC(3, 1) NOT NULL,
  ProsentTest      FOR COLUMN VEAVPT NUMERIC(3, 1) NOT NULL,
  Priskode         FOR COLUMN VEPKOD CHAR(1) NOT NULL,
  GmlKostp         FOR COLUMN VEGKPR NUMERIC(9, 2) NOT NULL,
  NyKostp          FOR COLUMN VENKPR NUMERIC(9, 2) NOT NULL,
  Feilkode         FOR COLUMN VEFEIK NUMERIC(2, 0) NOT NULL,
  Feilmelding      FOR COLUMN VEFEIL CHAR(100) NOT NULL DEFAULT ' ',
  DatoBeregn       FOR COLUMN VEBDAT DATE NOT NULL DEFAULT CURRENT_DATE,
  TypeBeregn       FOR COLUMN VETYPE CHAR(1) NOT NULL DEFAULT ' ',
  TidBeregn        FOR COLUMN VETTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  BeregnetAv       FOR COLUMN VEBUSR CHAR(10) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE VGKEST IS 'Feillog kostprisberegning';

