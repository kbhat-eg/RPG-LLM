
CREATE OR REPLACE TABLE XEDUST
(
   UID              BIGINT    NOT NULL GENERATED
   ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
   NAVN             VARCHAR(255),
   BESKRIVELSE      VARCHAR(255),
   FIRMA            INTEGER,
   AVDELING         INTEGER,
   OPPRETTETAV      VARCHAR(10),
   UTVALG_TYPE      INTEGER,
   PRIMARY KEY(UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XEDUST IS 'RA EgendefUtvalg';
