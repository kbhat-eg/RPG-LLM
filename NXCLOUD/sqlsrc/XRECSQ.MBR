
CREATE OR REPLACE TABLE XRECST
(
   UID              BIGINT    NOT NULL GENERATED
   ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
   ID_NAVN          VARCHAR(255),
   NAVN             VARCHAR(255),
   FIRMA            VARCHAR(255),
   AVDELING         VARCHAR(255),
   PROSJEKT         VARCHAR(255),
   AKTIVITET        VARCHAR(255),
   BAERER           VARCHAR(255),
   AAR              VARCHAR(255),
   PERIODE          VARCHAR(255),
   RECORD_TYPE      VARCHAR(255),
   RAPPORT_ID       BIGINT,
   PARENT_ID        BIGINT,
   KOLONNE_INDEKS   INTEGER,
   PRIMARY KEY(UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XRECST IS 'RA OEDKolonne';
