CREATE OR REPLACE TABLE WINNER_MAP_HEAD FOR SYSTEM NAME NOWHST(
  UID                  NUMERIC(18, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
                       (START WITH 1 INCREMENT BY 1 CYCLE),
  Date                 FOR COLUMN NWHDAT DATE NOT NULL DEFAULT CURRENT_DATE,
  Time                 FOR COLUMN NWHTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  Regnr                FOR COLUMN NWHREG VARCHAR(50) NOT NULL DEFAULT ' ',
  Project_Number       FOR COLUMN NWHPNU NUMERIC(6, 0) NOT NULL,
  Project_Name         FOR COLUMN NWHPNA VARCHAR(35) NOT NULL DEFAULT ' ',
  Project_ContractNr   FOR COLUMN NWHKUN NUMERIC(6, 0) NOT NULL,
  Project_Reference    FOR COLUMN NWHKRF VARCHAR(20) NOT NULL DEFAULT ' ',
  FullName             FOR COLUMN NWHKNA VARCHAR(30) NOT NULL DEFAULT ' ',
  Addr                 FOR COLUMN NWHKAD VARCHAR(30) NOT NULL DEFAULT ' ',
  ZIP                  FOR COLUMN NWHKPN VARCHAR(15) NOT NULL DEFAULT ' ',
  Post                 FOR COLUMN NWHKPA VARCHAR(30) NOT NULL DEFAULT ' ',
  Phone                FOR COLUMN NWHKTL VARCHAR(15) NOT NULL DEFAULT ' ',
  Mobile               FOR COLUMN NWHMOB VARCHAR(15) NOT NULL DEFAULT ' ',
  Email                FOR COLUMN NWHMAI VARCHAR(50) NOT NULL DEFAULT ' ',
  FullNamel            FOR COLUMN NWHLNA VARCHAR(30) NOT NULL DEFAULT ' ',
  Addrl                FOR COLUMN NWHLAD VARCHAR(30) NOT NULL DEFAULT ' ',
  ZIPl                 FOR COLUMN NWHLPN VARCHAR(15) NOT NULL DEFAULT ' ',
  Postl                FOR COLUMN NWHLPA VARCHAR(30) NOT NULL DEFAULT ' ',
  Phonel               FOR COLUMN NWHLTL VARCHAR(15) NOT NULL DEFAULT ' ',
  Mobilel              FOR COLUMN NWHLOB VARCHAR(15) NOT NULL DEFAULT ' ',
  Emaill               FOR COLUMN NWHLAI VARCHAR(50) NOT NULL DEFAULT ' ',
  FullNamef            FOR COLUMN NWHFNA VARCHAR(30) NOT NULL DEFAULT ' ',
  Addrf                FOR COLUMN NWHFAD VARCHAR(30) NOT NULL DEFAULT ' ',
  ZIPf                 FOR COLUMN NWHFPN VARCHAR(15) NOT NULL DEFAULT ' ',
  Postf                FOR COLUMN NWHFPA VARCHAR(30) NOT NULL DEFAULT ' ',
  Phonef               FOR COLUMN NWHFTL VARCHAR(15) NOT NULL DEFAULT ' ',
  Mobilef              FOR COLUMN NWHFOB VARCHAR(15) NOT NULL DEFAULT ' ',
  Emailf               FOR COLUMN NWHFAI VARCHAR(50) NOT NULL DEFAULT ' ',
  DateOffer            FOR COLUMN NWHTDA DATE NOT NULL DEFAULT CURRENT_DATE,
  DateLegal            FOR COLUMN NWHGDA DATE NOT NULL DEFAULT CURRENT_DATE,
  DateReg_TO           FOR COLUMN NWHLDA DATE NOT NULL DEFAULT CURRENT_DATE,
  FreightExVAT         FOR COLUMN NWHFRK NUMERIC(9, 2) NOT NULL,
  OurRef_Title         FOR COLUMN NWHSNA VARCHAR(20) NOT NULL DEFAULT ' ',
  OurRef_Mail          FOR COLUMN NWHSMA VARCHAR(50) NOT NULL DEFAULT ' ',
  OurRef_Code          FOR COLUMN NWHSNR NUMERIC(2, 0) NOT NULL,
  Deliv_Terms_Code     FOR COLUMN NWHLBK NUMERIC(2, 0) NOT NULL,
  Deliv_Terms_Value    FOR COLUMN NWHLBT VARCHAR(20) NOT NULL DEFAULT ' ',
  Transport_Terms_Code FOR COLUMN NWHLMK NUMERIC(2, 0) NOT NULL,
  Status_flag          FOR COLUMN NWHSTF VARCHAR(10) NOT NULL,
  Status_date          FOR COLUMN NWHSTD DATE NOT NULL DEFAULT CURRENT DATE,
  Status_time          FOR COLUMN NWHSTT TIME NOT NULL DEFAULT CURRENT TIME,
  OPPRETTETBRUKER      FOR COLUMN NWHOUS VARCHAR(128) NOT NULL,
  ENDRETBRUKER         FOR COLUMN NWHEUS VARCHAR(128) NOT NULL,
  OPPRETTET            FOR COLUMN NWHOTS TIMESTAMP  DEFAULT CURRENT_TIMESTAMP,
  AUTOOBRUKER          FOR COLUMN NWHOOU VARCHAR(128) WITH DEFAULT USER ,
  ENDRET               FOR COLUMN NWHETS TIMESTAMP NOT NULL FOR EACH ROW ON
                       UPDATE AS ROW CHANGE TIMESTAMP,
  AUTOEBRUKER          FOR COLUMN NWHAEU VARCHAR(128) GENERATED ALWAYS AS
                      ( SESSION_USER ))
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE WINNER_MAP_HEAD IS 'Mappingtabell Hode Winner integrasjon';

CREATE UNIQUE INDEX WINNER_MAP_HEAD_UPD
       FOR SYSTEM NAME NOWHIU ON WINNER_MAP_HEAD(Date,Time);
CREATE INDEX WINNER_MAP_HEAD_READ
       FOR SYSTEM NAME NOWHIR ON WINNER_MAP_HEAD(Date,Time);
CREATE INDEX WINNER_MAP_HEAD_READ1
       FOR SYSTEM NAME NOWHI1 ON WINNER_MAP_HEAD(Date,Time);


