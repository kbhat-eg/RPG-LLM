CREATE OR REPLACE TABLE FRONT_PAKKSEDDELHODE FOR SYSTEM NAME XPHOST
(
  Firma            FOR COLUMN XHFIRM NUMERIC(3, 0) NOT NULL,
  Guid             FOR COLUMN XHGUID CHAR(40) NOT NULL DEFAULT ' ',
  Status           FOR COLUMN XHSTAT NUMERIC(1, 0) NOT NULL,
  PakkseddelID     FOR COLUMN XHPAID CHAR(17) NOT NULL DEFAULT ' ',
  Versjonsnummer   FOR COLUMN XHPVER NUMERIC(3, 0) NOT NULL,

  Pakkseddeldato   FOR COLUMN XHPADT DATE NOT NULL DEFAULT CURRENT_DATE,
  Leveringsstart   FOR COLUMN XHPSDT DATE NOT NULL DEFAULT CURRENT_DATE,
  Leveringslutt    FOR COLUMN XHPEDT DATE NOT NULL DEFAULT CURRENT_DATE,
  BestillingsNr    FOR COLUMN XHBNUM CHAR(17) NOT NULL DEFAULT ' ',
  LevOrdrenummer   FOR COLUMN XHLONR CHAR(35) NOT NULL DEFAULT ' ',
  Prosjektnummer   FOR COLUMN XHPRNR CHAR(35) NOT NULL DEFAULT ' ',
  TransportStadie  FOR COLUMN XHTRST CHAR(3) NOT NULL DEFAULT ' ',
  TransportMate    FOR COLUMN XHTMAT CHAR(25) NOT NULL DEFAULT ' ',
  TransportInfo    FOR COLUMN XHTINF CHAR(256) NOT NULL DEFAULT ' ',
  TotalVolum       FOR COLUMN XHTVOL NUMERIC(8, 4) NOT NULL,
  TotalVekt        FOR COLUMN XHTVEK NUMERIC(10, 4) NOT NULL,
  TotalHoyde       FOR COLUMN XHTHOY NUMERIC(6, 3) NOT NULL,
  TotalLengde      FOR COLUMN XHTLEN NUMERIC(6, 3) NOT NULL,
  TotalBredde      FOR COLUMN XHTBRE NUMERIC(6, 3) NOT NULL,
  LeveringsMate    FOR COLUMN XHLMAT CHAR(3) NOT NULL DEFAULT ' ',
  LeveringsBeting  FOR COLUMN XHLBET CHAR(3) NOT NULL DEFAULT ' ',
  ManVaremottak    FOR COLUMN XHMVAR NUMERIC(1, 0) NOT NULL)

ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE FRONT_PAKKSEDDELHODE   IS 'Pakkseddel hode - forsystem';
