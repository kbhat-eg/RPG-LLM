CREATE OR REPLACE TABLE LXL5ST (
    UID NUMERIC(15, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
        (START WITH 1 INCREMENT BY 1 CYCLE),
    Identnr           FOR COLUMN LX5IDE NUMERIC(10, 0) NOT NULL DEFAULT 0,
    Bonusleverandor   FOR COLUMN LX5BLE NUMERIC(6, 0) NOT NULL DEFAULT 0,
    Navn              FOR COLUMN LX5BLN CHAR(35) NOT NULL DEFAULT ' ',
    Leverandor        FOR COLUMN LX5LEV NUMERIC(6, 0) NOT NULL DEFAULT 0,
    Overgruppe        FOR COLUMN LX5OGR NUMERIC(2, 0) NOT NULL DEFAULT 0,
    Hovedgruppe       FOR COLUMN LX5HGR NUMERIC(2, 0) NOT NULL DEFAULT 0,
    Undergruppe       FOR COLUMN LX5UGR NUMERIC(3, 0) NOT NULL DEFAULT 0,
    Modulnummer       FOR COLUMN LX5MOD NUMERIC(8, 0) NOT NULL DEFAULT 0,
    Varenummer        FOR COLUMN LX5VAR CHAR(15) NOT NULL DEFAULT ' ',
    Enhet             FOR COLUMN LX5ENH CHAR(3) NOT NULL DEFAULT ' ',
    Leveringstype     FOR COLUMN LX5LTY NUMERIC(1, 0) NOT NULL DEFAULT 0,
    Bonustype         FOR COLUMN LX5BTY CHAR(2) NOT NULL DEFAULT ' ',
    Bonusnavn         FOR COLUMN LX5BNA CHAR(35) NOT NULL DEFAULT ' ',
    Fradato           FOR COLUMN LX5FDA DATE NOT NULL DEFAULT CURRENT_DATE,
    Tildato           FOR COLUMN LX5TDA DATE NOT NULL DEFAULT CURRENT_DATE,
    Merknad           FOR COLUMN LX5MRK CHAR(50) NOT NULL DEFAULT ' ',
    Bonusgrunnlag     FOR COLUMN LX5BGR NUMERIC(11, 2) NOT NULL DEFAULT 0,
    Bonus             FOR COLUMN LX5BON NUMERIC(9, 0) NOT NULL DEFAULT 0,
    Beregnet_bonus    FOR COLUMN LX5BDA DATE NOT NULL DEFAULT CURRENT_DATE,
    PRIMARY KEY(UID))
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE LXL5ST IS 'SlipStream leverand√∏rbonus';
