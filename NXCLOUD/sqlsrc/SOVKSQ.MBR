CREATE OR REPLACE TABLE SOVKST(
  Firma            FOR COLUMN SOVFIR NUMERIC(3, 0) NOT NULL,
  Bilagstype       FOR COLUMN SOVTYP CHAR(4) NOT NULL DEFAULT,
  Aarstall         FOR COLUMN SOVAAR NUMERIC(4, 0) NOT NULL,
  Bilagsnummer     FOR COLUMN SOVBIL NUMERIC(8, 0) NOT NULL,
  Bilaglinje       FOR COLUMN SOVBLI NUMERIC(4, 0) NOT NULL,
  Dato             FOR COLUMN SOVDAT DATE NOT NULL DEFAULT CURRENT_DATE,
  Tid              FOR COLUMN SOVTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  Oppdatert        FOR COLUMN SOVOPD CHAR(1) NOT NULL DEFAULT)
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE SOVKST IS 'Statistikkposter for kostf√∏ring';

CREATE INDEX SOVKI1 ON SOVKST(SOVFIR,SOVOPD);
