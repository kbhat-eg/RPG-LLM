CREATE OR REPLACE TABLE ILNKST(
  COMPANY          FOR COLUMN ILFIRM NUMERIC(3, 0) NOT NULL,
  WS_USER          FOR COLUMN ILWUSR CHAR(20) NOT NULL,
  WS_PASS          FOR COLUMN ILWPAS CHAR(50) NOT NULL,
  LNK_MAL          FOR COLUMN ILLMAL CHAR(100) NOT NULL,
  LNK_REQ          FOR COLUMN ILLREQ CHAR(100) NOT NULL,
  LNK_RESP         FOR COLUMN ILLRES CHAR(100) NOT NULL,
  LNK_SERV         FOR COLUMN ILLSRV CHAR(200) NOT NULL,
  OPPRETTETDATO    FOR COLUMN ILODAT DATE NOT NULL DEFAULT CURRENT_DATE,
  OPPRETTETKL      FOR COLUMN ILOTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  OPPRETTETAV      FOR COLUMN ILOUSR CHAR(10) NOT NULL DEFAULT 'NXSYSTEM')
ON REPLACE PRESERVE ALL ROWS;

CREATE INDEX ILNKI1
 ON ILNKST (ILFIRM);

CREATE INDEX ILNKIR
 ON ILNKST (ILFIRM);

CREATE INDEX ILNKIU
 ON ILNKST (ILFIRM);

LABEL ON TABLE ILNKST IS 'LINKER TIL WEB-SERVICE';
