
CREATE OR REPLACE TABLE XEDDST
(
   UID              BIGINT    NOT NULL GENERATED
   ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
   DETALJ_TYPE      INTEGER,
   KEY1             VARCHAR(255),
   KEY2             VARCHAR(255),
   KEY3             VARCHAR(255),
   KEY4             VARCHAR(255),
   BESKRIVELSE      VARCHAR(255),
   DETALJ_INDEX     INTEGER,
   UTVALG_ID        BIGINT,
   EKSKLUDER         SMALLINT,
   PRIMARY KEY(UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XEDDST IS 'RA EgendefDetalj';
