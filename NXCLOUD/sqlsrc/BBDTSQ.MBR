-- ButikkLinje

CREATE OR REPLACE TABLE BBDTST(
 Firma               FOR COLUMN BIFIRM NUMERIC(3, 0) NOT NULL,
 Bongnr              FOR COLUMN BIBONG CHAR(12) CCSID 277 NOT NULL,
 Linjenr             FOR COLUMN BIBOLI NUMERIC(4, 0) NOT NULL,
 Leveringstype       FOR COLUMN BILETY NUMERIC(1, 0) NOT NULL DEFAULT 0,
 Varenr              FOR COLUMN BIVARE CHAR(15) CCSID 277 NOT NULL DEFAULT ' ',
 Varetekst1          FOR COLUMN BITEK1 CHAR(35) CCSID 277 NOT NULL DEFAULT ' ',
 Varetekst2          FOR COLUMN BITEK2 CHAR(35) CCSID 277 NOT NULL DEFAULT ' ',
 Enhet               FOR COLUMN BIENHE CHAR(3) CCSID 277 NOT NULL DEFAULT ' ',
 Antall              FOR COLUMN BIANTA NUMERIC(11, 3) NOT NULL DEFAULT 0,
 Valuta              FOR COLUMN BIVALU CHAR(3) CCSID 277 NOT NULL DEFAULT ' ',
 Salgspris           FOR COLUMN BISAPR NUMERIC(10, 3) NOT NULL DEFAULT 0,
 SalgsprisInklMva    FOR COLUMN BISAIM NUMERIC(9, 2) NOT NULL DEFAULT 0,
 SalgsprisOppr       FOR COLUMN BISAOP NUMERIC(10, 3) NOT NULL DEFAULT 0,
 Rabatt1             FOR COLUMN BIRAB1 NUMERIC(5, 2) NOT NULL DEFAULT 0,
 SumRabattbel1       FOR COLUMN BIRBE1 NUMERIC(10, 3) NOT NULL DEFAULT 0,
 SumRabattbel1InkMva FOR COLUMN BIRBI1 NUMERIC(9, 2) NOT NULL DEFAULT 0,
 Rabatt2             FOR COLUMN BIRAB2 NUMERIC(5, 2) NOT NULL DEFAULT 0,
 SumRabattbel2       FOR COLUMN BIRBE2 NUMERIC(10, 3) NOT NULL DEFAULT 0,
 SumRabattbel2InkMva FOR COLUMN BIRBI2 NUMERIC(9, 2) NOT NULL DEFAULT 0,
 BruksrettRabatt1    FOR COLUMN BIBRR1 NUMERIC(5, 2) NOT NULL DEFAULT 0,
 SumBruksrettRabbel1 FOR COLUMN BIBRB1 NUMERIC(10, 3) NOT NULL DEFAULT 0,
 SumBrrabBel1InkMva  FOR COLUMN BIBIB1 NUMERIC(9, 2) NOT NULL DEFAULT 0,
 BruksrettRabatt2    FOR COLUMN BIBRR2 NUMERIC(5, 2) NOT NULL DEFAULT 0,
 SumBruksrettRabbel2 FOR COLUMN BIBRB2 NUMERIC(10, 3) NOT NULL DEFAULT 0,
 SumBrrabBel2InkMva  FOR COLUMN BIBIB2 NUMERIC(9, 2) NOT NULL DEFAULT 0,
 OrdreRabatt         FOR COLUMN BIORAB NUMERIC(6, 3) NOT NULL DEFAULT 0,
 SumOrdreRabattbel   FOR COLUMN BIORBE NUMERIC(10, 3) NOT NULL DEFAULT 0,
 SumOrabBelInkMva    FOR COLUMN BIORBI NUMERIC(9, 2) NOT NULL DEFAULT 0,
 Nettopris           FOR COLUMN BINTPR NUMERIC(10, 3) NOT NULL DEFAULT 0,
 NettoprisInklMva    FOR COLUMN BINTIM NUMERIC(9, 2) NOT NULL DEFAULT 0,
 Kostpris            FOR COLUMN BIKOPR NUMERIC(10, 3) NOT NULL DEFAULT 0,
 KostprisInklMva     FOR COLUMN BIKOIM NUMERIC(9, 2) NOT NULL DEFAULT 0,
 KostprisOppr        FOR COLUMN BIKOOP NUMERIC(10, 3) NOT NULL DEFAULT 0,
 Avdeling            FOR COLUMN BIAVDE NUMERIC(4, 0) NOT NULL DEFAULT 0,
 Lager               FOR COLUMN BILAGE NUMERIC(2, 0) NOT NULL DEFAULT 0,
 Selger              FOR COLUMN BISELG NUMERIC(4, 0) NOT NULL DEFAULT 0,
 Serienr             FOR COLUMN BISERI CHAR(20) CCSID 277 NOT NULL DEFAULT ' ',
 Kommentar           FOR COLUMN BIKOMM CHAR(70) CCSID 277 NOT NULL DEFAULT ' ',
 Nobbnr              FOR COLUMN BINOBB NUMERIC(8, 0) NOT NULL DEFAULT 0,
 Modulnr             FOR COLUMN BIMODN NUMERIC(8, 0) NOT NULL DEFAULT 0,
 Priskode            FOR COLUMN BIKPRI CHAR(1) CCSID 277 NOT NULL DEFAULT ' ',
 MvaKode             FOR COLUMN BIKMVA CHAR(1) CCSID 277 NOT NULL DEFAULT ' ',
 MvaBerGrlag         FOR COLUMN BIFMVA NUMERIC(9, 2) NOT NULL DEFAULT 0,
 MvaGrunnlag         FOR COLUMN BIGMVA NUMERIC(5, 2) NOT NULL DEFAULT 0,
 MvaSats             FOR COLUMN BISMVA NUMERIC(5, 2) NOT NULL DEFAULT 0,
 MvaBelop            FOR COLUMN BIBMVA NUMERIC(9, 2) NOT NULL DEFAULT 0,
 Kampanje            FOR COLUMN BIKAMP CHAR(5) CCSID 277 NOT NULL DEFAULT ' ',
 Almenning           FOR COLUMN BIALME NUMERIC(4, 0) NOT NULL DEFAULT 0,
 BruksrettType       FOR COLUMN BIBRTY NUMERIC(1, 0) NOT NULL DEFAULT 0,
 Privat              FOR COLUMN BIPRIV NUMERIC(1, 0) NOT NULL DEFAULT 0,
 Ordrerabattkode     FOR COLUMN BIKORD NUMERIC(1, 0) NOT NULL DEFAULT 0,
 TerminalRefnr       FOR COLUMN BITREF NUMERIC(6, 0) NOT NULL DEFAULT 0,
 Ordrenr             FOR COLUMN BIORNR NUMERIC(8, 0) NOT NULL DEFAULT 0,
 OrdreSuffix         FOR COLUMN BIORSU NUMERIC(2, 0) NOT NULL DEFAULT 0,
 OrdreLinjenr        FOR COLUMN BIORLI NUMERIC(4, 0) NOT NULL DEFAULT 0,
 Bestillingsnr       FOR COLUMN BIBENR NUMERIC(8, 0) NOT NULL DEFAULT 0,
 BestillingSuffix    FOR COLUMN BIBESU NUMERIC(2, 0) NOT NULL DEFAULT 0,
 BestillingLinjenr   FOR COLUMN BIBELI NUMERIC(4, 0) NOT NULL DEFAULT 0,
 Gavekortnr          FOR COLUMN BIGAVE CHAR(12) CCSID 277 NOT NULL DEFAULT ' ',
 GavekortKundenr     FOR COLUMN BIGKUN NUMERIC(6, 0) NOT NULL DEFAULT 0,
 GavekortNavn        FOR COLUMN BIGNAV CHAR(35) CCSID 277 NOT NULL DEFAULT ' ',
 SumSalg             FOR COLUMN BISUMS NUMERIC(11, 3) NOT NULL DEFAULT 0,
 SumSalgInklMva      FOR COLUMN BISSIM NUMERIC(11, 2) NOT NULL DEFAULT 0,
 SumSalgOppr         FOR COLUMN BISUMO NUMERIC(11, 2) NOT NULL DEFAULT 0,
 SumRabatt           FOR COLUMN BISUMR NUMERIC(11, 3) NOT NULL DEFAULT 0,
 SumRabattInklMva    FOR COLUMN BISRIM NUMERIC(11, 2) NOT NULL DEFAULT 0,
 SumNetto            FOR COLUMN BISUMN NUMERIC(11, 3) NOT NULL DEFAULT 0,
 SumNettoInklMva     FOR COLUMN BISNIM NUMERIC(11, 2) NOT NULL DEFAULT 0,
 SumKost             FOR COLUMN BISUMK NUMERIC(11, 2) NOT NULL DEFAULT 0,
 SumKostInklMva      FOR COLUMN BISKIM NUMERIC(11, 2) NOT NULL DEFAULT 0,
 SumKostOppr         FOR COLUMN BISUKO NUMERIC(11, 2) NOT NULL DEFAULT 0,
 OverstyrtSalgspris  FOR COLUMN BIOVSP NUMERIC(1, 0) NOT NULL DEFAULT 0,
 FraStatistikk       FOR COLUMN BIFSTA NUMERIC(1, 0) NOT NULL DEFAULT 0,
 OpprettetDato       FOR COLUMN BIODAT DATE NOT NULL DEFAULT CURRENT DATE,
 OpprettetTid        FOR COLUMN BIOTIM TIME NOT NULL DEFAULT CURRENT TIME,
 OpprettetAv         FOR COLUMN BIOUSR CHAR(10) CCSID 277 NOT NULL DEFAULT ' ',
 EndretDato          FOR COLUMN BIEDAT DATE NOT NULL DEFAULT CURRENT DATE,
 EndretTid           FOR COLUMN BIETIM TIME NOT NULL DEFAULT CURRENT TIME,
 EndretAv            FOR COLUMN BIEUSR CHAR(10) CCSID 277 NOT NULL DEFAULT ' ')
 ON REPLACE PRESERVE ALL ROWS;

CREATE UNIQUE INDEX BBDTI1
	ON BBDTST (BIFIRM, BIBONG, BIBOLI);

CREATE UNIQUE INDEX BBDTIR
	ON BBDTST (BIFIRM, BIBONG, BIBOLI);

CREATE UNIQUE INDEX BBDTIU
	ON BBDTST (BIFIRM, BIBONG, BIBOLI);

LABEL ON TABLE BBDTST
	IS 'ButikkLinje';
