CREATE OR REPLACE TABLE CBUTST(
  Bonustype    FOR COLUMN CBTTYP CHAR(2) CCSID 277 NOT NULL DEFAULT ' ',
  Undertekst   FOR COLUMN CBTTEK CHAR(160) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

CREATE UNIQUE INDEX CBUTIU on CBUTST(CBTTYP);
CREATE INDEX CBUTIR on CBUTST(CBTTYP);
CREATE INDEX CBUTI1 on CBUTST(CBTTYP);

LABEL ON TABLE CBUTST IS 'Tilleggstekst bonustyper';

