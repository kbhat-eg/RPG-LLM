CREATE OR REPLACE TABLE FXL3ST (
    UID NUMERIC(15, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
        (START WITH 1 INCREMENT BY 1 CYCLE),
    Filgruppe         FOR COLUMN FXLFGR CHAR(3) NOT NULL DEFAULT ' ',
    Firma             FOR COLUMN FXLFIR NUMERIC(3, 0) NOT NULL DEFAULT 0,
    Varenummer        FOR COLUMN FXLVAR CHAR(15) NOT NULL DEFAULT ' ',
    Nobbnummer        FOR COLUMN FXLNOB CHAR(8) NOT NULL DEFAULT ' ',
    Modulnummer       FOR COLUMN FXLMOD CHAR(8) NOT NULL DEFAULT ' ',
    Overgruppe        FOR COLUMN FXLOGR NCHAR(2) NOT NULL DEFAULT ' ',
    Hovedgruppe       FOR COLUMN FXLHGR CHAR(2) NOT NULL DEFAULT ' ',
    Undergruppe       FOR COLUMN FXLUGR CHAR(3) NOT NULL DEFAULT ' ',
    Varetekst1        FOR COLUMN FXLTX1 CHAR(35) NOT NULL DEFAULT ' ',
    Varetekst2        FOR COLUMN FXLTX2 CHAR(35) NOT NULL DEFAULT ' ',
    GTIN              FOR COLUMN FXLGTI  CHAR(14) NOT NULL DEFAULT ' ',
    Leverandornavn    FOR COLUMN FXLLNAV CHAR(35) NOT NULL DEFAULT ' ',
    BasisEnhet        FOR COLUMN FXLENHE CHAR(3) NOT NULL DEFAULT ' ',
    StatEnhet         FOR COLUMN FXLENHS CHAR(3) NOT NULL DEFAULT ' ',
    LagerEnhet        FOR COLUMN FXLENHL CHAR(3) NOT NULL DEFAULT ' ',
    Prisgiver         FOR COLUMN FXLPGIV CHAR(6) NOT NULL DEFAULT ' ',
    Utgaardato        FOR COLUMN FXLUDAT DATE NOT NULL DEFAULT CURRENT_DATE,
    Feiltext          FOR COLUMN FXLFEIL CHAR(50) NOT NULL DEFAULT ' ',
    Opprettetdato     FOR COLUMN FXLODAT DATE NOT NULL DEFAULT CURRENT_DATE,
    Endretdato        FOR COLUMN FXLEDAT DATE NOT NULL DEFAULT CURRENT_DATE,
    Endretkl          FOR COLUMN FXLETIM TIME NOT NULL DEFAULT CURRENT_TIME,
    Endretav          FOR COLUMN FXLEUSER CHAR(10) NOT NULL DEFAULT ' ',
    PRIMARY KEY(UID))
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE FXL3ST IS 'SlipStream tabell 3';
