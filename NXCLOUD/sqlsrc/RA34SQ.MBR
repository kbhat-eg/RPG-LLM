CREATE OR REPLACE TABLE RA34ST(
  UID              FOR COLUMN RKFUID NUMERIC(18, 0) NOT NULL
                   GENERATED ALWAYS AS IDENTITY
                   (START WITH 1 INCREMENT BY 1 CYCLE),
  Firma            FOR COLUMN RKFFIR NUMERIC(3, 0) NOT NULL,
  Fact_selskap     FOR COLUMN RKFSEL CHAR(10) NOT NULL DEFAULT ' ',
  Fact_bankkonto   FOR COLUMN RKFBKK NUMERIC(11, 0) NOT NULL,
  Klientnummer     FOR COLUMN RKFKLI NUMERIC(8, 0) NOT NULL,
  Fastsiffer1      FOR COLUMN RKFFA1 NUMERIC(1, 0) NOT NULL,
  Fastsiffer2      FOR COLUMN RKFFA2 NUMERIC(2, 0) NOT NULL,
  Fastsiffer3      FOR COLUMN RKFFA3 NUMERIC(3, 0) NOT NULL,
  Fastsiffer4      FOR COLUMN RKFFA4 NUMERIC(4, 0) NOT NULL,
  Iban             FOR COLUMN RKFIBA CHAR(30) NOT NULL DEFAULT ' ',
  Swift            FOR COLUMN RKFSWI CHAR(30) NOT NULL DEFAULT ' ',
  Split_post       FOR COLUMN RKFSPL CHAR(1) NOT NULL DEFAULT ' ',
  FTP_izzy         FOR COLUMN RKFFTP CHAR(50) NOT NULL DEFAULT ' ',
  FTP_bruker       FOR COLUMN RKFUSR CHAR(50) NOT NULL DEFAULT ' ',
  FTP_pass         FOR COLUMN RKFPWD CHAR(50) NOT NULL DEFAULT ' ',
  Folder           FOR COLUMN RKFFLD CHAR(50) NOT NULL DEFAULT ' ',
  Otyp1            FOR COLUMN RKFOT1 CHAR(2) NOT NULL DEFAULT ' ',
  Otyp2            FOR COLUMN RKFOT2 CHAR(2) NOT NULL DEFAULT ' ',
  Otyp3            FOR COLUMN RKFOT3 CHAR(2) NOT NULL DEFAULT ' ',
  Otyp4            FOR COLUMN RKFOT4 CHAR(2) NOT NULL DEFAULT ' ',
  Otyp5            FOR COLUMN RKFOT5 CHAR(2) NOT NULL DEFAULT ' ',
  SistEndretDat    FOR COLUMN RKFEDA DATE NOT NULL DEFAULT CURRENT_DATE,
  SistEndretTid    FOR COLUMN RKFETI TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv         FOR COLUMN RKFEUS CHAR(10) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE RA34ST IS 'Factoring via izzy - parm';

CREATE UNIQUE INDEX RA34IU ON RA34ST(RKFFIR);
CREATE INDEX RA34IR ON RA34ST(RKFFIR);


