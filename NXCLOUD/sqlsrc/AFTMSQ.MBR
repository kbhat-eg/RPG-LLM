-- MAPPING AV SKUFFER FOR PROA, FRA V6.10

  CREATE OR REPLACE TABLE AFTMST
    ( UID INTEGER GENERATED ALWAYS AS IDENTITY,
      FIRMA INTEGER NOT NULL,
      SKRIVER VARCHAR(55) NOT NULL,
      SKUFF VARCHAR(55) NOT NULL,
      MEDIA VARCHAR(55) NOT NULL,
      OPPRETTETDATO DATE NOT NULL DEFAULT CURRENT_DATE,
      ENDRETDATO DATE NOT NULL DEFAULT CURRENT_DATE,
      ENDRETTID TIME NOT NULL DEFAULT CURRENT_TIME,
      ENDRETAV CHAR(10) NOT NULL,
      PRIMARY KEY(UID)
   )
   ON REPLACE PRESERVE ALL ROWS;

   CREATE UNIQUE INDEX AFTMIU ON AFTMST(FIRMA, SKRIVER, SKUFF);
