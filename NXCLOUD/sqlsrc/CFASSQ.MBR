CREATE OR REPLACE TABLE CFASST(
  Firma            FOR COLUMN CFAFIR NUMERIC(3, 0) NOT NULL DEFAULT 0,
  Fase             FOR COLUMN CFAFAS CHAR(4) NOT NULL DEFAULT ' ',
  Fasetekst        FOR COLUMN CFAFTK CHAR(35) NOT NULL DEFAULT ' ',
  OpprettetDato    FOR COLUMN CFAODA DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretDato       FOR COLUMN CFAEDA DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretKl         FOR COLUMN CFAETI TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv         FOR COLUMN CFAEUS CHAR(10) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

CREATE UNIQUE INDEX CFASIU on CFASST(CFAFIR,CFAFAS);
CREATE INDEX CFASIR on CFASST(CFAFIR,CFAFAS);
CREATE INDEX CFASI1 on CFASST(CFAFIR,CFAFAS);

LABEL ON TABLE CFASST IS 'Byggefaser';
