-- Overføring VareSlettet

CREATE OR REPLACE TABLE BOVSST( 
 Firma             FOR COLUMN FIRM NUMERIC(3, 0) NOT NULL,
 Varenr            FOR COLUMN VARE CHAR(15) CCSID 277 NOT NULL,
 SlettetDato       FOR COLUMN SDAT DATE,
 SlettetTid        FOR COLUMN STIM TIME,
 SlettetAv         FOR COLUMN SUSR CHAR(10) CCSID 277)
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE BOVSST
	IS 'Overføring VareSlettet';
