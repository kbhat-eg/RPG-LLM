CREATE OR REPLACE TABLE WIMPST(
  UID                  NUMERIC(18, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
                       (START WITH 1 INCREMENT BY 1 CYCLE),
  Ekstkey              FOR COLUMN NWIKEY CHAR(30) NOT NULL DEFAULT ' ',
  Line                 FOR COLUMN NWILIN NUMERIC(6, 0) NOT NULL,
  Fullrec              FOR COLUMN NWIREC CHAR(1000) NOT NULL DEFAULT ' ',
  Status_flag          FOR COLUMN NWISTF CHAR(10) DEFAULT 'Overf. ',
  Status_date          FOR COLUMN NWISTD DATE  DEFAULT CURRENT DATE,
  Status_time          FOR COLUMN NWISTT TIME  DEFAULT CURRENT TIME)
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE WIMPST IS 'Importtabell Winner integrasjon';

CREATE UNIQUE INDEX WIMPIU ON WIMPST(NWIKEY,NWILIN);
CREATE INDEX WIMPI1 ON WIMPST(NWIKEY,NWILIN);


