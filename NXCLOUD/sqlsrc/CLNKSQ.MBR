CREATE OR REPLACE TABLE CLNKST(
  Firma              FOR COLUMN CLNKFI  NUMERIC(3, 0) NOT NULL DEFAULT 0,
  Nobbkategori       FOR COLUMN CLNKKA  NUMERIC(4, 0) NOT NULL DEFAULT 0,
  Kategoritekst      FOR COLUMN CLNKTX  CHAR(40) NOT NULL DEFAULT ' ',
  Kategoriikon       FOR COLUMN CLNICO  CHAR(50) NOT NULL DEFAULT ' ',
  OpprettetDato      FOR COLUMN CLNKOD DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretDato         FOR COLUMN CLNKED DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretKl           FOR COLUMN CLNKET TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv           FOR COLUMN CLNKEU CHAR(10) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

CREATE UNIQUE INDEX CLNKIU on CLNKST(CLNKFI,CLNKKA);
CREATE INDEX CLNKIR on CLNKST(CLNKFI,CLNKKA);
CREATE INDEX CLNKI1 on CLNKST(CLNKFI,CLNKKA);

LABEL ON TABLE CLNKST IS 'Nobbkategori på leverandør';
