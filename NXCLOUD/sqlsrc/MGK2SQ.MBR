CREATE OR REPLACE TABLE MGK2ST(
  UID                NUMERIC(15, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
                     (START WITH 1 INCREMENT BY 1 CYCLE),
  Kampanje           FOR COLUMN JMVKAM CHAR(5) NOT NULL,
  Prisgruppe         FOR COLUMN JMVPRG CHAR(2) NOT NULL DEFAULT ' ',
  Varenummer         FOR COLUMN JMVVAR CHAR(15) NOT NULL DEFAULT ' ',
  Leverandor         FOR COLUMN JMVLDO NUMERIC(6, 0) NOT NULL DEFAULT 0,
  KampInnkjPris      FOR COLUMN JMVKAI NUMERIC(9, 2) NOT NULL DEFAULT 0,
  KampKostPris       FOR COLUMN JMVKPR NUMERIC(9, 2) NOT NULL DEFAULT 0,
  AnbefaltPris       FOR COLUMN JMVKAA NUMERIC(9, 2) NOT NULL DEFAULT 0,
  PaslagPris         FOR COLUMN JMVKAU NUMERIC(9, 2) NOT NULL DEFAULT 0,
  VareAnsvarlig      FOR COLUMN JMVANS CHAR(10) NOT NULL DEFAULT ' ',
  InnkjDatoFra       FOR COLUMN JMVIDF DATE NOT NULL,
  InnkjDatoTil       FOR COLUMN JMVIDT DATE NOT NULL,
  LaastLokalt        FOR COLUMN JMVLAS NUMERIC(1, 0) NOT NULL DEFAULT 0,
  OverfDato          FOR COLUMN JMVODA DATE NOT NULL,
  EndretDato         FOR COLUMN JMVEDA DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretKl           FOR COLUMN JMVETI TIME NOT NULL DEFAULT CURRENT_TIME,
  SolveEndrDato      FOR COLUMN JMVSDA DATE NOT NULL DEFAULT CURRENT_DATE,
  SolveEndrKl        FOR COLUMN JMVSTI TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv           FOR COLUMN JMVEUS CHAR(10) NOT NULL DEFAULT ' ');

ALTER TABLE MGK2ST ADD CONSTRAINT MGK2PK PRIMARY KEY (UID);

LABEL ON TABLE MGK2ST IS 'Solve kamp  V EKSPORT';
