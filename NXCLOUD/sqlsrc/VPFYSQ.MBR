CREATE OR REPLACE TABLE VPFYST(
  UID              NUMERIC(15, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
                   (START WITH 1 INCREMENT BY 1 CYCLE),
  Firma            FOR COLUMN VPFFIR NUMERIC(3, 0) NOT NULL DEFAULT 0,
  Varenummer       FOR COLUMN VPFVAR CHAR(15) NOT NULL DEFAULT ' ',
  Prisgruppe       FOR COLUMN VPFPRG CHAR(2) NOT NULL DEFAULT ' ',
  FraDato          FOR COLUMN VPFFDA DATE NOT NULL DEFAULT CURRENT_DATE,
  TilDato          FOR COLUMN VPFTDA DATE NOT NULL DEFAULT CURRENT_DATE,
  SortimentKode    FOR COLUMN VPFSKO CHAR(10) NOT NULL DEFAULT ' ',
  Enhet1           FOR COLUMN VPFEN1 CHAR(3) NOT NULL DEFAULT ' ',
  InnkjPris1       FOR COLUMN VPFIN1 NUMERIC(9, 2) NOT NULL DEFAULT 0,
  KostPris1        FOR COLUMN VPFKO1 NUMERIC(9, 2) NOT NULL DEFAULT 0,
  Prisforslag1     FOR COLUMN VPFOR1 NUMERIC(9, 2) NOT NULL DEFAULT 0,
  Enhet2           FOR COLUMN VPFEN2 CHAR(3) NOT NULL DEFAULT ' ',
  InnkjPris2       FOR COLUMN VPFIN2 NUMERIC(9, 2) NOT NULL DEFAULT 0,
  KostPris2        FOR COLUMN VPFKO2 NUMERIC(9, 2) NOT NULL DEFAULT 0,
  Prisforslag2     FOR COLUMN VPFOR2 NUMERIC(9, 2) NOT NULL DEFAULT 0,
  Enhet3           FOR COLUMN VPFEN3 CHAR(3) NOT NULL DEFAULT ' ',
  InnkjPris3       FOR COLUMN VPFIN3 NUMERIC(9, 2) NOT NULL DEFAULT 0,
  KostPris3        FOR COLUMN VPFKO3 NUMERIC(9, 2) NOT NULL DEFAULT 0,
  Prisforslag3     FOR COLUMN VPFOR3 NUMERIC(9, 2) NOT NULL DEFAULT 0,
  SolveVersjon  FOR COLUMN VPFVER NUMERIC(15, 0) NOT NULL DEFAULT 0,
  SolveLastTms  FOR COLUMN VPFTMS TIMESTAMP DEFAULT CURRENT_TIMESTAMP NOT NULL,
  Fident        FOR COLUMN VPFFID NUMERIC(15, 0) NOT NULL DEFAULT 0,
  OppretDato       FOR COLUMN VPFODA DATE NOT NULL DEFAULT CURRENT_DATE,
  OppretKl         FOR COLUMN VPFOTI TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretDato       FOR COLUMN VPFEDA DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretKl         FOR COLUMN VPFETI TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv         FOR COLUMN VPFEUS CHAR(10) NOT NULL DEFAULT ' ');

ALTER TABLE VPFYST ADD CONSTRAINT VPFYPK PRIMARY KEY (UID);

LABEL ON TABLE VPFYST IS 'Prisforslag utveksling';
