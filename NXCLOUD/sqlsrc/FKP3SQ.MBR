CREATE OR REPLACE TABLE FKP3ST(
  UID              FOR COLUMN F3VUID NUMERIC(18, 0) NOT NULL
                   GENERATED ALWAYS AS IDENTITY
                   (START WITH 1 INCREMENT BY 1 CYCLE),
  Firma            FOR COLUMN F3FIRM NUMERIC(3, 0) NOT NULL,
  Kundenummer      FOR COLUMN F3KUND NUMERIC(6, 0) NOT NULL,
  Kundeprosjekt    FOR COLUMN F3KPRO NUMERIC(6, 0) NOT NULL,
  ProfileText      FOR COLUMN F3PMTX CHAR(50) NOT NULL DEFAULT ' ',
  ProfileVarer     FOR COLUMN F3NOIT NUMERIC(9, 0) NOT NULL,
  ProfileKey       FOR COLUMN F3PKEY CHAR(15) NOT NULL DEFAULT ' ',
  OpprettetDato    FOR COLUMN F3ODAT DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl      FOR COLUMN F3OTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv      FOR COLUMN F3OUSR CHAR(10) NOT NULL DEFAULT ' ',
  EndretDato       FOR COLUMN F3EDAT DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretTid        FOR COLUMN F3ETIM TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv         FOR COLUMN F3EUSR CHAR(10) NOT NULL DEFAULT ' ')

ON REPLACE PRESERVE ALL ROWS;

  CREATE INDEX FKP3I1
   ON FKP3ST (F3FIRM, F3KUND, F3KPRO);

  CREATE INDEX FKP3IU
   ON FKP3ST (F3FIRM, F3KUND, F3KPRO);

  CREATE INDEX FKP3IR
   ON FKP3ST (F3FIRM, F3KUND, F3KPRO);

LABEL ON INDEX FKP3IU IS 'Kundeprosjekt - Profile Manager info';
LABEL ON INDEX FKP3IR IS 'Kundeprosjekt - Profile Manager info';
LABEL ON INDEX FKP3I1 IS 'Kundeprosjekt - Profile Manager info';
LABEL ON TABLE FKP3ST IS 'Kundeprosjekt - Profile Manager info';
