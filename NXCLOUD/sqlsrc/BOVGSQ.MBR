-- Overføring Varegruppe

CREATE OR REPLACE TABLE BOVGST( 
 Firma                FOR COLUMN FIRM NUMERIC(3, 0) NOT NULL,
 Overgruppe           FOR COLUMN OGRP NUMERIC(2, 0) NOT NULL,
 Hovedgruppe          FOR COLUMN HGRP NUMERIC(2, 0) NOT NULL,
 Undergruppe          FOR COLUMN UGRP NUMERIC(3, 0) NOT NULL,
 Beskrivelse          FOR COLUMN BESK CHAR(35) CCSID 277,
 Produkstansvarlig    FOR COLUMN PRAN CHAR(35) CCSID 277,
 Dekningsgrad         FOR COLUMN DEKN NUMERIC(5, 2),
 IkkeAvrunding        FOR COLUMN IAVR NUMERIC(1, 0),
 IkkeLageroppdatering FOR COLUMN ILAG NUMERIC(1, 0),
 OpprettetDato        FOR COLUMN ODAT DATE,
 EndretDato           FOR COLUMN EDAT DATE,
 EndretTid            FOR COLUMN ETIM TIME,
 EndretAv             FOR COLUMN EUSR CHAR(10) CCSID 277)
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE BOVGST
	IS 'Overføring Varegruppe';
