
CREATE OR REPLACE TABLE XSLSST
(
   UID              BIGINT    NOT NULL GENERATED
   ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
   RAPPORT_TYPE     INTEGER,
   FIRMA            INTEGER,
   AVDELINGER       VARCHAR(255),
   SCHEDULE_ID      BIGINT,
   PRIMARY KEY(UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XSLSST IS 'RA Lagerstyringsrapport';
