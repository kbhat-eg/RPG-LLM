CREATE OR REPLACE TABLE AKJDST(
  UID              NUMERIC(15, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
                   (START WITH 1 INCREMENT BY 1 CYCLE),
  Kjedekode        FOR COLUMN AQKKOD CHAR(2) NOT NULL DEFAULT ' ',
  Kjedenavn        FOR COLUMN AQNAVN CHAR(35) NOT NULL,
  LogoUrl          FOR COLUMN AQLOGO CHAR(200) NOT NULL DEFAULT ' ',
  RapportLogoUrl   FOR COLUMN AQRLOG CHAR(200) NOT NULL DEFAULT ' ',
  Hjemmeside       FOR COLUMN AQHMPG CHAR(200) NOT NULL DEFAULT ' ',
  OpprettetDato    FOR COLUMN AQODAT DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretDato       FOR COLUMN AQEDAT DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretKl         FOR COLUMN AQETIM TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv         FOR COLUMN AQEUSR CHAR(10) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

ALTER TABLE AKJDST ADD CONSTRAINT AKJDPK PRIMARY KEY (UID);
CREATE UNIQUE INDEX AKJDIU on AKJDST(AQKKOD);
CREATE INDEX AKJDIR on AKJDST(AQKKOD);
CREATE INDEX AKJDI1 on AKJDST(AQKKOD);

LABEL ON TABLE AKJDST IS 'Kjede koder';
