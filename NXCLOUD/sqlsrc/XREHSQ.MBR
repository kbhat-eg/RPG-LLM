
CREATE OR REPLACE TABLE XREHST
(
   UID              BIGINT    NOT NULL GENERATED
   ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
   NAVN             VARCHAR(255),
   BESKRIVELSE      VARCHAR(255),
   TA_MED_BLANKE    SMALLINT,
   DYNAMISK         SMALLINT,
   RAMME_VERT       INTEGER,
   RAMME_HORIS      INTEGER,
   RAMME_SUM        INTEGER,
   RAMME_OVER_SUM   INTEGER,
   RAPPORT_TYPE     INTEGER,
   PRIMARY KEY(UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XREHST IS 'RA OEDRapport';
