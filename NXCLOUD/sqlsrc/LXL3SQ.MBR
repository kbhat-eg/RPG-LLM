CREATE OR REPLACE TABLE LXL3ST (
    UID NUMERIC(15, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
        (START WITH 1 INCREMENT BY 1 CYCLE),
    Identnr           FOR COLUMN LX3IDE NUMERIC(10, 0) NOT NULL DEFAULT 0,
    Varenummer        FOR COLUMN LX3VAR CHAR(15) NOT NULL DEFAULT ' ',
    NOBBnummer        FOR COLUMN LX3NOB NUMERIC(8, 0) NOT NULL DEFAULT 0,
    Varetekst1        FOR COLUMN LX3TX1 CHAR(35) NOT NULL DEFAULT ' ',
    Varetekst2        FOR COLUMN LX3TX2 CHAR(35) NOT NULL DEFAULT ' ',
    Antall            FOR COLUMN LX3ANT NUMERIC(11, 3) NOT NULL DEFAULT 0,
    Enhet             FOR COLUMN LX3ENH CHAR(3) NOT NULL DEFAULT ' ',
    BruttoPris        FOR COLUMN LX3BSA NUMERIC(9, 2) NOT NULL DEFAULT 0,
    NettoPris         FOR COLUMN LX3NSA NUMERIC(9, 2) NOT NULL DEFAULT 0,
    SumBrutto         FOR COLUMN LX3BSU NUMERIC(11, 2) NOT NULL DEFAULT 0,
    Rabatt1           FOR COLUMN LX3RA1 NUMERIC(5, 2) NOT NULL DEFAULT 0,
    Rabatt2           FOR COLUMN LX3RA2 NUMERIC(5, 2) NOT NULL DEFAULT 0,
    SumNetto          FOR COLUMN LX3NSU NUMERIC(11, 2) NOT NULL DEFAULT 0,
    Kostpris          FOR COLUMN LX3KOS NUMERIC(9, 2) NOT NULL DEFAULT 0,
    SumKost           FOR COLUMN LX3SKO NUMERIC(11, 2) NOT NULL DEFAULT 0,
    Dekningsbidrag    FOR COLUMN LX3DBI NUMERIC(11, 2) NOT NULL DEFAULT 0,
    Dekningsgrad      FOR COLUMN LX3DGR NUMERIC(5, 2) NOT NULL DEFAULT 0,
    Linjenummer       FOR COLUMN LX3LIN NUMERIC(4, 0) NOT NULL DEFAULT 0,
    GTIN              FOR COLUMN LX3GTI NUMERIC(14, 0) NOT NULL DEFAULT 0,
    BestiltAntall     FOR COLUMN LX3BAN NUMERIC(11, 3) NOT NULL DEFAULT 0,
    BestiltEnhet      FOR COLUMN LX3BEN CHAR(3) NOT NULL DEFAULT ' ',
    KundensOrdrenr    FOR COLUMN LX3KOR CHAR(15) NOT NULL DEFAULT ' ',
    KundensOrdreLine  FOR COLUMN LX3KOL CHAR(15) NOT NULL DEFAULT ' ',
    SalgsordreNummer  FOR COLUMN LX3NUM CHAR(12) NOT NULL DEFAULT ' ',
    SSCCNummer        FOR COLUMN LX3SSC CHAR(35) NOT NULL DEFAULT ' ',

    PRIMARY KEY(UID))
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE LXL3ST IS 'SlipStream ordrelinjer';
