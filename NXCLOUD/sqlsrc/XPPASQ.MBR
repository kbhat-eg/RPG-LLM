CREATE OR REPLACE TABLE FRONT_PAKKSEDDELPART FOR SYSTEM NAME XPPAST
(
  Firma            FOR COLUMN XPFIRM NUMERIC(3, 0) NOT NULL,
  Guid             FOR COLUMN XPGUID CHAR(40) NOT NULL DEFAULT ' ',
  Parttype         FOR COLUMN XPPATY CHAR(2) NOT NULL DEFAULT ' ',
  PartnerID        FOR COLUMN XPPART CHAR(35) NOT NULL DEFAULT ' ',
  PartnerName      FOR COLUMN XPNAVN CHAR(35) NOT NULL DEFAULT ' ',
  Adresse1         FOR COLUMN XPADR1 CHAR(35) NOT NULL DEFAULT ' ',
  Adresse2         FOR COLUMN XPADR2 CHAR(35) NOT NULL DEFAULT ' ',
  Postnummer       FOR COLUMN XPPONR CHAR(35) NOT NULL DEFAULT ' ',
  Sted             FOR COLUMN XPSTED CHAR(35) NOT NULL DEFAULT ' ',
  Landkode         FOR COLUMN XPLAND CHAR(3) NOT NULL DEFAULT ' ',
  Kontaktperson    FOR COLUMN XPKONT CHAR(70) NOT NULL DEFAULT ' ',
  Telefonnummer    FOR COLUMN XPTLNR CHAR(35) NOT NULL DEFAULT ' ',
  Email            FOR COLUMN XPEMAL CHAR(70) NOT NULL DEFAULT ' ')

ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE FRONT_PAKKSEDDELPART   IS 'Pakkseddel parter - forsystem';
