CREATE OR REPLACE TABLE MGK1ST(
  UID                NUMERIC(15, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
                     (START WITH 1 INCREMENT BY 1 CYCLE),
  Kampanje           FOR COLUMN JMKAMP CHAR(5) NOT NULL,
  Prisgruppe         FOR COLUMN JMPRGR CHAR(2) NOT NULL DEFAULT ' ',
  Beskrivelse        FOR COLUMN JMBESK CHAR(30) NOT NULL,
  KapanjeFraDato     FOR COLUMN JMFDAT DATE NOT NULL,
  KapanjeTilDato     FOR COLUMN JMTDAT DATE NOT NULL,
  Ansvarlig          FOR COLUMN JMANSV CHAR(10)  NOT NULL DEFAULT '  ',
  ForstOvfDato       FOR COLUMN JMFODA DATE NOT NULL,
  OverfDato          FOR COLUMN JMODAT DATE NOT NULL,
  EndretDato         FOR COLUMN JMEDAT DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretKl           FOR COLUMN JMETIM TIME NOT NULL DEFAULT CURRENT_TIME,
  SolveEndrDato      FOR COLUMN JMSEDA DATE NOT NULL DEFAULT CURRENT_DATE,
  SolveEndrKl        FOR COLUMN JMSETI TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv           FOR COLUMN JMEUSR CHAR(10) NOT NULL DEFAULT ' ');

ALTER TABLE MGK1ST ADD CONSTRAINT MGK1PK PRIMARY KEY (UID);

LABEL ON TABLE MGK1ST IS 'Solve kampH -EKSPORT ';
