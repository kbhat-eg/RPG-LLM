CREATE OR REPLACE TABLE FODMST(
  Firma            FOR COLUMN FDFIRM NUMERIC(3, 0) NOT NULL,
  Ordrenr          FOR COLUMN FDNUMM NUMERIC(8, 0) NOT NULL,
  Suffix           FOR COLUMN FDSUFF NUMERIC(2, 0) NOT NULL,
  Linjenr          FOR COLUMN FDLINE NUMERIC(4, 0) NOT NULL,
  OpprSalgspris    FOR COLUMN FDOSAP NUMERIC(9, 2) NOT NULL,
  OpprTilbudspris  FOR COLUMN FDOTIP NUMERIC(9, 2) NOT NULL,
  OpprKostpris     FOR COLUMN FDOKOP NUMERIC(9, 2) NOT NULL,
  OpprInnkjPris    FOR COLUMN FDOINP NUMERIC(9, 2) NOT NULL,
  OpprPriskode     FOR COLUMN FDOPPK CHAR(1) NOT NULL DEFAULT ' ',
  OpprKundepris    FOR COLUMN FDOKPR NUMERIC(9, 2) NOT NULL,
  OpprKundeRab1    FOR COLUMN FDOKR1 NUMERIC(5, 2) NOT NULL,
  OpprKundeRab2    FOR COLUMN FDOKR2 NUMERIC(5, 2) NOT NULL,
  OpprKundePrisK   FOR COLUMN FDOKPK CHAR(1) NOT NULL DEFAULT ' ',
  OpprBestiltAnt   FOR COLUMN FDOANT NUMERIC(11, 2) NOT NULL,
  OpprBestiltEnh   FOR COLUMN FDOENH CHAR(3) NOT NULL DEFAULT ' ',
  Timestamp        FOR COLUMN FDTMST TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
  OpprettetDato    FOR COLUMN FDODAT DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl      FOR COLUMN FDOTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv      FOR COLUMN FDOUSR CHAR(10) NOT NULL DEFAULT ' ',
  EndretDato       FOR COLUMN FDEDAT DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretTid        FOR COLUMN FDETIM TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv         FOR COLUMN FDEUSR CHAR(10) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

  CREATE INDEX FODMI1
   ON FODMST (FDFIRM, FDNUMM, FDSUFF, FDLINE);

  CREATE INDEX FODMIU
   ON FODMST (FDFIRM, FDNUMM, FDSUFF, FDLINE);

  CREATE INDEX FODMIR
   ON FODMST (FDFIRM, FDNUMM, FDSUFF, FDLINE);

LABEL ON TABLE FODMST IS 'Ordre-linje MGR skygge';
