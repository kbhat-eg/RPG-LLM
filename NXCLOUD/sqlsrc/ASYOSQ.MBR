-- ADMINISTRASJON/OPPSETT

CREATE OR REPLACE TABLE ASYOST(
 UID                FOR COLUMN ASUIDN NUMERIC(9, 0) NOT NULL,
 SYSTEM             FOR COLUMN ASNSYS CHAR(3) CCSID 277 DEFAULT ' ',
 FUNKSJON           FOR COLUMN ASNFUN CHAR(20) CCSID 277 DEFAULT ' ',
 BESKRIVELSE        FOR COLUMN ASNBES CHAR(35) CCSID 277 DEFAULT ' ',
 PROGRAM            FOR COLUMN ASPROG CHAR(128) CCSID 277 DEFAULT ' ',
 OPPRETTETAV        FOR COLUMN ASOUSR CHAR(10) NOT NULL DEFAULT ' ',
 OPPRETTETKL        FOR COLUMN ASOTIM TIME NOT NULL DEFAULT CURRENT_TIME,
 OPPRETTETDATO      FOR COLUMN ASODAT DATE NOT NULL DEFAULT CURRENT_DATE,
 ENDRETDATO         FOR COLUMN ASEDAT DATE NOT NULL DEFAULT CURRENT_DATE,
 ENDRETKL           FOR COLUMN ASETIM TIME NOT NULL DEFAULT CURRENT_TIME,
 ENDRETAV           FOR COLUMN ASEUSR CHAR(10) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

CREATE UNIQUE INDEX ASYOIU
 ON ASYOST (ASNSYS, ASNFUN);

CREATE UNIQUE INDEX ASYOI1
 ON ASYOST (ASNSYS, ASNFUN);

CREATE UNIQUE INDEX ASYOIR
 ON ASYOST (ASNSYS, ASNFUN);

LABEL ON TABLE ASYOST
 IS 'ADMINISTRASJONS OPPSETT';
