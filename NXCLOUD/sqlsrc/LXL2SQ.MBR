CREATE OR REPLACE TABLE LXL2ST (
    UID NUMERIC(15, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
        (START WITH 1 INCREMENT BY 1 CYCLE),
    Identnr           FOR COLUMN LX2IDE NUMERIC(10, 0) NOT NULL DEFAULT 0,
    Firma             FOR COLUMN LX2FIR NUMERIC(3, 0) NOT NULL DEFAULT 0,
    Overgruppe        FOR COLUMN LX2OGR NUMERIC(2, 0) NOT NULL DEFAULT 0,
    Hovedgruppe       FOR COLUMN LX2HGR NUMERIC(2, 0) NOT NULL DEFAULT 0,
    Undergruppe       FOR COLUMN LX2UGR NUMERIC(3, 0) NOT NULL DEFAULT 0,
    Lager             FOR COLUMN LX2LAG NUMERIC(2, 0) NOT NULL DEFAULT 0,
    NOBBnummer        FOR COLUMN LX2NOB NUMERIC(8, 0) NOT NULL DEFAULT 0,
    Varenummer        FOR COLUMN LX2VAR CHAR(15) NOT NULL DEFAULT ' ',
    Varetekst1        FOR COLUMN LX2TX1 CHAR(35) NOT NULL DEFAULT ' ',
    Varetekst2        FOR COLUMN LX2TX2 CHAR(35) NOT NULL DEFAULT ' ',
    Enhet             FOR COLUMN LX2ENH CHAR(3) NOT NULL DEFAULT ' ',
    TelletKvantum     FOR COLUMN LX2TQT NUMERIC(11, 2) DEFAULT 0,
    Lagerbeholdning   FOR COLUMN LX2LBE NUMERIC(11, 2) DEFAULT 0,
    Telledifferanse   FOR COLUMN LX2DIF NUMERIC(11, 2) DEFAULT 0,
    Salgspris         FOR COLUMN LX2SAP NUMERIC(9, 2) DEFAULT 0,
    Salgsverdi        FOR COLUMN LX2SVE NUMERIC(13, 2) DEFAULT 0,
    Kostpris          FOR COLUMN LX2KOS NUMERIC(9, 2) DEFAULT 0,
    Kostverdi         FOR COLUMN LX2KVE NUMERIC(13, 2) DEFAULT 0,
    TelletVerdi       FOR COLUMN LX2TVE NUMERIC(11, 2) DEFAULT 0,
    Lagerverdi        FOR COLUMN LX2LVE NUMERIC(13, 2) DEFAULT 0,
    Avviksverdi       FOR COLUMN LX2DVE NUMERIC(13, 2) DEFAULT 0,
    Tellekode         FOR COLUMN LX2ABC CHAR(2) NOT NULL DEFAULT ' ',
    Lagerplass        FOR COLUMN LX2LOC CHAR(8) NOT NULL DEFAULT ' ',
    Lagerplass1       FOR COLUMN LX2LO1 CHAR(8) NOT NULL DEFAULT ' ',
    Lagerplass2       FOR COLUMN LX2LO2 CHAR(8) NOT NULL DEFAULT ' ',
    PRIMARY KEY(UID))
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE LXL2ST IS 'SlipStream avviksliste';
