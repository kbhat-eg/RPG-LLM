CREATE OR REPLACE TABLE CLREST(
  Firma            FOR COLUMN CLRFIR NUMERIC(3, 0) NOT NULL DEFAULT 0,
  Leverandor       FOR COLUMN CLRLEV NUMERIC(6, 0) NOT NULL DEFAULT 0,
  Region           FOR COLUMN CLRREG NUMERIC(4, 0) NOT NULL DEFAULT 0,
  OpprettetDato    FOR COLUMN CLRODA DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretDato       FOR COLUMN CLREDA DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretKl         FOR COLUMN CLRETI TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv         FOR COLUMN CLREUS CHAR(10) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

CREATE UNIQUE INDEX CLREIU on CLREST(CLRFIR,CLRLEV,CLRREG);
CREATE INDEX CLREIR on CLREST(CLRFIR,CLRLEV,CLRREG);
CREATE INDEX CLREI1 on CLREST(CLRFIR,CLRLEV,CLRREG);

LABEL ON TABLE CLREST IS 'Leverand√∏r/Distrikt';
