
CREATE OR REPLACE TABLE XSQCST
(
   UID              BIGINT    NOT NULL GENERATED
   ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
   QUERY_ID         BIGINT,
   FELT_ID          BIGINT,
   ORDERBY_INDEX    INTEGER,
   ORDERBY_RETNING  INTEGER,
   VALGT            SMALLINT,
   PRIMARY KEY(UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XSQCST IS 'RA SqlQueryColumn';
