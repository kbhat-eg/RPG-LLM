CREATE OR REPLACE TABLE IBUTST(
  UID                 INTEGER GENERATED ALWAYS AS IDENTITY,
  Company             FOR COLUMN IBFIRM NUMERIC(3, 0) NOT NULL,
  Bilagsnummer        FOR COLUMN IBBILN NUMERIC(8, 0) NOT NULL,
  Bilagsdato          FOR COLUMN IBBDAT DATE NOT NULL,
  Forfallsdato        FOR COLUMN IBFDAT DATE NOT NULL,
  Bilagskode          FOR COLUMN IBBILK NUMERIC(2, 0) NOT NULL,
  Bilagstekst         FOR COLUMN IBTEXT CHAR(20) NOT NULL DEFAULT,
  Debetavd            FOR COLUMN IBDAVD NUMERIC(4, 0) NOT NULL,
  Debetkonto          FOR COLUMN IBDKTO NUMERIC(6, 0) NOT NULL,
  Debetspes           FOR COLUMN IBDSPS NUMERIC(4, 0) NOT NULL,
  Debetmvakode        FOR COLUMN IBDMOM CHAR(1) NOT NULL DEFAULT,
  Kreditavd           FOR COLUMN IBCAVD NUMERIC(4, 0) NOT NULL,
  Kredittonto         FOR COLUMN IBCKTO NUMERIC(6, 0) NOT NULL,
  Kreditspes          FOR COLUMN IBCSPS NUMERIC(4, 0) NOT NULL,
  Kreditmvakode       FOR COLUMN IBCMOM CHAR(1) NOT NULL DEFAULT,
  Belop               FOR COLUMN IBBELP NUMERIC(10, 2) NOT NULL,
  Mvagrunnlag         FOR COLUMN IBMVAG NUMERIC(10, 2) NOT NULL,
  Kundenummer         FOR COLUMN IBKUND NUMERIC(6, 0) NOT NULL,
  Kreditreferanse     FOR COLUMN IBKREF NUMERIC(8, 0) NOT NULL,
  Kid                 FOR COLUMN IBKIDI CHAR(25) NOT NULL DEFAULT,
  Kortkid             FOR COLUMN IBKIDR CHAR(9) NOT NULL DEFAULT,
  Valutakode          FOR COLUMN IBVALK CHAR(3) NOT NULL DEFAULT,
  Valutabelop         FOR COLUMN IBVALB NUMERIC(11, 2) NOT NULL,
  Prosjekt            FOR COLUMN IBPROJ CHAR(6) NOT NULL DEFAULT,
  Aktivitet           FOR COLUMN IBAKTI CHAR(6) NOT NULL DEFAULT,
  OpprettetDato       FOR COLUMN IBODAT DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl         FOR COLUMN IBOTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv         FOR COLUMN IBOUSR CHAR(10) NOT NULL DEFAULT 'NXSYSTEM',
  Transaksjonstype    FOR COLUMN IBTYPE CHAR(4) NOT NULL DEFAULT,
  Betbet              FOR COLUMN IBBETB NUMERIC(2, 0) NOT NULL,
  Lest_flagg          FOR COLUMN IBFLAG CHAR(1) NOT NULL DEFAULT,
  Bongnummer          FOR COLUMN IBBONG CHAR(12) NOT NULL DEFAULT,
  Depositum           FOR COLUMN IBDEPO CHAR(1) NOT NULL DEFAULT)
ON REPLACE PRESERVE ALL ROWS;

ALTER TABLE IBUTST ADD CONSTRAINT IB_UID PRIMARY KEY (UID);

LABEL ON TABLE IBUTST IS 'Regnskapstranser fra butikk til NAV';
