CREATE OR REPLACE TABLE ASPLST(
  UID              NUMERIC(15, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
                   (START WITH 1 INCREMENT BY 1 CYCLE),
  Firmanr          FOR COLUMN XSFIRM NUMERIC(3, 0) NOT NULL DEFAULT 0,
  Batch            FOR COLUMN XSBATC CHAR(50) NOT NULL,
  Xml              FOR COLUMN XSXMLD CLOB(50M),
  "TIME"           FOR COLUMN XSTIME TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
  "USER"           FOR COLUMN XSUSER CHAR(10) NOT NULL)
  ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE ASPLST IS 'Temp spool base';
