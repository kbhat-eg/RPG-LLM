
CREATE OR REPLACE TABLE XBMKST
(
UID                     BIGINT NOT NULL GENERATED
 ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
MESTER_BYGG_MESTER_ID   BIGINT NOT NULL,
FILGRUPPE               VARCHAR (20),
FIRMA_NR                BIGINT NOT NULL,
KUNDE_NR                BIGINT NOT NULL,
ORG_NUMMER              BIGINT,
KOST_BOENHET            BIGINT,
FAKTOR_BOENHET          NUMERIC (10, 2),
ANDEL_HUSKJEDE          NUMERIC (10, 2),
OPPRETTET_DATO          DATE DEFAULT CURRENT_DATE,
BRUKER                  VARCHAR(20),
PRIMARY KEY (UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XBMKST IS 'RA MestergruppenByggmesterKunde';
