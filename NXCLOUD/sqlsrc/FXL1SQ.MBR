CREATE OR REPLACE TABLE FXL1ST (
    UID NUMERIC(15, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
        (START WITH 1 INCREMENT BY 1 CYCLE),
    Firma             FOR COLUMN FXLFIR NUMERIC(3, 0) NOT NULL DEFAULT 0,
    Varenummer        FOR COLUMN FXLVAR CHAR(15) NOT NULL DEFAULT ' ',
    Varetekst         FOR COLUMN FXLTXT CHAR(35) NOT NULL DEFAULT ' ',
    Leverandornr      FOR COLUMN FXLLDOR NUMERIC(6, 0) DEFAULT 0,
    Leverandornavn    FOR COLUMN FXLLNAV CHAR(35) NOT NULL DEFAULT ' ',
    Basisenhet        FOR COLUMN FXLENH0 CHAR(3) NOT NULL DEFAULT ' ',
    Enhet             FOR COLUMN FXLENHE CHAR(3) NOT NULL DEFAULT ' ',
    Prisgiver         FOR COLUMN FXLPGIV NUMERIC(6, 0) DEFAULT 0,
    Prisgruppe        FOR COLUMN FXLPRGR CHAR(2) NOT NULL DEFAULT ' ',
    Returkode         FOR COLUMN FXLRKOD CHAR(1) NOT NULL DEFAULT ' ',
    Prisdato          FOR COLUMN FFXPDAT DATE NOT NULL DEFAULT CURRENT_DATE,
    Gmlsalgspris      FOR COLUMN FXLGSAP NUMERIC(9, 2) DEFAULT 0,
    Gmlkostpris       FOR COLUMN FXLGKOS NUMERIC(9, 2) DEFAULT 0,
    Gmlinnkpris       FOR COLUMN FXLGINP NUMERIC(9, 2) DEFAULT 0,
    Gmlnobbpris       FOR COLUMN FXLGNOB NUMERIC(9, 2) DEFAULT 0,
    GmlDG             FOR COLUMN FXLGDG NUMERIC(5, 2) DEFAULT 0,
    Nysalgspris       FOR COLUMN FXLNSAP NUMERIC(9, 2) DEFAULT 0,
    Salgprisendr      FOR COLUMN FXLSAPP NUMERIC(5, 2) DEFAULT 0,
    Nykostpris        FOR COLUMN FXLNKOS NUMERIC(9, 2) DEFAULT 0,
    Koprprisendr      FOR COLUMN FXLKOPP NUMERIC(5, 2) DEFAULT 0,
    Nyinnkpris        FOR COLUMN FXLNINK NUMERIC(9, 2) DEFAULT 0,
    Innkprisendr      FOR COLUMN FXLINPP NUMERIC(5, 2) DEFAULT 0,
    Nynobbpris        FOR COLUMN FXLNNOB NUMERIC(9, 2) DEFAULT 0,
    NyDG              FOR COLUMN FXLNDG NUMERIC(5, 2) DEFAULT 0,
    DGendr            FOR COLUMN FXLDGP  NUMERIC(5, 2) DEFAULT 0,
    Gmlvaregr         FOR COLUMN FXLGVGR CHAR(7) NOT NULL DEFAULT ' ',
    Nyvaregr          FOR COLUMN FXLNVGR CHAR(7) NOT NULL DEFAULT ' ',
    GmlModul          FOR COLUMN FXLGMOD NUMERIC(8, 0) DEFAULT 0,
    NyModul           FOR COLUMN FXLNMOD NUMERIC(8, 0) DEFAULT 0,
    Kampanje          FOR COLUMN FXLKAMP CHAR(5) NOT NULL DEFAULT ' ',
    Kampfradato       FOR COLUMN FFXFDAT DATE NOT NULL DEFAULT CURRENT_DATE,
    Kamptildato       FOR COLUMN FFXTDAT DATE NOT NULL DEFAULT CURRENT_DATE,
    Kampenh1          FOR COLUMN FXLKEN1 CHAR(3) NOT NULL DEFAULT ' ',
    Kampkost1         FOR COLUMN FXLKKO1 NUMERIC(9, 2) DEFAULT 0,
    Kamptilb1         FOR COLUMN FXLKTI1 NUMERIC(9, 2) DEFAULT 0,
    Kampenh2          FOR COLUMN FXLKEN2 CHAR(3) NOT NULL DEFAULT ' ',
    Kampkost2         FOR COLUMN FXLKKO2 NUMERIC(9, 2) DEFAULT 0,
    Kamptilb2         FOR COLUMN FXLKTI2 NUMERIC(9, 2) DEFAULT 0,
    Kampenh3          FOR COLUMN FXLKEN3 CHAR(3) NOT NULL DEFAULT ' ',
    Kampkost3         FOR COLUMN FXLKKO3 NUMERIC(9, 2) DEFAULT 0,
    Kamptilb3         FOR COLUMN FXLKTI3 NUMERIC(9, 2) DEFAULT 0,
    Utgaardato        FOR COLUMN FXLUDAT DATE NOT NULL DEFAULT CURRENT_DATE,
    Feiltext          FOR COLUMN FXLFEIL CHAR(50) NOT NULL DEFAULT ' ',
    Opprettetdato     FOR COLUMN FXLODAT DATE NOT NULL DEFAULT CURRENT_DATE,
    Endretdato        FOR COLUMN FXLEDAT DATE NOT NULL DEFAULT CURRENT_DATE,
    Endretkl          FOR COLUMN FXLETIM TIME NOT NULL DEFAULT CURRENT_TIME,
    Endretav          FOR COLUMN FXLEUSER CHAR(10) NOT NULL DEFAULT ' ',
    PRIMARY KEY(UID))
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE FXL1ST IS 'SlipStream tabell 1';
