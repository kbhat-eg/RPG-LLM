CREATE OR REPLACE TABLE ZPFAST(
  Instans          FOR COLUMN ZCINST CHAR(10) CCSID 277 NOT NULL,
  Firma            FOR COLUMN ZCFIRM NUMERIC(3, 0) NOT NULL,
  Kundegruppe      FOR COLUMN ZCKUGR CHAR(10) CCSID 277 NOT NULL,
  Kundekjopstype   FOR COLUMN ZCKKTY CHAR(10) CCSID 277 NOT NULL,
  Avdelingsgruppe  FOR COLUMN ZCAGRP NUMERIC(4, 0) NOT NULL,
  Overgruppe       FOR COLUMN ZCOGRP NUMERIC(2, 0) NOT NULL,
  Hovedgruppe      FOR COLUMN ZCHGRP NUMERIC(2, 0) NOT NULL,
  Undergruppe      FOR COLUMN ZCUGRP NUMERIC(3, 0) NOT NULL,
  Leverandor       FOR COLUMN ZCLDOR NUMERIC(6, 0) NOT NULL,
  Modul            FOR COLUMN ZCMODN NUMERIC(8, 0) NOT NULL,
  StartFaktor      FOR COLUMN ZCSTFA NUMERIC(12, 6) NOT NULL,
  TargetFaktor     FOR COLUMN ZCTGFA NUMERIC(12, 6) NOT NULL,
  FloorFaktor      FOR COLUMN ZCFLFA NUMERIC(12, 6) NOT NULL,
  Timestamp        FOR COLUMN ZCTMST TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
  OpprettetDato    FOR COLUMN ZCODAT DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl      FOR COLUMN ZCOTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv      FOR COLUMN ZCOUSR CHAR(10) NOT NULL DEFAULT ' ',
  EndretDato       FOR COLUMN ZCEDAT DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretTid        FOR COLUMN ZCETIM TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv         FOR COLUMN ZCEUSR CHAR(10) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

  CREATE INDEX ZPFAI1
   ON ZPFAST (ZCINST, ZCFIRM, ZCKUGR, ZCKKTY, ZCAGRP, ZCOGRP, ZCHGRP, ZCUGRP, +
   ZCLDOR, ZCMODN);

  CREATE UNIQUE INDEX ZPFAIR
   ON ZPFAST (ZCINST, ZCFIRM, ZCKUGR, ZCKKTY, ZCAGRP, ZCOGRP, ZCHGRP, ZCUGRP, +
   ZCLDOR, ZCMODN);

  CREATE INDEX ZPFAIU
   ON ZPFAST (ZCINST, ZCFIRM, ZCKUGR, ZCKKTY, ZCAGRP, ZCOGRP, ZCHGRP, ZCUGRP, +
   ZCLDOR, ZCMODN);

  CREATE INDEX ZPFAI2
   ON ZPFAST (ZCINST, ZCFIRM, ZCKUGR, ZCKKTY, ZCAGRP, ZCLDOR, ZCMODN);

  CREATE INDEX ZPFAI3
   ON ZPFAST (ZCINST, ZCFIRM, ZCKUGR, ZCKKTY, ZCAGRP, ZCMODN);

  CREATE INDEX ZPFAI4
   ON ZPFAST (ZCINST, ZCFIRM, ZCKUGR, ZCKKTY, ZCAGRP, ZCOGRP, ZCHGRP, ZCUGRP, +
   ZCLDOR);

  CREATE INDEX ZPFAI5
   ON ZPFAST (ZCINST, ZCFIRM, ZCKUGR, ZCKKTY, ZCAGRP, ZCOGRP, ZCHGRP, ZCUGRP);

LABEL ON TABLE ZPFAST IS 'PÃ¥slagsinformasjon for Zilliant';
