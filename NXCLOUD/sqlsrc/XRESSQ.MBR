
CREATE OR REPLACE TABLE XRESST
(
   UID              BIGINT    NOT NULL GENERATED
   ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
   PARENT_ID        BIGINT,
   RAD_ID           BIGINT,
   OPERATOR         INTEGER,
   PRIMARY KEY(UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XRESST IS 'RA OEDSummerad';
