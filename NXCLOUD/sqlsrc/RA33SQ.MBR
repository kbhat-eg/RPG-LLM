CREATE OR REPLACE TABLE RA33ST(
  UID              FOR COLUMN RKKUID NUMERIC(18, 0) NOT NULL
                   GENERATED ALWAYS AS IDENTITY
                   (START WITH 1 INCREMENT BY 1 CYCLE),
  Firma            FOR COLUMN RKKFIR NUMERIC(3, 0) NOT NULL,
  Type             FOR COLUMN RKKTYP CHAR(6) NOT NULL DEFAULT ' ',
  Score1           FOR COLUMN RKKSC1 NUMERIC(3, 0) NOT NULL,
  Grense1          FOR COLUMN RKKGR1 NUMERIC(8, 0) NOT NULL,
  Score2           FOR COLUMN RKKSC2 NUMERIC(3, 0) NOT NULL,
  Grense2          FOR COLUMN RKKGR2 NUMERIC(8, 0) NOT NULL,
  Score3           FOR COLUMN RKKSC3 NUMERIC(3, 0) NOT NULL,
  Grense3          FOR COLUMN RKKGR3 NUMERIC(8, 0) NOT NULL,
  Score4           FOR COLUMN RKKSC4 NUMERIC(3, 0) NOT NULL,
  Grense4          FOR COLUMN RKKGR4 NUMERIC(8, 0) NOT NULL,
  Score5           FOR COLUMN RKKSC5 NUMERIC(3, 0) NOT NULL,
  Grense5          FOR COLUMN RKKGR5 NUMERIC(8, 0) NOT NULL,
  SistEndretDat    FOR COLUMN RKKEDA DATE NOT NULL DEFAULT CURRENT_DATE,
  SistEndretTid    FOR COLUMN RKKETI TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv         FOR COLUMN RKKEUS CHAR(10) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE RA33ST IS 'Grenser for kreditvurdering';

CREATE UNIQUE INDEX RA33IU ON RA33ST(RKKUID);
CREATE INDEX RA33I1 ON RA33ST(RKKFIR,RKKTYP);
CREATE INDEX RA33IR ON RA33ST(RKKUID);


