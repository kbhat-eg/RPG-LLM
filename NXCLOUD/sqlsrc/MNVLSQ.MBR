-- Lagring av menyvalg i Nexstep klient, fra v6.10

  CREATE OR REPLACE TABLE MNVLST
    ( MNVLST_ID INTEGER GENERATED ALWAYS AS IDENTITY,
      FIRMAKODE INTEGER NOT NULL,
      BRUKERID CHAR(10) NOT NULL,
      TEKSTTAG VARCHAR(40) NOT NULL,
      KOMMANDO VARCHAR(40) NOT NULL DEFAULT '',
      MNVLST_ID_PARENT INTEGER DEFAULT 0,
      OPPRDAT DATE NOT NULL DEFAULT CURRENT_DATE,
      ENDRETDAT DATE NOT NULL DEFAULT CURRENT_DATE,
      ENDRETTIM TIME NOT NULL DEFAULT CURRENT_TIME,
      ENDRETAV CHAR(10) NOT NULL,
      PRIMARY KEY(MNVLST_ID),
      CONSTRAINT MNVLST UNIQUE (FIRMAKODE,BRUKERID,TEKSTTAG, MNVLST_ID_PARENT)
   )
ON REPLACE PRESERVE ALL ROWS;
