CREATE OR REPLACE TABLE WINNER_CATALOG_VENDOR FOR SYSTEM NAME NOWCST(
  UID                  NUMERIC(18, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
                       (START WITH 1 INCREMENT BY 1 CYCLE),
  Company              FOR COLUMN NCFIRM NUMERIC(3, 0) NOT NULL,
  Supplier             FOR COLUMN NCSUPP VARCHAR(30) NOT NULL DEFAULT ' ',
  Catalog_name         FOR COLUMN NCCTLG VARCHAR(30) NOT NULL DEFAULT ' ',
  Model_Code           FOR COLUMN NCMODL VARCHAR(30) NOT NULL DEFAULT ' ',
  NX_Supplier          FOR COLUMN NCLDOR NUMERIC(6, 0) NOT NULL,
  Cat_exp_Date         FOR COLUMN NCCDAT DATE NOT NULL DEFAULT CURRENT_DATE,
  OPPRETTETBRUKER      FOR COLUMN NCCOUS VARCHAR(128) NOT NULL,
  ENDRETBRUKER         FOR COLUMN NCCEUS VARCHAR(128) NOT NULL,
  OPPRETTET            FOR COLUMN NCCOTS TIMESTAMP  DEFAULT CURRENT_TIMESTAMP,
  AUTOOBRUKER          FOR COLUMN NCCOOU VARCHAR(128) WITH DEFAULT USER ,
  ENDRET               FOR COLUMN NCCETS TIMESTAMP NOT NULL FOR EACH ROW ON
                       UPDATE AS ROW CHANGE TIMESTAMP,
  AUTOEBRUKER          FOR COLUMN NCCAEU VARCHAR(128) GENERATED ALWAYS AS
                      ( SESSION_USER ))
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE WINNER_CATALOG_VENDOR IS 'Winner kobling katalog/leverand√∏r';

CREATE UNIQUE INDEX WINNER_CATALOG_VENDOR_UPD
 FOR SYSTEM NAME NOWCIU ON WINNER_CATALOG_VENDOR(Company,Supplier);
CREATE INDEX WINNER_CATALOG_VENDOR_READ
 FOR SYSTEM NAME NOWCIR ON WINNER_CATALOG_VENDOR(Company,Supplier);
CREATE INDEX WINNER_CATALOG_VENDOR_READ1
 FOR SYSTEM NAME NOWCI1 ON WINNER_CATALOG_VENDOR(Company,Supplier);
CREATE INDEX WINNER_CATALOG_VENDOR_VEND_SUPP
 FOR SYSTEM NAME NOWCI2 ON WINNER_CATALOG_VENDOR(Company,NX_Supplier,Supplier);


