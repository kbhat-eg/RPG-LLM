CREATE OR REPLACE TABLE ITNAST(
  UID              INTEGER GENERATED ALWAYS AS IDENTITY,
  Company          FOR COLUMN ITFIRM NUMERIC(3, 0) NOT NULL,
  Program          FOR COLUMN ITPROG CHAR(40) NOT NULL,
  Command          FOR COLUMN ITCOMD CHAR(20) NOT NULL,
  Params           FOR COLUMN ITPARM CHAR(2048) NOT NULL,
  OpprettetDato    FOR COLUMN ITODAT DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl      FOR COLUMN ITOTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv      FOR COLUMN ITOUSR CHAR(10) NOT NULL DEFAULT 'NXSYSTEM')
ON REPLACE PRESERVE ALL ROWS;

ALTER TABLE ITNAST ADD CONSTRAINT IT_UID PRIMARY KEY (UID);

LABEL ON TABLE ITNAST IS 'Replikeringsposter til NAV';
