CREATE OR REPLACE TABLE FRONT_PAKKSEDDELKOLLI FOR SYSTEM NAME XPKOST
(
  Firma            FOR COLUMN XOFIRM NUMERIC(3, 0) NOT NULL,
  Guid             FOR COLUMN XOGUID CHAR(40) NOT NULL DEFAULT ' ',
  ForsStrukturNiva FOR COLUMN XOFSNI CHAR(18) NOT NULL DEFAULT ' ',
  ForsNiva         FOR COLUMN XOFNIV NUMERIC(6, 0) NOT NULL,
  ForsStrukturPare FOR COLUMN XOFSPA CHAR(18) NOT NULL DEFAULT ' ',
  Kollitype        FOR COLUMN XOKOTY CHAR(3) NOT NULL DEFAULT ' ',
  Pakningsantall   FOR COLUMN XOPANT NUMERIC(8, 0) NOT NULL,
  Serienummer      FOR COLUMN XOKSNR CHAR(20) NOT NULL DEFAULT ' ',
  SSCCkode         FOR COLUMN XOSSCC NUMERIC(18, 0) NOT NULL,
  Hoyde            FOR COLUMN XOHOYD NUMERIC(8, 4) NOT NULL,
  Hoydeenhet       FOR COLUMN XOHENH CHAR(3) NOT NULL DEFAULT ' ',
  Lengde           FOR COLUMN XOLENG NUMERIC(8, 4) NOT NULL,
  Lengdeenhet      FOR COLUMN XOLENH CHAR(3) NOT NULL DEFAULT ' ',
  Bredde           FOR COLUMN XOBRED NUMERIC(8, 4) NOT NULL,
  Breddeenhet      FOR COLUMN XOBENH CHAR(3) NOT NULL DEFAULT ' ',
  Volum            FOR COLUMN XOVOLU NUMERIC(8, 4) NOT NULL,
  Volumenhet       FOR COLUMN XOVENH CHAR(3) NOT NULL DEFAULT ' ',
  Bruttovek        FOR COLUMN XOBVKT NUMERIC(10, 4) NOT NULL,
  Nettovekt        FOR COLUMN XONVKT NUMERIC(10, 4) NOT NULL,
  Vektenhet        FOR COLUMN XOVEEN CHAR(3) NOT NULL DEFAULT ' ')

ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE FRONT_PAKKSEDDELKOLLI  IS 'Pakkseddel kolli - forsystem';
