   ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
   KEY1             VARCHAR(255),
   KEY2             VARCHAR(255),
   KEY3             VARCHAR(255),
   KEY4             VARCHAR(255),
   KEY5             VARCHAR(255),
   STRINGPARAM1     VARCHAR(255),
   STRINGPARAM2     VARCHAR(255),
   STRINGPARAM3     VARCHAR(255),
   STRINGPARAM4     VARCHAR(255),
   STRINGPARAM5     VARCHAR(255),
   INTPARAM1        INTEGER,
   INTPARAM2        INTEGER,
   INTPARAM3        INTEGER,
   INTPARAM4        INTEGER,
    INTPARAM5        INTEGER,
    HODE_ID          INTEGER,
    RAPPORT_ID       INTEGER,
    PRIMARY KEY(UID)
 )
ON REPLACE PRESERVE ALL ROWS;
 LABEL ON TABLE XDOLST IS 'RA Drilldown_Objekt';





