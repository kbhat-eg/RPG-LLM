CREATE OR REPLACE TABLE WINNER_MAP_LINE FOR SYSTEM NAME NOWLST(
  UID                  NUMERIC(18, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
                       (START WITH 1 INCREMENT BY 1 CYCLE),
  Date                 FOR COLUMN NWLDAT DATE NOT NULL DEFAULT CURRENT_DATE,
  Time                 FOR COLUMN NWLTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  MainPos              FOR COLUMN NWLVLI NUMERIC(4, 0) NOT NULL,
  SubPos               FOR COLUMN NWLTLI NUMERIC(4, 0) NOT NULL,
  Code                 FOR COLUMN NWLLVN VARCHAR(15) NOT NULL DEFAULT ' ',
  Text                 FOR COLUMN NWLTEK VARCHAR(100) NOT NULL DEFAULT ' ',
  Quant                FOR COLUMN NWLANT NUMERIC(9, 3) NOT NULL,
  Unit                 FOR COLUMN NWLENH VARCHAR(3) NOT NULL DEFAULT ' ',
  PointPrice           FOR COLUMN NWLSPR NUMERIC(9, 2) NOT NULL,
  CostPriceExVAT       FOR COLUMN NWLKPR NUMERIC(9, 2) NOT NULL,
  Disc1prc             FOR COLUMN NWLRA1 NUMERIC(5, 2) NOT NULL,
  Disc2prc             FOR COLUMN NWLRA2 NUMERIC(5, 2) NOT NULL,
  Disc3prc             FOR COLUMN NWLRA3 NUMERIC(5, 2) NOT NULL,
  CalcGroup_Nbr        FOR COLUMN NWLGNR NUMERIC(3, 0) NOT NULL,
  CalcGroup_Name       FOR COLUMN NWLGNA VARCHAR(15) NOT NULL DEFAULT ' ',
  EndPoint             FOR COLUMN NWLHNG NUMERIC(1, 0) NOT NULL,
  Supplier             FOR COLUMN NWLLEV VARCHAR(30) NOT NULL DEFAULT ' ',
  Catalog_Name         FOR COLUMN NWLKAT VARCHAR(30) NOT NULL DEFAULT ' ',
  Model_Code           FOR COLUMN NWLMOD VARCHAR(10) NOT NULL DEFAULT ' ',
  OPPRETTETBRUKER      FOR COLUMN NWLOUS VARCHAR(128) NOT NULL,
  ENDRETBRUKER         FOR COLUMN NWLEUS VARCHAR(128) NOT NULL,
  OPPRETTET            FOR COLUMN NWLOTS TIMESTAMP  DEFAULT CURRENT_TIMESTAMP,
  AUTOOBRUKER          FOR COLUMN NWLOOU VARCHAR(128) WITH DEFAULT USER ,
  ENDRET               FOR COLUMN NWLETS TIMESTAMP NOT NULL FOR EACH ROW ON
                       UPDATE AS ROW CHANGE TIMESTAMP,
  AUTOEBRUKER          FOR COLUMN NWLAEU VARCHAR(128) GENERATED ALWAYS AS
                      ( SESSION_USER ))
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE WINNER_MAP_LINE IS 'Mappingtabell Linje Winner integrasjon';

CREATE UNIQUE INDEX WINNER_MAP_LINE_UPD
       FOR SYSTEM NAME NOWLIU ON WINNER_MAP_LINE(Date,Time,MainPos);
CREATE INDEX WINNER_MAP_LINE_READ
       FOR SYSTEM NAME NOWLIR ON WINNER_MAP_LINE(Date,Time,MainPos);
CREATE INDEX WINNER_MAP_LINE_READ1
       FOR SYSTEM NAME NOWLI1 ON WINNER_MAP_LINE(Date,Time,MainPos);



