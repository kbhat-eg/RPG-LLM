CREATE OR REPLACE TABLE AFPMST(
  UID              NUMERIC(15, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
                   (START WITH 1 INCREMENT BY 1 CYCLE),
  Printer          FOR COLUMN AFMPRI CHAR(50) NOT NULL,
  Media            FOR COLUMN AFMMED CHAR(50) NOT NULL,
  OpprettetDato    FOR COLUMN AFMODA DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetTid     FOR COLUMN AFMOTI TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretDato       FOR COLUMN AFMEDA DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretTid        FOR COLUMN AFMETI TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv      FOR COLUMN AFMOUS CHAR(10) NOT NULL DEFAULT ' ',
  EndretAv         FOR COLUMN AFMEUS CHAR(10) NOT NULL DEFAULT ' ')
  ON REPLACE PRESERVE ALL ROWS;

ALTER TABLE AFPMST ADD CONSTRAINT AFPMPK PRIMARY KEY (UID);
CREATE UNIQUE INDEX AFPMIU ON AFPMST(AFMPRI, AFMMED);

LABEL ON TABLE AFPMST IS 'Printer media';
