-- Overføring Selger

CREATE OR REPLACE TABLE BOSEST( 
 Firma             FOR COLUMN FIRM NUMERIC(3, 0) NOT NULL,
 Selger            FOR COLUMN SELG NUMERIC(4, 0) NOT NULL,
 Navn                              CHAR(20) CCSID 277,
 EmailAdresse      FOR COLUMN MAIL CHAR(50) CCSID 277,
 OpprettetDato     FOR COLUMN ODAT DATE NOT NULL DEFAULT CURRENT_DATE,
 OpprettetTid      FOR COLUMN OTIM TIME NOT NULL DEFAULT CURRENT_TIME,
 OpprettetAv       FOR COLUMN OUSR CHAR(10) CCSID 277 NOT NULL DEFAULT ' ',
 EndretDato        FOR COLUMN EDAT DATE NOT NULL DEFAULT CURRENT_DATE,
 EndretTid         FOR COLUMN ETIM TIME NOT NULL DEFAULT CURRENT_TIME,
 EndretAv          FOR COLUMN EUSR CHAR(10) CCSID 277 NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE BOSEST
	IS 'Overføring Selger';
