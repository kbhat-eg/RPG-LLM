 ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
MESTER_BYGG_MESTER_ID     BIGINT NOT NULL,
AAR                       INTEGER NOT NULL,
MND                       INTEGER,
KVARTAL                   INTEGER,
AKKUMULERT_UNDER_BYGGING  NUMERIC (10, 2),
AKKUMULERT_IGANG          NUMERIC (10, 2),
AKKUMULERT_FERDIG         NUMERIC (10, 2),
OPPRETTET_DATO            DATE DEFAULT CURRENT_DATE,
BRUKER                    VARCHAR(20),
PRIMARY KEY (UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XBMBST IS 'RA MestergruppenByggmesterBolig';
