
CREATE OR REPLACE TABLE XSCLST
(
   UID              BIGINT    NOT NULL GENERATED
   ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
   FELT_INDEX       INTEGER,
   NAVN             VARCHAR(255),
   BESKRIVELSE      VARCHAR(255),
   DATATYPE_ID      INTEGER,
   DATATYPE         VARCHAR(255),
   SCALE            INTEGER,
   PRECISION        INTEGER,
   TABELL_ID        BIGINT,
   PRIMARY KEY(UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XSCLST IS 'RA SqlColumn';
