
CREATE OR REPLACE TABLE XSQWST
(
   UID              BIGINT    NOT NULL GENERATED
   ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
   WHERE_INDEX      INTEGER,
   QRYFELT_ID       BIGINT,
   OPERATOR         INTEGER,
   ANDOR            INTEGER,
   VERDI            VARCHAR(255),
   PRIMARY KEY(UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XSQWST IS 'RA SqlQueryWhere';
