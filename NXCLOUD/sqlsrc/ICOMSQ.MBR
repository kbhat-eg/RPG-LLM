CREATE OR REPLACE TABLE ICOMST(
  UID              INTEGER GENERATED ALWAYS AS IDENTITY,
  Company          FOR COLUMN ICFIRM NUMERIC(3, 0) NOT NULL,
  Bilagsnummer     FOR COLUMN ICBILN NUMERIC(8, 0) NOT NULL,
  Bilagsdato       FOR COLUMN ICBDAT DATE NOT NULL,
  Forfallsdato     FOR COLUMN ICFDAT DATE NOT NULL,
  Bilagskode       FOR COLUMN ICBILK NUMERIC(2, 0) NOT NULL,
  Bilagstekst      FOR COLUMN ICTEXT CHAR(20) NOT NULL DEFAULT,
  Debetavd         FOR COLUMN ICDAVD NUMERIC(4, 0) NOT NULL,
  Debetkonto       FOR COLUMN ICDKTO NUMERIC(6, 0) NOT NULL,
  Debetspes        FOR COLUMN ICDSPS NUMERIC(4, 0) NOT NULL,
  Debetmvakode     FOR COLUMN ICDMOM CHAR(1) NOT NULL DEFAULT,
  Kreditavd        FOR COLUMN ICCAVD NUMERIC(4, 0) NOT NULL,
  Kredittonto      FOR COLUMN ICCKTO NUMERIC(6, 0) NOT NULL,
  Kreditspes       FOR COLUMN ICCSPS NUMERIC(4, 0) NOT NULL,
  Kreditmvakode    FOR COLUMN ICCMOM CHAR(1) NOT NULL DEFAULT,
  Belop            FOR COLUMN ICBELP NUMERIC(10, 2) NOT NULL,
  Leverandornummer FOR COLUMN ICKUND NUMERIC(6, 0) NOT NULL,
  Kreditreferanse  FOR COLUMN ICKREF NUMERIC(8, 0) NOT NULL,
  Kid              FOR COLUMN ICKIDI CHAR(25) NOT NULL DEFAULT,
  Valutakode       FOR COLUMN ICVALK CHAR(3) NOT NULL DEFAULT,
  Valutabelop      FOR COLUMN ICVALB NUMERIC(11, 2) NOT NULL,
  Prosjekt         FOR COLUMN ICPROJ CHAR(6) NOT NULL DEFAULT,
  Aktivitet        FOR COLUMN ICAKTI CHAR(6) NOT NULL DEFAULT,
  Selgersfakt      FOR COLUMN ICSFAK CHAR(15) NOT NULL DEFAULT,
  OpprettetDato    FOR COLUMN ICODAT DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl      FOR COLUMN ICOTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv      FOR COLUMN ICOUSR CHAR(10) NOT NULL DEFAULT 'NXSYSTEM',
  Bokforingsdato   FOR COLUMN ICBODA DATE NOT NULL)
ON REPLACE PRESERVE ALL ROWS;

ALTER TABLE ICOMST ADD CONSTRAINT IC_UID PRIMARY KEY (UID);

LABEL ON TABLE ICOMST IS 'Regnskapstranser fra Compello til NAV';
