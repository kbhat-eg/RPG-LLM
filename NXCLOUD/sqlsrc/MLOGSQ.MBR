CREATE OR REPLACE TABLE MLOGST(
  UID              INTEGER GENERATED ALWAYS AS IDENTITY,
  Company          FOR COLUMN MLOFIR NUMERIC(3, 0) NOT NULL,
  Transtype        FOR COLUMN MLOTYP CHAR(4) NOT NULL DEFAULT,
  Transdato        FOR COLUMN MLODAT DATE NOT NULL DEFAULT CURRENT_DATE,
  Transtid         FOR COLUMN MLOTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  Type_al1         FOR COLUMN MLOTA1 CHAR(1) NOT NULL DEFAULT,
  Type_nu1         FOR COLUMN MLOTN1 NUMERIC(6, 0) NOT NULL,
  Type_nu2         FOR COLUMN MLOTN2 NUMERIC(6, 0) NOT NULL,
  Type_al2         FOR COLUMN MLOTA2 CHAR(6) NOT NULL DEFAULT,
  Feilmeld         FOR COLUMN MLOERR CHAR(200) NOT NULL DEFAULT,
  Serverid         FOR COLUMN MLOSRV CHAR(30) NOT NULL DEFAULT,
  Abbonentid       FOR COLUMN MLOAID CHAR(10) NOT NULL DEFAULT,
  Behandlet        FOR COLUMN MLOBEH CHAR(1) NOT NULL DEFAULT,
  OpprettetDato    FOR COLUMN MLOODA DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl      FOR COLUMN MLOOTI TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv      FOR COLUMN MLOOUS CHAR(10) NOT NULL DEFAULT 'NXSYSTEM')
ON REPLACE PRESERVE ALL ROWS;

CREATE UNIQUE INDEX MLOGIU ON MLOGST(UID);
CREATE INDEX MLOGI1 ON MLOGST(MLOFIR,MLODAT DESC,MLOTIM DESC,MLOTYP);

LABEL ON TABLE MLOGST IS 'Logg på masterdata overføringer     ';
