CREATE OR REPLACE TABLE CKALST (
    UID NUMERIC(15, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
        (START WITH 1 INCREMENT BY 1 CYCLE),
    CKAUST_UID        NUMERIC(15, 0) NOT NULL ,
    Funksjon          FOR COLUMN CKLFUN CHAR(20) NOT NULL DEFAULT ' ',
    Beskrivelse       FOR COLUMN CKLBES VARCHAR(200) NOT NULL DEFAULT ' ',
    Opprettetdato     FOR COLUMN CKAODA DATE NOT NULL DEFAULT CURRENT_DATE,
    Opprettetkl       FOR COLUMN CKAOTI TIME NOT NULL DEFAULT CURRENT_TIME,
    PRIMARY KEY(UID))
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE CKALST IS 'Nedlastings logg PDFarkiv';
