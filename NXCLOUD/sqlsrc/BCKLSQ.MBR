CREATE OR REPLACE TABLE BCKLST(
  UID              BIGINT GENERATED ALWAYS AS IDENTITY PRIMARY KEY,
  firma            numeric(3, 0) NOT NULL,
  bongnr           varchar(15) NOT NULL,
  phone            varchar(20),
  memberNo         varchar(50),
  correlation      varchar(50),
  processed        smallint not null default 0)
ON REPLACE PRESERVE ALL ROWS;
CREATE UNIQUE INDEX BCKLI1 ON BCKLST(firma, bongnr);
