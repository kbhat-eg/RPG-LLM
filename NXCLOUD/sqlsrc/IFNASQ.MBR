CREATE OR REPLACE TABLE IFNAST(
  UID              INTEGER GENERATED ALWAYS AS IDENTITY,
  Company          FOR COLUMN IFFIRM NUMERIC(3, 0) NOT NULL,
  Program          FOR COLUMN IFPROG CHAR(40) NOT NULL,
  Command          FOR COLUMN IFCOMD CHAR(20) NOT NULL,
  Params           FOR COLUMN IFPARM VARCHAR(2048) NOT NULL,
  OpprettetDato    FOR COLUMN IFODAT DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl      FOR COLUMN IFETIM TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv      FOR COLUMN IFEUSR CHAR(10) NOT NULL DEFAULT 'NAVSYSTEM')
ON REPLACE PRESERVE ALL ROWS;

ALTER TABLE IFNAST ADD CONSTRAINT IF_UID PRIMARY KEY (UID);

CREATE INDEX IFNAI1
	ON IFNAST (IFFIRM, IFPROG);
CREATE INDEX IFNAI2
	ON IFNAST (IFFIRM, IFODAT,IFETIM);
CREATE INDEX IFNAIR
	ON IFNAST (UID);

LABEL ON TABLE IFNAST IS 'Replikeringsposter fra NAV';
