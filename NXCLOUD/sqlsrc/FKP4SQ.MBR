CREATE OR REPLACE TABLE FKP4ST(
  UID              FOR COLUMN F4VUID NUMERIC(18, 0) NOT NULL
                   GENERATED ALWAYS AS IDENTITY
                   (START WITH 1 INCREMENT BY 1 CYCLE),
  Firma            FOR COLUMN F4FIRM NUMERIC(3, 0) NOT NULL,
  Kundenummer      FOR COLUMN F4KUND NUMERIC(6, 0) NOT NULL,
  Kundeprosjekt    FOR COLUMN F4KPRO NUMERIC(6, 0) NOT NULL,
  EgenskapGUID     FOR COLUMN F4GUID CHAR(100) NOT NULL DEFAULT ' ',
  Egenskap         FOR COLUMN F4NAVN CHAR(100) NOT NULL DEFAULT ' ',
  OpprettetDato    FOR COLUMN F4ODAT DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl      FOR COLUMN F4OTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv      FOR COLUMN F4OUSR CHAR(10) NOT NULL DEFAULT ' ',
  EndretDato       FOR COLUMN F4EDAT DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretTid        FOR COLUMN F4ETIM TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv         FOR COLUMN F4EUSR CHAR(10) NOT NULL DEFAULT ' ')

ON REPLACE PRESERVE ALL ROWS;

  CREATE INDEX FKP4I1
   ON FKP4ST (F4FIRM, F4KUND, F4KPRO, F4GUID);

  CREATE INDEX FKP4IU
   ON FKP4ST (F4FIRM, F4KUND, F4KPRO, F4GUID);

  CREATE INDEX FKP4IR
   ON FKP4ST (F4FIRM, F4KUND, F4KPRO, F4GUID);

LABEL ON INDEX FKP4IU IS 'Kundeprosjekt - Sustainability';
LABEL ON INDEX FKP4IR IS 'Kundeprosjekt - Sustainability';
LABEL ON INDEX FKP4I1 IS 'Kundeprosjekt - Sustainability';
LABEL ON TABLE FKP4ST IS 'Kundeprosjekt - Sustainability';
