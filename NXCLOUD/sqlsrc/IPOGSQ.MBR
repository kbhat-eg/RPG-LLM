CREATE OR REPLACE TABLE IPOGST(
  UID              INTEGER GENERATED ALWAYS AS IDENTITY,
  Company          FOR COLUMN IIFIRM NUMERIC(3, 0) NOT NULL,
  Bilagsnummer     FOR COLUMN IIBILN NUMERIC(8, 0) NOT NULL,
  Bilagsdato       FOR COLUMN IIBDAT DATE NOT NULL,
  Forfallsdato     FOR COLUMN IIFDAT DATE NOT NULL,
  Bilagskode       FOR COLUMN IIBILK NUMERIC(2, 0) NOT NULL,
  Bilagstekst      FOR COLUMN IITEXT CHAR(20) NOT NULL DEFAULT,
  Debetavd         FOR COLUMN IIDAVD NUMERIC(4, 0) NOT NULL,
  Debetkonto       FOR COLUMN IIDKTO NUMERIC(6, 0) NOT NULL,
  Debetspes        FOR COLUMN IIDSPS NUMERIC(4, 0) NOT NULL,
  Debetmvakode     FOR COLUMN IIDMOM CHAR(1) NOT NULL DEFAULT,
  Kreditavd        FOR COLUMN IICAVD NUMERIC(4, 0) NOT NULL,
  Kredittonto      FOR COLUMN IICKTO NUMERIC(6, 0) NOT NULL,
  Kreditspes       FOR COLUMN IICSPS NUMERIC(4, 0) NOT NULL,
  Kreditmvakode    FOR COLUMN IICMOM CHAR(1) NOT NULL DEFAULT,
  Belop            FOR COLUMN IIBELP NUMERIC(10, 2) NOT NULL,
  Leverandornummer FOR COLUMN IIKUND NUMERIC(6, 0) NOT NULL,
  Kreditreferanse  FOR COLUMN IIKREF NUMERIC(8, 0) NOT NULL,
  Kid              FOR COLUMN IIKIDI CHAR(25) NOT NULL DEFAULT,
  Valutakode       FOR COLUMN IIVALK CHAR(3) NOT NULL DEFAULT,
  Valutabelop      FOR COLUMN IIVALB NUMERIC(11, 2) NOT NULL,
  Prosjekt         FOR COLUMN IIPROJ CHAR(6) NOT NULL DEFAULT,
  Aktivitet        FOR COLUMN IIAKTI CHAR(6) NOT NULL DEFAULT,
  Selgersfakt      FOR COLUMN IISFAK CHAR(15) NOT NULL DEFAULT,
  OpprettetDato    FOR COLUMN IIODAT DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl      FOR COLUMN IIOTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv      FOR COLUMN IIOUSR CHAR(10) NOT NULL DEFAULT 'NXSYSTEM',
  Bokforingsdato   FOR COLUMN IIBODA DATE NOT NULL,
  Underprosjekt    FOR COLUMN IIUPRO CHAR(6) NOT NULL DEFAULT,
  Transaksjonstype FOR COLUMN IITYPE CHAR(4) NOT NULL DEFAULT,
  Betbet           FOR COLUMN IIBETB NUMERIC(2, 0) NOT NULL,
  Lest_flagg       FOR COLUMN IIFLAG CHAR(1) NOT NULL DEFAULT,
  Remittering      FOR COLUMN IIREMI CHAR(3) NOT NULL DEFAULT)
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE IPOGST IS 'Regnskapstranser fra innkj√∏p til POG';
