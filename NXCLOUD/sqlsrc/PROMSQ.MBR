CREATE OR REPLACE TABLE PROMST(
  UID              FOR COLUMN PMVUID NUMERIC(18, 0) NOT NULL
                   GENERATED ALWAYS AS IDENTITY
                   (START WITH 1 INCREMENT BY 1 CYCLE),
  WorkMember       FOR COLUMN PMMEMB CHAR(10) NOT NULL DEFAULt ' ',
  ProfileText      FOR COLUMN PMPMTX CHAR(50) NOT NULL DEFAULT ' ',
  ProfileVarer     FOR COLUMN PMNOIT NUMERIC(9, 0) NOT NULL,
  ProfileKey       FOR COLUMN PMPKEY CHAR(15) NOT NULL DEFAULT ' ',
  OpprettetDato    FOR COLUMN PMODAT DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl      FOR COLUMN PMOTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv      FOR COLUMN PMOUSR CHAR(10) NOT NULL DEFAULT ' ')

ON REPLACE PRESERVE ALL ROWS;

  CREATE INDEX PROMIR
   ON PROMST (PMMEMB, PMPKEY);

LABEL ON INDEX PROMIR IS 'Profile Manager - workfile';
LABEL ON TABLE PROMST IS 'Profiel Manager - workfile';
