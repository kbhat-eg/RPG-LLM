
CREATE OR REPLACE TABLE XMBMST
(
UID                     BIGINT NOT NULL GENERATED
 ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
BYGGMESTER_NAVN         VARCHAR(255),
FILGRUPPE               VARCHAR (20),
ORG_NUMMER              BIGINT,
KOST_BOENHET            NUMERIC (10, 2),
OPPRETTET_DATO          DATE DEFAULT current_date,
BRUKER                  VARCHAR(20),
PRIMARY KEY (UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XMBMST IS 'RA MestergruppenByggmester';
