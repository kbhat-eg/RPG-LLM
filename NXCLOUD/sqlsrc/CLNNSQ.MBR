CREATE OR REPLACE TABLE CLNNST(
  Firma              FOR COLUMN CLNNFI  NUMERIC(3, 0) NOT NULL DEFAULT 0,
  Nobbkategori       FOR COLUMN CLNNKA  NUMERIC(4, 0) NOT NULL DEFAULT 0,
  Overgruppe         FOR COLUMN CLNNOG  NUMERIC(2, 0) NOT NULL DEFAULT 0,
  Hovedgruppe        FOR COLUMN CLNNHG  NUMERIC(2, 0) NOT NULL DEFAULT 0,
  Undergruppe        FOR COLUMN CLNNUG  NUMERIC(3, 0) NOT NULL DEFAULT 0,
  OpprettetDato      FOR COLUMN CLNNOD DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretDato         FOR COLUMN CLNNED DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretKl           FOR COLUMN CLNNET TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv           FOR COLUMN CLNNEU CHAR(10) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

CREATE UNIQUE INDEX CLNNIU on CLNNST(CLNNFI,CLNNKA,CLNNOG,CLNNHG,CLNNUG);
CREATE INDEX CLNNIR on CLNNST(CLNNFI,CLNNKA,CLNNOG,CLNNHG,CLNNUG);
CREATE INDEX CLNNI1 on CLNNST(CLNNFI,CLNNKA,CLNNOG,CLNNHG,CLNNUG);

LABEL ON TABLE CLNNST IS 'Nobbkategori mot varegrupper';
