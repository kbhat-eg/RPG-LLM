CREATE OR REPLACE TABLE  LEVERANDORKLASSIFISERING FOR SYSTEM NAME CLKKST
(
   UID FOR COLUMN CLKUID NUMERIC(18, 0) NOT NULL GENERATED ALWAYS
                   AS IDENTITY (START WITH 1 INCREMENT BY 1 CYCLE),
   KJEDEKLASSIFISERING FOR COLUMN CLKREF NUMERIC(2, 0) NOT NULL,
   LEVERANDOR          FOR COLUMN CLKLEV NUMERIC(6, 0) NOT NULL,
   OPPRETTETBRUKER     FOR COLUMN CLKOUS VARCHAR(128) NOT NULL,
   ENDRETBRUKER        FOR COLUMN CLKEUS VARCHAR(128) NOT NULL,
   OPPRETTET           FOR COLUMN CLKOTS TIMESTAMP  DEFAULT CURRENT_TIMESTAMP,
   ENDRET              FOR COLUMN CLKETS TIMESTAMP NOT NULL FOR EACH ROW ON
                       UPDATE AS ROW CHANGE TIMESTAMP,
   AUTOOBRUKER         FOR COLUMN CLKOOU VARCHAR(128) WITH DEFAULT USER ,
   AUTOEBRUKER         FOR COLUMN CLKAEU VARCHAR(128) GENERATED ALWAYS AS
                      ( SESSION_USER )
)
ON REPLACE PRESERVE ALL ROWS
;

LABEL ON TABLE  LEVERANDORKLASSIFISERING IS
'Kjedeklassifisering av leverand√∏r';


CREATE UNIQUE INDEX  LEVERANDORKLASSIFISERING_UX FOR SYSTEM NAME CLKKIU
ON  LEVERANDORKLASSIFISERING(UID);

CREATE INDEX  LEVERANDORKLASSIFISERING_I1 FOR SYSTEM NAME CLKKI1 ON
                     LEVERANDORKLASSIFISERING (LEVERANDOR);

