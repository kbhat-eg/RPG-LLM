CREATE OR REPLACE TABLE ADK1ST(
  ID               NUMERIC(15, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
                   (START WITH 1 INCREMENT BY 1 CYCLE),
  ADHKID           NUMERIC(15, 0) NOT NULL ,
  Kjedekode        CHAR(2) NOT NULL DEFAULT ' ',
  Forhandlernr     CHAR(10) NOT NULL DEFAULT ' ',
  Navn             CHAR(20) NOT NULL DEFAULT ' ',
  Beskrivelse      VARCHAR(255) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE ADK1ST IS 'Portal - forhandlerinfo kjede';
