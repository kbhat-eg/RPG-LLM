
CREATE OR REPLACE TABLE XDUTST
(
   UID              BIGINT    NOT NULL GENERATED
   ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
   VALGT            SMALLINT,
   UTVALG_TYPE      INTEGER,
   RAPPORT_ID       BIGINT,
   PRIMARY KEY(UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XDUTST IS 'RA Drilldown_Utvalg';
