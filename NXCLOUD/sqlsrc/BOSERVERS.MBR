CREATE OR REPLACE TABLE BOSERVERS(
     brokerid varchar(40),
     created timestamp,
     version varchar(10),
     description varchar(150),
     ip varchar(30),
     dburl varchar(100),
     locked numeric(1),
     PRIMARY KEY(brokerid)
)
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE BOSERVERS IS 'Boservere';
