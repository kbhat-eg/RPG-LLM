CREATE OR REPLACE TABLE PAKKSEDDELMOTTATTLINJE FOR SYSTEM NAME LB2DST
(
  UID              FOR COLUMN P2LUID NUMERIC(18, 0) NOT NULL GENERATED ALWAYS
                   AS IDENTITY (START WITH 1 INCREMENT BY 1 CYCLE),
  Firma            FOR COLUMN P2LFIR NUMERIC(3, 0) NOT NULL,
  SSCCKode         FOR COLUMN P2LSSC NUMERIC(18, 0) NOT NULL,
  NXBestNr         FOR COLUMN P2LNUM NUMERIC(8, 0) NOT NULL,
  NXBestSuffiks    FOR COLUMN P2LSUF NUMERIC(2, 0) NOT NULL,
  NXBestLinjeNr    FOR COLUMN P2LLIN NUMERIC(6, 0) NOT NULL,
  PakkseddelID     FOR COLUMN P2LPID CHAR(17) NOT NULL DEFAULT ' ',
  Versjonsnummer   FOR COLUMN P2LPVE NUMERIC(3, 0) NOT NULL,
  Ar               FOR COLUMN P2LAAR NUMERIC(4, 0) NOT NULL,

  Status           FOR COLUMN P2STAT CHAR(2) NOT NULL DEFAULT ' ',
  Lasekode         FOR COLUMN P2SLCK NUMERIC(1, 0) NOT NULL,
  NOBBnummer       FOR COLUMN P2LNOB NUMERIC(8, 0) NOT NULL,
  ArtikkelNavn     FOR COLUMN P2LBES CHAR(70) NOT NULL DEFAULT ' ',
  VaremottatAntall FOR COLUMN P2LVAN NUMERIC(15, 4) NOT NULL,
  VaremotattEnhet  FOR COLUMN P2LVEN CHAR(3) NOT NULL DEFAULT ' ',

  OpprettetDato    FOR COLUMN P2LODA DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl      FOR COLUMN P2LOTI TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv      FOR COLUMN P2LOUS CHAR(10) NOT NULL DEFAULT ' ',
  EndretDato       FOR COLUMN P2LEDA DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretTid        FOR COLUMN P2LETI TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv         FOR COLUMN P2LEUS CHAR(10) NOT NULL DEFAULT ' ')

ON REPLACE PRESERVE ALL ROWS;


CREATE UNIQUE INDEX PAKKSEDDELMOTTATTLINJE_IU FOR SYSTEM NAME LB2DIU ON
                    PAKKSEDDELMOTTATTLINJE(Firma,SSCCKode,NXBestNr,
                    NXBestSuffiks,NXBestLinjeNr,PakkseddelID,Versjonsnummer,Ar);

CREATE UNIQUE INDEX PAKKSEDDELMOTTATTLINJE_IR FOR SYSTEM NAME LB2DIR ON
                    PAKKSEDDELMOTTATTLINJE(Firma,SSCCKode,NXBestNr,
                    NXBestSuffiks,NXBestLinjeNr,PakkseddelID,Versjonsnummer,Ar);

CREATE UNIQUE INDEX PAKKSEDDELMOTTATTLINJE_I1 FOR SYSTEM NAME LB2DI1 ON
                    PAKKSEDDELMOTTATTLINJE(Firma,SSCCKode,NXBestNr,
                    NXBestSuffiks,NXBestLinjeNr,PakkseddelID,Versjonsnummer,Ar);

CREATE INDEX PAKKSEDDELMOTTATTLINJE_I2 FOR SYSTEM NAME LB2DI2 ON
                    PAKKSEDDELMOTTATTLINJE(Firma,SSCCKode,NXBestNr,
                    NXBestSuffiks);


LABEL ON TABLE PAKKSEDDELMOTTATTLINJE    IS 'EDI Pakkseddel-Varemottak linje,-
 arbeidstabell';
LABEL ON INDEX PAKKSEDDELMOTTATTLINJE_IU IS 'EDI Pakkseddel-Varemottak linje,-
 arbeidstabell';
LABEL ON INDEX PAKKSEDDELMOTTATTLINJE_IR IS 'EDI Pakkseddel-Varemottak linje,-
 arbeidstabell';
LABEL ON INDEX PAKKSEDDELMOTTATTLINJE_I1 IS 'EDI Pakkseddel-Varemottak linje,-
 arbeidstabell';
LABEL ON INDEX PAKKSEDDELMOTTATTLINJE_I2 IS 'EDI Pakkseddel-Varemottak linje,-
 arbeidstabell';
