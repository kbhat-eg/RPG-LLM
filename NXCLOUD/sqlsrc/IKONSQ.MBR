CREATE OR REPLACE TABLE IKONST(
  Company          FOR COLUMN IKFIRM NUMERIC(3, 0) NOT NULL,
  Fra_bil          FOR COLUMN IKBFRA NUMERIC(2, 0) NOT NULL,
  Til_bil          FOR COLUMN IKBTIL NUMERIC(2, 0) NOT NULL,
  OpprettetDato    FOR COLUMN IKODAT DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl      FOR COLUMN IKOTIM TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv      FOR COLUMN IKOUSR CHAR(10) NOT NULL DEFAULT 'NXSYSTEM')
ON REPLACE PRESERVE ALL ROWS;

CREATE INDEX IKONI1
	ON IKONST (IKFIRM, IKBFRA);

CREATE INDEX IKONIR
	ON IKONST (IKFIRM, IKBFRA);

CREATE INDEX IKONIU
	ON IKONST (IKFIRM, IKBFRA);

LABEL ON TABLE IKONST IS 'Konvertering av bilagskoder';
