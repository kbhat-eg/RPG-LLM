-- Hjelpefil Innlesing spes.pris nobbstruktur

CREATE OR REPLACE TABLE FP6WST( 
 Firma               FOR COLUMN FPWFIR NUMERIC(3, 0) NOT NULL,
 Leverandornr        FOR COLUMN FPWLDO NUMERIC(6, 0) NOT NULL,
 Overgruppe          FOR COLUMN FPWOGR NUMERIC(2, 0) DEFAULT 0,
 Hovedgruppe         FOR COLUMN FPWHGR NUMERIC(2, 0) DEFAULT 0,
 Undergruppe         FOR COLUMN FPWUGR NUMERIC(3, 0) DEFAULT 0,
 Modulnr             FOR COLUMN FPWMOD NUMERIC(8, 0) DEFAULT 0,
 Nobbnr              FOR COLUMN FPWNOB NUMERIC(8, 0) DEFAULT 0,
 Rabattprosent       FOR COLUMN FPWRAB NUMERIC(5, 3) DEFAULT 0,
 Leveringstype       FOR COLUMN FPWLTY NUMERIC(1, 0) DEFAULT 0,
 Gyldigdato          FOR COLUMN FPWGDA DATE DEFAULT '0001-01-01',
 FraDato             FOR COLUMN FPWFDA DATE DEFAULT '0001-01-01',
 tILDato             FOR COLUMN FPWTDA DATE DEFAULT '0001-01-01',
 OpprettetDato       FOR COLUMN FPWODA DATE NOT NULL DEFAULT CURRENT_DATE,
 OpprettetTid        FOR COLUMN FPWOTI TIME NOT NULL DEFAULT CURRENT_TIME,
 OpprettetAv         FOR COLUMN FPWOUS CHAR(10) NOT NULL DEFAULT ' ',
 EndretDato          FOR COLUMN FPWEDA DATE NOT NULL DEFAULT CURRENT_DATE,
 EndretTid           FOR COLUMN FPWETI TIME NOT NULL DEFAULT CURRENT_TIME,
 EndretAv            FOR COLUMN FPWEUS CHAR(10) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;


LABEL ON TABLE FP6WST
	IS 'Tempor√¶r innl.spes.pris ';
