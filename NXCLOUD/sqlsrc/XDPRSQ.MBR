
CREATE OR REPLACE TABLE XDPRST
(
   UID              BIGINT    NOT NULL GENERATED
   ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
   RAPPORT_ID       BIGINT,
   PERIODE_TYPE     INTEGER,
   FRADATO          DATE,
   TILDATO          DATE,
   FRAPERIODE       INTEGER,
   TILPERIODE       INTEGER,
   FRAAAR           INTEGER,
   TILAAR           INTEGER,
   PRIMARY KEY(UID)
)
ON REPLACE PRESERVE ALL ROWS;
LABEL ON TABLE XDPRST IS 'RA Drilldown_Periode';
