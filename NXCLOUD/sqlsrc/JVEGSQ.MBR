CREATE OR REPLACE TABLE JVEGST(
  UID              NUMERIC(18, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
                   (START WITH 1 INCREMENT BY 1 CYCLE),
  Nobbnummer       FOR COLUMN JVWNOB NUMERIC(8, 0) NOT NULL,
  EgenskapGuid     FOR COLUMN JVWGUI VARCHAR(50) NOT NULL,
  Navn             FOR COLUMN JVWNAV VARCHAR(256) NOT NULL DEFAULT ' ',
  Verdi            FOR COLUMN JVWVRD VARCHAR(10) NOT NULL DEFAULT ' ',
  Enhet            FOR COLUMN JVWENH VARCHAR(10) NOT NULL DEFAULT ' ',
  OpprettetDato    FOR COLUMN JVWODA DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl      FOR COLUMN JVWOTI TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv      FOR COLUMN JVWOUS CHAR(10) NOT NULL DEFAULT ' ',
  EndretDato       FOR COLUMN JVWEDA DATE NOT NULL DEFAULT CURRENT_DATE,
  EndretKl         FOR COLUMN JVWETI TIME NOT NULL DEFAULT CURRENT_TIME,
  EndretAv         FOR COLUMN JVWEUS CHAR(10) NOT NULL DEFAULT ' ')
ON REPLACE PRESERVE ALL ROWS;

CREATE UNIQUE INDEX JVEGIU ON JVEGST(JVWNOB, JVWGUI);
CREATE INDEX JVEGI1 ON JVEGST(JVWNOB, JVWGUI);

LABEL ON TABLE JVEGST IS 'Vare egenskaper';
