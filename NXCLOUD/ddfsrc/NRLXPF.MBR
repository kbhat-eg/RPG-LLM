      *
      * NIB-innlesing RELEX min/maks
      *
     A          R NRLXPFR
     A            NRFIRM         3S 0       TEXT('Firma')
     A                                      COLHDG('Firma')
     A            NRLAGE         2S 0       TEXT('Lager')
     A                                      COLHDG('Lager')
     A            NRVARE         8A         TEXT('Varenr')
     A                                      COLHDG('Varenr')
     A            NRNMIN        11S 3       TEXT('Nytt min lager')
     A                                      COLHDG('Nytt' 'min lager')
     A            NRNMAX        11S 3       TEXT('Nytt max lager')
     A                                      COLHDG('Nytt' 'max lager')
     A            NRKABC         2A         TEXT('ABC-kode')
     A                                      COLHDG('ABC-kode')
