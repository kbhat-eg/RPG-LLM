      *
     fausrl1    if   e           k disk
     frc600d    cf   e             workstn
      *
      * Definering av Local data area
      *
     d                uds
     d w_type                300    300
     d w_edat                          d   datfmt(*iso)
      *
     d l_user                911    920
     d l_firm                944    946  0
     d l_fnav                951    980
      *
     d s               s              1  0
     d w_firm          s              3  0
     d w_in03          s              1
      *
     c     dummy         tag
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      * Definering av felter                                            *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      *
      * Benyttede skjermbilder.
      * - - - - - - - - - - - - - - - - -
      *
      * A2BLD  - A2 Bilde for valg av parametre
      *
      /eject
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      * A2BLD Behandle bilde for valg av utskriftskriteria              *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      *
     c     a2taga        tag
      *
      * Bare data
      *
     c                   exfmt     a2bld
      *
     c                   eval      *in31 = *off
     c                   eval      *in32 = *off
      *
      * Gå ut
      *
     c                   if        *inkc = *on                                  .....
     c                   eval      w_in03 = '1'                                     .
     c                   goto      xslutt                                           .
     c                   endif                                                  .....
      *
      * Sjekke om "Eldre enn dato" er gyldig
      *
      *
     c     *dmy          test(d)                 a2edat                 31
     c                   if        *in31 = *on                                           .....
     c                   goto      a2taga                                                    .
     c                   endif                                                           .....
      *
     c     *dmy          move      a2edat        w_edat                                      .
      *
      *  Kontroll type utskrift
      *
     c                   if        a2type <> 'J'                                         .....
     c                             and a2type <> 'N'                                         .
     c                   move      *on           *in32                          Pos.cur.     .
     c                   goto      a2taga                                                    .
     c                   endif                                                           .....
      *
     c                   move      a2type        w_type                                      .
      *
      * Call utskriftsrutinen
      *
     c                   call      'RC600C'
     c                   parm                    w_in03
     c                   parm                    w_type
     c                   parm                    w_edat
      *
      /eject
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      * Avslutt program                                                 *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      *
     c     xslutt        tag
     c                   eval      *inlr = *on
     c                   return
      *
      /eject
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      * Subrutine for initiering av program                             *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      *
     c     *inzsr        begsr
      *
      *
      * Legge inn firmanummer
      *
     c                   move      l_firm        w_firm
     c                   move      'J'           a2type
     c                   eval      w_in03 = ' '
      *
     c                   endsr
      /eject
