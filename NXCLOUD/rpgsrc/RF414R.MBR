      * Definering av konstanter
      *
     d c_sfil          s              2  0 inz(12)
      *
      * Forklaring på spesielle felter
      * som benyttes ved sub-files.
      * - - - - - - - - - - - - - - - - -
      * w_stel - Subfile-teller
      * w_spge - Sidestørrelse i subfile (page)
      * w_srrn - Relativt recordnummer i subfile
      * w_sfrn - Første recordnummer på siden
      * w_ssrn - Siste recordnummer på siden
      * w_curn - Current recordnummer på siden
      * w_virn - Siden som skal vises
      * w_afld - Aktivt feltnavn
      * w_arow - Aktiv rad for cursor
      * w_acol - Aktiv kolonne for cursor
      *
      * Benyttede skjermbilder.
      * - - - - - - - - - - - - - - - - -
      * B1SFL  - B1 Subfile - record
      * B2CTL  - B2 Subfile - control record
      * B2CMD  - B2 Eget skjermbilde for CMD-taster
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * B2CTL Behandle åpningsbildet med subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
      * Send Alt
      *
     c     b2taga        tag
      *
     c                   write     b2cmd
      *
      * Bare data
      *
     c     b2tagb        tag
      *
     c                   exsr      dsp_subfile
      *
      * Behandling av funksjonstaster
      *
     c                   select
     c                   when      *inkc or *inkl
     c                   goto      avslutt
     c                   when      *inke
     c                   exsr      forny
     c                   goto      b2tagb
     c                   when      *in10
     c                   exsr      crt_subfile
     c                   goto      b2tagb
     c                   when      *in11
     c                   exsr      bck_subfile
     c                   goto      b2tagb
     c                   when      *in21
     c                   eval      *in22 = *on
     c                   goto      b2taga
     c                   endsl
      *
      * Behandling av subfile
      *
     c                   exsr      subfile
      *
     c                   goto      b2taga
      *
      * Avslutt program
      *
     c     avslutt       tag
      *
     c                   eval      *inlr = *on
     c                   return
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for fornying av subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     forny         begsr
      *
     c                   if        w_sfrn <> 0
     c     w_sfrn        chain     b1sfl
     c                   endif
      *
     c                   select
     c                   when      w_seqe = 'B'
     c                   eval      rrf0l2_0txt = b10txt
     c     rrf0l2_key    setll     rrf0l2
     c                   other
     c                   eval      rrf0l1_0rap = b10rap
     c     rrf0l1_key    setll     rrf0l1
     c                   endsl
      *
     c                   exsr      clr_subfile
     c                   exsr      crt_subfile
      *
     c     end_forny     endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for posisjonering i subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     posisjoner    begsr
      *
      * Rapport
      *
     c                   if        b20rap <> 0
     c                   eval      w_seqe = *blank
     c                   eval      rrf0l1_0rap = b20rap
     c     rrf0l1_key    setll     rrf0l1
     c                   goto      end_pos
     c                   endif
      *
      * Beskrivelse
      *
     c                   if        b20txt <> *blank
     c                   eval      w_seqe = 'B'
     c                   eval      rrf0l2_0txt = b20txt
     c     rrf0l2_key    setll     rrf0l2
     c                   goto      end_pos
     c                   endif
      *
     c     end_pos       tag
      *
      * Blanker subfile, og fyller opp i.h.t. posisjonering
      *
     c                   exsr      clr_subfile
     c                   exsr      crt_subfile
      *
      * Blanker posisjoneringsfelter
      *
     c                   eval      b20rap = 0
     c                   eval      b20txt = *blank
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for behandling av subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     subfile       begsr
      *
     c                   eval      b_forn = *off
      *
     c                   if        *in22 = *off
     c                   eval      *in22 = *on
     c                   else
     c                   eval      *in22 = *off
     c                   endif
      *
     c                   if        w_curn > 0
     c                   eval      w_virn = w_curn
     c                   endif
      *
      * Leser og behandler subfile
      *
     c                   do        w_ssrn
     c                   readc     b1sfl
      *
     c                   if        %eof
     c                   leave
     c                   endif
      *
     c                   eval      *in22 = *off
     c                   eval      w_virn = w_srrn
      *
      * Kopier
      *
     c                   if        b1valg = 3
     c                   eval      s_0rap = b10rap
     c                   exsr      kopier
     c                   eval      b1valg = 0
     c                   update    b1sfl
     c                   iter
     c                   endif
      *
     c                   if        b_forn = *on
     c                   exsr      forny
     c                   endif
      *
     c                   enddo
      *
     c     end_subfile   endsr
      *
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for å kopiere
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     kopier        begsr
      *
      * Kopier raporthode
      *
     c                   eval      rrf0l1_0fir = w_ffir
     c                   eval      rrf0l1_0rap = s_0rap
     c     rrf0l1_key    setll     rrf0l1r                                60
     c     rrf0l1_key    reade     rrf0l1r                                60
      *
     c                   dow       *in60 = *off                                 EOF
      *
     c                   eval      rrf0lu_0fir = w_tfir
     c                   eval      rrf0lu_0rap = s_0rap
     c     rrf0lu_key    chain     rrf0lur                            63
      *
     c                   if        *in63 = *on
     c                   eval      rr0fir = w_tfir
     c                   write     rrf0lur
     c                   endif
      *
     c     rrf0l1_key    reade     rrf0l1r                                60
      *
     c                   enddo
      *
      * Kopier raportlinjer
      *
     c                   eval      rrf5l1_5fir = w_ffir
     c                   eval      rrf5l1_5rap = s_0rap
     c     rrf5l1_key    setll     rrf5l1r                                61
     c     rrf5l1_key    reade     rrf5l1r                                61
      *
     c                   dow       *in61 = *off                                 EOF
      *
     c                   eval      rrf5lu_5fir = w_tfir
     c                   eval      rrf5lu_5rap = s_0rap
     c                   eval      rrf5lu_5lin = rr5lin
     c     rrf5lu_key    chain     rrf5lur                            63
      *
     c                   if        *in63 = *on
     c                   eval      rr5fir = w_tfir
     c                   write     rrf5lur
     c                   endif
      *
     c     rrf5l1_key    reade     rrf5l1r                                61
      *
     c                   enddo
      *
      * Kopier fra-til begrep
      *
     c                   eval      rrf8l1_8fir = w_ffir
     c                   eval      rrf8l1_8rap = s_0rap
     c     rrf8l1_key    setll     rrf8l1r                                62
     c     rrf8l1_key    reade     rrf8l1r                                62
      *
     c                   dow       *in62 = *off                                 EOF
      *
     c                   eval      rrf8lu_8fir = w_tfir
     c                   eval      rrf8lu_8rap = s_0rap
     c                   eval      rrf8lu_8lin = rr8lin
     c                   eval      rrf8lu_8lØp = rr8lØp
     c     rrf8lu_key    chain     rrf8lur                            63
      *
     c                   if        *in63 = *on
     c                   eval      rr8fir = w_tfir
     c                   write     rrf8lur
     c                   endif
      *
     c     rrf8l1_key    reade     rrf8l1r                                62
      *
     c                   enddo
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for tømming av subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     clr_subfile   begsr
      *
     c                   eval      *in14 = *on
     c                   write     b2ctl
     c                   eval      *in14 = *off
      *
     c                   eval      *in15 = *off
     c                   eval      w_srrn = 0
     c                   eval      w_sfrn = 0
     c                   eval      w_ssrn = 0
     c                   eval      w_spge = 0
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for å fylle opp subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     crt_subfile   begsr
      *
     c                   eval      w_spge = w_spge + c_sfil
     c                   eval      w_srrn = w_ssrn
      *
     c                   dou       w_srrn = w_spge
      *
     c                   select
     c                   when      w_seqe = 'B'
     c                   read      rrf0l2
     c                   other
     c                   read      rrf0l1
     c                   endsl
      *
     c                   if        %eof or
     c                             rr0fir <> w_ffir
     c                   eval      *in15 = *on
     c                   leave
     c                   endif
      *
     c                   eval      w_srrn = w_srrn + 1
      *
     c                   select
     c                   when      w_seqe = 'B'
     c                   eval      rrf0l2_0txt = rr0txt
     c                   other
     c                   eval      rrf0l1_0rap = rr0rap
     c                   endsl
      *
     c                   eval      b1valg = 0
     c                   eval      b10rap = rr0rap
     c                   eval      b10txt = rr0txt
      *
     c                   write     b1sfl
      *
     c                   enddo
      *
     c                   if        w_srrn > w_ssrn
     c                   eval      w_sfrn = w_ssrn + 1
     c                   endif
      *
     c                   eval      w_ssrn = w_srrn
     c                   eval      w_virn = w_sfrn
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for å bla tilbake en side i subfilen
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     bck_subfile   begsr
      *
     c                   select
     c                   when      w_seqe = 'B' and
     c                             %eof(rrf0l2)
     c     rrf0l2_key    setgt     rrf0l2
     c                   readp     rrf0l2
     c                   when      w_seqe = *blank and
     c                             %eof(rrf0l1)
     c     rrf0l1_key    setgt     rrf0l1
     c                   readp     rrf0l1
     c                   endsl
      *
      * Leser tilbake en side
      *
     c                   eval      w_stel = 0
      *
     c                   dou       w_stel = w_ssrn + c_sfil
      *
     c                   select
     c                   when      w_seqe = 'B'
     c                   readp     rrf0l2
     c                   other
     c                   readp     rrf0l1
     c                   endsl
      *
     c                   if        %eof or
     c                             rr0fir <> w_ffir
     c                   leave
     c                   endif
      *
     c                   eval      w_stel = w_stel + 1
      *
     c                   select
     c                   when      w_seqe = 'B'
     c                   eval      rrf0l2_0txt = rr0txt
     c                   other
     c                   eval      rrf0l1_0rap = rr0rap
     c                   endsl
      *
     c                   enddo
      *
     c                   select
     c                   when      w_seqe = 'B' and
     c                             %eof(rrf0l2)
     c     rrf0l2_key    setll     rrf0l2
     c                   when      w_seqe = *blank and
     c                             %eof(rrf0l1)
     c     rrf0l1_key    setll     rrf0l1
     c                   endsl
      *
      * Blanker og fyller opp subfile
      *
     c                   exsr      clr_subfile
     c                   exsr      crt_subfile
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
