      *
     c                   eval      afirl1_firm = w_tfir
     c     afirl1_key    chain     afirl1
      *
     c                   eval      a1tfir = w_tfir
     c                   if        %found
     c                   move      aafnav        a1tnav
     c                   endif
      *
      * Posisjonerer databasen, og fyller opp subfile
      *
     c                   eval      rrf0l2_0fir = w_ffir
      *
     c                   eval      rrf0l1_0fir = w_ffir
     c                   eval      rrf0l1_0rap = 0
     c     rrf0l1_key    setll     rrf0l1
      *
     c                   exsr      crt_subfile
      *
     c                   endsr
