     c                   parm                    c2ponr
     c                   parm                    c2sted
     c                   endif
      *
     c                   goto      c2taga
      *
     c                   endif
      *
      * Validering av input
      *
     c                   if        c2navn = *blank
     c                   eval      *in31 = *on
     c                   goto      c2taga
     c                   endif
      *
     c                   if        c2ponr <> 0
     c                   eval      apospf_ponr = c2ponr
     c     apospf_key    chain     apospf
     c                   if        not %found
     c                   eval      *in32 = *on
     c                   goto      c2taga
     c                   endif
     c                   eval      c2sted = apsted
     c                   endif
      *
