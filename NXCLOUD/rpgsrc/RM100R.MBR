     c                   eval      c2adr1 = rmadr1
     c                   eval      c2adr2 = rmadr2
     c                   eval      c2ponr = rmponr
     c                   eval      c2tlfn = rmtlfn
     c                   eval      c2faks = rmfaks
     c                   eval      c2mail = rmmail
     c                   eval      c2urla = rmurla
     c                   eval      c2kprs = rmkprs
     c                   eval      c2ktlf = rmktlf
     c                   eval      c2kmob = rmkmob
     c                   eval      c2kmai = rmkmai
     c                   eval      c2bgrn = rmbgrn
     c                   eval      c2eusr = rmeusr
     c     *dmy          move      rmodat        c2odat
     c     *dmy          move      rmedat        c2edat
