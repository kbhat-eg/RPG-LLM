     c                   eval      c2adr1 = *blank
     c                   eval      c2adr2 = *blank
     c                   eval      c2ponr = 0
     c                   eval      c2tlfn = *blank
     c                   eval      c2faks = *blank
     c                   eval      c2mail = *blank
     c                   eval      c2urla = *blank
     c                   eval      c2kprs = *blank
     c                   eval      c2ktlf = *blank
     c                   eval      c2kmob = *blank
     c                   eval      c2kmai = *blank
     c                   eval      c2bgrn = 0
     c                   eval      c2eusr = *blank
     c                   eval      c2odat = 0
     c                   eval      c2edat = 0
     c                   endif
      *
      * Henter poststed
      *
     c                   if        c2ponr <> 0
     c                   eval      apospf_ponr = c2ponr
     c     apospf_key    chain     apospf
     c                   if        %found
     c                   eval      c2sted = apsted
     c                   endif
     c                   endif
      *
     c                   if        b1valg = 3
     c                   eval      c1alme = k1alme
     c                   endif
      *
     c                   if        b1valg = 5
     c                   eval      *in30 = *on
     c                   endif
      *
     c     c2taga        tag
