     c                   eval      rmadr2 = c2adr2
     c                   eval      rmponr = c2ponr
     c                   eval      rmtlfn = c2tlfn
     c                   eval      rmfaks = c2faks
     c                   eval      rmmail = c2mail
     c                   eval      rmurla = c2urla
     c                   eval      rmkprs = c2kprs
     c                   eval      rmktlf = c2ktlf
     c                   eval      rmkmob = c2kmob
     c                   eval      rmkmai = c2kmai
     c                   eval      rmbgrn = c2bgrn
      *
     c                   time                    rmedat
