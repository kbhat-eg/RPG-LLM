      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     vis_logg      begsr
      *
     c                   eval      c2numm = 0
5.60 c                   eval      c2aarr = 0
     c                   eval      c2suff = 0
     c                   eval      c2date = 0
     c                   eval      c2time = 0
     c                   eval      c2kode = *blank
     c                   eval      c2user = *blank
     c                   eval      c2wsid = *blank
     c                   eval      c2jtxt = *blank
      *
5.60 c                   eval      floglr_aarr = p_aarr
     c                   eval      floglr_numm = p_numm
     c                   eval      floglr_suff = b1suff
     c                   eval      floglr_kode = b1kode
     c     *dmy          move      b1date        floglr_date
     c     *hms          move      b1time        floglr_time
     c     floglr_key    chain     floglrr
      *
     c                   if        %found
5.60 c                   eval      c2aarr = fuaarr
     c                   eval      c2numm = funumm
     c                   eval      c2suff = fusuff
     c                   eval      c2kode = fukode
     c                   eval      c2user = fuuser
     c                   eval      c2wsid = fuwsid
     c     *dmy          move      fudate        c2date
     c     *hms          move      futime        c2time
      *
      * Hent inn tekst
      *
     c                   eval      vlkol1_jtyp = fukode
     c     vlkol1_key    chain     vlkol1r
     c                   if        %found
     c                   eval      c2jtxt = vajtxt
     c                   endif
     c                   endif
      *
     c                   exfmt     c2win
      *
     c                   endsr
