      * Subrutine for å bla tilbake en side i subfilen
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     bck_subfile   begsr
      *
     c                   if        *in15
     c     flogl3_key    setgt     flogl3
     c                   readp     flogl3
     c                   endif
      *
      * Leser tilbake en side
      *
     c                   eval      w_stel = 0
      *
     c                   dou       w_stel = w_ssrn + c_sfil
      *
     c                   readp     flogl3
      *
     c                   if        %eof or
     c                             fufirm <> w_firm
     c                   leave
     c                   endif
      *
5.60 c                   if        fuaarr <> p_aarr or
     c                             funumm <> p_numm
     c                   leave
     c                   endif
      *
     c                   eval      w_stel = w_stel + 1
      *
     c                   eval      flogl3_date = fudate
     c                   eval      flogl3_time = futime
      *
     c                   enddo
      *
     c                   if        %eof
     c     flogl3_key    setll     flogl3
     c                   endif
      *
      * Blanker og fyller opp subfile
      *
     c                   exsr      clr_subfile
     c                   exsr      crt_subfile
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for display av subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     dsp_subfile   begsr
      *
     c                   if        w_srrn <> 0
