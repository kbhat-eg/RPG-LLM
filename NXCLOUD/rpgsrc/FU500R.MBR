      *
      * Definering av parametre
      *
     d p_firm          s                   like(fufirm)
5.60 d p_aarr          s                   like(fuaarr)
6.nu d p_numm          s                   like(funumm)
     d p_suff          s                   like(fusuff)
7.01 d p_regi          s             10
7.01 d p_fore          s             10
7.01 d p_for8          s              8
7.01 d p_fort          s             10
      *
      * Definering av Information Data Structure - display feedback
      *
     d dspfbk          ds
     d  d_fcrn               378    379I 0
      *
      * Definering av variabler i nøkler
      *
5.60 d flogl3_aarr     s                   like(fuaarr)
     d flogl3_numm     s                   like(funumm)
     d flogl3_date     s                   like(fudate)
     d flogl3_time     s                   like(futime)
5.60 d floglr_aarr     s                   like(fuaarr)
     d floglr_numm     s                   like(funumm)
     d floglr_suff     s                   like(fusuff)
     d floglr_kode     s                   like(fukode)
     d floglr_date     s                   like(fudate)
     d floglr_time     s                   like(futime)
     d vlkol1_jtyp     s                   like(vajtyp)
      *
      * Definering av variable
      *
     d w_fcrn          s              4  0
     d w_stel          s              4  0
     d w_spge          s              4  0
     d w_srrn          s              4  0
     d w_sfrn          s                   like(w_srrn)
     d w_ssrn          s                   like(w_srrn)
      *
     d b_valg_ok       s              1    inz(*off)
      *
     d w_firm          s                   like(fufirm)
      *
      * Definering av konstanter
      *
7.xx d c_sfil          s              2  0 inz(7)
      *
      * Forklaring på spesielle felter
      * som benyttes ved sub-files.
      * - - - - - - - - - - - - - - - - -
      * w_fcrn - Første recordnummer på siden
      * w_stel - Subfile-teller
      * w_spge - Sidestørrelse i subfile (page)
      * w_srrn - Relativt recordnummer i subfile
      * w_sfrn - Første recordnummer på siste side
      * w_ssrn - Siste recordnummer på siste side
      * w_curn - Current recordnummer på siden
      * w_virn - Side og cursorplassering som skal vises
      * w_afld - Aktivt feltnavn
      * w_arow - Aktiv rad for cursor
      * w_acol - Aktiv kolonne for cursor
      *
      * Benyttede skjermbilder.
      * - - - - - - - - - - - - - - - - -
      * B1SFL  - B1 Subfile - record
      * B2CTL  - B2 Subfile - control record
      * B2CMD  - B2 Eget skjermbilde for CMD-taster
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * B2CTL Behandle åpningsbildet med subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
      * Send Alt
      *
     c     b2taga        tag
      *
     c                   write     b2cmd
      *
      * Bare data
      *
     c     b2tagb        tag
      *
     c                   eval      b2numm = p_numm
5.60 c                   eval      b2aarr = p_aarr
     c                   exsr      dsp_subfile
      *
      * Behandling av funksjonstaster
      *
     c                   select
     c                   when      *inkc or *inkl
     c                   goto      avslutt
     c                   when      *inke
     c                   exsr      forny
     c                   goto      b2tagb
     c                   when      *in10
     c                   exsr      crt_subfile
     c                   goto      b2tagb
     c                   when      *in11
     c                   exsr      bck_subfile
     c                   goto      b2tagb
     c                   when      *in21
     c                   eval      *in22 = *on
     c                   goto      b2taga
7.01 c                   when      *inkv
7.01 c                             or *inkw
7.01 c                             or *inkx
7.01 c                             or *inky
7.01 c                   eval      p_regi = 'ORDRE'
7.01 c                   move      p_numm        p_for8
7.01 c                   eval      p_fore = p_for8
7.01 c                   dow       %subst(p_fore:1:1) = '0'
7.01 c                   eval      p_fore = %subst(p_fore:2:9)
7.01 c                   enddo
7.01 c                   eval      p_fort = *blank
7.01 c                   call      'EL001R'
7.01 c                   parm                    p_regi
7.01 c                   parm                    p_fore
7.01 c                   parm                    p_fort
     c                   endsl
      *
      * Posisjonering
      *
     c                   if        b2date <> 0
     c                   exsr      posisjoner
     c                   goto      b2tagb
     c                   endif
      *
      * Behandling av subfile
      *
     c                   exsr      subfile
     c                   if        b_valg_ok = *on
     c                   goto      avslutt
     c                   endif
      *
     c                   goto      b2taga
      *
      * Avslutt program
      *
     c     avslutt       tag
      *
     c                   eval      *inlr = *on
     c                   return
      *
