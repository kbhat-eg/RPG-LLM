     frkunl1    if   e           k disk    rename(rkunpfr:rkunl1r)
      *
     d p_firm          s                   like(rkfirm)
     d p_kund          s                   like(rkkund)
     d p_faty          s                   like(rkfaty)
     d p_retk          s              1
      *
     d w_firm          s                   like(rkfirm)
     d w_kund          s                   like(rkkund)
      *
     d                uds
     d l_user                911    920
     d l_firm                944    946  0
     d l_fnav                951    980
      *
     d rkunl1_kund     s                   like(rkkund)
      *
      * Parameterliste
      *
     c     *entry        plist
     c                   parm                    p_retk
     c                   parm                    p_firm
     c                   parm                    p_kund
     c                   parm                    p_faty
      *
      * Legge inn firmanummer
      *
     c                   eval      w_firm = p_firm
     c                   eval      p_retk = *blank
     c                   eval      p_faty = 0
      *
      * Sjekke kunde
      *
     c                   eval      rkunl1_kund = p_kund
     c     rkunl1_key    chain     rkunl1r                            60
      *
     c                   if        *in60 = *on
     c                   move      'N'           p_retk
     c                   goto      slutt
     c                   endif
      *
      * Er kunde sperret?
      *
     c                   if        rksprk = 'J'
     c                   move      'S'           p_retk
     c                   goto      slutt
     c                   endif
      *
     c                   move      rkfaty        p_faty
