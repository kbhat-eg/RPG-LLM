      *
      *  Definering av datastrukturer for filnavn
      *
     d                 ds
     d d_fnav                        64
     d  d_filtyp                      7    overlay(d_fnav)     inz('prisfil')
     d  d_skill1                      1    overlay(d_fnav:8)   inz('_')
     d  d_butiid                     15    overlay(d_fnav:9)
     d  d_skill2                      1    overlay(d_fnav:24)  inz('_')
     d  d_kunden                      6    overlay(d_fnav:25)
     d  d_skill3                      1    overlay(d_fnav:31)  inz('_')
     d  d_kprosj                      6    overlay(d_fnav:32)
     d  d_fileks                      4    overlay(d_fnav:38)
      *
      *  Definering av variable i parameter
      *
     d p_buid          s             15
     d p_firm          s              3
     d p_kund          s              6
     d p_kpro          s              6
     d p_rfmt          s             10
     d p_fnav          s             64
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *  Setter fil ekstension avhengig av formatvalg
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c                   select
     c                   when      p_rfmt = 'NOBB'
     c                   eval      d_fileks = '.DAT'
     c                   when      p_rfmt = 'REGNEARK'
     c                   eval      d_fileks = '.XLS'
6.31 c                   when      p_rfmt = 'REGNEARKPO'
6.31 c                   eval      d_fileks = '.XLS'
     c                   other
     c                   eval      d_fileks = '.TXT'
     c                   endsl
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *  Bygger filnavn av parametre
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c                   eval      d_butiid = p_buid
     c                   eval      d_kunden = p_kund
     c                   eval      d_kprosj = p_kpro
     c                   eval      p_fnav = d_fnav
