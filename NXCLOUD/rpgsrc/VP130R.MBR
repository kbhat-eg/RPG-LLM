      *
     c     xd1win        begsr
      *
     c                   eval      vpgrlr_glag = d1glag
     c                   eval      vpgrlr_gavd = d1gavd
     c     vpgrlr_key    chain     vpgrlr
      *
     c                   exfmt     d1win
      *
     c                   if        *inkc or *inkl
     c                   leavesr
     c                   endif
      *
     c                   eval      b_forn = *on
      *
      * Sletter rad
      *
     c                   eval      vpgrlu_glag = d1glag
     c                   eval      vpgrlu_gavd = d1gavd
     c     vpgrlu_key    chain     vpgrlur
      *
     c                   delete    vpgrlur
      *
     c                   endsr
      *
