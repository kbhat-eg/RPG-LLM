     c                   eval      vpgrlr_gavd = k1gavd
     c     vpgrlr_key    chain     vpgrlr
     c                   if        %found
     c                   eval      *in33 = *on
     c                   goto      k1taga
     c                   endif
      *
      * Kaller vedlikeholdsprogram
      *
     c                   eval      c1glag = s_glag
     c                   eval      c1gavd = s_gavd
