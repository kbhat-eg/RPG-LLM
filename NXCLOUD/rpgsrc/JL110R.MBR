      * Behandling av funksjonstaster
      *
     c                   if        *inkc or *inkl
     c                   eval      b_anul = *on
     c                   leavesr
     c                   endif
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for å behandle bilde for ny/endring/vise (c2BLD)
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     xc2bld        begsr
      *
     c                   eval      w_arow = 0
     c                   eval      w_acol = 0
      *
     c                   eval      jlkplr_ptel = c1ptel
     c     jlkplr_key    chain     jlkplr
      *
     c                   if        %found
     c                   eval      c2pnav = jkpnav
     c                   eval      c2pttl = jkpttl
     c                   eval      c2ptlf = jkptlf
     c                   eval      c2pmob = jkpmob
     c                   eval      c2pfax = jkpfax
     c                   eval      c2pmai = jkpmai
     c                   eval      c2prol = jkprol
     c                   eval      c2pusr = jkpusr
     c                   if        jkpoda <> *loval
     c     *dmy          move      jkpoda        c2poda
     c                   else
     c                   eval      c2poda = 0
     c                   endif
     c                   if        jkpeda <> *loval
     c     *dmy          move      jkpeda        c2peda
     c                   else
     c                   eval      c2peda = 0
     c                   endif
     c                   else
     c                   eval      c2pnav = *blank
     c                   eval      c2pttl = *blank
     c                   eval      c2ptlf = *blank
     c                   eval      c2pmob = *blank
     c                   eval      c2pfax = *blank
     c                   eval      c2pmai = *blank
     c                   eval      c2prol = *blank
     c                   eval      c2pusr = *blank
     c                   eval      c2poda = 0
     c                   eval      c2peda = 0
     c                   endif
      *
     c                   if        b1valg = 3
     c                   eval      c1ptel = k1ptel
     c                   endif
      *
     c                   if        b1valg = 5 or p_valg = 5
     c                   eval      *in30 = *on
     c                   endif
      *
     c     c2taga        tag
      *
     c                   exfmt     c2bld
      *
     c                   if        *inkc or *inkl or *in30
     c                   goto      end_c2pld
     c                   endif
      *
      * Spørring
      *
     c                   if        *inkd
     c                   goto      c2taga
     c                   endif
      *
      * Validering av input
      *
     c                   if        c2pnav = *blank
     c                   eval      *in31 = *on
     c                   goto      c2taga
     c                   endif
      *
      * Oppdatering
      *
     c                   clear                   jlkplur
      *
     c                   eval      jlkplu_ptel = c1ptel
     c     jlkplu_key    chain     jlkplur
      *
     c                   eval      jkpnav = c2pnav
     c                   eval      jkpttl = c2pttl
     c                   eval      jkptlf = c2ptlf
     c                   eval      jkpmob = c2pmob
     c                   eval      jkpfax = c2pfax
     c                   eval      jkpmai = c2pmai
     c                   eval      jkprol = c2prol
      *
     c                   time                    jkpeda
     c                   time                    jkpeti
     c                   eval      jkpusr = l_user
      *
     c                   if        %found
     c                   update    jlkplur
     c                   else
     c                   eval      jkpldo = w_ldor
     c                   eval      jkptel = c1ptel
     c                   time                    jkpoda
     c                   write     jlkplur
     c                   endif
      *
      * Oppdater subfile
      *
     c                   if        b1valg = 2
     c                   eval      b1pnav = jkpnav
6.30 c*                   eval      b1pttl = jkpttl
6.30 c*                   eval      b1ptlf = jkptlf
     c                   eval      b1pmob = jkpmob
6.30 c                   eval      b1prol = jkprol
6.30 c                   eval      b1pmai = jkpmai
     c                   endif
      *
     c                   if        b_oppr = *on or
     c                             b1valg = 3
     c                   eval      b_forn = *on
     c                   endif
      *
     c     end_c2pld     tag
      *
     c                   eval      *in30  = *off
      *
     c                   endsr
