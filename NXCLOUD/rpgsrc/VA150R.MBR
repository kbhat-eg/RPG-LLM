     c                   if        *inkc or *inkl
     c                   goto      end_k1win
     c                   endif
      *
      * Sjekk om utfylt
      *
     c                   if        k1otyp = *blank
     c                   eval      *in31 = *on
     c                   goto      k1taga
     c                   endif
