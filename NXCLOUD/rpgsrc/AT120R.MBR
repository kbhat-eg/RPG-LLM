     d c_sfil          s              2  0 inz(14)
      *
      * Forklaring på spesielle felter
      * som benyttes ved sub-files.
      * - - - - - - - - - - - - - - - - -
      * w_fcrn - Første recordnummer på siden
      * w_stel - Subfile-teller
      * w_spge - Sidestørrelse i subfile (page)
      * w_srrn - Relativt recordnummer i subfile
      * w_sfrn - Første recordnummer på siste side
      * w_ssrn - Siste recordnummer på siste side
      * w_curn - Current recordnummer på siden
      * w_virn - Side og cursorplassering som skal vises
      * w_afld - Aktivt feltnavn
      * w_arow - Aktiv rad for cursor
      * w_acol - Aktiv kolonne for cursor
      *
      * Benyttede skjermbilder.
      * - - - - - - - - - - - - - - - - -
      * B1SFL  - B1 Subfile - record
      * B2CTL  - B2 Subfile - control record
      * B2CMD  - B2 Eget skjermbilde for CMD-taster
      * C1BLD  - C1 Skjermbilde for innlegging av nøkkel ved oppretting
      * C1MSG  - C1 Meldingsbilde for C1BLD
      * C2BLD  - C2 Skjermbilde for opprette/endre/vise
      * D1WIN  - D1 Vindu for slette (Delete)
      * K1WIN  - K1 Vindu for kopiere
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * B2CTL Behandle åpningsbildet med subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
      * Send Alt
      *
     c     b2taga        tag
      *
     c                   write     b2cmd
      *
      * Bare data
      *
     c     b2tagb        tag
      *
     c                   exsr      dsp_subfile
      *
      * Behandling av funksjonstaster
      *
     c                   select
     c                   when      *inkc or *inkl
     c                   goto      avslutt
     c                   when      *inke
     c                   exsr      forny
     c                   goto      b2tagb
     c                   when      *inkf
     c                   exsr      xc1bld
     c                   goto      b2taga
     c                   when      *in10
     c                   exsr      crt_subfile
     c                   goto      b2tagb
     c                   when      *in11
     c                   exsr      bck_subfile
     c                   goto      b2tagb
     c                   when      *in21
     c                   eval      *in22 = *on
     c                   goto      b2taga
     c                   endsl
      *
      * Posisjonering
      *
     c                   if        b2sgrn <> *blank
     c                   exsr      posisjoner
     c                   goto      b2tagb
     c                   endif
      *
      * Behandling av subfile
      *
     c                   exsr      subfile
      *
     c                   goto      b2taga
      *
      * Avslutt program
      *
     c     avslutt       tag
      *
     c                   eval      *inlr = *on
     c                   return
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for fornying av subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     forny         begsr
      *
     c                   if        w_fcrn <> 0
     c     w_fcrn        chain     b1sfl
     c                   endif
      *
     c                   eval      asgrl1_sgrn = b1sgrn
     c                   eval      asgrl1_sgrs = b1sgrs
     c                   eval      asgrl1_sgru = b1sgru
     c     asgrl1_key    setll     asgrl1
      *
     c                   exsr      clr_subfile
     c                   exsr      crt_subfile
      *
     c     end_forny     endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for posisjonering i subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     posisjoner    begsr
      *
      * Posisjoner startverdi på overgruppe
      *
     c                   if        b2sgrn <> *blank
     c                   eval      w_seqe = ' '
     c                   eval      asgrl1_sgrn = b2sgrn
     c     asgrl1_key    setll     asgrl1
     c                   goto      end_pos
     c                   endif
      *
     c     end_pos       tag
      *
      * Blanker subfile, og fyller opp i.h.t. posisjonering
      *
     c                   eval      *in22 = *off
      *
     c                   exsr      clr_subfile
     c                   exsr      crt_subfile
      *
      * Blanker posisjoneringsfelter
      *
     c                   eval      b2sgrn = *blank
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for behandling av subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     subfile       begsr
      *
     c                   eval      b_forn = *off
      *
     c                   if        *in22 = *off
     c                   eval      *in22 = *on
     c                   else
     c                   eval      *in22 = *off
     c                   endif
      *
     c                   eval      w_virn = w_fcrn
      *
      * Leser og behandler subfile
      *
     c                   do        w_ssrn
     c                   readc     b1sfl
      *
     c                   if        %eof
     c                   leave
     c                   endif
      *
     c                   eval      *in22 = *off
     c                   eval      w_virn = w_srrn
      *
      * Endre
      *
     c                   if        b1valg = 2
     c                   eval      c1sgrn = b1sgrn
     c                   eval      c1sgrs = b1sgrs
     c                   eval      c1sgru = b1sgru
     c                   exsr      xc2bld
     c                   eval      b1valg = 0
     c                   update    b1sfl
     c                   iter
     c                   endif
      *
      * Kopier
      *
     c                   if        b1valg = 3
     c                   eval      k1sgrn = b1sgrn
     c                   eval      k1sgrs = b1sgrs
     c                   eval      k1sgru = b1sgru
     c                   exsr      xk1win
     c                   eval      b1valg = 0
     c                   update    b1sfl
     c                   iter
     c                   endif
      *
      * Slett
      *
     c                   if        b1valg = 4
     c                   eval      d1sgrn = b1sgrn
     c                   eval      d1sgrs = b1sgrs
     c                   eval      d1sgru = b1sgru
     c                   exsr      xd1win
     c                   eval      b1valg = 0
     c                   update    b1sfl
     c                   iter
     c                   endif
      *
      * Vis
      *
     c                   if        b1valg = 5
     c                   eval      c1sgrn = b1sgrn
     c                   eval      c1sgrs = b1sgrs
     c                   eval      c1sgru = b1sgru
     c                   exsr      xc2bld
     c                   eval      b1valg = 0
     c                   update    b1sfl
     c                   iter
     c                   endif
     c                   enddo
      *
     c                   if        b_forn = *on
     c                   exsr      forny
     c                   endif
      *
     c     end_subfile   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for behandling av skjermbilde for ny rad (C1BLD)
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     xc1bld        begsr
      *
     c                   eval      b_oppr = *on
     c                   eval      b_forn = *off
      *
     c                   eval      c1sgrn = b2sgrn
      *
     c     c1taga        tag
      *
     c                   exfmt     c1bld
      *
      * Behandling av funksjonstaster
      *
     c                   select
     c                   when      *inkc or *inkl
     c                   goto      end_c1bld
     c                   endsl
      *
      *......................................................
      * Spørring
      *
     c                   if        *inkd = *on
      *
     c                   if        w_afld = 'c1sgru'
     c*>                 call      'VA520R'
     c*>                 parm                    c1sgru
     c*>                 parm                    w_ktxt
     c*>                 eval      c2ktxt = w_ktxt
     c                   goto      c1taga
     c                   endif
      *
     c                   endif
      *
      *................................................................
      *
      * Validering av input
      *
     c                   if        c1sgrn = *blank
     c                   if        c1sgrs = *blank
     c                   if        c1sgru = *blank
     c                   goto      end_c1bld
     c                   endif
     c                   endif
     c                   endif
      *
     c                   if        c1sgrs = *blank
     c                   if        c1sgru = *blank
     c                   eval      *in31 = *on
     c                   goto      c1taga
     c                   endif
     c                   endif
      *
     c                   if        c1sgrs <> *blank
     c                   if        c1sgru <> *blank
     c                   eval      *in31 = *on
     c                   goto      c1taga
     c                   endif
     c                   endif
      *
     c                   if        c1sgrs = c1sgrn
     c                   eval      *in31 = *on
     c                   goto      c1taga
     c                   endif
      *
     c                   if        c1sgru <> *blank
     c                   eval      ausrl1_user = c1sgru
     c     ausrl1_key    chain     ausrl1                             91
     c                   if        *in91 = *on
     c                   eval      *in32 = *on
     c                   goto      c1taga
     c                   endif
     c                   endif
      *
      * Sender opp informasjonsbilde dersom rad finnes fra før
      *
     c                   eval      asgrlr_sgrn = c1sgrn
     c                   eval      asgrlr_sgrs = c1sgrs
     c                   eval      asgrlr_sgru = c1sgru
     c     asgrlr_key    chain     asgrlr                             90
     c                   if        *in90 = *off
     c                   exsr      xc1msg
     c                   if        b_anul = *on
     c                   goto      c1taga
     c                   endif
     c                   endif
      *
      * Kaller vedlikeholdsprogram
      *
     c                   exsr      xc2bld
      *
     c                   eval      c1sgrn = *blank
     c                   eval      c1sgrs = *blank
     c                   eval      c1sgru = *blank
      *
     c                   goto      c1taga
      *
     c     end_c1bld     tag
      *
     c                   eval      b_oppr = *off
     c                   eval      b2sgrn = *blank
      *
     c                   if        b_forn = *on
     c                   exsr      forny
     c                   endif
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for utlegging av opprettelsesmelding
      * Meldingsbildet legges ut dersom det forsøkes å opprette en rad som
      * finnes fra før.
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     xc1msg        begsr
      *
     c                   eval      b_anul = *off
      *
     c                   exfmt     c1msg
      *
      * Behandling av funksjonstaster
      *
     c                   if        *inkc or *inkl
     c                   eval      b_anul = *on
     c                   goto      end_c1msg
     c                   endif
      *
     c     end_c1msg     endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for å behandle bilde for ny/endring/vise (C2BLD)
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     xc2bld        begsr
      *
     c                   eval      w_arow = 0
     c                   eval      w_acol = 0
      *
     c                   eval      asgrlr_sgrn = c1sgrn
     c                   eval      asgrlr_sgrs = c1sgrs
     c                   eval      asgrlr_sgru = c1sgru
     c     asgrlr_key    chain     asgrlr                             90
      *
     c                   if        *in90 = *off
     c                   eval      c2sgrt = atsgrt
      *
     c                   else
     c                   eval      c2sgrt = *blank
     c                   endif
      *
     c                   if        b1valg = 3
     c                   eval      c1sgrn = k1sgrn
     c                   eval      c1sgrs = k1sgrs
     c                   eval      c1sgru = k1sgru
     c                   endif
      *
     c                   if        b1valg = 5
     c                   eval      *in30 = *on
     c                   endif
      *
     c     c2taga        tag
      *
     c                   exfmt     c2bld
      *
     c                   if        *inkc or *inkl or *in30
     c                   goto      end_c2bld
     c                   endif
      *
      * Validering av input
      *
     c                   if        c2sgrt = *blank
     c                   eval      *in31 = *on
     c*>                 goto      c2taga
     c                   endif
      *
      * Oppdatering av gruppe-kode-register
      *
     c                   clear                   asgrlur
      *
     c                   eval      asgrlu_sgrn = c1sgrn
     c                   eval      asgrlu_sgrs = c1sgrs
     c                   eval      asgrlu_sgru = c1sgru
     c     asgrlu_key    chain     asgrlur                            90
      *
     c                   eval      atsgrt = c2sgrt
      *
     c                   if        *in90 = *off
     c                   update    asgrlur
     c                   else
     c                   eval      atsgrn = c1sgrn
     c                   eval      atsgrs = c1sgrs
     c                   eval      atsgru = c1sgru
     c                   write     asgrlur
     c                   endif
      *
      * Oppdater subfile
      *
     c                   if        b1valg = 2
     c*>                 eval      b1otxt = atsgrs
     c*>                 eval      b1okhv = atsgru
     c*>                 eval      b1opra = vgopra
     c                   endif
      *
     c                   if        b_oppr = *on or
     c                             b1valg = 3
     c                   eval      b_forn = *on
     c                   endif
      *
     c     end_c2bld     tag
      *
     c                   eval      *in30  = *off
      *
     c                   endsr
