      * Tar vare på nøkkel til rad det skal kopieres fra
      *
     c                   eval      s_sgrn = k1sgrn
     c                   eval      s_sgrs = k1sgrs
     c                   eval      s_sgru = k1sgru
      *
      * Fyller kopierings-bildet
      *
     c                   eval      asgrlr_sgrn = k1sgrn
     c                   eval      asgrlr_sgrs = k1sgrs
     c                   eval      asgrlr_sgru = k1sgru
     c     asgrlr_key    chain     asgrlr                             90
      *
     c     k1taga        tag
      *
     c                   exfmt     k1win
      *
     c                   if        *inkc or *inkl
     c                   goto      end_k1win
     c                   endif
      *
      * Sjekk om utfylt
      *
     c                   if        k1sgrn = *blank
     c                   if        k1sgrs = *blank
     c                   if        k1sgru = *blank
     c                   eval      *in31 = *on
     c                   goto      k1taga
     c                   endif
     c                   endif
     c                   endif
      *
      * Sjekker om rad finnes fra før
      *
     c                   eval      asgrlr_sgrn = k1sgrn
     c                   eval      asgrlr_sgrs = k1sgrs
     c                   eval      asgrlr_sgru = k1sgru
     c     asgrlr_key    chain     asgrlr                             90
     c                   if        *in90 = *off
     c                   eval      *in32 = *on
     c                   goto      k1taga
     c                   endif
      *
      * Kaller vedlikeholdsprogram
      *
     c                   eval      c1sgrn = s_sgrn
     c                   eval      c1sgrs = s_sgrs
     c                   eval      c1sgru = s_sgru
     c                   exsr      xc2bld
