     c     xd1win        begsr
      *
     c                   eval      asgrlr_sgrn = d1sgrn
     c                   eval      asgrlr_sgrs = d1sgrs
     c                   eval      asgrlr_sgru = d1sgru
     c     asgrlr_key    chain     asgrlr                             90
      *
     c                   exfmt     d1win
      *
     c                   if        *inkc or *inkl
     c                   goto      end_d1win
     c                   endif
      *
     c                   eval      b_forn = *on
      *
      * Sletter overgruppe
      *
     c                   eval      asgrlu_sgrn = d1sgrn
     c                   eval      asgrlu_sgrs = d1sgrs
     c                   eval      asgrlu_sgru = d1sgru
     c     asgrlu_key    chain     asgrlur                            90
     c                   delete    asgrlur
      *
     c     end_d1win     endsr
      *
