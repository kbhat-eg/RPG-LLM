      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for å bla tilbake en side i subfilen
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     bck_subfile   begsr
      *
     c                   if        *in15
     c     asgrl1_key    setgt     asgrl1
     c                   readp     asgrl1
     c                   endif
      *
      * Leser tilbake en side
      *
     c                   eval      w_stel = 0
      *
     c                   dou       w_stel = w_ssrn + c_sfil
      *
     c                   readp     asgrl1
      *
     c                   if        %eof
     c                   leave
     c                   endif
      *
     c                   eval      w_stel = w_stel + 1
      *
     c                   eval      asgrl1_sgrn = atsgrn
     c                   eval      asgrl1_sgrs = atsgrs
     c                   eval      asgrl1_sgru = atsgru
      *
     c                   enddo
      *
     c                   if        %eof
     c     asgrl1_key    setll     asgrl1
     c                   endif
      *
      * Blanker og fyller opp subfile
      *
     c                   exsr      clr_subfile
     c                   exsr      crt_subfile
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for display av subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     dsp_subfile   begsr
      *
     c                   if        w_srrn <> 0
     c                   eval      *in12 = *on
     c                   else
     c                   write     b2cmd
     c                   endif
      *
     c                   eval      *in13 = *on
     c                   exfmt     b2ctl
      *
     c                   eval      *in12 = *off
     c                   eval      *in13 = *off
      *
     c                   eval      w_fcrn = d_fcrn
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for initiering av program
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     *inzsr        begsr
      *
      * Key for posisjonering på gruppe-kode
      *
     c     asgrl1_key    klist
     c                   kfld                    asgrl1_sgrn
     c                   kfld                    asgrl1_sgrs
     c                   kfld                    asgrl1_sgru
      *
      * Key for oppslag på gruppe-kode
      *
     c     asgrlr_key    klist
     c                   kfld                    asgrlr_sgrn
     c                   kfld                    asgrlr_sgrs
     c                   kfld                    asgrlr_sgru
      *
      * Key for oppdatering av gruppe-kode
      *
     c     asgrlu_key    klist
     c                   kfld                    asgrlu_sgrn
     c                   kfld                    asgrlu_sgrs
     c                   kfld                    asgrlu_sgru
      *
      * Key for oppslag på bruker-register
      *
     c     ausrl1_key    klist
     c                   kfld                    ausrl1_user
      *
     c                   eval      *in22 = *on
      *
      * Posisjonerer databasen, og fyller opp subfile
      *
     c                   eval      asgrl1_sgrn = *blank
     c                   eval      asgrl1_sgrs = *blank
     c                   eval      asgrl1_sgru = *blank
     c     asgrl1_key    setll     asgrl1
      *
     c                   exsr      crt_subfile
      *
     c                   endsr
