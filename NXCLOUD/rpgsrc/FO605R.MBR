     c                   if        vaoakk = 0
     c                   eval      w_valg = 1
     c                   exsr      param
     c                   eval      w_valg = 2
     c                   exsr      param
     c                   eval      w_valg = 3
     c                   exsr      param
     c                   goto      neste
     c                   endif
      *
      * ORDREPAPIRER : Subrutinen skal kjøres en gang
      *
     c                   if        vaoakk = 1
     c                   eval      w_valg = 1
     c                   exsr      param
     c                   goto      neste
     c                   endif
      *
      * PAKKSEDDEL : Subrutinen skal ikke kjøres
      *
     c                   if        vaoakk = 2
     c                   goto      neste
     c                   endif
      *
      * FAKTURA/KREDITNOTA: Subrutinen skal ikke kjøres
      *
     c                   if        vaoakk = 3
     c                   goto      neste
     c                   endif
      *
