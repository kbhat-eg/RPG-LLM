     c*                  eval      f1otyp = '99'                                faktura/kreditnota
     c*                  endif
     c*                  endif
      *
     c                   goto      f2taga
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * F2 Innlegging av periode og faktura-dato
      *    Bildet vises bare dersom akk.type=3
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
      *
     c     f2taga        tag
      *
      * Hent opplysninger fra ORDRETYPE-REGISTER
      *
     c*                  eval      votyl1_otyp = f1otyp
     c*    votyl1_key    chain     votyl1r                            90
      *
      * Er dette en Faktura/Kreditnota, hvis ikke, avslutt
      *
     c*                  if        f1otyp <> '99'
     c*                  if        vaoakk <> 3
     c*                  goto      start
     c*                  endif
     c*                  else
     c*                  eval      vaoakk = 3
     c*                  endif
      *
      * Nullstiller periode og fakturadato
      *
     c                   move      udate         f2peri
     c                   move      udate         f2dato
      *
      * Send alt
      *
     c     f2tagb        tag
     c                   exfmt     f2bld
      *
      * F3=Gå tilabke
      *
     c     *inkc         cabeq     *on           xslutt
      *
      * F4=Legg jobb i jobbkø
      *
     c                   if        *inkd  = *on
     c                   eval      p_dirk = 2
     c                   endif
      *
      * Er periode blanket ut ?
      *
     c                   if        f2peri = 0
     c                   eval      *in31  = *on
     c                   goto      f2tagb
     c                   endif
      *
      * Er periode en gyldig periode
      *
     c                   eval      w_mnd1 = 0
     c                   eval      w_aar1 = 0
      *
     c                   if        f2peri <> 0
     c                   move      f2peri        w_dato_6
     c                   movel     01            w_dato_6
     c     *dmy          test(d)                 w_dato_6               31
     c                   if        *in31 = *on
     c                   goto      f2tagb
     c                   endif
     c     *dmy          move      w_dato_6      w_dato
     c                   extrct    w_dato:*m     w_mnd1
     c                   extrct    w_dato:*y     w_aar1
     c                   endif
      *
      * Sjekk om årstall avviker med mer enn 1 år
      *
     c                   time                    w_dato
     c                   extrct    w_dato:*y     w_aar3
     c                   eval      w_avik = w_aar3 - w_aar1
     c                   if        w_avik < -1 or
     c                             w_avik >  1
     c                   eval      *in34 = *on
     c                   goto      f2tagb
     c                   endif
      *
      * Sjekk om periode er lavere enn "avsluttet periode" fra status
      *
     c                   if        w_sp_per <> 0
     c                   movel     w_aar1        w_peri
     c                   move      w_mnd1        w_peri
     c                   if        w_peri <= w_sp_per
     c                   eval      *in36 = *on
     c                   goto      f2tagb
     c                   endif
     c                   endif
      *
      * Varsle dersom årstall avviker fra dagens
      * m/mulighet for overstyring
      *
     c                   if        *inks = *off
     c                   if        w_avik <> 0
     c                   eval      *in35 = *on
     c                   goto      f2tagb
     c                   endif
     c                   endif
      *
      * Er fakturadato en gyldig dato
      *
     c                   eval      w_mnd2 = 0
     c                   eval      w_aar2 = 0
      *
     c                   if        f2dato <> 0
     c     *dmy          test(d)                 f2dato                 32
     c                   if        *in32 = *on
     c                   goto      f2tagb
     c                   endif
     c     *dmy          move      f2dato        w_dato
     c                   extrct    w_dato:*m     w_mnd2
     c                   extrct    w_dato:*y     w_aar2
     c                   endif
      *
      * Er fakturadato innefor periode
      *
     c                   if        *inks = *off
     c                   if        w_mnd1 <> w_mnd2
     c                   eval      *in33 = *on
     c                   goto      f2tagb
     c                   endif
     c                   endif
      *
     c                   if        *inks = *off
     c                   if        w_aar1 <> w_aar2
     c                   eval      *in33 = *on
     c                   goto      f2tagb
     c                   endif
     c                   endif
      *
      * Er fakturering igang fra en annen skjerm ?
      *
     c                   eval      w_kode = 01
     c                   eval      w_onof = 0
     c                   call      'FA730R'
     c                   parm                    w_kode
     c                   parm                    w_onof
      *
     c                   if        w_onof = 1
     c                   goto      xslutt
     c                   endif
      *
      * Kjøres overføring til regnskap fra en annen skjerm ?
      *
     c                   eval      w_kode = 02
     c                   eval      w_onof = 0
     c                   call      'FA730R'
     c                   parm                    w_kode
     c                   parm                    w_onof
      *
     c                   if        w_onof = 1
     c                   goto      xslutt
     c                   endif
      *
      * Oppdater jobb-register med aktiv-flagg
      *
     c                   eval      w_kode = 01
     c                   eval      w_onof = 1
     c                   call      'FA720R'
     c                   parm                    w_kode
     c                   parm                    w_onof
      *
     c                   goto      start
