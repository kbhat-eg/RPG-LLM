     c                   when      w_seqe = 'B'
     c                   eval      rp1pl2_alfa = rp1alf
     c                   other
     c                   eval      rp1pl1_pros = rp1pro
     c                   endsl
     c                   endif
      *
     c     dd200         tag
      *
     c                   enddo
      *
     c                   if        b_eof = *on
     c                   select
     c                   when      w_seqe = 'B'
     c     rp1pl2_key    setll     rp1pl2
     c                   other
     c     rp1pl1_key    setll     rp1pl1
     c                   endsl
     c                   endif
