      *
     c     clr_subfile   begsr
      *
     c                   eval      *in14 = *on
     c                   write     b2ctl
     c                   eval      *in14 = *off
      *
     c                   eval      *in15 = *off
     c                   eval      w_srrn = 0
     c                   eval      w_sfrn = 0
     c                   eval      w_ssrn = 0
     c                   eval      w_spge = 0
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for å fylle opp subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     crt_subfile   begsr
      *
     c                   eval      w_spge = w_spge + c_sfil
     c                   eval      w_srrn = w_ssrn
      *
     c                   dou       w_srrn = w_spge
      *
     c                   select
     c                   when      w_seqe = 'B'
     c                   read      rp1pl2                                 15
     c                   other
     c                   read      rp1pl1                                 15
     c                   endsl
      *
     c                   if        %eof or
     c                             rp1frm <> w_firm
     c                   eval      *in15 = *on
     c                   leave
     c                   endif
      *
      * Ikke ta med prosjekt som er avsluttet
     c                   if        rp1sts = 'A'
     c                   goto      dd100
     c                   endif
      *
     c                   eval      w_srrn = w_srrn + 1
      *
     c                   eval      b1valg = 0
     c                   eval      b1pros = rp1pro
      *Finn kundenavn
     c                   eval      rkunl1_kund = rp1knr
     c     rkunl1_key    chain     rkunl1                             61
     c                   if        *in61 = '0'
     c                   eval      b1navn = rknavn
     c                   else
     c                   move      *blank        b1navn
     c                   endif
      *
     c                   eval      b1txt1 = rp1txt
     c                   eval      b1uprs = rp1rup
      *
     c                   write     b1sfl
      *
     c     dd100         tag
      *
     c                   enddo
      *
     c                   if        w_srrn > w_ssrn
     c                   eval      w_sfrn = w_ssrn + 1
     c                   endif
      *
     c                   eval      w_ssrn = w_srrn
     c                   eval      w_virn = w_sfrn
      *
     c                   endsr
      *
