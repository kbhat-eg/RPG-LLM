      * Subrutine for å bla tilbake en side i subfilen
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     bck_subfile   begsr
      *
     c                   select
     c                   when      w_seqe = 'B'
     c     rp1pl2_key    setll     rp1pl2
     c                   other
     c     rp1pl1_key    setll     rp1pl1
     c                   endsl
      *
      * Leser tilbake en side + posisjoneringsrad
      *
     c                   eval      b_eof  = *off
     c                   eval      w_stel = 0
      *
     c                   dou       w_stel = c_sfil + 1
      *
     c                   select
     c                   when      w_seqe = 'B'
     c                   readp     rp1pl2
     c                   other
     c                   readp     rp1pl1
     c                   endsl
      *
     c                   if        %eof or
     c                             rp1frm <> w_firm
     c                   eval      b_eof = *on
     c                   leave
     c                   endif
      *
      *
      * Ikke ta med prosjekt som er avsluttet
     c                   if        rp1sts = 'A'
     c                   goto      dd200
     c                   endif
      *
     c                   eval      w_stel = w_stel + 1
      *
     c                   if        w_stel <= c_sfil
