     c                   parm                    c1ltxt
     c                   goto      c1taga
     c                   endif
      *
     c                   endif
      *
     c                   if        c1tlag = 0
     c                   goto      end_c1bld
     c                   endif
      *
      * Validering
      * Sender opp informasjonsbilde dersom rad finnes fra før
      *
     c                   eval      vutsl1_tlag = c1tlag
     c                   eval      vutsl1_tvar = c1tvar
     c     vutsl1_key    chain     vutsl1
     c                   if        %found
     c                   exsr      xc2msg
     c                   goto      c1taga
     c                   endif
      *
      * Kaller vedlikeholdsprogram
