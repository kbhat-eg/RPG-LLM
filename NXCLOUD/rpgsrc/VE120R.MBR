     c                   endif
      *
     c                   time                    vuteda
     c                   time                    vuteti
     c                   eval      vuteus = l_user
      *
     c                   if        %found
     c                   update    vutslur
     c                   else
     c                   eval      vutfir = w_firm
     c                   eval      vutlag = c1tlag
     c                   eval      vutvar = c1tvar
     c                   time                    vutoda
     c                   time                    vutoti
     c                   eval      vutous = l_user
     c                   write     vutslur
     c                   eval      b_forn = *on
     c                   endif
      *
      * Oppdater subfile
      *
     c                   if        b1valg = 2
     c                   eval      b1tlag = vutlag
     c                   eval      b1tvar = vutvar
     c                   endif
      *
     c                   if        b_oppr = *on or
