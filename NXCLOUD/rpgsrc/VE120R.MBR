     c                   eval      w_srrn = w_ssrn
      *
     c                   dou       w_srrn = w_spge
      *
     c                   read      vutslr
      *
     c                   if        %eof or
     c                             vutfir <> w_firm
     c                   eval      *in15 = *on
     c                   leave
     c                   endif
      *
     c                   eval      w_srrn = w_srrn + 1
      *
     c                   eval      vutslr_tlag = vutlag
     c                   eval      vutslr_tvar = vutvar
     c                   eval      vvarl1_vare = vutvar
     c     vvarl1_key    chain     vvarl1
     c                   if        not %found
     c                   eval      vvtek1 = *blanks
     c                   endif
      *
     c                   eval      b1valg = 0
     c                   eval      b1tlag = vutlag
     c                   eval      b1tvar = vutvar
     c                   eval      b1tek1 = vvtek1
      *
     c                   write     b1sfl
      *
     c                   enddo
      *
     c                   if        w_srrn > w_ssrn
     c                   eval      w_sfrn = w_ssrn + 1
     c                   endif
      *
     c                   eval      w_ssrn = w_srrn
     c                   eval      w_virn = w_sfrn
      *
     c                   endsr
