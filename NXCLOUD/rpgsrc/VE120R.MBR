      * finnes fra før.
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     xc2msg        begsr
      *
     c                   exfmt     c2msg
      *
      * Behandling av funksjonstaster
      *
     c                   if        *inkc or *inkl
     c                   leavesr
     c                   endif
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for å behandle bilde for ny/endring/vise (C2BLD)
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     xc2bld        begsr
      *
     c                   eval      w_arow = 0
     c                   eval      w_acol = 0
      *
     c                   eval      vutsl1_tlag = c1tlag
     c                   eval      vutsl1_tvar = c1tvar
     c     vutsl1_key    chain     vutsl1
      *
     c                   if        %found
     c                   eval      c2tenh = vutenh
     c                   eval      c2tgti = vutgti
     c                   eval      c2tpls = vutpls
     c                   if        vutoda <> *loval
     c     *dmy          move      vutoda        c2toda
     c                   else
     c                   eval      c2toda = 0
     c                   endif
     c                   if        vuteda <> *loval
     c     *dmy          move      vuteda        c2teda
     c                   else
     c                   eval      c2teda = 0
     c                   endif
     c                   eval      c2teus = vuteus
     c                   else
     c                   eval      c2tenh = *blank
     c                   eval      c2tpls = *blank
     c                   eval      c2tgti = *blank
     c                   eval      c2teda = 0
     c                   eval      c2toda = 0
     c                   endif
      *
     c                   if        b1valg = 5
     c                   eval      *in30 = *on
     c                   endif
      *
     c     c2taga        tag
      *
     c                   exfmt     c2bld
      *
     c                   if        *inkc or *inkl or *in30
     c                   goto      end_c2bld
     c                   endif
