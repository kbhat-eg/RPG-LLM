      *
     c                   goto      c1taga
      *
     c     end_c1bld     tag
      *
     c                   eval      b_oppr = *off
     c                   eval      b2tlag = 0
     c                   eval      b2tvar = *blanks
      *
     c                   if        b_forn = *on
     c                   exsr      forny
