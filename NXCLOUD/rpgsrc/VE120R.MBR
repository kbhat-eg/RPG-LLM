     d w_stat          s              1
     d w_disp          s              1
     d w_frad          s               d   datfmt(*iso)
     d w_tild          s               d   datfmt(*iso)
     d w_etxt          s                   like(vaetxt)
     d w_vare          s                   like(vevare)
