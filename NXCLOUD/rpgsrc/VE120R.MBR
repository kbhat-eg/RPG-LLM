     c                   parm                    w_firm
     c                   parm                    w_vare
     c                   parm                    c2tenh
     c                   parm                    w_etxt
     c                   goto      c2taga
      *
     c                   endif
      *
      * Validering av input
      *
      *
      * Enhet må fylles ut
      *
     c                   if        c2tenh = *blank
     c                   eval      *in31 = *on
     c                   goto      c2taga
     c                   endif
      *
      * Enhet må finnes
      *
     c                   eval      venhl1_enhe = c2tenh
     c     venhl1_key    chain     venhl1
     c                   if        not %found
     c                   eval      *in33 = *on
     c                   goto      c2taga
     c                   endif
      *
      * Enhet må finnes på varen
      *
     c                   eval      vvenl1_vare = c1tvar
     c                   eval      vvenl1_enhe = c2tenh
     c     vvenl1_key    chain     vvenl1
     c                   if        not %found
     c                   eval      *in33 = *on
     c                   goto      c2taga
     c                   endif
      *
      * Oppdatering
      *
     c                   clear                   vutslur
      *
     c                   eval      vutslu_tlag = c1tlag
     c                   eval      vutslu_tvar = c1tvar
     c     vutslu_key    chain     vutslur
      *
     c                   eval      vutenh = c2tenh
     c                   eval      vutpls = c2tpls
      *
      * Hent GTIN nummer basert på vare og enhet
      *
     c**                 eval      vutgti = c2tgti
     c                   eval      w_vare = c1tvar
     c                   eval      w_gtin = 0
     c                   eval      w_ldor = 0
     c                   eval      w_stat = *blanks
     c                   call      'VE713R'
     c                   parm                    w_firm
     c                   parm                    w_ldor
     c                   parm                    w_vare
     c                   parm                    c2tenh
     c                   parm                    w_gtin
     c                   parm                    w_stat
      *
     c                   if        w_gtin = 0
