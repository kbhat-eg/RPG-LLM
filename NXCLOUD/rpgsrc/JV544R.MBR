6.10  * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
6.10  *
6.10 c     sjekk_sts3    begsr
6.10  *
6.10  *
6.10 c                   eval      jvpglr_vare = vvvare
6.10 c     jvpglr_key    setll     jvpglr
6.10 c     jvpglr_key    reade     jvpglr
6.10 c                   dow       not %eof
6.10  *
6.10 c                   if        jwgenh = vvenhe
6.10  * finner om denne er salgsenhet 1
6.10 c                   eval      vvenl1_vare = vvvare
6.10 c     vvenl1_key    chain     vvenl1
6.10 c                   if        not %found(vvenl1)
6.10 c                   leavesr
6.10 c                   endif
6.10 c                   if        vesaen > 1
6.10 c                   leavesr
6.10 c                   endif
6.10  * sjekker om omregningsfaktor på gml.pakn. er lik ny pakning
6.10 c
6.10 c                   eval      jvpkl1_vare = vvvare
6.30 c                   eval      jvpkl1_ldor = vvnlev
6.10 c     jvpkl1_key    setll     jvpkl1
6.10 c     jvpkl1_key    reade     jvpkl1
6.10 c                   dow       not %eof(jvpkl1)
6.10 c                   if        jwenhe = jwgenh and
6.10 c                             jwofak=  jwgofa
6.10  * status settes til E
6.10 c                   eval      b_bytt = *on
6.10 c                   leavesr
6.10 c                   endif
6.10 c     jvpkl1_key    reade     jvpkl1
6.10 c                   enddo
6.10  *
6.10 c                   endif
6.10  *
6.10 c     jvpglr_key    reade     jvpglr
6.10 c                   enddo
