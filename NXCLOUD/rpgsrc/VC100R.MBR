      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     bck_subfile   begsr
      *
     c                   if        *in15
     c                   select
     c                   when      w_seqe = 'B'
     c     vshel2_key    setgt     vshel2
     c                   readp     vshel2
     c                   other
     c     vshel1_key    setgt     vshel1
     c                   readp     vshel1
     c                   endsl
     c                   endif
      *
      * Leser tilbake en side
      *
     c                   eval      w_stel = 0
      *
     c                   dou       w_stel = w_ssrn + c_sfil
      *
     c                   select
     c                   when      w_seqe = 'B'
     c                   readp     vshel2
     c                   other
     c                   readp     vshel1
     c                   endsl
      *
     c                   if        %eof or
     c                             vcfirm <> w_firm
     c                   leave
     c                   endif
      *
     c                   eval      w_stel = w_stel + 1
      *
     c                   select
     c                   when      w_seqe = 'B'
     c                   eval      vshel2_besk = vcbesk
     c                   other
     c                   eval      vshel1_vsor = vcvsor
     c                   endsl
      *
     c                   enddo
      *
     c                   if        %eof
     c                   select
     c                   when      w_seqe = 'B'
     c     vshel2_key    setll     vshel2
     c                   other
     c     vshel1_key    setll     vshel1
     c                   endsl
     c                   endif
      *
      * Blanker og fyller opp subfile
      *
     c                   exsr      clr_subfile
     c                   exsr      crt_subfile
      *
     c                   endsr
