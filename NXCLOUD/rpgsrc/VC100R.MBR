      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for å fylle opp subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     crt_subfile   begsr
      *
     c                   eval      w_spge = w_spge + c_sfil                     SFL END
     c                   eval      w_srrn = w_ssrn
      *
     c                   dou       w_srrn = w_spge
      *
     c                   select
     c                   when      w_seqe = 'B'
     c                   read      vshel2
     c                   other
     c                   read      vshel1
     c                   endsl
      *
     c                   if        %eof or                                      EOF
     c                             vcfirm <> w_firm
     c                   eval      *in15 = *on
     c                   leave
     c                   endif
      *
     c                   eval      w_srrn = w_srrn + 1
      *
     c                   select
     c                   when      w_seqe = 'B'
     c                   eval      vshel2_besk = vcbesk
     c                   other
     c                   eval      vshel1_vsor = vcvsor
     c                   endsl
      *
     c                   eval      b1valg = 0
     c                   eval      b1vsor = vcvsor
     c                   eval      b1lage = vclage
     c                   eval      b1besk = vcbesk
     c                   eval      b1type = vctype
      *
     c                   select
     c                   when      b1type = 'H'
     c                   eval      b1tbes = 'Handleliste Nettbutikk'
     c                   when      b1type = 'V'
     c                   eval      b1tbes = 'Vareliste'
     c                   other
     c                   eval      b1tbes = *blank
     c                   endsl
      *
     c                   write     b1sfl
      *
     c                   enddo
      *
     c                   if        w_srrn > w_ssrn
     c                   eval      w_sfrn = w_ssrn + 1
     c                   endif
      *
     c                   eval      w_ssrn = w_srrn
     c                   eval      w_virn = w_sfrn
      *
     c                   endsr
