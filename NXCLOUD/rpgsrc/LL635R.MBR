     c                   endif
      *
     c                   if        a1fdat > a1tdat
     c                   move      *on           *in34
     c                   goto      a1taga
     c                   endif
      *
     c                   if        a1syst <> ' '
     c                             and a1syst <> 'F'
     c                             and a1syst <> 'L'
     c                             and a1syst <> 'B'
