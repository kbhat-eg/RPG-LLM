     c                   exsr      sblagr
     c                   if        w_retk <> *blank
     c                   eval      *in42 = *on
     c                   goto      a1taga
     c                   endif
     c                   endif
      *
     c                   if        a1tlag < 99
     c                   eval      w_lage = a1tlag
     c                   exsr      sblagr
     c                   if        w_retk <> *blank
     c                   eval      *in43 = *on
     c                   goto      a1taga
     c                   endif
