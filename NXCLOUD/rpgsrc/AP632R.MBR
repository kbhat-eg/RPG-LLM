      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      *
     c                   eval      akobl1_bilk = p_bilk
     c     akobl1_key    chain     akobl1r
     c                   if        %found
     c                   eval      p_ok = '1'
     c                   eval      p_year = abyear
