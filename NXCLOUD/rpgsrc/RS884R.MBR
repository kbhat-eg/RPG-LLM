      *
     frp3al1    if   e           k disk    rename(rp3apfr:rp3al1r)
      *
     d txt             s             30    dim(2) ctdata perrcd(1)
      *
     d p_firm          s                   like(rp3frm)
     d p_pros          s                   like(rp3pro)
     d p_pupr          s                   like(rp3upr)
     d p_pakt          s                   like(rp3akt)
     d p_retk          s              1
     d p_txt1          s                   like(rp3txt)
     d p_txt2          s                   like(rp3tx2)
      *
     d w_firm          s                   like(rp3frm)
      *
     d rp3al1_pros     s                   like(rp3pro)
     d rp3al1_pupr     s                   like(rp3upr)
     d rp3al1_pakt     s                   like(rp3akt)
      *
     d                uds
     d l_user                911    920
     d l_firm                944    946  0
     d l_fnav                951    980
      *
      * Parameterliste
      *
     c     *entry        plist
     c                   parm                    p_retk
     c                   parm                    p_pros
     c                   parm                    p_pupr
     c                   parm                    p_pakt
     c                   parm                    p_txt1
     c                   parm                    p_txt2
      *
      * Legge inn firmanummer
      *
     c                   eval      w_firm = l_firm
     c                   eval      p_retk = *blank
     c                   eval      p_txt1 = *blank
     c                   eval      p_txt2 = *blank
      *
      * Sjekke aktivitet
      *
     c                   eval      rp3al1_pros = p_pros
     c                   eval      rp3al1_pupr = p_pupr
     c                   eval      rp3al1_pakt = p_pakt
     c     rp3al1_key    chain     rp3al1r                            60
      *
      *
     c                   if        %found(rp3al1)
     c                   move      rp3txt        p_txt1
     c                   move      rp3tx2        p_txt2
     c                   else
     c                   eval      p_retk = 'N'
     c                   endif
      /space 3
