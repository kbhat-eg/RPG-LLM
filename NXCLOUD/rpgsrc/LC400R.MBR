     c     *inzsr        begsr
      *
      * Key for les av TELLING
      *
     c     ltell1_key    klist
     c                   kfld                    w_firm
     c                   kfld                    w_batc
      *
      * Key for oppdatering av TELLING
      *
     c     ltellu_key    klist
     c                   kfld                    w_firm
     c                   kfld                    w_batc
     c                   kfld                    ltellu_numm
     c                   kfld                    ltellu_suff
     c                   kfld                    ltellu_line
      *
      *  Key for les av BUNT-HODE
      *
     c     lbhel1_key    klist
     c                   kfld                    w_firm
     c                   kfld                    w_batc
      *
      *  Key for oppdatering av BUNT-HODE
      *
     c     lbhelu_key    klist
     c                   kfld                    w_firm
     c                   kfld                    w_batc
     c                   kfld                    lbhelu_numm
      *
      *  Key for les av BUNT-LINJE
      *
     c     lbdtl1_key    klist
     c                   kfld                    w_firm
     c                   kfld                    w_batc
      *
      *  Key for oppdatering av BUNT-LINJE
      *
     c     lbdtlu_key    klist
     c                   kfld                    w_firm
     c                   kfld                    w_batc
     c                   kfld                    lbdtlu_numm
     c                   kfld                    lbdtlu_line
      *
      * Parametre
      *
     c     *entry        plist
     c                   parm                    p_firm
     c                   parm                    p_batc
      *
     c                   eval      w_firm = p_firm
     c                   eval      w_batc = p_batc
      *
     c                   endsr
