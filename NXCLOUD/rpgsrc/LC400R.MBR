      *  HOVEDPROGRAM
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
      *  Sletter rader i TELLING for denne BATCH
      *
     c     ltell1_key    setll     ltell1
     c     ltell1_key    reade     ltell1                                 90
      *
     c                   dow       *in90 = *off
      *
     c                   eval      ltellu_batc = lebatc
     c                   eval      ltellu_numm = lenumm
     c                   eval      ltellu_suff = lesuff
     c                   eval      ltellu_line = leline
     c     ltellu_key    chain     ltellur                            91
     c                   if        *in91 = *off
     c                   delete    ltellur
     c                   endif
      *
     c     ltell1_key    reade     ltell1                                 90
     c                   enddo
      *
      *  Sletter rader i BUNT-HODE for denne BATCH
      *
     c                   eval      *in90 = *off
     c     lbhel1_key    setll     lbhel1
     c     lbhel1_key    reade     lbhel1                                 90
      *
     c                   dow       *in90 = *off
      *
     c                   eval      lbhelu_batc = lubatc
     c                   eval      lbhelu_numm = lunumm
     c     lbhelu_key    chain     lbhelur                            91
     c                   if        *in91 = *off
     c                   delete    lbhelur
     c                   endif
      *
     c     lbhel1_key    reade     lbhel1                                 90
     c                   enddo
      *
      *  Sletter rader i BUNT-LINJE for denne BATCH
      *
     c                   eval      *in90 = *off
     c     lbdtl1_key    setll     lbdtl1
     c     lbdtl1_key    reade     lbdtl1                                 90
      *
     c                   dow       *in90 = *off
      *
     c                   eval      lbdtlu_batc = lvbatc
     c                   eval      lbdtlu_numm = lvnumm
     c                   eval      lbdtlu_line = lvline
     c     lbdtlu_key    chain     lbdtlur                            91
     c                   if        *in91 = *off
     c                   delete    lbdtlur
     c                   endif
      *
     c     lbdtl1_key    reade     lbdtl1                                 90
     c                   enddo
