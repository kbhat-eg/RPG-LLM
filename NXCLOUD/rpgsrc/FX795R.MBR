     c                   endif
      *
      * Oppdatering av loggfil
      *
     c                   if        a1spop = 'O'
     c                   eval      d_firm = l_firm
     c                   eval      d_user = l_user
     c                   eval      d_wsid = l_wsid
     c                   eval      d_aarr = a1aarr
     c                   eval      d_numm = a1numm
     c                   eval      d_suff = a1suff
     c                   eval      d_kode = a1kode
     c                   time                    d_date
     c                   time                    d_time
      *
      * Kaller program for oppdatering
      *
     c                   call      'FU900R'
     c                   parm                    w_stat
     c                   parm                    d_urec
     c                   endif
      *
      * Kaller program for spørring
      *
     c                   if        a1spop = ' '
     c                   call      'FU500R'
     c                   parm                    w_firm
     c                   parm                    a1numm
     c                   parm                    a1suff
     c                   endif
      *
     c                   eval      a1numm = 0
     c                   eval      a1suff = 0
     c                   eval      a1kode = ' '
     c                   eval      a1spop = ' '
     c                   goto      a1tag1
