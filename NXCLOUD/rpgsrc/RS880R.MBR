      * Parameterliste
      *
     c     *entry        plist
     c                   parm                    p_retk
     c                   parm                    p_pros
     c                   parm                    p_pupr
     c                   parm                    p_txt1
     c                   parm                    p_txt2
      *
      * Legge inn firmanummer
      *
     c                   eval      w_firm = l_firm
     c                   eval      p_retk = *blank
     c                   eval      p_txt1 = *blank
     c                   eval      p_txt2 = *blank
      *
      * Sjekke prosjekt
      *
     c                   eval      rp1pl1_pros = p_pros
     c     rp1pl1_key    chain     rp1pl1r                            60
      *
      * Sjekk om underprosjekt finnes
      *
     c                   if        p_pupr <> *blank
     c                   eval      rp2ul1_pros = p_pros
     c                   eval      rp2ul1_pupr = p_pupr
     c     rp2ul1_key    chain     rp2ul1r                            61
     c                   endif
      *
      *
     c                   if        *in60 = *on
     c                   move      'N'           p_retk
     c                   move      txt(1)        p_txt1
     c                   goto      slutt
     c                   endif
      *
      *
     c                   if        *in61 = *on
     c                   move      'X'           p_retk
     c                   move      txt(3)        p_txt1
     c                   goto      slutt
     c                   endif
      *
      * Er prosjekt sperret?
      *
     c**                 if        rp1sts <> *loval
