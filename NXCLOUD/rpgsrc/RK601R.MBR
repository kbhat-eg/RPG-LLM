      *
      * Les kunderegister
      *
     c     rkunl2_key    setll     rkunl2                                 90                 .
     c     rkunl2_key    reade     rkunl2                                 90
      *
     c                   dow       *in90 = *off
      *
     c                   if        rkalfa = w_alfa
     c                   exsr      xsrkont
      *
     c                   if        w_ikke = ' '
     c                   eval      ant = ant + 1
     c                   else
     c                   eval      ant = 1
     c                   endif
      *
     c                   else
      *
     c                   eval      ant = 1
     c                   endif
      *
     c                   select
     c                   when      ant = 1
     c                   eval      w_kund = rkkund
     c                   eval      w_navn = rknavn
     c                   eval      w_gate = rkgate
     c                   eval      w_ponr = rkponr
     c                   eval      w_tlfn = rktlfn
     c                   eval      w_mobn = rkmobn
      *
     c                   when      ant = 2
     c                   eval      d1kund = w_kund
     c                   exsr      utskrift
     c                   eval      d1kund = rkkund
     c                   exsr      utskrift
      *
     c                   when      ant > 2
     c                   eval      d1kund = rkkund
     c                   exsr      utskrift
     c                   endsl
      *
     c                   eval      w_alfa = rkalfa
      *
     c     rkunl2_key    reade     rkunl2                                 90
     c                   enddo
      *
     c     slutt         tag
      *
     c                   eval      *inlr = *on
      *
      /eject
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for utskrift
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     utskrift      begsr
      *
      * Sjekker om overflow
      *
     c                   if        *in30 = *on                                               .
     c                   write     a1hdr                                                     .
     c                   eval      *in30 = *off                                              .
     c                   endif                                                           .....
      *
     c                   eval      rkunl1_kund = d1kund
     c     rkunl1_key    chain     rkunl1                             90
      *
     c                   eval      d1navn = xknavn
     c                   eval      d1gate = xkgate
     c                   eval      d1sted = xksted
     c                   eval      d1tlfn = xktlfn
     c                   eval      d1mobn = xkmobn
      *
     c                   write     d1det
      *
     c                   endsr
      /eject
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for kontroll
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     xsrkont       begsr
      *
     c                   eval      w_ikke = ' '
      *
      * Sjekk navn
      *
     c                   if        l_navn = 'J' and
     c                             rknavn <> w_navn
     c                   eval      w_ikke = 'J'
     c                   endif
      *
      * Sjekk gateadresse
      *
     c                   if        l_gate = 'J' and
     c                             rkgate <> w_gate
     c                   eval      w_ikke = 'J'
     c                   endif
      *
      * Sjekk postnummer
      *
     c                   if        l_ponr = 'J' and
     c                             rkponr <> w_ponr
     c                   eval      w_ikke = 'J'
     c                   endif
      *
      * Sjekk telefonnummer
      *
     c                   if        l_tlfn = 'J' and
     c                             rktlfn <> w_tlfn
     c                   eval      w_ikke = 'J'
     c                   endif
      *
      * Sjekk mobilnummer
      *
     c                   if        l_mobn = 'J' and
     c                             rkmobn <> w_mobn
     c                   eval      w_ikke = 'J'
     c                   endif
      *
     c                   endsr
      /eject
