     c                   eval      b1valg = 0
     c                   update    b1sfl
     c                   iter
     c                   endif
      *
      * Slett
      *
     c                   if        b1valg = 4
     c                   eval      d1otyp = b1otyp
     c                   eval      d1kkat = b1kkat
7.02 c                   eval      d1ftyp = b1ftyp
     c                   eval      d1line = b1line
     c                   exsr      xd1win
     c                   eval      b1valg = 0
     c                   update    b1sfl
     c                   iter
     c                   endif
      *
      * Vis
      *
     c                   if        b1valg = 5
     c                   eval      c1otyp = b1otyp
     c                   eval      c1kkat = b1kkat
7.02 c                   eval      c1ftyp = b1ftyp
     c                   eval      c1line = b1line
     c                   exsr      xc2bld
     c                   eval      b1valg = 0
     c                   update    b1sfl
     c                   iter
     c                   endif
      *
     c                   enddo
      *
     c                   if        b_forn = *on
     c                   exsr      forny
     c                   endif
      *
     c     end_subfile   endsr
