     fafprlr    if   e           k disk    rename(afprpfr:afprlrr)
      *
     d                uds
     d l_firm                944    946  0
     d l_avde                947    950  0
      *
      * Definering av parametre
      *
     d p_filg          s                   like(afsfil)
     d p_vprt          s                   like(afsprt)
     d p_ok            s              1
      *
      * Definering av variabler i n√∏kler
      *
     d afprlr_sfil     s                   like(afsfil)
     d afprlr_sprt     s                   like(afsprt)
      *
      * Definering av variable
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      * Hent inn post fra formularregister                              *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      *
     c                   eval      afprlr_sfil = p_filg
     c                   eval      afprlr_sprt = p_vprt
     c     afprlr_key    chain     afprlrr
     c                   if        %found
     c                   eval      p_ok = '1'
     c                   else
     c                   eval      p_ok = '0'
     c                   endif
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      * Avslutt program                                                 *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      *
     c                   eval      *inlr = *on
     c                   return
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      * Subrutine for initiering av program                             *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      *
     c     *inzsr        begsr
      *
      * Parameterliste
      *
     c     *entry        plist
     c                   parm                    p_filg
     c                   parm                    p_vprt
     c                   parm                    p_ok
      *
      * Key for rutineregister
      *
     c     afprlr_key    klist
     c                   kfld                    afprlr_sfil
     c                   kfld                    afprlr_sprt
      *
     c                   endsr
