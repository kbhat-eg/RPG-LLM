      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      * Definering av felter                                            *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      * A2BLD Behandle bilde for valg av utskriftskriteria              *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      *
     c     a2taga        tag
      *
      * Bare data
      *
     c                   exfmt     a2bld
      *
     c                   eval      *in31 = *off
     c                   eval      *in32 = *off
     c                   eval      *in33 = *off
      *
      * Gå ut
      *
     c                   if        *inkc = *on                                  .....
     c                   move      '1'           p_in03                             .
     c                   goto      xslutt                                           .
     c                   endif                                                  .....
      *
      * Spørring
      *
     c                   if        *inkd = *on
     c                   exsr      spr_rut
     c                   goto      a2taga
     c                   endif
      *
      *  Kontroll kundekategori
      *
     c                   if        a2katf > a2katt                                       .....
     c                   move      *on           *in34                          Pos.cur.     .
     c                   goto      a2taga                                                    .
     c                   endif                                                           .....
      *
      * Skriv parameter til local
      *
     c                   eval      l_katf = a2katf
     c                   eval      l_katt = a2katt
     c                   eval      l_kadr = a2kadr
     c                   eval      l_kstd = a2kstd
     c                   eval      l_kkat = a2kkat
     c                   eval      l_korg = a2korg
     c                   eval      l_kpnr = a2kpnr
     c                   eval      l_kkpe = a2kkpe
     c                   eval      l_kema = a2kema
     c                   eval      l_ktlf = a2ktlf
     c                   eval      l_kfax = a2kfax
      *
      /eject
