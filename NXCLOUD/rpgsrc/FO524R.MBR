      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     ffo524d    cf   e             workstn
      *
      * Definering av parametre
      *
     d p_kopr          s              9  2
      *
      * Definering av variable
      *
     d w_kopr          s              9  2
     d w_rab1          s             11  4
     d w_rab2          s             11  4
     d w_frak          s             11  4
     d w_pasl          s             11  4
      *
     d w_kopr_gml      s              9  2
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Hovedprogram
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
      * Viser bildet
      *
     c     k1tagb        tag
      *
     c                   exfmt     k1bld
      *
      * F3=Avslutt
      *
     c                   if        *inkc or *inkl
     c                   goto      avslutt
     c                   endif
      *
      * Tar vare p√• "gammel" kostpris for test for vising av bilde
      *
     c                   eval      w_kopr_gml = k1kopr
      *
      * Beregner ny kostpris
      *
     c                   eval      k1kopr = k1inpr
     c                   eval      w_rab1 = k1rab1 / 100
     c                   eval      w_rab2 = k1rab2 / 100
     c                   eval      w_frak = k1frak / 100
     c                   eval      w_pasl = k1pasl / 100
      *
     c                   eval      w_rab1 = w_rab1 * k1kopr
     c                   eval      w_kopr = k1kopr - w_rab1
     c                   eval      w_rab2 = w_rab2 * w_kopr
      *
     c                   eval      w_frak = w_frak * k1kopr
     c                   eval      w_pasl = w_pasl * k1kopr
      *
     c                   eval      k1kopr = k1kopr - w_rab1 - w_rab2 +
     c                                      w_frak + w_pasl
      *
     c                   if        k1kopr <> w_kopr_gml
     c                   goto      k1tagb
     c                   endif
      *
     c                   eval      p_kopr = k1kopr
      *
      * Avslutter program
      *
     c     avslutt       tag
      *
     c                   eval      *inlr = *on
     c                   return
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for initiering av program
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     *inzsr        begsr
      *
      * Parametre
      *
     c     *entry        plist
     c                   parm                    p_kopr
      *
      * Nullstiller parameter
      *
     c                   eval      p_kopr = 0
      *
     c                   endsr
