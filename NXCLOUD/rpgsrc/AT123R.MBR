      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     xd1win        begsr
      *
     c/exec sql
     c+ select bruker,menynavn
     c+ into  :w_amuusr,:w_amumnu
     c+ from amnust
     c+ where amuuid = :d1uuid
     c/end-exec
     c                   eval      d1uusr = w_amuusr
     c                   eval      d1umnu = w_amumnu
      *
     c                   exfmt     d1win
      *
     c                   if        *inkc or *inkl
     c                   goto      end_d1win
     c                   endif
      *
     c                   eval      b_forn = *on
      *
      * Sletter nokklel
      *
     c/exec sql
     c+ delete
     c+ from amnust
     c+ where amuuid = :d1uuid
     c/end-exec
      *
     c     end_d1win     endsr
