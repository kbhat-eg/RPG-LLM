      *
     c     xc2bld        begsr
      *
     c                   eval      w_arow = 0
     c                   eval      w_acol = 0
      *
     c                   if        b_oppr = *on  and b1valg <> 3
     c                   eval      c2uusr = c1uusr
     c                   eval      c2umnu = c1umnu
     c                   else
     c/exec sql
     c+ select bruker,menynavn
     c+ into  :w_amuusr,:w_amumnu
     c+ from amnust
     c+ where amuuid = :b1uuid
     c/end-exec
      *
     c                   if        b1valg = 3
     c                   eval      c2uusr = c1uusr
     c                   eval      c2umnu = c1umnu
     c                   else
     c                   eval      c2uusr = w_amuusr
     c                   eval      c2umnu = w_amumnu
     c                   endif
     c                   endif
