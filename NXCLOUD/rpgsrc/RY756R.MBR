      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     oppdater      begsr
      *
     c                   if        d_inpr = 0
     c                   leavesr
     c                   endif
      *
     c                   eval      w_lvar = %trim('0' + %subst(d_vare:8:6))
      *
     c                   eval      jvarl8_ldor = 009998
     c                   eval      jvarl8_lvar = w_lvar
     c     jvarl8_key    chain     jvarl8                             91
     c                   if        *in91 = *on
     c                   leavesr
     c                   endif
      *
      * Oppdaterer vare-pris
      *
     c                   exsr      oppd_pris
      *
      * Oppretter påslag på frigitte varer med salgspris
      *
     c                   if        d_sapr = 0
     c                   leavesr
     c                   endif
      *
     c                   eval      jvdtl1_dvar = jvvare
     c     jvdtl1_key    chain     jvdtl1                             91
     c                   if        *in91 = *on or
     c                             jvdhko = 1
     c                   leavesr
     c                   endif
      *
     c                   clear                   jpfalur
      *
     c                   eval      jcfirm = w_firm
     c                   eval      jcogrp = jvogrp
     c                   eval      jchgrp = jvhgrp
     c                   eval      jcugrp = jvugrp
     c                   eval      jcldor = jvldor
     c                   eval      jcmodn = jvmodn
     c                   eval      jcvare = jvvare
     c                   eval      jckofa = 1
     c                   eval(h)   jcsafa = d_sapr / d_inpr
      *
     c                   time                    jcodat
     c                   time                    jcedat
     c                   time                    jcetim
      *
     c                   write     jpfalur
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for oppdatering av vare-pris
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     oppd_pris     begsr
      *
     c                   eval      jvprlu_vare = jvvare
     c     jvprlu_key    chain     jvprlu                             91
      *
     c                   eval      jxvare = jvvare
     c                   eval      jxpris = d_inpr
     c                   eval      jxldor = jvldor
     c                   eval      jxlvar = jvlvar
     c                   eval      jxgdat = *loval
     c                   eval      jxrsts = 1
      *
     c                   if        d_pdat <> 0
     c     *ymd          move      d_pdat        jxgdat
     c                   endif
      *
     c                   if        *in91 = *off
     c                   update    jvprlur
     c                   else
     c                   write     jvprlur
     c                   endif
      *
     c                   endsr
