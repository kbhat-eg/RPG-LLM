     c                   move      *zeros        afsave
     c                   move      a2prty        afprty
     c                   move      a2inst        afinst
     c                   move      a2prfi        afprfi
     c                   move      a2bgir        afbgir
     c                   move      a2mvak        afmvak
     c                   move      a2kidd        afkidk
     c                   move      a2ocrk        afocrk
     c                   move      *zeros        afipds
     c                   move      *blank        afhex1
     c                   move      *blank        afhex2
     c                   move      a2prog        afprog
     c                   move      a2chgv        afchgv
     c                   move      *blank        aftvbt
     c                   move      *blank        af0rot
     c                   move      *zeros        afskin
     c                   move      *zeros        afskut
     c                   move      *blank        afpkva
     c                   move      *blank        afdplx
     c                   move      *zeros        afarki
     c                   move      *zeros        afetik
     c                   move      *zeros        afwork
     c                   move      *zeros        afpdfa
      *
     c                   write     aforlur
     c                   endif
      *
     c***                if        %found
     c***                move      a2txt1        aftxt1
     c***                move      a2acpi        afacpi
     c***                move      a2alin        afalin
