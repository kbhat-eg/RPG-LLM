     d aforlu_ruti     s                   like(afruti)
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      *  Oppretter standard rutiner                                     *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      *
     c                   move      *zero         i
     c     i             doueq     45
     c                   add       1             i
     c                   move      rut1(i)       a2rec1
     c                   move      rut2(i)       a2rec2
      *
     c                   move      a2ruti        aforlu_ruti
     c     aforlu_key    chain     aforlur                            90
      *
     c                   if        not%found
     c                   move      a2ruti        afruti
     c                   move      a2txt1        aftxt1
     c                   move      *blank        aftxt2
     c                   move      *blank        afmld1
     c                   move      *blank        afmld2
     c                   move      *blank        afmld3
     c                   move      a2form        afform
     c                   move      a2outq        afoutq
