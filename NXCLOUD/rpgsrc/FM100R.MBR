      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for display av subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     dsp_subfile   begsr
      *
     c                   if        w_srrn <> 0
     c                   eval      *in12 = *on
     c                   else
     c                   write     b2cmd
     c                   endif
      *
     c                   eval      *in13 = *on
     c                   exfmt     b2ctl
      *
     c                   eval      *in12 = *off
     c                   eval      *in13 = *off
      *
     c                   eval      w_fcrn = d_fcrn
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for behandling av hardkodet søk  (F1WIN)
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     xf1win1       begsr
      *
     c                   exsr      xf1nul
     c                   eval      f1alt1 = 'VISA Purchasing          '
     c                   eval      f1alt2 = 'Europay Purchasing       '
     c                   eval      f1alt3 = '                         '
     c                   eval      f1alt4 = '                         '
     c                   eval      f1alt5 = '                         '
     c                   eval      f1alt6 = '                         '
     c                   eval      f1alt7 = '                         '
     c                   eval      f1alt8 = '                         '
      *
     c                   exfmt     f1win
      *
     c                   if        *inkc or *inkl
     c                   goto      end_f1win1
     c                   endif
      *
     c                   select
     c                   when      f1val1 = '1'
     c                   eval      w_kftp = 01
     c                   eval      w_ktpn = f1alt1
     c                   when      f1val2 = '1'
     c                   eval      w_kftp = 02
     c                   eval      w_ktpn = f1alt2
     c                   when      f1val3 = '1'
     c                   eval      w_kftp = 03
     c                   eval      w_ktpn = f1alt3
     c                   when      f1val4 = '1'
     c                   eval      w_kftp = 04
     c                   eval      w_ktpn = f1alt4
     c                   when      f1val5 = '1'
     c                   eval      w_kftp = 05
     c                   eval      w_ktpn = f1alt5
     c                   when      f1val6 = '1'
     c                   eval      w_kftp = 06
     c                   eval      w_ktpn = f1alt6
     c                   when      f1val7 = '1'
     c                   eval      w_kftp = 07
     c                   eval      w_ktpn = f1alt7
     c                   when      f1val8 = '1'
     c                   eval      w_kftp = 08
     c                   eval      w_ktpn = f1alt8
     c                   endsl
      *
     c     end_f1win1    endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for å blanke ut hardkodet spørrebilde
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     xf1nul        begsr
      *
     c                   eval      f1alt1 = *blank
     c                   eval      f1alt2 = *blank
     c                   eval      f1alt3 = *blank
     c                   eval      f1alt4 = *blank
     c                   eval      f1alt5 = *blank
     c                   eval      f1alt6 = *blank
     c                   eval      f1alt7 = *blank
     c                   eval      f1alt8 = *blank
     c                   eval      f1val1 = *blank
     c                   eval      f1val2 = *blank
     c                   eval      f1val3 = *blank
     c                   eval      f1val4 = *blank
     c                   eval      f1val5 = *blank
     c                   eval      f1val6 = *blank
     c                   eval      f1val7 = *blank
     c                   eval      f1val8 = *blank
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for å sette kort/avtaletype i klartekst (kort)
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     avt_tp_sh     begsr
      *
     c                   if        fmkftp = 1
     c                   eval      b1type = 'VISA Purchasing'
     c                   else
     c                   if        fmkftp = 2
     c                   eval      b1type = 'Europay Purchasing'
     c                   else
     c                   eval      b1type = 'Udefinert....'
     c                   endif
     c                   endif
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for å sette kort/avtaletype i klartekst (full)
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     avt_tp_fu     begsr
      *
     c                   if        fmkftp = 1
     c                   eval      c2ktpn = 'VISA Purchasing'
     c                   else
     c                   if        fmkftp = 2
     c                   eval      c2ktpn = 'Europay Purchasing'
     c                   else
     c                   eval      c2ktpn = 'Ukjent'
     c                   endif
     c                   endif
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for å finne høyeste sekv.nr. på kunde/prosjekt/korttype
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     finn_ktsq     begsr
      *
     c                   eval      fmkal1_kund = c2kund
     c                   eval      fmkal1_kpro = c2kpro
     c                   eval      fmkal1_kftp = c2kftp
      *
     c     fmkal1_key2   setll     fmkal1
     c     fmkal1_key2   reade     fmkal1r
      *
     c                   dow       not %eof
     c     fmkal1_key2   reade     fmkal1r
     c                   enddo
      *
     c                   eval      c2ktsq = fmktsq + 1
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for behandling av slette-bildet (D1WIN)
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     xd1win        begsr
      *
     c                   eval      fmkalr_kund = d1kund
     c                   eval      fmkalr_kpro = d1kpro
     c                   eval      fmkalr_kftp = d1kftp
     c                   eval      fmkalr_ktsq = d1ktsq
     c     fmkalr_key    chain     fmkalr
      *
     c                   eval      rkunl1_kund = d1kund
     c     rkunl1_key    chain     rkunl1
     c                   if        %found(rkunl1)
     c                   eval      d1knav = rknavn
     c                   else
     c                   eval      d1knav = *blank
     c                   endif
      *
     c                   if        d1kpro <> *zero
     c                   eval      fkprl1_kund = d1kund
     c                   eval      fkprl1_kpro = d1kpro
     c     fkprl1_key    chain     fkprl1
     c                   if        %found(fkprl1)
     c                   eval      d1kpnv = flnavn
     c                   else
     c                   eval      d1kpnv = *blank
     c                   endif
     c                   endif
      *
     c                   eval      d1type = b1type
     c                   eval      d1hldr = fmhldr
     c                   eval      d1krnr = fmkrnr
      *
     c                   exfmt     d1win
      *
     c                   if        *inkc or *inkl
     c                   goto      end_d1win
     c                   endif
      *
      * Sletter rad
      *
     c                   eval      fmkalu_kund = d1kund
     c                   eval      fmkalu_kpro = d1kpro
     c                   eval      fmkalu_kftp = d1kftp
     c                   eval      fmkalu_ktsq = d1ktsq
     c     fmkalu_key    chain     fmkalur
      *
     c                   delete    fmkalur
      *
     c                   eval      b_forn = *on
      *
     c     end_d1win     endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for initiering av program
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     *inzsr        begsr
      *
      * Parametre
      *
     c     *entry        plist
     c                   parm                    p_firm
     c                   parm                    p_kund
      *
      * Key for oppslag på kort/fakt.avtale-register
      *
     c     fmkalr_key    klist
     c                   kfld                    w_firm
     c                   kfld                    fmkalr_kund
     c                   kfld                    fmkalr_kpro
     c                   kfld                    fmkalr_kftp
     c                   kfld                    fmkalr_ktsq
      *
      * Key for posisjonering på kunde
      *
     c     fmkal1_key    klist
     c                   kfld                    w_firm
     c                   kfld                    fmkal1_kund
      *
      * Key for les siste nr på kunde/prosjekt/korttype
      *
     c     fmkal1_key2   klist
     c                   kfld                    w_firm
     c                   kfld                    fmkal1_kund
     c                   kfld                    fmkal1_kpro
     c                   kfld                    fmkal1_kftp
      *
      * Key for oppdat. av kort/fakt.avtale-register
      *
     c     fmkalu_key    klist
     c                   kfld                    w_firm
     c                   kfld                    fmkalu_kund
     c                   kfld                    fmkalu_kpro
     c                   kfld                    fmkalu_kftp
     c                   kfld                    fmkalu_ktsq
      *
      * Key for oppslag på kunde
      *
     c     rkunl1_key    klist
     c                   kfld                    w_firm
     c                   kfld                    rkunl1_kund
      *
      * Key for oppslag på kundeprosjekt
      *
     c     fkprl1_key    klist
     c                   kfld                    w_firm
     c                   kfld                    fkprl1_kund
     c                   kfld                    fkprl1_kpro
      *
      *
      * Henter parameter, og initierer skjermbildet
      *
     c                   time                    w_ddat
     c                   eval      w_firm = p_firm
     c                   eval      w_kund = p_kund
      *
     c                   eval      *in22 = *on
      *
      * Sjekker om tilleggsmodul er installert
      *
     c                   eval      w_modu = c_modu
     c                   eval      w_disp = '1'
     c                   eval      w_stat = '0'
     c                   eval      b_tlgx = *on
      *
     c                   call      'AX700R'
     c                   parm                    w_modu
     c                   parm                    w_disp
     c                   parm                    w_stat
      *
     c                   if        w_stat = '1'
     c                   eval      b_tlgx = *off
     c                   endif
      *
      * Posisjonerer databasen, og fyller opp subfile
      *
     c                   eval      fmkal1_kund = w_kund
     c     fmkal1_key    setll     fmkal1
      *
     c                   exsr      crt_subfile
      *
     c                   endsr
