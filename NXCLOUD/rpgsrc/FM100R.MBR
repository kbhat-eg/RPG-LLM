      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     crt_subfile   begsr
      *
     c                   eval      w_spge = w_spge + c_sfil
     c                   eval      w_srrn = w_ssrn
      *
      * Fyller opp subfile
      *
     c                   dou       w_srrn = w_spge
      *
     c                   read      fmkal1
      *
     c                   if        %eof
     c                   eval      *in15 = *on
     c                   leave
     c                   endif
      *
     c                   if        fmfirm <> w_firm or
     c                             fmkund <> w_kund
     c                   eval      *in15 = *on
     c                   leave
     c                   endif
      *
      * Skriver til subfile
      *
     c                   eval      w_srrn = w_srrn + 1
      *
     c                   eval      fmkal1_kund = fmkund
      *
     c                   eval      b1valg = 0
     c                   eval      b1kund = fmkund
     c                   eval      b1kpro = fmkpro
     c                   eval      b1kftp = fmkftp
     c                   eval      b1ktsq = fmktsq
      *
     c                   exsr      avt_tp_sh
      *
     c                   if        fmkrnr <> 0
     c                   move      fmkrnr        w_kort
     c                   eval      b1kort = '*' +
     c                             %subst(w_kort:11:6)
     c                   else
     c                   eval      b1kort = *blank
     c                   endif
      *
     c                   movel     fmhldr        b1navn
     c                   move      fmoblg        b1obli
      *
     c                   write     b1sfl
      *
     c                   enddo
      *
     c                   if        w_srrn > w_ssrn
     c                   eval      w_sfrn = w_ssrn + 1
     c                   endif
      *
     c                   eval      w_ssrn = w_srrn
     c                   eval      w_virn = w_sfrn
      *
     c                   endsr
