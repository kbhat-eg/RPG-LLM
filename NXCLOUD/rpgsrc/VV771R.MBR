8.03 d  hlrefr                       20    overlay(hlrec:60)
8.03 d  hlsyst                        1    overlay(hlrec:80)
8.03 d  hlaltk                        2    overlay(hlrec:81)
8.03 d  hlrest                        1    overlay(hlrec:83)
8.03 d  hltype                        1    overlay(hlrec:84)
