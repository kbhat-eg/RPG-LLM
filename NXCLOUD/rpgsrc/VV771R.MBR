     c                   dow       not %eof
      *
8.01 c*                  if        vlenhl = ' '
8.01 c*                  goto      les_neste
8.01 c*                  endif
      *
     c                   eval      w_vtyp_gml = vlvtyp
      *
      * Sjekk lager på varen
      *
     c                   if        vlbehl <> 0 or
     c                             vloord <> 0 or
     c                             vlbbes <> 0
     c                   goto      les_neste
     c                   endif
      *
      * Hent vareinfo
      *
     c                   eval      vvarlr_vare = vlvare
     c     vvarlr_key    chain     vvarlr
     c                   if        not %found(vvarlr)
     c                   goto      les_neste
     c                   endif
      *
      * Sjekker om vare finnes i tellebunt på denne batchen
      *
         vlvtyp = 'S';
         vvvtyp = 'S';
         w_anta = 0;

         exec sql
         select count(*)
            into :w_anta
          from  lbdtpf
            where lvfirm = :w_firm and
                  lvbatc = :p_batc and
                  lvlage = :p_lage and
                  lvvare = :vlvare;
         if w_anta > 0;
            vlvtyp = 'L';
            vvvtyp = 'L';
         endif;

