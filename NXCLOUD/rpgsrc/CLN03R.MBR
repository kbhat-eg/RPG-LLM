     c                   eval      c2nntx = w_nntx
      *
     c                   eval      w_nllv = c2nllv
     c                   exsr      finn_lev
     c                   eval      c2nltx = w_lnav
      *
     c                   if        b1valg = 5
     c                   eval      *in30 = *on
     c                   endif
      *
