     c                   eval      w_acol = 0
      *
     c                   eval      clnlir_nlka = c2nlka
     c                   eval      clnlir_nllv = c2nllv
     c     clnlir_key    chain     clnlir
      *
     c                   if        %found
     c     *dmy          move      clnlod        c2eoda
     c     *dmy          move      clnled        c2eeda
     c                   eval      c2eeus = clnleu
6.34 c                   endif
      *
     c                   eval      w_nlka = c2nlka
