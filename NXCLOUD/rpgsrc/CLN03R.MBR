      *
     c                   eval      clnliu_nlka = c2nlka
     c                   eval      clnliu_nllv = c2nllv
     c     clnliu_key    chain     clnliur
      *
     c                   time                    clnled
     c                   time                    clnlet
     c                   eval      clnleu = l_user
      *
     c                   if        %found
     c                   update    clnliur
     c                   else
     c                   eval      clnlfi = w_firm
     c                   eval      clnlka = c2nlka
     c                   eval      clnllv = c2nllv
     c                   time                    clnlod
6.10 c                   time                    clnlet
6.10 c                   eval      clnleu = l_user
     c                   write     clnliur
     c                   endif
      *
      * Oppdater subfile
      *
     c                   if        b1valg = 2
     c                   eval      b1nlka = clnlka
     c*                  eval      b1nntx = clnltx
     c                   endif
      *
     c                   if        b_oppr = *on or
