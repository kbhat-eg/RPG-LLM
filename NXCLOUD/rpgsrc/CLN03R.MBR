      *
     c     forny         begsr
      *
     c                   if        w_fcrn <> 0
     c     w_fcrn        chain     b1sfl
     c                   endif
      *
     c                   eval      clnli1_nlka = b1nlka
     c     clnli1_key    setll     clnli1
      *
     c                   exsr      clr_subfile
     c                   exsr      crt_subfile
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for posisjonering i subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     posisjoner    begsr
      *
      * Nobbkategori
      *
     c                   if        b2nlka <> 0
     c                   eval      clnli1_nlka = b2nlka
     c     clnli1_key    setll     clnli1
     c                   endif
      *
     c     end_pos       tag
      *
      * Blanker subfile, og fyller opp i.h.t. posisjonering
      *
     c                   eval      *in22 = *off
      *
     c                   exsr      clr_subfile
     c                   exsr      crt_subfile
      *
      * Blanker posisjoneringsfelter
      *
     c                   eval      b2nlka = 0
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for behandling av subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     subfile       begsr
      *
     c                   eval      b_forn = *off
      *
     c                   if        *in22 = *off
     c                   eval      *in22 = *on
     c                   else
     c                   eval      *in22 = *off
     c                   endif
      *
     c                   eval      w_virn = w_fcrn
      *
      * Leser og behandler subfile
      *
     c                   do        w_ssrn
     c                   readc     b1sfl
      *
     c                   if        %eof
     c                   leave
     c                   endif
      *
     c                   eval      *in22 = *off
     c                   eval      w_virn = w_srrn
      *
      * Endre
      *
     c                   if        b1valg = 2
     c                   eval      c2nlka = b1nlka
     c                   eval      c2nllv = b1nllv
     c                   exsr      xc2bld
     c                   eval      b1valg = 0
     c                   update    b1sfl
     c                   iter
     c                   endif
      *
      * Slett
      *
     c                   if        b1valg = 4
     c                   eval      d1nlka = b1nlka
     c                   eval      d1nllv = b1nllv
     c                   exsr      xd1win
     c                   eval      b1valg = 0
     c                   update    b1sfl
     c                   iter
     c                   endif
      *
      * Vis
      *
     c                   if        b1valg = 5
     c                   eval      c2nlka = b1nlka
     c                   eval      c2nllv = b1nllv
     c                   exsr      xc2bld
     c                   eval      b1valg = 0
     c                   update    b1sfl
     c                   iter
     c                   endif
      *
     c                   enddo
      *
     c                   if        b_forn = *on
     c                   exsr      forny
     c                   endif
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for behandling av skjermbilde for ny rad (C1BLD)
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     xc1bld        begsr
      *
     c                   eval      b_oppr = *on
     c                   eval      b_forn = *off
      *
     c                   eval      c1nlka = 0
     c                   eval      c1nntx = *blank
     c                   eval      c1nllv = 0
     c                   eval      c1nltx = *blank
      *
     c     c1taga        tag
      *
     c                   exfmt     c1bld
      *
      * Behandling av funksjonstaster
      *
     c                   select
     c                   when      *inkc or *inkl
     c                   goto      end_c1bld
     c                   endsl
      *
      * Søking
      *
     c                   if        *inkd = *on
     c                   if        w_afld = 'C1NLKA'
     c                   exsr      sok_kat
     c                   eval      c1nlka = w_nlka
     c                   eval      c1nntx = w_nktx
     c                   goto      c1taga
     c                   endif
      *
     c                   if        w_afld = 'C1NLLV'
     c                   exsr      sok_lev
     c                   goto      c1taga
     c                   endif
     c                   endif
      *
     c                   if        c1nlka = 0
     c                   goto      end_c1bld
     c                   endif
      *
      * Validering
      * Sender opp informasjonsbilde dersom rad finnes fra før
      *
     c                   eval      clnlir_nlka = c1nlka
     c                   eval      clnlir_nllv = c1nllv
     c     clnlir_key    chain     clnlir
     c                   if        %found
     c                   exsr      xc2msg
     c                   goto      c1taga
     c                   endif
      *
      * Sjekk om leverandør er koblet til hovedkategori
      *
     c                   eval      w_nlka = c1nlka
     c                   exsr      sjekk_hkat
      *
      * Kaller vedlikeholdsprogram
      *
     c                   eval      c2nlka = c1nlka
     c                   eval      c2nllv = c1nllv
     c                   exsr      xc2bld
      *
     c                   eval      c1nlka = 0
     c                   eval      c1nntx = *blank
     c                   eval      c1nllv = 0
     c                   eval      c1nltx = *blank
      *
     c                   goto      c1taga
      *
     c     end_c1bld     tag
      *
     c                   eval      b_oppr = *off
     c                   eval      b2nlka = 0
      *
     c                   if        b_forn = *on
     c                   exsr      forny
     c                   endif
      *
     c                   endsr
