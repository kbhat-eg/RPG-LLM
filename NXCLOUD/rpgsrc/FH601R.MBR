     c                   if        fhkpro = 0
      *
     c                   eval      rkunl1_kund = fhkund
     c     rkunl1_key    chain     rkunl1                             91
     c                   if        *in91 = *off
     c                   eval      a_navn(w_etik) = rknavn
     c                   eval      a_adr1(w_etik) = rkgate
     c                   eval      a_padr(w_etik) = rksted
