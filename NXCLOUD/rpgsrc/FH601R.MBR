      * Identifiserer siste prisfil- eller prisbok-forsendelse
      *
     c                   if        d_valg = 'F'
      *
     c     fkdil3_key1   setll     fkdil3
     c     fkdil3_key1   reade     fkdil3                                 90
     c                   if        *in90 or fhftis = *loval
     c                   goto      avslutt
     c                   endif
      *
     c                   else
      *
     c     fkdil4_key1   setll     fkdil4
     c     fkdil4_key1   reade     fkdil4                                 90
     c                   if        *in90 or fhbtis = *loval
     c                   goto      avslutt
     c                   endif
      *
     c                   endif
      *
      * Hode/sideskift
      *
     c                   write     a1hdr
      *
      * Leser kunder/lev.adresser i siste forsendelse
      *
     c                   if        d_valg = 'F'
      *
     c                   eval      fkdil3_ftis = fhftis
     c     fkdil3_key2   setll     fkdil3
     c     fkdil3_key2   reade     fkdil3                                 90
     c                   dow       *in90 = *off
     c                   exsr      behandling
     c     fkdil3_key2   reade     fkdil3                                 90
     c                   enddo
      *
     c                   else
      *
     c                   eval      fkdil4_btis = fhbtis
     c     fkdil4_key2   setll     fkdil4
     c     fkdil4_key2   reade     fkdil4                                 90
     c                   dow       *in90 = *off
     c                   exsr      behandling
     c     fkdil4_key2   reade     fkdil4                                 90
     c                   enddo
      *
     c                   endif
      *
      * Skriver detaljlinje
      *
     c                   exsr      detalj
      *
      * Avslutt
      *
     c     avslutt       tag
      *
     c                   eval      *inlr = *on
     c                   return
