     c                   eval      a_kund(w_etik) = %trim(a_kund(w_etik)) +
     c                                              '/' + w_kpro_alf
      *
     c                   eval      fkprl1_kund = fhkund
     c                   eval      fkprl1_kpro = fhkpro
     c     fkprl1_key    chain     fkprl1                             91
     c                   if        *in91 = *off
     c                   eval      a_navn(w_etik) = flnavn
     c                   eval      a_adr1(w_etik) = fladr1
     c                   eval      a_adr2(w_etik) = fladr2
     c                   movel     flponr        a_padr(w_etik)
