      * Sjekker om rad finnes fra før
      *
     c                   eval      awsdlr_wsid = k1wsid
     c                   eval      awsdlr_ruti = k1ruti
     c     awsdlr_key    chain     awsdlr
     c                   if        %found
     c                   eval      *in32 = *on
     c                   goto      k1taga
     c                   endif
      *
      * Kaller vedlikeholdsprogram
      *
     c                   eval      c1wsid = s_wsid
