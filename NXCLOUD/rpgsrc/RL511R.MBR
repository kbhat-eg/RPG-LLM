     c     rltrl1_key    setll     rltrl1
     c                   other
     c                   eval      rltrl5_samb = b1samb
     c     rltrl5_key    setll     rltrl5
     c                   endsl
      *
     c                   exsr      clr_subfile
     c                   exsr      crt_subfile
      *
     c     end_forny     endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for posisjonering i subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     posisjoner    begsr
      *
      *
      * Posisjoner startverdi, regnskapsår endret
      *
     c                   if        b2raar <> w_raar                                          .
     c                   eval      b2samb = w_samb                                           .
     c                   eval      b2levr = w_levr                                           .
     c                   eval      w_raar = b2raar                                           .
      *
     c                   if        b2samb = 0 and
     c                             b2levr = 0
     c                   eval      b2samb = 1
     c                   endif
      *                                                                 .
     c                   endif                                                           .....
      *
      * Samlegiro
      *
     c                   if        b2samb <> 0
     c                   eval      w_seqe = *blank
     c                   eval      w_samb = b2samb
     c                   eval      w_levr = 0
     c                   eval      rltrl5_samb = b2samb
     c     rltrl5_key    setll     rltrl5
     c                   goto      end_pos
     c                   endif
      *
      * Leverandørnummer
      *
     c                   if        b2levr <> 0
     c                   eval      w_seqe = 'B'
     c                   eval      w_levr = b2levr
     c                   eval      w_samb = 0
     c                   eval      rltrl1_levr = b2levr
     c     rltrl1_key    setll     rltrl1
     c                   goto      end_pos
     c                   endif
      *
     c     end_pos       tag
      *
      * Blanker subfile, og fyller opp i.h.t. posisjonering
      *
     c                   eval      *in22 = *off
      *
     c                   exsr      clr_subfile
     c                   exsr      crt_subfile
      *
      * Blanker posisjoneringsfelter
      *
     c                   eval      b2samb = 0
     c                   eval      b2levr = 0
      *
     c                   endsr
