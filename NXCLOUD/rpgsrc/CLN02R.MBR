     c                   eval      b2nnka = 0
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for behandling av subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     subfile       begsr
      *
     c                   eval      b_forn = *off
      *
     c                   if        *in22 = *off
     c                   eval      *in22 = *on
     c                   else
     c                   eval      *in22 = *off
     c                   endif
      *
     c                   eval      w_virn = w_fcrn
      *
      * Leser og behandler subfile
      *
     c                   do        w_ssrn
     c                   readc     b1sfl
      *
     c                   if        %eof
     c                   leave
     c                   endif
      *
     c                   eval      *in22 = *off
     c                   eval      w_virn = w_srrn
      *
      * Endre
      *
     c                   if        b1valg = 2
     c                   eval      c2nntx = b1nntx
     c                   eval      c2nnka = b1nnka
     c                   eval      c2nnog = b1nnog
     c                   eval      c2nnhg = b1nnhg
     c                   eval      c2nnug = b1nnug
     c                   exsr      xc2bld
     c                   eval      b1valg = 0
     c                   update    b1sfl
     c                   iter
     c                   endif
      *
      * Slett
      *
     c                   if        b1valg = 4
     c                   eval      d1nnka = b1nnka
     c                   eval      d1nntx = b1nntx
     c                   eval      d1nnog = b1nnog
     c                   eval      d1nnhg = b1nnhg
     c                   eval      d1nnug = b1nnug
     c                   exsr      xd1win
     c                   eval      b1valg = 0
     c                   update    b1sfl
     c                   iter
     c                   endif
      *
      * Vis
      *
     c                   if        b1valg = 5
     c                   eval      c2nnka = b1nnka
     c                   eval      c2nntx = b1nntx
     c                   eval      c2nnog = b1nnog
     c                   eval      c2nnhg = b1nnhg
     c                   eval      c2nnug = b1nnug
     c                   exsr      xc2bld
     c                   eval      b1valg = 0
     c                   update    b1sfl
     c                   iter
     c                   endif
      *
     c                   enddo
      *
     c                   if        b_forn = *on
     c                   exsr      forny
     c                   endif
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for behandling av skjermbilde for ny rad (C1BLD)
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     xc1bld        begsr
      *
     c                   eval      b_oppr = *on
     c                   eval      b_forn = *off
      *
     c                   eval      c1nnka = 0
     c                   eval      c1nnog = 0
     c                   eval      c1nnhg = 0
     c                   eval      c1nnug = 0
     c                   eval      c1nntx = *blank
     c                   eval      c1ogtx = *blank
     c                   eval      c1hgtx = *blank
     c                   eval      c1ugtx = *blank
      *
     c     c1taga        tag
      *
     c                   exfmt     c1bld
      *
      * Behandling av funksjonstaster
      *
     c                   select
     c                   when      *inkc or *inkl
     c                   goto      end_c1bld
     c                   endsl
      *
      * Søking
      *
     c                   if        *inkd = *on
     c                   if        w_afld = 'C1NNKA'
     c                   exsr      sok_kat
     c                   eval      c1nnka = w_nnka
     c                   eval      c1nntx = w_nktx
     c                   goto      c1taga
     c                   endif
      *
     c                   if        w_afld = 'C1NNOG'
     c                   exsr      sok_ogr
     c                   eval      c1nnog = w_ogrp
     c                   eval      c1ogtx = w_otxt
     c                   goto      c1taga
     c                   endif
      *
     c                   if        w_afld = 'C1NNHG'
     c                   exsr      sok_hgr
     c                   eval      c1nnhg = w_hgrp
     c                   eval      c1hgtx = w_htxt
     c                   goto      c1taga
     c                   endif
      *
     c                   if        w_afld = 'C1NNUG'
     c                   exsr      sok_ugr
     c                   eval      c1nnug = w_ugrp
     c                   eval      c1ugtx = w_utxt
     c                   goto      c1taga
     c                   endif
     c                   endif
      *
      *
     c                   if        c1nnka = 0
     c                   goto      end_c1bld
     c                   endif
      *
      * Validering
      * Sender opp informasjonsbilde dersom overgruppe ikke er fylt ut
      *
     c                   if        c1nnog = 0
     c                   exsr      xc3msg
     c                   goto      c1taga
     c                   endif
      *
      * Sender opp informasjonsbilde dersom rad finnes fra før
      *
     c                   eval      clnnir_nnka = c1nnka
     c                   eval      clnnir_nnog = c1nnog
     c                   eval      clnnir_nnhg = c1nnhg
     c                   eval      clnnir_nnug = c1nnug
     c     clnnir_key    chain     clnnir
     c                   if        %found
     c                   exsr      xc2msg
     c                   goto      c1taga
     c                   endif
      *
      * Kaller vedlikeholdsprogram
      *
     c                   eval      c2nnka = c1nnka
     c                   eval      c2nnog = c1nnog
     c                   eval      c2nnhg = c1nnhg
     c                   eval      c2nnug = c1nnug
     c                   exsr      xc2bld
      *
     c                   eval      c1nnog = 0
     c                   eval      c1nnhg = 0
     c                   eval      c1nnug = 0
     c                   eval      c1ogtx = *blank
     c                   eval      c1hgtx = *blank
     c                   eval      c1ugtx = *blank
      *
     c                   goto      c1taga
      *
     c     end_c1bld     tag
      *
     c                   eval      b_oppr = *off
     c                   eval      b2nnka = 0
      *
     c                   if        b_forn = *on
     c                   exsr      forny
     c                   endif
      *
     c                   endsr
