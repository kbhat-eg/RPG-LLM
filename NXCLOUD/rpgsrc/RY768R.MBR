6.3q  *       260515 bof  Datoformat sql
      //
      // - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      //
     fjvarl1    if   e           k disk    rename(jvarpfr:jvarl1r)
     fvvarl1    if   e           k disk    rename(vvarpfr:vvarl1r)
      //
      // Definering av variable
      //
     d b_inlr          s              1    inz(*off)
     d w_vare          s                   like(jvvare)
     d w_firm          s                   like(vvfirm)
     d w_teller        s              5  0
      //
       // ------------------------------------- Prototypes
     D P_JV400R        PR                  ExtPgm('JV400R')
     d  firma                              like(vvfirm)
     d  vare                               like(jvvare)
     d  inlr                          1
      *
6.3q C/Exec SQL
6.3q C+  Set Option DatFmt=*ISO
6.3q C/End-Exec
      // - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      // Hovedprogram
      // - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      /FREE

        w_firm = 0;
        w_teller = 0;

        // Finner varer hvor jvnobb <> jvvare
        EXEC sql
         declare vareNobbCursor cursor for
            select jvvare from jvarpf
            where jvnobb <> 0 and jvnobb <> jvvare
            for read only;

        exec sql
         open vareNobbCursor;

       exec sql
         fetch next from vareNobbCursor
           into :w_vare;

       doW (SQLCode >= 0 and SQLCode <> 100);

         // Kaller program for sletting av vare i LVR
         b_inlr = *on;
         CallP P_JV400R (w_firm : w_vare : b_inlr);
         w_teller+=1;

         //if (w_teller = 10);    // for test
         //leave;
         //endif;

         exec sql
           fetch next from vareNobbCursor
             into :w_vare;
       endDo;

       exec SQL
         close vareNobbCursor;
       //
       // - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
       // Avslutt program
       // - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
       //
       *inlr = *on;
       return;
       //
       // - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
       // Subrutine for initiering av program
       // - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
       //
       begsr *inzsr;
         //

       endsr;
      /END-FREE
