     c                   eval      w_virn = w_sfrn
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for å bla tilbake en side i subfilen
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     bck_subfile   begsr
      *
     c     vgkti1_key    setgt     vgkti1
     c                   readp     vgkti1
      *
      * Leser tilbake en side
      *
     c                   eval      w_stel = 0
      *
     c                   dou       w_stel = w_ssrn + c_sfil
      *
     c                   readp     vgkti1
      *
     c                   if        %eof or
     c                             vtfirm <> w_firm
     c                   leave
     c                   endif
      *
     c                   eval      w_stel = w_stel + 1
      *
     c                   eval      vgkti1_ftyp = vtftyp
      *
     c                   enddo
      *
     c                   if        %eof
     c     vgkti1_key    setll     vgkti1
     c                   endif
      *
      * Blanker og fyller opp subfile
      *
     c                   exsr      clr_subfile
     c                   exsr      crt_subfile
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for display av subfile
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     dsp_subfile   begsr
      *
     c                   if        w_srrn <> 0
     c                   eval      *in12 = *on
     c                   else
     c                   write     b2cmd
     c                   endif
      *
     c                   eval      *in13 = *on
     c                   exfmt     b2ctl
      *
     c                   eval      *in12 = *off
     c                   eval      *in13 = *off
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for vedlikehold av kostprisfaktorer
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     param         begsr
      *
     c                   call      'VG205R'
      *
     c                   endsr
      *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for initiering av program
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     *inzsr        begsr
      *
      * Parametre
      *
      * Key for posisjonering på faktyratype
      *
     c     vgktiu_key    klist
     c                   kfld                    w_firm
     c                   kfld                    vgktiu_ftyp
      *
      *
     c     vgkti1_key    klist
     c                   kfld                    w_firm
     c                   kfld                    vgkti1_ftyp
      *
      * Key for kontoplan
      *
     c     rhovl1_key    klist
     c                   kfld                    w_firm
     c                   kfld                    rhovl1_kont
     c                   kfld                    rhovl1_spes
     c                   kfld                    rhovl1_avde
      *
      *
      * Henter verdier fra LDA, og initierer skjermbildet
      *
     c                   eval      w_firm = l_firm
      *
     c                   eval      *in22 = *on
      *
      * Posisjonerer databasen, og fyller opp subfile
      *
     c                   eval      vgktiu_ftyp = ' '
     c     vgktiu_key    setll     vgktiu
      *
     c                   exsr      crt_subfile
      *
     c                   endsr
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Hent tekster når linje skal vedlikeholdes/Se på
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     hent_txt      begsr
      *
      * Konto
      *
      * Henter tekst
     c                   move      c2hkto        rhovl1_kont
     c                   eval      rhovl1_spes = 0
     c                   eval      rhovl1_avde = 0
     c     rhovl1_key    chain     rhovl1
      *
     c                   if        %found
     c                   eval      c2htxt = rhtext
     c                   else
     c                   move      *blanks       c2htxt
     c                   endif
      *
      * Mot konto
      *
      * Henter tekst
     c                   move      c2motk        rhovl1_kont
     c                   eval      rhovl1_spes = 0
     c                   eval      rhovl1_avde = 0
     c     rhovl1_key    chain     rhovl1
      *
     c                   if        %found
     c                   eval      c2mtxt = rhtext
     c                   else
     c                   move      *blanks       c2mtxt
     c                   endif
      *
     c                   endsr
