      *
     c     xc2bld        begsr
      *
     c                   eval      w_arow = 0
     c                   eval      w_acol = 0
      *
     c                   eval      vgktiu_ftyp = c1ftyp
     c     vgktiu_key    chain     vgktiu
      *
     c                   if        %found
     c**                 eval      c2ftyp = vtftyp
     c                   eval      c2kalk = vtkalk
     c                   eval      c2hkto = vthkto
     c                   eval      c2motk = vtmotk
     c                   exsr      hent_txt
     c                   eval      c2eusr = vttusr
     c     *dmy          move      vttdat        c2edat
     c                   else
     c                   eval      c2kalk = *blank
     c                   eval      c2hkto = *zero
     c                   eval      c2mtxt = *blanks
     c                   eval      c2htxt = *blanks
     c                   eval      c2motk = *zero
     c                   eval      c2eusr = *blank
     c                   eval      c2edat = 0
     c                   endif
      *
     c                   if        b1valg = 3
     c                   eval      c1ftyp = k1ftyp
     c                   endif
      *
     c                   if        b1valg = 5
     c                   eval      *in30 = *on
     c                   endif
      *
     c     c2taga        tag
      *
     c                   exfmt     c2bld
      *
      * Feilindikatorer off
     c                   eval      *in42 = *off
     c                   eval      *in43 = *off
     c                   eval      *in47 = *off
      *
     c                   if        *inkc or *inkl or *in30
     c                   goto      end_c2bld
     c                   endif
      *
      * Spørring
      *
     c                   if        *inkd
      * Call kontoplan
      *
     c                   if        w_afld = 'C2HKTO'
      *
     c                   eval      w_kont = 0
     c                   eval      w_spes = 0
     c                   eval      w_avde = 0
     c                   eval      w_tx30 = ' '
      *
     c                   call      'RH500R'
     c                   parm                    w_firm
     c                   parm                    w_kont
     c                   parm                    w_spes
     c                   parm                    w_avde
     c                   parm                    w_tx30
      *
     c                   if        w_kont <> 0
     c                   eval      c2hkto = w_kont
     c                   eval      c2htxt = w_tx30
     c                   endif
      *
     c                   goto      c2taga
     c                   endif
      * Mot konto
     c                   if        w_afld = 'C2MOTK'
      *
     c                   eval      w_kont = 0
     c                   eval      w_spes = 0
     c                   eval      w_avde = 0
     c                   eval      w_tx30 = ' '
      *
     c                   call      'RH500R'
     c                   parm                    w_firm
     c                   parm                    w_kont
     c                   parm                    w_spes
     c                   parm                    w_avde
     c                   parm                    w_tx30
      *
     c                   if        w_kont <> 0
     c                   eval      c2motk = w_kont
     c                   eval      c2mtxt = w_tx30
     c                   endif
      *
     c                   goto      c2taga
     c                   endif
     c                   endif
      *
      * Validering av input
      *
      *Faktortype må være utfylt
      *
     c                   if        c2kalk = *blank
     c                   eval      *in42 = *on
     c                   exsr      xc1msg
     c                   goto      c2taga
     c                   endif
      *
      * Dersom kontering er valgt må begge kontofelter være utfylt
