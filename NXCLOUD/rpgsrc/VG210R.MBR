     c                   eval      c2htxt = *blanks
     c                   eval      *in43 = *on
     c                   exsr      xc1msg
     c                   goto      c2taga
     c                   endif
      *
     c                   if        c2motk = 0
     c                             and c2hkto <> 0
     c                   eval      c2htxt = *blanks
     c                   eval      *in43 = *on
     c                   exsr      xc1msg
     c                   goto      c2taga
     c                   endif
      *
      * Hovedbokskonto må finnes i kontoplan
      *
     c                   if        c2hkto <> 0
     c                   move      c2hkto        rhovl1_kont
     c                   eval      rhovl1_spes = 0
     c                   eval      rhovl1_avde = 0
     c     rhovl1_key    chain     rhovl1
      *
     c                   if        not%found
     c                   eval      *in47 = *on
     c                   eval      c2htxt = *blanks
     c                   exsr      xc1msg
     c                   goto      c2taga
     c                   endif
     c                   endif
      *
      *
      * Motkonto må finnes i kontoplan
      *
     c                   if        c2motk <> 0
     c                   move      c2motk        rhovl1_kont
     c                   eval      rhovl1_spes = 0
     c                   eval      rhovl1_avde = 0
     c     rhovl1_key    chain     rhovl1
      *
     c                   if        not%found
     c                   eval      *in47 = *on
     c                   eval      c2mtxt = *blanks
