      *
     c     fornyvare     begsr
      *
     c                   eval      sqlquery  = '+
     c                             select +
     c                                b.jvqfir +
     c                              , b.jvqvnr +
     c                              , b.jvqldo +
     c                              , b.jvquno +
     c                              , b.jvqofa +
     c                              , ifnull(d.jvldor, -1) as jvldor +
     c                              , ifnull(c.jvkvnr, -1) as jvkvnr +
     c                              from jvarpf a +
     c                              inner join jvklpf b on cast(b.jvquno +
     c                                as char(15)) = a.jvvare +
     c                              left outer join jvkhpf c on +
     c                                c.jvkfir = b.jvqfir and +
     c                              c.jvkvnr = b.jvqvnr +
     c                                and c.jvkldo = b.jvqldo +
     c                              left outer join jvarpf d on +
     c                                   d.jvvare = c.jvkvnr +
     c                              where a.jvvare in ( +
     c                                select e1.jvquno from jvklpf e1 +
     c                                where e1.jvqfir = ? +
     c                              ) and b.jvqfir = ?'
     c
     c                   if        w_vare_p <> *blank
     c                   eval      sqlquery = %trim(sqlquery) +
     c                               ' and ( (a.jvedat = current_date) and +
     c                                       (a.jvvare = ?) ) '
     c                   else
     c                   eval      sqlquery = %trim(sqlquery) +
     c                               ' and (a.jvedat = current_date) '
     c                   endif
     c/exec sql
     c+ PREPARE SQL_STMT_V FROM :sqlquery
     c/end-exec
     c
     c/exec sql
     c+ DECLARE STMT_V CURSOR FOR SQL_STMT_V
     c/end-exec
     c                   exsr      sjekk_sql
     c                   if        w_vare_p <> *blank
     c/exec sql
     c+ OPEN STMT_V using :w_firm, :w_firm, :w_vare_p
     c/end-exec
     c                   else
     c/exec sql
     c+ OPEN STMT_V using :w_firm, :w_firm
     c/end-exec
     c                   endif
     c
     c                   eval      w_tell = 0
     c                   dow       w_tell < w_maxv                              så ikke looper
     c                   eval      w_tell = w_tell + 1
     c/exec sql
     c+ fetch STMT_V
     c+ into  :jvkfir, :jvqvnr, :jvqldo, :jvquno, :jvqofa, :w_jvldor, :jvkvnr
     c/end-exec
     c                   exsr      sjekk_sql
     c                   if        sqlcod = 100
     c                   leave
     c                   endif
     c                   eval      jvvare = jvqvnr
     c                   call      'JK702R'
     c                   parm                    jvvare
     c                   parm                    w_ldor
     c                   parm                    w_dumm
     c*                   exsr      nobbkopivare
     c                   enddo
     c/exec sql
     c+ close STMT_V
     c/end-exec
      *
     c                   endsr
