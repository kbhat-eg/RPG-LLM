      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      * Subrutine for fornying av vare / pakning
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
      *
     c     fornypris     begsr
      *
     c                   eval      sqlquery  = '+
     c                             select +
     c                                b.jvqfir +
     c                              , b.jvqvnr +
     c                              , b.jvqldo +
     c                              , b.jvquno +
     c                              , b.jvqofa +
     c                              , ifnull(d.jvldor, -1) as jvldor +
     c                              , ifnull(c.jvkvnr, -1) as jvkvnr +
     c                              from jvprpf a +
     c                              inner join jvklpf b on b.jvquno = a.jxvare +
     c                              left outer join jvkhpf c on +
     c                                c.jvkfir = b.jvqfir and +
     c                              c.jvkvnr = b.jvqvnr +
     c                                and c.jvkldo = b.jvqldo +
     c                              left outer join jvarpf d on +
     c                                   d.jvvare = c.jvkvnr +
     c                              where a.jxvare in ( +
     c                                select e1.jvquno from jvklpf e1 +
     c                                where e1.jvqfir = ? +
     c                              ) and b.jvqfir = ? '
     c                   if        w_vare_p <> *blank
     c                   eval      sqlquery = %trim(sqlquery) +
     c                               ' and ( (a.jxedat = current_date) and +
     c                                       (a.jxvare = ?) ) '
     c                   else
     c                   eval      sqlquery = %trim(sqlquery) +
     c                               ' and (a.jxedat = current_date) '
     c                   endif
     c/exec sql
     c+ PREPARE SQL_STMT_P FROM :sqlquery
     c/end-exec
     c
     c/exec sql
     c+ DECLARE STMT_P CURSOR FOR SQL_STMT_P
     c/end-exec
     c                   exsr      sjekk_sql
