      *
     c     xc2bld        begsr
      *
     c                   eval      w_arow = 0
     c                   eval      w_acol = 0
      *
     c                   eval      mjlolr_skaf = c1skaf
5.20 c                   eval      mjlolr_lage = c1lage
     c                   eval      mjlolr_avde = c1avde
     c     mjlolr_key    chain     mjlolr
      *
     c                   if        %found
     c                   eval      c2eann = mjeann
     c                   eval      c2sted = mjsted                                            .
     c                   eval      c2adr1 = mjadr1                                            .
     c                   eval      c2adr2 = mjadr2                                            .
     c                   eval      c2pste = mjpste                                            .
     c                   eval      c2eusr = mjeusr
     c     *dmy          move      mjedat        c2edat
     c                   else
     c                   eval      c2eann = 0
     c                   eval      c2sted = *blank                                            .
     c                   eval      c2adr1 = *blank                                            .
     c                   eval      c2adr2 = *blank                                            .
     c                   eval      c2pste = *blank                                            .
     c                   eval      c2eusr = *blank
     c                   eval      c2edat = 0
     c                   endif
      *
     c                   if        b1valg = 3
     c                   eval      c1skaf = k1skaf
5.20 c                   eval      c1lage = k1lage
     c                   eval      c1avde = k1avde
     c                   endif
      *
     c                   if        b1valg = 5
     c                   eval      *in30 = *on
     c                   endif
      *
     c                   eval      w_lage = c1lage
     c                   exsr      sjekk_lager
     c                   eval      c1lagn = w_latx
      *
     c                   eval      w_avde = c1avde
     c                   exsr      sjekk_avd
     c                   eval      c1avdn = w_avtx
      *
     c     c2taga        tag
      *
     c                   exfmt     c2bld
      *
     c                   if        *inkc or *inkl or *in30
     c                   goto      end_c2bld
     c                   endif
