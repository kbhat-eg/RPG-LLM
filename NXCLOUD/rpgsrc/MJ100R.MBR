     c                   eval      mjlolu_skaf = c1skaf
5.20 c                   eval      mjlolu_lage = c1lage
     c                   eval      mjlolu_avde = c1avde
     c     mjlolu_key    chain     mjlolur
      *
     c                   eval      mjeann = c2eann
     c                   eval      mjsted = c2sted                                            .
     c                   eval      mjadr1 = c2adr1                                            .
     c                   eval      mjadr2 = c2adr2                                            .
     c                   eval      mjpste = c2pste                                            .
     c                   time                    mjedat
     c                   time                    mjetim
     c                   eval      mjeusr = l_user
      *
     c                   if        %found
6.10 c                   time                    mjedat
6.10 c                   time                    mjetim
6.10 c                   eval      mjeusr = l_user
