     c                   else
     c                   eval      mjfirm = w_firm
     c                   eval      mjskaf = c1skaf
5.20 c                   eval      mjlage = c1lage
     c                   eval      mjavde = c1avde
     c                   time                    mjodat
6.10 c                   time                    mjotim
6.10 c                   eval      mjousr = l_user
6.10 c                   time                    mjedat
6.10 c                   time                    mjetim
6.10 c                   eval      mjeusr = l_user
     c                   write     mjlolur
     c                   endif
      *
