     c                   eval      nkmanu = c2manu
     c                   eval      nkform = c2form
     c                   eval      nkzipp = c2zipp
     c                   eval      nkftpn = c2ftpn
     c                   eval      nkmapp = c2mapp
     c                   eval      nklink = c2link
     c                   eval      nkfile = c2file
     c                   eval      nkkprs = c2kprs
