      *                                                                 *
      *                                                                 *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      *
      *
     fausrl1    if   e           k disk    rename(ausrpfr:ausrl1r)
      *
     d txt             s             30    dim(2) ctdata perrcd(1)
      *
     d p_user          s                   like(abuser)
     d p_navn          s                   like(abnavn)
     d p_retk          s              1
      *
     d w_user          s                   like(abuser)
      *
     d ausrl1_user     s                   like(abuser)
      *
      * Parameterliste
      *
     c     *entry        plist
     c                   parm                    p_retk
     c                   parm                    p_user
     c                   parm                    p_navn
      *
      * Blanker felter
      *
     c                   eval      p_retk = *blank
     c                   eval      p_navn = *blank
      *
      * Sjekk bruker
      *
     c                   eval      ausrl1_user = p_user
     c     ausrl1_key    chain     ausrl1r                            60
      *
     c                   if        *in60 = *on                                           .....
     c                   move      'N'           p_retk                                      .
     c                   move      txt(1)        p_navn                                      .
     c                   goto      slutt                                                     .
     c                   endif                                                           .....
      *
      * Er bruker sperret? dvs. Levlid=00 ?
      *
     c                   if        ablevl = *zero                                        .....
     c                   move      'S'           p_retk                                      .
     c                   move      txt(2)        p_navn                                      .
     c                   goto      slutt                                                     .
     c                   endif                                                           .....
      *
     c                   move      abnavn        p_navn
      *
      /eject
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      * Avslutt program                                                 *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      *
     c     slutt         tag
      *
     c                   eval      *inlr = *on
     c                   return
      *
      /eject
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      * Subrutine for initiering av program                             *
      * - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
      *
     c     *inzsr        begsr
      *
      *
      * Key for bruker
      *
     c     ausrl1_key    klist
     c                   kfld                    ausrl1_user
      *
     c                   endsr
