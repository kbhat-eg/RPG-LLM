3.11 c                   if        *in91 = *on
3.11 c                   write     awsdlur
3.11 c                   endif
3.11  *
3.11 c                   eval      awsdlu_wsid = awwsid
3.11 c                   eval      awsdlu_ruti = 'FTILB2'
3.11 c                   eval      awruti = 'FTILB2'
3.11  *
3.11 c     awsdlu_key    chain     awsdlur                            91
3.11 c                   if        *in91 = *on
3.11 c                   write     awsdlur
3.11 c                   endif
