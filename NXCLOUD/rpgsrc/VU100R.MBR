     c                   readp     vstrl3
     c                   when      w_seqe = 'B'
     c     vstrl2_key    setgt     vstrl2
     c                   readp     vstrl2
     c                   other
     c     vstrl1_key    setgt     vstrl1
     c                   readp     vstrl1
     c                   endsl
     c                   endif
      *
      * Leser tilbake en side
