      *
     c                   dou       w_srrn = w_spge
      *
     c                   select
     c                   when      w_seqe = 'C'
     c                   read      vstrl3
     c                   when      w_seqe = 'B'
     c                   read      vstrl2
     c                   other
     c                   read      vstrl1
     c                   endsl
      *
     c                   if        %eof or
     c                             vufirm <> w_firm
     c                   eval      *in15 = *on
     c                   leave
     c                   endif
      * Legg ikke ut post, hvis ikke strukturvare er blank.
     c                   if        vuvars = *blank
      *
     c                   eval      w_srrn = w_srrn + 1
      *
     c                   select
     c                   when      w_seqe = 'C'
     c                   eval      vstrl3_besk = vubesk
     c                   when      w_seqe = 'B'
     c                   eval      vstrl2_vars = vuvars
     c                   other
     c                   eval      vstrl1_vare = vuvare
     c                   eval      vstrl1_vars = vuvars
     c                   endsl
      *
     c                   eval      b1valg = 0
     c                   eval      b1vare = vuvare
6.10 c**                 eval      b1vars = vuvars
     c                   eval      b1besk = vubesk
6.10 c**                 eval      b1anta = vuanta
6.10 c                   eval      b1kstr = vukstr
      *
     c                   write     b1sfl
      *
     c                   endif
      *
     c                   enddo
