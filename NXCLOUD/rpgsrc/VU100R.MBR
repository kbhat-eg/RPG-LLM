      *
     c                   eval      w_stel = w_stel + 1
      *
     c                   select
     c                   when      w_seqe = 'C'
     c                   eval      vstrl3_besk = vubesk
     c                   when      w_seqe = 'B'
     c                   eval      vstrl2_vars = vuvars
     c                   other
     c                   eval      vstrl1_vare = vuvare
     c                   eval      vstrl1_vars = vuvars
     c                   endsl
      *
     c                   enddo
