             PGM

/* Initiering av parametre.                                          */

             DCL        VAR(&IN03) TYPE(*CHAR) LEN(1)
             DCL        VAR(&WALI) TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&WAPO) TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&MLD1) TYPE(*CHAR) LEN(40)
             DCL        VAR(&MLD2) TYPE(*CHAR) LEN(40)
             DCL        VAR(&SYST) TYPE(*CHAR) LEN(2)

/* Oppdaterer LDA med programnavn og kode for om formularparametre   */
/* skal endres.                                                      */

             CHGDTAARA  DTAARA(*LDA (365 02)) VALUE(&SYST)

             CALL       PGM(RE120R) PARM(&IN03)

             IF         COND(&IN03 *EQ '1') THEN(DO)
             GOTO       CMDLBL(ENDPGM)
             ENDDO

             RTVDTAARA  DTAARA(*LDA (471 2)) RTNVAR(&SYST)

/* Varselbilde 1                                                     */

             CHGVAR     VAR(&WALI) VALUE(4)
             CHGVAR     VAR(&WAPO) VALUE(2)
             CHGVAR     VAR(&MLD1) VALUE('Innlesing av poster    **')
             CHGVAR     VAR(&MLD2) VALUE('** Eksterne systemer  ***')
             CALL       PGM(AA006C)  PARM(&WALI &WAPO &MLD1 &MLD2)

/* Call av utskriftsprogram til batch/demand                         */

             IF         COND(&SYST *EQ '01') THEN(DO)
             CALL       PGM(RE121C)
             ENDDO

             IF         COND(&SYST *EQ '02') THEN(DO)
             CALL       PGM(RE122C)
             ENDDO

             IF         COND(&SYST *EQ '03') THEN(DO)
             CALL       PGM(RE123C)
             ENDDO

             IF         COND(&SYST *EQ '04') THEN(DO)
             CALL       PGM(RE124C)
             ENDDO

 ENDPGM:     ENDPGM
