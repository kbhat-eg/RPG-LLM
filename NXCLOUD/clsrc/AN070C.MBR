             PGM        PARM(&KODE &DENS &VOLU)

             DCL        VAR(&KODE)     TYPE(*CHAR) LEN(1)
             DCL        VAR(&DENS)     TYPE(*CHAR) LEN(10)
             DCL        VAR(&VOLU)     TYPE(*CHAR) LEN(6)
             DCL        VAR(&MELD)     TYPE(*CHAR) LEN(50)

/* Varsle dersom nattjobb ikke var definert                          */
/* (Density hentes fra nattjobb 1)                                   */

             IF         COND(&KODE *EQ '1') THEN(DO)
             CHGVAR     VAR(&MELD) VALUE('Nattjobb 1 ikke definert, +
                           dette må gjøres først.')
             CALL       PGM(AA005R)  PARM(&MELD)
             GOTO       CMDLBL(ENDPGM)
             ENDDO

/* Initiering av tapen ASBACK                                        */

             INZTAP     DEV(TAP01) NEWVOL(&VOLU) CHECK(*NO) +
                                   DENSITY(&DENS)
             MONMSG     MSGID(CPF6762) EXEC(DO)
             CHGVAR     VAR(&MELD) VALUE('Det er satt inn feil type +
                          tape i båndstasjonen')
             CALL       PGM(AA005R)  PARM(&MELD)
             GOTO       CMDLBL(ENDPGM)
             ENDDO

             MONMSG     MSGID(CPF6760) EXEC(DO)
             CHGVAR     VAR(&MELD) VALUE('Jeg fant ingen tape i +
                          tape-stasjonen ???')
             CALL       PGM(AA005R)  PARM(&MELD)
             GOTO       CMDLBL(ENDPGM)
             ENDDO

             IF         COND(&VOLU *EQ 'ASBACK') THEN(DO)
             CHGVAR     VAR(&MELD) VALUE('Backup-tape for bruk i +
                          nattjobben er klargjort')
             ENDDO

             IF         COND(&VOLU *EQ 'SAVSYS') THEN(DO)
             CHGVAR     VAR(&MELD) VALUE('Backup-tape for +
                          full systembackup er klargjort')
             ENDDO
             CALL       PGM(AA005R)  PARM(&MELD)
             GOTO       CMDLBL(ENDPGM)

 ENDPGM:     ENDPGM
