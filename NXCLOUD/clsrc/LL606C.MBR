             CHKOBJ     OBJ(LWA1PF) OBJTYPE(*FILE) MBR(&WMM1)
             MONMSG     MSGID(CPF9815) EXEC(DO)
             ADDPFM     FILE(LWA1PF) MBR(&WMM1) TEXT('Poster til +
                          lagerverdiliste LL608R')
             ADDLFM     FILE(LWA1LU) MBR(&WMM1) +
                          DTAMBRS((*CURRENT/LWA1PF (&WMM1)))
             ADDLFM     FILE(LWA1L1) MBR(&WMM1) +
                          DTAMBRS((*CURRENT/LWA1PF (&WMM1)))
             ENDDO
/*                                                                   */
             OVRDBF     FILE(LWA1LU) TOFILE(*FILE) MBR(&WMM1)
             OVRDBF     FILE(LWA1L1) TOFILE(*FILE) MBR(&WMM1)

/*                                                                   */
/* Les lagerregister                                                 */
/*                                                                   */
             IF         COND(&TYPE *NE '2') THEN(DO)
             CALL       PGM(LL607R) PARM(&PARM)
             ENDDO
/*                                                                   */
/* Les telleregister                                                 */
/*                                                                   */
             IF         COND(&TYPE *EQ '2') THEN(DO)
             CALL       PGM(LL608R) PARM(&PARM)
             ENDDO

