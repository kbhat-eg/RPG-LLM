             ENDDO


/*8.01*/     chgvar     var(&CO402_KEY) value('PROA_VERSJON_LAGERVERDI')
/*8.01*/     rtvdtaara  dtaara(*lda (931 3)) rtnvar(&fgr)
/*8.01*/     rtvdtaara  dtaara(*lda (944 3)) rtnvar(&fix)
/*8.01*/     chgvar     var(&fir)   value(&fix)
