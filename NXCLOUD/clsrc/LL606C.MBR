             RMVM       FILE(LWA1LU) MBR(&WMM1)
             MONMSG     MSGID(CPF7310)
             RMVM       FILE(LWA1L1) MBR(&WMM1)
             MONMSG     MSGID(CPF7310)
             RMVM       FILE(LWA1PF) MBR(&WMM1)
             MONMSG     MSGID(CPF7310)

