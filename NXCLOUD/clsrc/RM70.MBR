             PGM
             DCLF       FILE(RM70)
             DCL        VAR(&FNAV)     TYPE(*CHAR) LEN(30)

             MONMSG     MSGID(CPF0001) EXEC(GOTO MELDE)
             MONMSG     MSGID(CPA0701) EXEC(GOTO START)
             MONMSG     MSGID(CPF0000) EXEC(GOTO START)
             MONMSG     MSGID(CPF9999) EXEC(GOTO START)

 START:      RTVDTAARA  DTAARA(*LDA (951 30)) RTNVAR(&FNAV)
             CHGVAR     VAR(&VALG)     VALUE(0)
             CHGVAR     VAR(&L_FNAV)   VALUE(&FNAV)
             SNDRCVF    RCDFMT(A1MNU)

/* Tester på funksjonstaster                                         */

/* F1                                                                */
             IF         COND(&IN01) THEN(DO)
             CALL       PGM(AA001C)
             GOTO       CMDLBL(START)
             ENDDO

/* F3                                                                */
             IF         COND(&IN03) THEN(GOTO CMDLBL(ENDPGM))

/* F5                                                                */
             IF         COND(&IN05) THEN(DO)
             CALL       PGM(AM80)
             GOTO       CMDLBL(START)
             ENDDO

/* F6                                                                */
             IF         COND(&IN06) THEN(DSPMSG)

/* F10                                                               */
             IF         COND(&IN10) THEN(CALL QCMD)

/* F12                                                               */
             IF         COND(&IN12) THEN(GOTO CMDLBL(ENDPGM))

/* Valg 1 - Utskrift av prosjektregnskap                             */

             IF         COND(&VALG *EQ 1) THEN(DO)
             CALL       PGM(RP620C)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 2 - Utskrift av saldo/budsjett                              */

             IF         COND(&VALG *EQ 2) THEN(DO)
             CALL       PGM(RP623C)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 3 - Utskrift av prosjektrapport                              */

             IF         COND(&VALG *EQ 3) THEN(DO)
             CALL       PGM(RP630C)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 4 - Prosjektmal                                              */

             IF         COND(&VALG *EQ 4) THEN(DO)
             CALL       PGM(RP520C)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg  5 Vedlikehold av prosjekt                                   */

             IF         COND(&VALG *EQ 5) THEN(DO)
             CALL       PGM(RP100R)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg  6 Vedlikehold av prosjektmal                                */

             IF         COND(&VALG *EQ 6) THEN(DO)
             CALL       PGM(RP200R)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg  7 Vedlikehold av prosjektleder                              */

             IF         COND(&VALG *EQ 7) THEN(DO)
             CALL       PGM(RP150R)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg  8 Vedlikehold av prosjekt Kategori                          */

             IF         COND(&VALG *EQ 8) THEN(DO)
             CALL       PGM(RP001R)  Parm('PRKA')
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 90                                                           */

             IF         COND(&VALG *EQ 90) THEN(DO)
             SIGNOFF
             ENDDO

/* Gå til start                                                      */

             GOTO       CMDLBL(START)

 MELDE:      CALL       PGM(AA004C)
             GOTO       CMDLBL(START)

/* Slutt                                                             */
 ENDPGM:     ENDPGM
