             pgm        parm(&fil &lib &grp)

             dcl        var(&fil)     type(*char) len(10)
             dcl        var(&lib)     type(*char) len(10)
             dcl        var(&clib)    type(*char) len(10)
             dcl        var(&grp)     type(*char) len(10)
             dcl        var(&asp)     type(*char) len(10)

             chgvar     var(&asp)     value('ASP_' *cat %sst(&lib 4 3))

             rtvjoba    curlib(&clib)
             chgcurlib  curlib(&clib)

             rvkobjaut obj(&lib/&fil) objtype(*file) user(*all) aut(*all)

             grtobjaut obj(&lib/&fil) objtype(*file) user(&grp) aut(*all)

             grtobjaut obj(&lib/&fil) objtype(*file) user(*public) aut(*exclude)

             rvkobjaut obj(&lib/&fil) objtype(*file) user(qdftown) aut(*all)

             rvkobjaut obj(&lib/&fil) objtype(*file) user(&asp) aut(*all)

 end:        endpgm
