             PGM        PARM(&MEMBR &FEIL)

/* Initiering av parametre.                                          */

             DCL        VAR(&MEMBR)  TYPE(*CHAR) LEN(10)
             DCL        VAR(&FEIL) TYPE(*CHAR) LEN(1) VALUE('0')

/* Kopierer bestillingsfil med riktig member til overføringsfil      */

             CPYF       FROMFILE(LWJEPF) TOFILE(*CURLIB/WT31ORD) +
                          FROMMBR(&MEMBR) TOMBR(WT31ORD) +
                          MBROPT(*REPLACE) CRTFILE(*YES) FMTOPT(*NOCHK)

/* Sender fil med valgt member til Jernia                            */

             SNDNETF    FILE(WT31ORD) TOUSRID((WEBCOM JERN400)) +
                          MBR(WT31ORD)
             MONMSG     MSGID(CPF8064) EXEC(DO)
             CHGVAR     VAR(&FEIL) VALUE('1')
             ENDDO

/* Sletter member etter overføring og overføringsfil                 */

             RMVM       FILE(LWJEPF) MBR(&MEMBR)
             DLTF       FILE(*CURLIB/WT31ORD)

 END:        ENDPGM
