             PGM        PARM(&FIRM &NUMM &SUFF)

/* Definering av parametere og variable                              */

             DCL        VAR(&FIRM)     TYPE(*DEC)  LEN(3 0)
/*6.nu*/     DCL        VAR(&NUMM)     TYPE(*DEC)  LEN(8 0)
             DCL        VAR(&SUFF)     TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&IN03)     TYPE(*CHAR) LEN(1)
             DCL        VAR(&BTCH)     TYPE(*CHAR) LEN(1)

/* Program for utskriftsparametere                                   */

             CALL       PGM(AP600R)    PARM('LO650' '0' '0' &IN03 &BTCH)

             IF         COND(&IN03 *EQ '1') THEN(GOTO CMDLBL(END))

/* Overstyring av printerfile                                        */

             CALL       PGM(AP601C)    PARM('LO650P' &BTCH)

/* Kaller opp program                                                */

             CALL       PGM(LO650R)    PARM(&FIRM &NUMM &SUFF)

             CALL       PGM(AP606C)    PARM('LO650P')
 END:        ENDPGM
