             PGM        PARM(&P_PREC &P_MAPP &P_FILN &P_BATC)

             DCL        VAR(&P_PREC) TYPE(*CHAR) LEN(55)
             DCL        VAR(&P_MAPP) TYPE(*CHAR) LEN(12)
             DCL        VAR(&P_FILN) TYPE(*CHAR) LEN(12)
             DCL        VAR(&P_BATC) TYPE(*CHAR) LEN(1)

             IF         COND(&P_BATC *EQ 'Y') THEN(SBMJOB CMD(CALL +
                          PGM(LL862C) PARM(&P_PREC &P_MAPP &P_FILN +
                          &P_BATC)) JOB(UTTR_SSTAT) MSGQ(*NONE))
             ELSE       CMD(CALL PGM(*LIBL/LL862C) PARM(&P_PREC    +
                          &P_MAPP &P_FILN &P_BATC))

 END:        ENDPGM
