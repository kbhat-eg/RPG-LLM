             PGM        PARM(&LPPARM &TIL &FRA &SUBJ)

             DCL        VAR(&LPPARM) TYPE(*CHAR) LEN(256)
             DCL        VAR(&TIL)    TYPE(*CHAR) LEN(50)
             DCL        VAR(&FRA)    TYPE(*CHAR) LEN(50)
             DCL        VAR(&SUBJ)   TYPE(*CHAR) LEN(250)

             DCL        VAR(&IN03) TYPE(*CHAR) LEN(1)
             DCL        VAR(&BTCH) TYPE(*CHAR) LEN(1)

/* Program for printerstyring                                        */

             CALL       PGM(AP600R) PARM('LH600' '0' '0' &IN03 &BTCH)

             CHGVAR     VAR(&BTCH) VALUE('0')

/* Generering                                                        */

             IF         COND(&BTCH *EQ '1') THEN(SBMJOB CMD(CALL +
                          PGM(LH702C) PARM(&LPPARM &TIL &FRA +
                          &SUBJ)) JOB(BEST_FORSL) MSGQ(*NONE))
             ELSE       CMD(CALL PGM(*LIBL/LH702C) PARM(&LPPARM &TIL +
                          &FRA &SUBJ))

 END:        ENDPGM
