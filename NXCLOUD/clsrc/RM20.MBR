             GOTO       CMDLBL(START)
             ENDDO

/* Valg 4 - Saldobalanse                                             */

             IF         COND(&VALG *EQ 4) THEN(DO)
             CALL       PGM(RH620C)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 5 - Resultatrapporter                                        */

             IF         COND(&VALG *EQ 5) THEN(DO)
             CALL       PGM(RR500R)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 6 - Aldersfordelt saldoliste kunder                          */

             IF         COND(&VALG *EQ 6) THEN(DO)
             CALL       PGM(RK630C)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 7 - Generalnota kunder                                       */

             IF         COND(&VALG *EQ 7) THEN(DO)
             CALL       PGM(RK690C)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 8 - Aldersfordelt saldoliste leverandører                    */

             IF         COND(&VALG *EQ 8) THEN(DO)
             CALL       PGM(RL630C)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 9 - Prosjektregnskap                                         */

             IF         COND(&VALG *EQ 9) THEN(DO)
             CALL       PGM(RP620C)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 10 - Prosjektregnskap                                        */

             IF         COND(&VALG *EQ 10) THEN(DO)
             CALL       PGM(RP520C)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 90                                                           */

             IF         COND(&VALG *EQ 90) THEN(DO)
             SIGNOFF
             ENDDO

/* Gå til start                                                      */

             GOTO       CMDLBL(START)

 MELDE:      CALL       PGM(AA004C)
             GOTO       CMDLBL(START)

/* Slutt                                                             */
 ENDPGM:     ENDPGM
