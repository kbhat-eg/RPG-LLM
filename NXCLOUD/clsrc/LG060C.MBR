/*                                                                   */
/*           Generere   INNG_FAKTURA-HODE-REGISTER                   */
/*                                                                   */
 STRPGM:     PGM        PARM(&WPFGRP &WPNULL)
/*                                                                   */
             DCL        VAR(&WPFGRP) TYPE(*CHAR) LEN(3)
             DCL        VAR(&WPNULL) TYPE(*CHAR) LEN(1)
             DCL        VAR(&WPDBIB) TYPE(*CHAR) LEN(6) VALUE('ADB400')
             DCL        VAR(&WPSBIB) TYPE(*CHAR) LEN(6) VALUE('ASLAGR' )
/*                                                                   */
             DCL        VAR(&WPRGBF) TYPE(*CHAR) LEN(6) VALUE('SIHEBF')
             DCL        VAR(&WPRGPF) TYPE(*CHAR) LEN(6) VALUE('SIHEPF')
             DCL        VAR(&WPRGLU) TYPE(*CHAR) LEN(6) VALUE('SIHELU')
             DCL        VAR(&WPRGL1) TYPE(*CHAR) LEN(6) VALUE('SIHEL1')
             DCL        VAR(&WPRGL2) TYPE(*CHAR) LEN(6) VALUE('SIHEL2')
             DCL        VAR(&WPRGL3) TYPE(*CHAR) LEN(6) VALUE('SIHEL3')
             DCL        VAR(&WPRGL4) TYPE(*CHAR) LEN(6) VALUE('SIHEL4')
             DCL        VAR(&WPRGL5) TYPE(*CHAR) LEN(6) VALUE('SIHEL5')
             DCL        VAR(&WPRGL6) TYPE(*CHAR) LEN(6) VALUE('SIHEL6')
             DCL        VAR(&WPRGL7) TYPE(*CHAR) LEN(6) VALUE('SIHEL7')
             DCL        VAR(&WPRGL8) TYPE(*CHAR) LEN(6) VALUE('SIHEL8')
             DCL        VAR(&WPRGL9) TYPE(*CHAR) LEN(6) VALUE('SIHEL9')
             DCL        VAR(&WPRGLA) TYPE(*CHAR) LEN(6) VALUE('SIHELA')
             DCL        VAR(&WPRGLK) TYPE(*CHAR) LEN(6) VALUE('SIHELK')
             DCL        VAR(&WPRGLR) TYPE(*CHAR) LEN(6) VALUE('SIHELR')
/*                                                                   */
/*           Setter bibliotek til current                            */
/*                                                                   */
             CHGVAR     VAR(%SST(&WPDBIB 4 3)) VALUE(&WPFGRP)
             CHGCURLIB  CURLIB(&WPDBIB)
/*                                                                   */
/*           Kalle opp fellesprogram                                 */
/*                                                                   */
             CALL       PGM(AG112C) PARM(&WPFGRP &WPNULL &WPDBIB +
                          &WPSBIB &WPRGBF &WPRGPF &WPRGLU &WPRGL1 +
                          &WPRGL2 &WPRGL3 &WPRGL4 &WPRGL5 &WPRGL6 +
                          &WPRGL7 &WPRGL8 &WPRGL9 &WPRGLA &WPRGLK +
                          &WPRGLR)
/*                                                                   */
 ENDPGM:     ENDPGM
