             PGM

/* Bygger hjelpefile dersom den ikke finnes:                         */

             CHKOBJ     OBJ(GKTIPF) OBJTYPE(*FILE)
             MONMSG     MSGID(CPF9815) EXEC(DO)
             CRTPF      FILE(ADB/GKTIPF) SRCFILE(ASTELE/QDDFSRC) +
                          TEXT('Returer kontoinformasjon - Input')
             ENDDO

/* Kopierer over til hjelpefile --> GKOBPF                           */

             CPYF       FROMFILE(GKTOPF) TOFILE(GKOBPF) MBROPT(*ADD)
             MONMSG     MSGID(CPF0000) /* Fanger opp ev. feil +
                          v/kopiering av tom file. */

/* Kopierer over til hjelpefile:                                     */

             CPYF       FROMFILE(GKTOPF) TOFILE(GKTIPF) +
                          MBROPT(*REPLACE)
             MONMSG     MSGID(CPF2817) /* Fanger opp ev. feil +
                          v/kopiering av tom file. */

/* Tømmer file:                                                      */

             CLRPFM     FILE(GKTOPF) /* Tømmer file GKTOPF */

/* Call av hovedprogram for konvertering av kontoinformasjon:        */

             CALL       PGM(DB810R)

/* Tømmer file:                                                      */

             CLRPFM     FILE(GKTIPF)

 ENDPGM:     ENDPGM
