             PGM        PARM(&HVA &NUM &SUF)

             DCL        VAR(&HVA)   TYPE(*CHAR) LEN(2)
             DCL        VAR(&NUM)   TYPE(*CHAR) LEN(8)
             DCL        VAR(&SUF)   TYPE(*CHAR) LEN(2)

             DCL        VAR(&HVA_D)   TYPE(*DEC) LEN(2)
             DCL        VAR(&NUM_D)   TYPE(*DEC) LEN(8)
             DCL        VAR(&SUF_D)   TYPE(*DEC) LEN(2)

             CHGVAR     VAR(&HVA_D) VALUE(&HVA)
             CHGVAR     VAR(&NUM_D) VALUE(&NUM)
             CHGVAR     VAR(&SUF_D) VALUE(&SUF)

             CALL       PGM(FO100R) PARM(&HVA_D &NUM_D &SUF_D)
             ENDPGM
