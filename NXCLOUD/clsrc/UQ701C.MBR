             PGM        PARM(&USER &PASW &UTKO)

             DCL        VAR(&USER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&PASW) TYPE(*CHAR) LEN(10)
             DCL        VAR(&UTKO) TYPE(*CHAR) LEN(10)

             CHGUSRPRF  USRPRF(&USER) PASSWORD(&PASW) PWDEXP(*NO) +
                          OUTQ(QUSRSYS/&UTKO)

             ENDPGM
