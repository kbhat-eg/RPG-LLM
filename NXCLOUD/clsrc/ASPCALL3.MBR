/*--------------------------------------------------------------------------*/
/*       130516 bkv  Koblet til kommandoen ASPCALL                          */
/*                   Gir mulighet til å kjøre CALL med begrenset bruker     */
/*                                                                          */
/*       CRTCMD CMD(ASKORR700/ASPCALL3) PGM(*LIBL/ASPCALL3)                 */
/*       SRCFILE(ASKORR700/QCMDSRC) ALWLMTUSR(*YES)                         */
/*                                                                          */
/*--------------------------------------------------------------------------*/

             PGM        parm(&PGMP &PARM1 &PARM2 &PARM3)
             DCL        VAR(&PGMP)     TYPE(*CHAR) LEN(10)
             DCL        VAR(&PARM1)    TYPE(*CHAR) LEN(3)
             DCL        VAR(&PARM2)    TYPE(*CHAR) LEN(8)
             DCL        VAR(&PARM3)    TYPE(*CHAR) LEN(2)

/*           IF         COND(&PGMP *EQ 'WRKSPLF') THEN(DO)                  */
/*              WRKSPLF                                                     */
/*              GOTO       CMDLBL(ENDPGM)                                   */
/*           ENDDO                                                          */

             CALL       PGM(&PGMP) PARM(&PARM1 &PARM2 &PARM3)


 ENDPGM:     ENDPGM
