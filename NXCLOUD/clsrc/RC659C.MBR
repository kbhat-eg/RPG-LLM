             PGM         parm(&selsk)

/* Definere parametere og variabler                                  */

             DCL        VAR(&IN03)  TYPE(*CHAR) LEN(1)
             DCL        VAR(&BTCH)  TYPE(*CHAR) LEN(1)
             DCL        VAR(&WSID)  TYPE(*CHAR) LEN(10)
             DCL        VAR(&WALI)  TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&WAPO)  TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&WANP)  TYPE(*DEC)  LEN(1 0) VALUE(0)
             DCL        VAR(&MLD1)  TYPE(*CHAR) LEN(40)
             DCL        VAR(&MLD2)  TYPE(*CHAR) LEN(40)
             DCL        VAR(&MLD3)  TYPE(*CHAR) LEN(50)
             DCL        VAR(&MLD4)  TYPE(*CHAR) LEN(50)
             DCL        VAR(&IN12)  TYPE(*CHAR) LEN(1)
             DCL        VAR(&SVAR)  TYPE(*CHAR) LEN(1)
             dcl        var(&selsk) TYPE(*CHAR) LEN(10)

/* Varselbilde 1                                                     */

             IF         COND(&BTCH *NE '1') THEN(DO)
             CHGVAR     VAR(&WALI) VALUE(4)
             CHGVAR     VAR(&WAPO) VALUE(44)
             CHGVAR     VAR(&MLD1) VALUE('I n k a s s o ...........')
             CHGVAR     VAR(&MLD2) VALUE('Legger ut betalinger ....')
             CALL       PGM(AA011C)  PARM(&WALI &WAPO &MLD1 &MLD2)
             ENDDO

/* Overstyring av printerfile                                        */

             CALL       PGM(AP601C)    PARM('RC659P' &BTCH)

             chkobj     obj(*libl/sfdebug) objtype(*file)
             monmsg     msgid(cpf9801) exec(do)
               crtpf    file(*curlib/sfdebug) rcdlen(128)
             enddo

/* Utskrift                                                          */

             CALL       PGM(RC659R) PARM(&WANP &selsk)

             IF         COND(&WANP *NE 1) THEN(DO)
             CHGVAR     VAR(&MLD3) VALUE('Ingen poster lagt ut for overføring')
             CHGVAR     VAR(&MLD4) VALUE('  Venligst bekreft.        ')
             CALL       PGM(AA007R)  PARM(&MLD3 &MLD4 &SVAR &IN12)
             GOTO       CMDLBL(ENDPGM)
             ENDDO

             call       pgm(aa005r)  Parm('Innbetalinger er lagt ut til +
                          overføring inkassobyrå')

             CALL       PGM(AP606C)    PARM('RC659P')

 ENDPGM:     ENDPGM
