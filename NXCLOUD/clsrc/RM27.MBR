             PGM
             DCLF       FILE(RM27)
             DCL        VAR(&FNAV)     TYPE(*CHAR) LEN(30)
             dcl        var(&fgr)      type(*char) len(3)
             dcl        var(&frm)      type(*char) len(3)
             dcl        var(&lib)      type(*char) len(10) value('ADB')
             dcl        var(&sts)      type(*char) len(1)
             dcl        var(&aspusr)   type(*char) len(10)

             DCL        VAR(&MODU)     TYPE(*CHAR) LEN(10)
             DCL        VAR(&DISP)     TYPE(*CHAR) LEN(1)
             DCL        VAR(&STAT)     TYPE(*CHAR) LEN(1)
             DCL        VAR(&jsta)     TYPE(*dec)  LEN(1 0) value(0)
             DCL        VAR(&jkey)     TYPE(*CHAR) LEN(41)  value(' ')
             DCL        VAR(&asp)      TYPE(*CHAR) LEN(1)   value(' ')

             MONMSG     MSGID(CPD0170) EXEC(GOTO MELDE2)
             MONMSG     MSGID(CPF0001) EXEC(GOTO MELDE)
             MONMSG     MSGID(CPA0701) EXEC(GOTO START)
             MONMSG     MSGID(CPF0000) EXEC(GOTO START)
             MONMSG     MSGID(CPF9999) EXEC(GOTO START)

 START:      RTVDTAARA  DTAARA(*LDA (951 30)) RTNVAR(&FNAV)

 /*      Sjekk om figruppe og firma har feiltranser fra Compello  */
             rtvdtaara  dtaara(*lda (931 3))  rtnvar(&fgr)
             rtvdtaara  dtaara(*lda (944 3))  rtnvar(&frm)
             chgvar     var(%substring(&lib 4 3)) value(&fgr)
             ovrdbf     file(rcoml2)  tofile(&lib/rcoml2)
             call       pgm(xc050r) parm(&frm &sts)
             chgvar     var(&in41) value('0')
             if         cond(&sts = 'F') then(do)
               chgvar   var(&in41) value('1')
             enddo

 /*      Dersom ASP-bruker legges flere valg ut på meny           */
             rtvdtaara  dtaara(*lda (911 3))  rtnvar(&aspusr)
             if         cond(&aspusr = 'ASP') then(do)
               chgvar   var(&asp) value('1')
             enddo
             if         cond(&aspusr = 'SST') then(do)
               chgvar   var(&asp) value('1')
             enddo
             if         cond(&aspusr = 'TME') then(do)
               chgvar   var(&asp) value('1')
             enddo

             CHGVAR     VAR(&VALG)     VALUE(0)
             CHGVAR     VAR(&L_FNAV)   VALUE(&FNAV)
             SNDRCVF    RCDFMT(A1MNU)

/* Tester på funksjonstaster                                         */

/* F1                                                                */
             IF         COND(&IN01) THEN(DO)
             CALL       PGM(AA001C)
             GOTO       CMDLBL(START)
             ENDDO

/* F3                                                                */
             IF         COND(&IN03) THEN(GOTO CMDLBL(ENDPGM))

/* F5                                                                */
             IF         COND(&IN05) THEN(DO)
             CALL       PGM(AM80)
             GOTO       CMDLBL(START)
             ENDDO

/* F6                                                                */
             IF         COND(&IN06) THEN(DSPMSG)

/* F10                                                               */
             IF         COND(&IN10) THEN(CALL QCMD)

/* F12                                                               */
             IF         COND(&IN12) THEN(GOTO CMDLBL(ENDPGM))


/* Valg  1 - Se på poster fra Compello                               */

             IF         COND(&VALG *EQ 1) THEN(DO)
             CALL       PGM(XC070R)
             GOTO       CMDLBL(START)
             ENDDO
/* Valg  2 - Overføring av posteringer fra Compello - Scanning   */

             IF         COND(&VALG *EQ 2) THEN(DO)
             CHGVAR     VAR(&MODU)     VALUE(CMP)
             CHGVAR     VAR(&DISP)     VALUE(1)
             CHGVAR     VAR(&STAT)     VALUE(0)
             CALL       PGM(AX700R) PARM(&MODU &DISP &STAT)
             IF         COND(&STAT *EQ '1') THEN(DO)
             GOTO       CMDLBL(START)
             ENDDO
             CALL       PGM(RF1TCOMP) PARM('COMP' &jkey &jsta)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg  3 - Registrering/Justering                                  */

             IF         COND(&VALG *EQ 3) THEN(DO)
             CALL       PGM(RF010C) PARM(COMP)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg  4 - Kontrolliste                                            */

             IF         COND(&VALG *EQ 4) THEN(DO)
             CALL       PGM(RF1KONTR) PARM(COMP)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg  5  - Journalliste                                           */

             IF         COND(&VALG *EQ 5) THEN(DO)
             CALL       PGM(RF1JOURN) PARM(COMP)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg  6  - Justere feiltranser fra Compello                       */

             IF         COND(&VALG *EQ 6) THEN(DO)
             ovrdbf     file(rcoml1)  tofile(&lib/rcoml1)
             ovrdbf     file(rcomlr)  tofile(&lib/rcomlr)
             ovrdbf     file(rcomlu)  tofile(&lib/rcomlu)
             CALL       PGM(xc060r)
             CALL       PGM(RF1TCOMP) PARM('COMP' &jkey &jsta)
             GOTO       CMDLBL(START)
             ENDDO

/* Valg  8  - Vise overførte bilag fra Compello                */

             if         cond(&valg *eq 8) then(do)
              if         cond(&asp *eq '1') then(do)
               runqry     qry(xc001q) qryfile((rcmtpf *first))
               runqry     qry(xc002q) qryfile((rcmtpf *first)) +
                          rcdslt(*yes)
              enddo
              goto       cmdlbl(start)
             enddo

/* Valg 11  - Se på fil RCOMPF                                       */

             IF         COND(&VALG *EQ 11) THEN(DO)
              if         cond(&asp *eq '1') then(do)
               dsppfm     file(&lib/rcompf)
              enddo
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 12  - Se på fil RCMPPF                                       */

             IF         COND(&VALG *EQ 12) THEN(DO)
              if         cond(&asp *eq '1') then(do)
               dsppfm     file(&lib/rcmppf)
              enddo
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 13  - Bygge fil RCOMPF                                       */

             IF         COND(&VALG *EQ 13) THEN(DO)
              if         cond(&asp *eq '1') then(do)
               call       pgm(rgcomc) parm(&fgr ' ')
              enddo
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 14  - Bygge fil RCMPPF i filgruppe                          */

             IF         COND(&VALG *EQ 14) THEN(DO)
              if         cond(&asp *eq '1') then(do)
               call       pgm(rgcmpc) parm(&fgr ' ')
              enddo
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 15  - Se på fil RCMTPF                                       */

             IF         COND(&VALG *EQ 15) THEN(DO)
              if         cond(&asp *eq '1') then(do)
               dsppfm     file(&lib/rcmtpf)
              enddo
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 16  - Se på fil RCMDPF                                       */

             IF         COND(&VALG *EQ 16) THEN(DO)
              if         cond(&asp *eq '1') then(do)
               dsppfm     file(&lib/rcmdpf)
              enddo
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 17  - Bygge fil RCMTPF                                       */

             IF         COND(&VALG *EQ 17) THEN(DO)
              if         cond(&asp *eq '1') then(do)
               call       pgm(rgcmtc) parm(&fgr ' ')
              enddo
              GOTO       CMDLBL(START)
             ENDDO

/* Valg 18  - Bygge fil RCMDPF i filgruppe                          */

             IF         COND(&VALG *EQ 18) THEN(DO)
              if         cond(&asp *eq '1') then(do)
               call       pgm(rgcmdc) parm(&fgr ' ')
              enddo
              GOTO       CMDLBL(START)
             ENDDO

/* Valg 19  - Bygge fil RCMRPF i filgruppe                          */

             IF         COND(&VALG *EQ 19) THEN(DO)
              if         cond(&asp *eq '1') then(do)
               call       pgm(rgcmrc) parm(&fgr ' ')
              enddo
              GOTO       CMDLBL(START)
             ENDDO

/* Valg 21  - Se på fil RCCOPF m/DFU                                 */

             IF         COND(&VALG *EQ 21) THEN(DO)
              if         cond(&asp *eq '1') then(do)
               upddta     file(rcompf)
              enddo
              GOTO       CMDLBL(START)
             ENDDO

/* Valg 25  - Se på fil RCMtl1 m/DFU                                 */

             IF         COND(&VALG *EQ 25) THEN(DO)
              if         cond(&asp *eq '1') then(do)
               upddta     file(rcmtl1)
              enddo
              GOTO       CMDLBL(START)
             ENDDO

/* Valg 26  - Se på fil RCMDPF m/DFU                                 */

             IF         COND(&VALG *EQ 26) THEN(DO)
              if         cond(&asp *eq '1') then(do)
               upddta     file(rcmdpf)
              enddo
              GOTO       CMDLBL(START)
             ENDDO

/* Valg 90                                                           */

             IF         COND(&VALG *EQ 90) THEN(DO)
             SIGNOFF
             ENDDO

/* Gå til start                                                      */

             GOTO       CMDLBL(START)

 MELDE:      CALL       PGM(AA004C)
             GOTO       CMDLBL(START)

 MELDE2:     CALL       PGM(AA00SC)
             GOTO       CMDLBL(START)

/* Slutt                                                             */

 ENDPGM:     ENDPGM
