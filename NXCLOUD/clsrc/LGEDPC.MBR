/*-------------------------------------------------------------------*/
/* ADBxxx                                                            */
/*            Dette programmet generer tabeller/indexer              */
/*  som ikke finnes i gitt bibliotek                                 */
/*                                                                   */
/* Programmet setter riktig grupperettigheter på tabell/index        */
/* Programmet starter journalføring av nye tabeller                  */
/*                                                                   */
/*-------------------------------------------------------------------*/

 STRPGM:     PGM        PARM(&FGRP &GRP &NULL)

             DCL        VAR(&FGRP)  TYPE(*CHAR) LEN(3)
             DCL        VAR(&GRP)   TYPE(*CHAR) LEN(7)
             DCL        VAR(&NULL)  TYPE(*CHAR) LEN(1)

             DCL        VAR(&LIB)   TYPE(*CHAR) LEN(50) VALUE('ADB')

             MONMSG     MSGID(CPF7030) /*  already being journaled  */

 /*          CHGVAR     VAR(%SST(&LIB 4 3)) VALUE(&FGRP)                  */


 /*LPHOST - Pakkseddel hode                                               */
             CALL PGM(AG908C) PARM('LPHOST' &FGRP '*NONE' ' ')

 /*LPPAST - Pakkseddel partner informasjon                                */
             CALL PGM(AG908C) PARM('LPPAST' &FGRP '*NONE' ' ')

 /*LPKOST - Pakkseddel kolli informasjon                                  */
             CALL PGM(AG908C) PARM('LPKOST' &FGRP '*NONE' ' ')

 /*LPKLST - Pakkseddel kolli linje informasjon                            */
             CALL PGM(AG908C) PARM('LPKLST' &FGRP '*NONE' ' ')

 /*LLPAST - Pakkseddelnivå pr leverandør                                  */
             CALL PGM(AG908C) PARM('LLPAST' &FGRP '*NONE' ' ')

 /*LBHOST - Bestillinger pr pakkseddel                                    */
             CALL PGM(AG908C) PARM('LBHOST' &FGRP '*NONE' ' ')

 /*LBLIST - Bestillinger pr pakkseddel - linjer                           */
             CALL PGM(AG908C) PARM('LBLIST' &FGRP '*NONE' ' ')

 /*LB2DST - Bestillinger varemottatte - linjer                            */
             CALL PGM(AG908C) PARM('LB2DST' &FGRP '*NONE' ' ')

 /*LBREST - Pakkseddel arb.reg                                            */
             CALL PGM(AG908C) PARM('LBREST' &FGRP '*NONE' ' ')

 ENDPGM:     ENDPGM
