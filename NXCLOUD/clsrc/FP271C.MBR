             pgm        parm(&firm &numm &suff &user)

/*      300517 bsa : Nytt program                                    */
/* K000 050620 bsa : Utvider med parameter for filgruppe pga LDA     */
/*                   problematikk.                                   */
/*                                                                   */

/* Initiering av parametere                                          */

             dcl        var(&firm)     type(*dec) len(3 0)
             dcl        var(&numm)     type(*dec) len(8 0)
             dcl        var(&suff)     type(*dec) len(2 0)
             dcl        var(&in03)     type(*char) len(1)
             dcl        var(&btch)     type(*char) len(1)
             dcl        var(&user)     type(*char) len(10)

             dcl        var(&filg) type(*char) len(3)
             dcl        var(&curl) type(*char) len(10)
             dcl        var(&qpgm) type(*char) len(10)

             rtvjoba    curlib(&curl)
             chgvar     var(&filg) value(%SST(&curl 4 3))
             chgvar     var(&qpgm) value('IQASP_' *tcat &filg *tcat +
                          'Q')

             call       pgm(&qpgm) parm(&user)


/* Overstyring av printerfile                                        */

             call       pgm(ap600r) parm('FP27B' '0' '0' &in03 &btch)

/* Kaller opp program for kontrolliste                               */

/*7.01       call       pgm(fp271r)    parm(&firm &numm &suff)      */
/*7.01*/     call       pgm(fp272r)    parm(&firm &numm &suff &filg)


end:         ENDPGM
