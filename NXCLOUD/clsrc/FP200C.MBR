             PGM

/* Initiering av parametere                                          */

             DCL        VAR(&IN03)     TYPE(*CHAR) LEN(1)
             DCL        VAR(&LAGR)     TYPE(*CHAR) LEN(2)
             DCL        VAR(&ALLE)     TYPE(*CHAR) LEN(1)
             DCL        VAR(&BTCH)     TYPE(*CHAR) LEN(1)
             DCL        VAR(&RUTI)     TYPE(*CHAR) LEN(10)
             DCL        VAR(&PROG)     TYPE(*CHAR) LEN(10)

             CALL       PGM(FP200R) PARM(&IN03 &LAGR &ALLE)

             IF         COND(&IN03 *EQ '1') THEN(GOTO CMDLBL(END))

             CALL       PGM(FP205R)

/* Henter standardrutine for etiketter                               */

             RTVDTAARA  DTAARA(*LDA (800 10)) RTNVAR(&RUTI)

             CALL       PGM(AP600R) PARM(&RUTI '1' '1' &IN03 &BTCH)

             RTVDTAARA  DTAARA(*LDA (871 10)) RTNVAR(&PROG)

             IF         COND(&IN03 *EQ '1') THEN(GOTO CMDLBL(END))


/* Call av utplukksprogram                                           */

             SBMJOB     CMD(CALL PGM(FP208C) PARM(&LAGR &ALLE)) +
                        JOB(ETIKETT)
/*           CALL PGM(FP208C) PARM(&LAGR &ALLE)                   */

 END:        ENDPGM
