             PGM        PARM(&LIST)

/* Initiering av parametre.                                          */

             DCL        VAR(&LIST) TYPE(*CHAR) LEN(120)

             DCL        VAR(&IN03) TYPE(*CHAR) LEN(1)
             DCL        VAR(&PROG) TYPE(*CHAR) LEN(10)
             DCL        VAR(&RAPP) TYPE(*CHAR) LEN(10)
             DCL        VAR(&BTCH) TYPE(*CHAR) LEN(1)

             DCL        VAR(&VALG)  TYPE(*CHAR) LEN(1)

/* Henter rapporttype fra parameter                                  */

             CHGVAR     VAR(&VALG) VALUE(%SUBSTRING(&LIST 4 1))

/* Behandling avhengig av rapporttype                                */

/* Rapporttype 1 */

             IF         COND(&VALG *EQ '1') THEN(DO)
             CHGVAR     VAR(&PROG) VALUE('JV601R')
             CHGVAR     VAR(&RAPP) VALUE('JV601P')
             ENDDO

/* Rapporttype 2 */

             IF         COND(&VALG *EQ '2') THEN(DO)
             CHGVAR     VAR(&PROG) VALUE('JV602R')
             CHGVAR     VAR(&RAPP) VALUE('JV602P')
             ENDDO

/* Oppdaterer LDA med navn p√• printfile                              */

             CHGDTAARA  DTAARA(*LDA (846 10)) VALUE(&RAPP)

/* Oppkall av utskriftsparametre.                                    */

             CALL       PGM(AP600R) PARM(&PROG '1' '0' &IN03 &BTCH)

/* Dersom F3 skal ikke rapporten skrives                             */

             IF         COND(&IN03 *EQ '1') THEN(GOTO CMDLBL(END))

/* Behandler utskrift avhengig av rapporttype                        */

/* Rapporttype 1 */

             IF         COND(&VALG *EQ '1') THEN(DO)
             IF         COND(&BTCH *EQ '1') THEN(SBMJOB CMD(CALL +
                          PGM(JV601C) PARM(&LIST)) +
                          JOB(UTSKR_VARE) MSGQ(*NONE))
             ELSE       CMD(CALL PGM(*LIBL/JV601C) PARM(&LIST))
             ENDDO

/* Rapporttype 2 */

             IF         COND(&VALG *EQ '2') THEN(DO)
             IF         COND(&BTCH *EQ '1') THEN(SBMJOB CMD(CALL +
                          PGM(JV602C) PARM(&LIST)) +
                          JOB(UTSKR_VARE) MSGQ(*NONE))
             ELSE       CMD(CALL PGM(*LIBL/JV602C) PARM(&LIST))
             ENDDO

 END:        ENDPGM
