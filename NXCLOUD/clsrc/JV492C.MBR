             PGM        PARM(&WPKODE &WPPROG)
/*                                                                   */
             DCL        VAR(&WPKODE) TYPE(*CHAR) LEN(1)
             DCL        VAR(&WPPROG) TYPE(*CHAR) LEN(10)
/*                                                                   */
             CHGVAR     VAR(&WPKODE)   VALUE(' ')
/*                                                                   */
             CHKOBJ     OBJ(*LIBL/&WPPROG) OBJTYPE(*PGM)
             MONMSG     MSGID(CPF9801) EXEC(GOTO CMDLBL(FEIL))
             GOTO       CMDLBL(ENDPGM)
/*                                                                   */
FEIL:        CHGVAR     VAR(&WPKODE)   VALUE('X')
/*                                                                   */
ENDPGM:      ENDPGM
