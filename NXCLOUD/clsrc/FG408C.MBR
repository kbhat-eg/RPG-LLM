
/*           Generere   VARESORTIMENT-LINJE                          */

 STRPGM:     PGM        PARM(&FGRP &NULL)

             DCL        VAR(&FGRP) TYPE(*CHAR) LEN(3)
             DCL        VAR(&NULL) TYPE(*CHAR) LEN(1)
             DCL        VAR(&DBIB) TYPE(*CHAR) LEN(6) VALUE('ADB400')
             DCL        VAR(&SBIB) TYPE(*CHAR) LEN(6) VALUE('ASOFAK')

             DCL        VAR(&RGBF) TYPE(*CHAR) LEN(6) VALUE('VSDTBF')
             DCL        VAR(&RGPF) TYPE(*CHAR) LEN(6) VALUE('VSDTPF')
             DCL        VAR(&RGLU) TYPE(*CHAR) LEN(6) VALUE('VSDTLU')
             DCL        VAR(&RGLR) TYPE(*CHAR) LEN(6) VALUE('VSDTLR')
             DCL        VAR(&RGL1) TYPE(*CHAR) LEN(6) VALUE('VSDTL1')
             DCL        VAR(&RGL2) TYPE(*CHAR) LEN(6) VALUE('VSDTL2')
             DCL        VAR(&RGL3) TYPE(*CHAR) LEN(6) VALUE('VSDTL3')
             DCL        VAR(&RGL4) TYPE(*CHAR) LEN(6) VALUE('VSDTL4')
             DCL        VAR(&RGL5) TYPE(*CHAR) LEN(6) VALUE('VSDTL5')
             DCL        VAR(&RGL6) TYPE(*CHAR) LEN(6) VALUE('VSDTL6')
             DCL        VAR(&RGL7) TYPE(*CHAR) LEN(6) VALUE('VSDTL7')

/*           Setter bibliotek til current                            */

             CHGVAR     VAR(%SST(&DBIB 4 3)) VALUE(&FGRP)
             CHGCURLIB  CURLIB(&DBIB)

/*           Kalle opp fellesprogram                                 */

             CALL       PGM(AG108C) PARM(&FGRP &NULL &DBIB +
                             &SBIB &RGBF &RGPF &RGLU &RGLR +
                             &RGL1 &RGL2 &RGL3 &RGL4 &RGL5 +
                             &RGL6 &RGL7)

 ENDPGM:     ENDPGM
