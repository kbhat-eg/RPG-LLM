             PGM        PARM(&REOR)

             DCL        VAR(&REOR) TYPE(*CHAR) LEN(1)
             DCL        VAR(&WJOB) TYPE(*CHAR) LEN(10)

/* Henter jobb-nummer                                                */

             RTVJOBA    JOB(&WJOB)

/* Submitter kall til styreprogram                                   */

             SBMJOB     CMD(CALL PGM(JV702C) PARM(&REOR &WJOB)) +
                          JOB(INNL_NOBB) JOBPTY(6) OUTPTY(6) +
                          MSGQ(*NONE)

 END:        ENDPGM
