/*                                                                   */
 STRPGM:     PGM        PARM(&ver)

             DCL        VAR(&klib) TYPE(*CHAR) LEN(10)
             DCL        VAR(&slib) TYPE(*CHAR) LEN(10)
             DCL        VAR(&ver)  TYPE(*CHAR) LEN(3)
             DCL        VAR(&ofg)  TYPE(*CHAR) LEN(3)
             DCL        VAR(&odl)  TYPE(*CHAR) LEN(10)

             rtvdtaara  dtaara(*lda (931 3)) rtnvar(&ofg)
             rtvdtaara  dtaara(*lda (934 10)) rtnvar(&odl)

 /*  Hvilke bibliotek skal ha korrigeringene                              */
             chgvar     var(&klib)           value('ASKIRR700')
             chgvar     var(&slib)           value('MVAXML_630')
             if         cond(&ver *eq '800') then(do)
               chgvar     var(&klib)           value('NXKORR')
               chgvar     var(&slib)           value('MVAXML_800')
             enddo

             addlible   lib(nxc_distr)
             monmsg     msgid(cpf0000)

             call       pgm(egd15c)  parm(&slib &klib)


 /*  Generere div ifler i alle filgrupper på denne versjon                */
             call       pgm(ag930c) parm('NXLOPF    ' &ver)

             call       pgm(ag930c) parm('RAT5PF    ' &ver)

             call       pgm(ag930c) parm('RAL5PF    ' &ver)


 /*  Generere logsikfile i alle filgrupper på denne versjon                */
             call       pgm(ag931c) parm('RHTRLF    ' &ver)


 END:
             chgdtaara  dtaara(*lda (931 03)) value(&ofg)
             chgdtaara  dtaara(*lda (934 10)) value(&odl)
             chgcurlib  curlib(&odl)
             ENDPGM
