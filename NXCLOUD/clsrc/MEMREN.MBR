
             PGM        PARM(&MEMB &FIRMA)

             DCL        VAR(&MEMB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&NYMEM)  TYPE(*CHAR) LEN(10)
             DCL        VAR(&FIRMA)  TYPE(*CHAR) LEN(3)

             CHGVAR     VAR(%SST(&NYMEM 1 4)) VALUE('TELE')
             CHGVAR     VAR(%SST(&NYMEM 5 3)) VALUE(&FIRMA)
             CHGVAR     VAR(%SST(&NYMEM 9 2)) VALUE('  ')

             RNMM       FILE(RBUNL1) MBR(&MEMB) NEWMBR(&NYMEM)
             MONMSG     MSGID(CPF0000)

             RNMM       FILE(RBUNL2) MBR(&MEMB) NEWMBR(&NYMEM)
             MONMSG     MSGID(CPF0000)

             RNMM       FILE(RBUNLU) MBR(&MEMB) NEWMBR(&NYMEM)
             MONMSG     MSGID(CPF0000)

             RNMM       FILE(RBUNPF) MBR(&MEMB) NEWMBR(&NYMEM)
             MONMSG     MSGID(CPF0000)

             RNMM       FILE(RTRAL1) MBR(&MEMB) NEWMBR(&NYMEM)
             MONMSG     MSGID(CPF0000)

             RNMM       FILE(RTRALU) MBR(&MEMB) NEWMBR(&NYMEM)
             MONMSG     MSGID(CPF0000)

             RNMM       FILE(RTRAPF) MBR(&MEMB) NEWMBR(&NYMEM)
             MONMSG     MSGID(CPF0000)

             ENDPGM
