             pgm        parm(&memb &stat)

             dcl        var(&memb)   type(*char) len(10)
             dcl        var(&stat)   type(*char) len(1)
             dcl        var(&mem1)   type(*char) len(1)
             dcl        var(&mem2)   type(*char) len(1)

/* Sjekker membere       */

             chgvar     var(&mem1) value('1')
             chgvar     var(&mem2) value('1')

             chkobj     obj(fpm2pf) objtype(*file) mbr(&memb)
             monmsg     msgid(cpf9815) exec(do)
             chgvar     var(&mem1) value('0')
             enddo

             chkobj     obj(fpsopf) objtype(*file) mbr(&memb)
             monmsg     msgid(cpf9815) exec(do)
             chgvar     var(&mem2) value('0')
             enddo

             if         cond(&mem1 *eq '1') then(do)
             chgvar     var(&stat) value('1')
             enddo

             if         cond(&mem2 *eq '1') then(do)
             chgvar     var(&stat) value('1')
             enddo


             ENDPGM
