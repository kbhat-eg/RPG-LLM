             PGM        PARM(&REOR &WJOB)

             DCL        VAR(&REOR) TYPE(*CHAR) LEN(1)
             DCL        VAR(&WJOB) TYPE(*CHAR) LEN(10)

/* Kall til styreprogram                                             */

             CALL       PGM(JV702R)

/* Kaller reorganiseringrutine dersom dette er valgt                 */

             IF         COND(&REOR = '1') THEN(CALL PGM(JREORG))

/* Ferdigmelding                                                     */

             SNDBRKMSG  MSG('ASVADM: Innlesing av NOBB +
                          integrasjonsfiler er ferdig') TOMSGQ(&WJOB)

 END:        ENDPGM
