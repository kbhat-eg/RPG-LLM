             PGM        PARM(&LPPARM &LKODE)
/*                                                                   */
             DCL        VAR(&LPPARM) TYPE(*CHAR) LEN(20)
             DCL        VAR(&LKODE)  TYPE(*CHAR) LEN(1)
             DCL        VAR(&BTCH) TYPE(*CHAR) LEN(1)
             DCL        VAR(&WSID) TYPE(*CHAR) LEN(10)

/* Program for overstyring av printerfile                            */

             CALL       PGM(AP601C) PARM('LU602P' &BTCH)

/* Call til program for oppdatering                                  */

             IF         COND(&LKODE *EQ '1') THEN(DO)
                CALL       PGM(LE703R) PARM(&LPPARM)
             ENDDO

/* Call til program for utskrift                                     */

             CALL       PGM(LU602R) PARM(&LPPARM &LKODE)

/* Melding dersom bakgrunnsjobb                                      */

             IF         COND(&BTCH *EQ '1') THEN(DO)
             RTVDTAARA  DTAARA(*LDA (901 10)) RTNVAR(&WSID)
             SNDBRKMSG  MSG('ASLAGR: Utskrift av tellebunt +
                          er ferdig') TOMSGQ(&WSID)
             ENDDO

/* Program for Ã¥ fjerne overstyring av printerfile                   */

             CALL       PGM(AP606C) PARM('LU602P')

/*                                                                   */
             ENDPGM
