             PGM        PARM(&FIRM &FGRP &RKAT &FEIL)

/* Initiering av parametre.                                          */

             DCL        VAR(&FIRM) TYPE(*DEC)  LEN(3 0)
             DCL        VAR(&FGRP) TYPE(*CHAR) LEN(3)
             DCL        VAR(&RKAT) TYPE(*DEC)  LEN(3 0)
             DCL        VAR(&FEIL) TYPE(*CHAR) LEN(1)

             DCL        VAR(&LIBL) TYPE(*CHAR) LEN(6) VALUE('ADBxxx')

             MONMSG     MSGID(CPF0000) EXEC(GOTO CMDLBL(ERR))

/* Overrider rabattkategori                                          */

             CHGVAR     VAR(%SST(&LIBL 4 3)) VALUE(&FGRP)

             CHKOBJ     OBJ(&LIBL/RA06L1) OBJTYPE(*FILE)

             OVRDBF     FILE(RA06L1) TOFILE(&LIBL/RA06L1) +
                          OVRSCOPE(*CALLLVL)

/* Sjekker om rabattkategori finnes                                  */

             CALL       PGM(FR192R) PARM(&FIRM &RKAT &FEIL)

             GOTO       CMDLBL(END)

 ERR:        CHGVAR     VAR(&FEIL) VALUE('1')

 END:        ENDPGM
