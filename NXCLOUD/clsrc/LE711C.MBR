             DCL        VAR(&WPTYPE) TYPE(*CHAR) LEN(1)
             DCL        VAR(&LPPARM) TYPE(*CHAR) LEN(64)
             DCL        VAR(&BTCH)     TYPE(*CHAR) LEN(1)

/* Overstyring av printerfile   */

             CALL       PGM(AP601C)    PARM('LE711P' &BTCH)
/*                                                                   */
/* Call til program for seleksjon                                    */
