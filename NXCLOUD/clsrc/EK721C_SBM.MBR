             PGM        PARM(&FIRM &PAAR &ALT1 &ALT2 &ALT3 &BOTY &WJOB)

             DCL        VAR(&FIRM) TYPE(*CHAR) LEN(3)
             DCL        VAR(&PAAR) TYPE(*CHAR) LEN(4)
             DCL        VAR(&ALT1) TYPE(*CHAR) LEN(1)
             DCL        VAR(&ALT2) TYPE(*CHAR) LEN(1)
             DCL        VAR(&ALT3) TYPE(*CHAR) LEN(1)
             DCL        VAR(&BOTY) TYPE(*CHAR) LEN(2)
             DCL        VAR(&WJOB) TYPE(*CHAR) LEN(10)

             DCL        VAR(&MLD)  TYPE(*CHAR) LEN(80)


/* Kall til kalkulasjonsprogram                                      */

             IF         COND(&ALT1 *EQ '1') THEN(DO)
             CALL       PGM(EK721R) PARM(&FIRM &PAAR &ALT1 &BOTY)
             ENDDO

 END:        ENDPGM
