             PGM

             DCL        VAR(&FIG) TYPE(*CHAR) LEN(3)
             DCL        VAR(&JOB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&MLD1)     TYPE(*CHAR) LEN(50)
             DCL        VAR(&MLD2)     TYPE(*CHAR) LEN(50)
             DCL        VAR(&SVAR)     TYPE(*CHAR) LEN(1)
             DCL        VAR(&IN12)     TYPE(*DEC)  LEN(1 0)

/* Varselbilde med bekreftelse p√• om jobb skal settes i gang         */

             CHGVAR     VAR(&MLD1) +
                        VALUE('Legger ut jobb i jobbplan       ')
             CHGVAR     VAR(&MLD2) +
                        VALUE('Bekreft at dette startes! ')
             CALL       PGM(AA007R)    PARM(&MLD1 &MLD2 &SVAR &IN12)

             IF         COND(&IN12 *EQ 1 ) +
                          THEN(GOTO CMDLBL(ENDPGM))

             IF         COND(&SVAR *EQ 'J') +
                          THEN(GOTO CMDLBL(VIDERE))
             ELSE       CMD(GOTO CMDLBL(ENDPGM))

 VIDERE:     RTVDTAARA  DTAARA(*LDA (931 3)) RTNVAR(&FIG)
             CHGVAR     VAR(&JOB) VALUE('NAVSYNC' *CAT &FIG)

             ADDJOBSCDE JOB(&JOB) CMD(CALL PGM(NXCLOUD/IT100C) +
                          PARM(&FIG)) FRQ(*WEEKLY) SCDDATE(*NONE) +
                          SCDDAY(*ALL) SCDTIME(070000) +
                          JOBD(*LIBL/NAVSYNC) +
                          JOBQ(*LIBL/NAVSYNC) USER(ASP) +
                          TEXT('Synkronisering av NAV til Nexstep')

 ENDPGM:     ENDPGM
