
/*           Generere   ORDRE-LINJE-REGISTER                         */

 STRPGM:     PGM        PARM(&FGRP &NULL)

             DCL        VAR(&FGRP) TYPE(*CHAR) LEN(3)
             DCL        VAR(&NULL) TYPE(*CHAR) LEN(1)
             DCL        VAR(&DBIB) TYPE(*CHAR) LEN(6) VALUE('ADB400')
             DCL        VAR(&SBIB) TYPE(*CHAR) LEN(6) VALUE('ASOFAK')

             DCL        VAR(&RGBF) TYPE(*CHAR) LEN(6) VALUE('FODTBF')
             DCL        VAR(&RGPF) TYPE(*CHAR) LEN(6) VALUE('FODTPF')
             DCL        VAR(&RGLU) TYPE(*CHAR) LEN(6) VALUE('FODTLU')
             DCL        VAR(&RGLR) TYPE(*CHAR) LEN(6) VALUE('FODTLR')
             DCL        VAR(&RGLK) TYPE(*CHAR) LEN(6) VALUE('FODTLK')
             DCL        VAR(&RGLL) TYPE(*CHAR) LEN(6) VALUE('FODTLL')
             DCL        VAR(&RGLV) TYPE(*CHAR) LEN(6) VALUE('FODTLV')
             DCL        VAR(&RGL1) TYPE(*CHAR) LEN(6) VALUE('FODTL1')
             DCL        VAR(&RGL2) TYPE(*CHAR) LEN(6) VALUE('FODTL2')
             DCL        VAR(&RGLG) TYPE(*CHAR) LEN(6) VALUE('FODTLG')
             DCL        VAR(&RGLX) TYPE(*CHAR) LEN(6) VALUE('FODTLX')

/*           Setter bibliotek til current                            */

             CHGVAR     VAR(%SST(&DBIB 4 3)) VALUE(&FGRP)
             CHGCURLIB  CURLIB(&DBIB)

/*           Kalle opp fellesprogram                                 */

             CALL       PGM(AG108C) PARM(&FGRP &NULL &DBIB +
                             &SBIB &RGBF &RGPF &RGLU &RGLR +
                             &RGLK &RGLL &RGLV &RGL1 &RGLG +
                             &RGL2 &RGLX)

 ENDPGM:     ENDPGM
