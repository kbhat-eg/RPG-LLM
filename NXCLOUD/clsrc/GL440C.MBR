             PGM        PARM(&WPPARM)
             DCL        VAR(&WPFIRM) TYPE(*DEC)  LEN(3 0)
             DCL        VAR(&WPFFIR) TYPE(*CHAR) LEN(3)
             DCL        VAR(&WPFNAV) TYPE(*CHAR) LEN(30)
             DCL        VAR(&WPPARM) TYPE(*CHAR) LEN(1)
             DCL        VAR(&WPUSER) TYPE(*CHAR) LEN(10)
             DCL        VAR(&WMM1) TYPE(*CHAR) LEN(10) VALUE('TELE   ')
/*                                                                   */
             IF         COND(&WPPARM *EQ 'L') THEN(DO)
                        CHGVAR VAR(&WMM1) VALUE('LONN   ')
             ENDDO
/*                                                                   */
             RTVDTAARA  DTAARA(*LDA (944 03)) RTNVAR(&WPFFIR)
             RTVDTAARA  DTAARA(*LDA (951 30)) RTNVAR(&WPFNAV)
             RTVDTAARA  DTAARA(*LDA (911 10)) RTNVAR(&WPUSER)
             CHGVAR     VAR(&WPFIRM) VALUE(&WPFFIR)
             CHGVAR     VAR(%SST(&WMM1 5 3)) VALUE(&WPFFIR)
/*                                                                   */
/* Bygger membere dersom de ikke finnes                              */
             CHKOBJ     OBJ(RBUNPF) OBJTYPE(*FILE) MBR(&WMM1)
             MONMSG     MSGID(CPF9815) EXEC(DO)
             ADDPFM     FILE(RBUNPF) MBR(&WMM1) TEXT('Poster fra +
                          renteberegning (RK435)')
             ENDDO
             CHKOBJ     OBJ(RBUNL1) OBJTYPE(*FILE) MBR(&WMM1)
             MONMSG     MSGID(CPF9815) EXEC(DO)
             ADDLFM     FILE(RBUNL1) MBR(&WMM1) DTAMBRS((RBUNPF +
                          (&WMM1))) TEXT('Poster fra renteberegning +
                          (RK535)')
             ENDDO
             CHKOBJ     OBJ(RBUNLU) OBJTYPE(*FILE) MBR(&WMM1)
             MONMSG     MSGID(CPF9815) EXEC(DO)
             ADDLFM     FILE(RBUNLU) MBR(&WMM1) DTAMBRS((RBUNPF +
                          (&WMM1))) TEXT('Poster fra renteberegning +
                          (RK535)')
             ENDDO
             CHKOBJ     OBJ(RTRAPF) OBJTYPE(*FILE) MBR(&WMM1)
             MONMSG     MSGID(CPF9815) EXEC(DO)
             ADDPFM     FILE(RTRAPF) MBR(&WMM1) TEXT('Poster fra +
                          renteberegning (RK435)')
             ENDDO
             CHKOBJ     OBJ(RTRAL1) OBJTYPE(*FILE) MBR(&WMM1)
             MONMSG     MSGID(CPF9815) EXEC(DO)
             ADDLFM     FILE(RTRAL1) MBR(&WMM1) DTAMBRS((RTRAPF +
                          (&WMM1))) TEXT('Poster fra renteberegning +
                          (RK535)')
             ENDDO
             CHKOBJ     OBJ(RTRALU) OBJTYPE(*FILE) MBR(&WMM1)
             MONMSG     MSGID(CPF9815) EXEC(DO)
             ADDLFM     FILE(RTRALU) MBR(&WMM1) DTAMBRS((RTRAPF +
                          (&WMM1))) TEXT('Poster fra renteberegning +
                          (RK535)')
             ENDDO
/*                                                                   */
/* Overide databasefile til aktuelt member                           */
/*                                                                   */
             OVRDBF     FILE(RBUNL1) MBR(&WMM1)
             OVRDBF     FILE(RBUNLU) MBR(&WMM1)
             OVRDBF     FILE(RTRAL1) MBR(&WMM1)
             OVRDBF     FILE(RTRALU) MBR(&WMM1)
/*                                                                   */
/* Call av bilagsregistrering                                        */
/*                                                                   */
             CALL       PGM(RF010R) PARM(&WPFIRM &WPFNAV &WPUSER)
/*                                                                   */
 ENDPGM:     ENDPGM
