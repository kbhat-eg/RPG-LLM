             PGM        PARM(&PLIST)

/* Initiering av parametre og variable                               */

             DCL        VAR(&PLIST) TYPE(*CHAR) LEN(260)
             DCL        VAR(&NATT)  TYPE(*CHAR) LEN(1) VALUE('1')

/* Kaller program for kopiering av vareinformasjon                   */

             CALL       PGM(VV781R) PARM(&PLIST &NATT)

/* Kaller program for sletting av varer                              */

             CALL       PGM(VV787R) PARM(&PLIST)

 END:        ENDPGM
