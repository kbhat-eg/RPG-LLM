             ENDDO
/* Valg 12 - lister/sletter                                          */
             IF         COND(&VALG *EQ 12) THEN(DO)
             CALL       PGM(JX480R) PARM('S')
             GOTO       CMDLBL(START)
             ENDDO

/* Valg 90                                                           */
             IF         COND(&VALG *EQ 90) THEN(DO)
             SIGNOFF
             ENDDO

