             DCL        VAR(&PARM)     TYPE(*CHAR) LEN(64)
             DCL        VAR(&BTCH)     TYPE(*CHAR) LEN(1)

/* Overstyring av printerfile                                        */

             CALL       PGM(AP601C)    PARM('RK634P' &BTCH)

/* Kaller opp program for kontrolliste                               */

             CALL       PGM(RK634R)    PARM(&PARM)

             CALL       PGM(AP606C)    PARM('RK634P')
