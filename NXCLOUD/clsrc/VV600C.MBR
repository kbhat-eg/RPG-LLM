             PGM        PARM(&LIST &MEMB)

/* Initiering av parametre.                                          */

             DCL        VAR(&LIST)     TYPE(*CHAR) LEN(80)
             DCL        VAR(&MEMB)     TYPE(*CHAR) LEN(10)
             DCL        VAR(&UTSK)     TYPE(*DEC)  LEN(1 0)
             DCL        VAR(&IN03)     TYPE(*CHAR) LEN(1)
             DCL        VAR(&RAPP)     TYPE(*CHAR) LEN(10)
             DCL        VAR(&BTCH)     TYPE(*CHAR) LEN(1)

/* Initierer variabel for utskrift som er valgt                      */

             CHGVAR     VAR(&UTSK)     VALUE(%SUBSTRING(&LIST 64 1))

/* Oppkall av utskriftsparametre.                                    */

             IF         COND(&UTSK *EQ 1) THEN(DO)
             CALL       PGM(AP600R)    PARM('VV603' '1' '0' &IN03 &BTCH)
             CHGVAR     VAR(&RAPP)     VALUE('VV603P')
             ENDDO

             IF         COND(&UTSK *EQ 2) THEN(DO)
             CALL       PGM(AP600R)    PARM('VV605' '1' '0' &IN03 &BTCH)
             CHGVAR     VAR(&RAPP)     VALUE('VV605P')
             ENDDO


/* Dersom F3 eller F12 skal ikke rapporten skrives                   */

             IF         COND(&IN03 *EQ '1') THEN(GOTO CMDLBL(END))

/* Oppdaterer LDA med navn på printfile                              */

             CHGDTAARA  DTAARA(*LDA (846 10)) VALUE(&RAPP)

/* Henter opplysning om utskriften skal kjøres i batch fra LDA       */

             RTVDTAARA  DTAARA(*LDA (863 1)) RTNVAR(&BTCH)

/* Utskrift                                                          */

             IF         COND(&BTCH *EQ '1') THEN(SBMJOB CMD(CALL +
                          PGM(VV601C) PARM(&LIST &MEMB)) +
                          JOB(UTSKR_VARE) MSGQ(*NONE))
             ELSE       CMD(CALL PGM(*LIBL/VV601C) PARM(&LIST &MEMB))

 END:        ENDPGM
