             PGM        PARM(&TYPE &EDAT)

/* Definere parametere og variabler                                  */

             DCL        VAR(&BTCH)     TYPE(*CHAR) LEN(1)
             DCL        VAR(&TYPE)     TYPE(*CHAR) LEN(1)
             DCL        VAR(&EDAT)     TYPE(*CHAR) LEN(10)
             DCL        VAR(&WSID)     TYPE(*CHAR) LEN(10)

/* Overstyring av printerfile                                        */

             CALL       PGM(AP601C)    PARM('RC601P' &BTCH)

/* Utskrift                                                          */

             CALL       PGM(RC601R) PARM(&TYPE &EDAT)

/* Melding dersom bakgrunnsjobb                                      */

             IF         COND(&BTCH *EQ '1') THEN(DO)
             RTVDTAARA  DTAARA(*LDA (901 10)) RTNVAR(&WSID)
             SNDBRKMSG  MSG('ASPGPL: Rutinen for sletting av kunder +
                        er ferdig') TOMSGQ(&WSID)
             ENDDO

             CALL       PGM(AP606C)    PARM('RC601P')
 END:        ENDPGM
