             PGM        PARM(&GLIN &RES)
             DCL        VAR(&GLIN)     TYPE(*CHAR) LEN(10)
             DCL        VAR(&RES)      TYPE(*CHAR) LEN(10)

             DCLF       FILE(ASPGPLK/HDWRSC)
             MONMSG     MSGID(CPF3142) EXEC(GOTO CMDLBL(LOOP_END))

/*   Finner ressurs for evt. tidligere ASP-linje                     */

LOOP_START:  RCVF

             MONMSG     MSGID(CPF0864) EXEC(GOTO LOOP_END)
             IF         COND(%SUBSTRING(&COCFGO 1 6) *EQ 'ASPLIN') +
                        THEN(DO)
                CHGVAR     VAR(&GLIN)  VALUE(&COCFGO)
                CHGVAR     VAR(&RES)   VALUE(&CORNAM)
                GOTO       CMDLBL(LOOP_END)
             ENDDO

             IF         COND(%SUBSTRING(&COCFGO 1 6) *EQ 'LINASP') +
                        THEN(DO)
                CHGVAR     VAR(&GLIN)  VALUE(&COCFGO)
                CHGVAR     VAR(&RES)   VALUE(&CORNAM)
                GOTO       CMDLBL(LOOP_END)
             ENDDO

             GOTO       CMDLBL(LOOP_START)

 LOOP_END:   ENDPGM
