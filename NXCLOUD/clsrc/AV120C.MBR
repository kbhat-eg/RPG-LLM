             PGM
             DCLF       FILE(AV120D)
             DCL        VAR(&MLD1)     TYPE(*CHAR) LEN(40)
             DCL        VAR(&MLD2)     TYPE(*CHAR) LEN(40)
             DCL        VAR(&WALI)     TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&WAPO)     TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&VER)      TYPE(*CHAR) LEN(3)
             dcl        var(&m)        type(*char) len(50)

             RTVDTAARA  DTAARA(*LDA (931 3)) RTNVAR(&A1FGR)
             RTVDTAARA  DTAARA(*LDA (1003 3)) RTNVAR(&A1MLJ)
             RTVOBJD    OBJ(JVARPF) OBJTYPE(*FILE) RTNLIB(&A1LVR)

             call       pgm(AR700R) parm(&A1MLJ &A1VER)

 BILDE:      SNDRCVF    RCDFMT(A1BLD)

/* Tester på funksjonstaster                                         */
/* F3                                                                */
             IF         COND(&IN03) THEN(GOTO CMDLBL(ENDPGM))
/* F12                                                               */
             IF         COND(&IN12) THEN(GOTO CMDLBL(ENDPGM))

             IF         COND((&A1DLTID *EQ 0) *AND (&A1DLTVI *EQ 0) *AND +
                          (&A1DLTTR *EQ 0) *AND (&A1CRTID *EQ 0) *AND +
                          (&A1CRTVI *EQ 0) *AND (&A1CRTTR *EQ 0)) +
                          THEN(GOTO CMDLBL(BILDE))

             if         cond(&a1crttr *eq 1) then(do)
               chgvar     var(&m) value('Oppdat. grunnlag for triggere +
                          kjøres. Ca 1 min.')
               call       pgm(aa005r) parm(&m)

               call       pgm(sqldef)
             enddo

              CHGVAR     VAR(&WALI) VALUE(14)
              CHGVAR     VAR(&WAPO) VALUE(24)
              CHGVAR     VAR(&MLD1) VALUE('Databasejobb           ')
              CHGVAR     VAR(&MLD2) VALUE('er nå sendt på jobbkø . .  ')
              CALL       PGM(AA006C)  PARM(&WALI &WAPO &MLD1 &MLD2)

/*               DLYJOB     DLY(03)                                           */

              CHGVAR     VAR(&VER) VALUE(%CHAR(&A1VER))

              CALL       PGM(AV121C) PARM(&A1DLTID &A1DLTVI &A1DLTTR +
                           &A1CRTID &A1CRTVI &A1CRTTR &A1FGR &A1LVR &A1MLJ +
                           &A1TABL &VER)

/*               GOTO       BILDE                                             */
/* Slutt                                                             */

 ENDPGM:     ENDPGM
