/*                                                                   */
/*           Generere   RADIOTERMINAL ORDRE-HODE FIL                 */
/*                                                                   */
 STRPGM:     PGM        PARM(&WPFGRP &WPNULL)
/*                                                                   */
             DCL        VAR(&WPFGRP) TYPE(*CHAR) LEN(3)
             DCL        VAR(&WPNULL) TYPE(*CHAR) LEN(1)
             DCL        VAR(&WPDBIB) TYPE(*CHAR) LEN(6) VALUE('ADB400')
             DCL        VAR(&WPSBIB) TYPE(*CHAR) LEN(6) VALUE('ASLAGR' )
/*                                                                   */
             DCL        VAR(&WPRGBF) TYPE(*CHAR) LEN(6) VALUE('HOHEBF')
             DCL        VAR(&WPRGPF) TYPE(*CHAR) LEN(6) VALUE('HOHEPF')
             DCL        VAR(&WPRGLU) TYPE(*CHAR) LEN(6) VALUE('HOHELU')
             DCL        VAR(&WPRGL1) TYPE(*CHAR) LEN(6) VALUE('HOHEL1')
             DCL        VAR(&WPRGL2) TYPE(*CHAR) LEN(6) VALUE('HOHEL2')
             DCL        VAR(&WPRGL3) TYPE(*CHAR) LEN(6) VALUE('HOHELR')
/*                                                                   */
/*           Setter bibliotek til current                            */
/*                                                                   */
             CHGVAR     VAR(%SST(&WPDBIB 4 3)) VALUE(&WPFGRP)
             CHGCURLIB  CURLIB(&WPDBIB)
/*                                                                   */
/*           Kalle opp fellesprogram                                 */
/*                                                                   */
             CALL       PGM(AG103C) PARM(&WPFGRP &WPNULL &WPDBIB +
                          &WPSBIB &WPRGBF &WPRGPF &WPRGLU &WPRGL1 +
                          &WPRGL2 &WPRGL3)

             STRJRNPF   FILE(HOHEPF) JRN(BUTIJRN) OMTJRNE(*OPNCLO)
/*                                                                   */
 ENDPGM:     ENDPGM
