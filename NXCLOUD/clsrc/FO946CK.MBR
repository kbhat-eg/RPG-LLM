             PGM        PARM(&JKEY &PARM)

/* Definering av parametere og variable                              */
/*6.nu*/     DCL        VAR(&PARM)     TYPE(*CHAR) LEN(34)
             DCL        var(&Jkey)     type(*char) len(41)
             dcl        var(&jsta)     type(*dec) len(2 0)
             dcl        var(&jmld)     type(*char) len(200)

             chgvar     var(&jsta) value(10)
             chgvar     var(&jmld) value('Starter FO946RK for kopi av +
                          faktura, parameter: ' *cat &parm *cat ' jobid: ' +
                          *cat &jkey)
             call       pgm(ab705r) parm(&jkey &jsta &jmld)

/* Kaller opp program for faktura                                    */
             CALL       PGM(FO946RK)    PARM(&PARM &JKEY)

             chgvar     var(&jsta) value(10)
             chgvar     var(&jmld) value('Returnerer fra FO946CK')
             call       pgm(ab705r) parm(&jkey &jsta &jmld)
             ENDPGM
