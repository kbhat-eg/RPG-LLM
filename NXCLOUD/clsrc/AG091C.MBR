
/*           Generere   XML Overf√∏ringsoversikt                      */

 STRPGM:     PGM        PARM(&FGRP &NULL)

             DCL        VAR(&FGRP) TYPE(*CHAR) LEN(3) VALUE('   ')
             DCL        VAR(&NULL) TYPE(*CHAR) LEN(1)
             DCL        VAR(&DBIB) TYPE(*CHAR) LEN(6) VALUE('ADB400')
             DCL        VAR(&SBIB) TYPE(*CHAR) LEN(6) VALUE('ASPGPL')

             DCL        VAR(&RGBF) TYPE(*CHAR) LEN(6) VALUE('AXMLBF')
             DCL        VAR(&RGPF) TYPE(*CHAR) LEN(6) VALUE('AXMLPF')
             DCL        VAR(&RGLU) TYPE(*CHAR) LEN(6) VALUE('AXMLLU')
             DCL        VAR(&RGL1) TYPE(*CHAR) LEN(6) VALUE('AXMLL1')
             DCL        VAR(&RGLR) TYPE(*CHAR) LEN(6) VALUE('AXMLLR')

/*           Setter bibliotek til current                            */

             CHGVAR     VAR(%SST(&DBIB 4 3)) VALUE(&FGRP)
             CHGCURLIB  CURLIB(&DBIB)

/*           Kalle opp fellesprogram                                 */

             CALL       PGM(AG102C) PARM(&FGRP &NULL &DBIB &SBIB +
                          &RGBF &RGPF &RGLU &RGL1 &RGLR)

 ENDPGM:     ENDPGM
