             PGM        PARM(&USER &XKUN &VSOR &DATO)

             DCL        VAR(&USER)     TYPE(*CHAR) LEN(10)
             DCL        VAR(&XKUN)     TYPE(*CHAR) LEN(6)
             DCL        VAR(&FIRM)     TYPE(*DEC)  LEN(3 0)
             DCL        VAR(&KUND)     TYPE(*DEC)  LEN(6 0)
             DCL        VAR(&VSOR)     TYPE(*CHAR) LEN(10)
             DCL        VAR(&DATO)     TYPE(*CHAR) LEN(10)

             DCL        VAR(&KPRO)     TYPE(*DEC)  LEN(6 0)
             DCL        VAR(&OGRP)     TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&HGRP)     TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&UGRP)     TYPE(*DEC)  LEN(3 0)
             DCL        VAR(&LDOR)     TYPE(*DEC)  LEN(6 0)
             DCL        VAR(&RFMT)     TYPE(*CHAR) LEN(10)
             DCL        VAR(&ENDR)     TYPE(*DEC)  LEN(1 0)
             DCL        VAR(&DAT2)     TYPE(*CHAR) LEN(10)
             DCL        VAR(&SVAL)     TYPE(*DEC)  LEN(1 0)
             DCL        VAR(&TVAL)     TYPE(*DEC)  LEN(1 0)
             DCL        VAR(&XVAL)     TYPE(*DEC)  LEN(1 0)
             DCL        VAR(&BVAL)     TYPE(*DEC)  LEN(1 0)
             DCL        VAR(&FTIS)     TYPE(*CHAR) LEN(26)
             DCL        VAR(&FLDR)     TYPE(*CHAR) LEN(12)
             DCL        VAR(&MEMB)     TYPE(*CHAR) LEN(10)
             DCL        VAR(&PTIME)    TYPE(*CHAR) LEN(12)
             DCL        VAR(&LETY)     TYPE(*DEC)  LEN(1 0)
             DCL        VAR(&LAGE)     TYPE(*DEC)  LEN(2 0)

/* Setter biblioteksliste                                            */

             CALL       PGM(AA722C) PARM(&USER)

             CHGVAR     VAR(&KUND)     VALUE(&XKUN)
             CHGVAR     VAR(&KPRO)     VALUE(0)
             CHGVAR     VAR(&OGRP)     VALUE(0)
             CHGVAR     VAR(&HGRP)     VALUE(0)
             CHGVAR     VAR(&UGRP)     VALUE(0)
             CHGVAR     VAR(&LDOR)     VALUE(0)
             CHGVAR     VAR(&RFMT)     VALUE('INTFILE')
             CHGVAR     VAR(&ENDR)     VALUE(0)
             CHGVAR     VAR(&DAT2)     VALUE(' ')
             CHGVAR     VAR(&SVAL)     VALUE(0)
             CHGVAR     VAR(&TVAL)     VALUE(0)
             CHGVAR     VAR(&XVAL)     VALUE(0)
             CHGVAR     VAR(&BVAL)     VALUE(0)
             CHGVAR     VAR(&FTIS) VALUE('0001-01-01-00.00.00.000000')
             CHGVAR     VAR(&FLDR)     VALUE(' ')
             CHGVAR     VAR(&MEMB)     VALUE(' ')

/* Henter dagens dato fra systemet                                   */

             RTVSYSVAL  SYSVAL(QTIME) RTNVAR(&PTIME)
             CHGVAR     VAR(%SST(&MEMB 1 1)) VALUE('P')
             CHGVAR     VAR(%SST(&MEMB 2 9)) VALUE(%SST(&PTIME 1 9))

             CALL       PGM(VP682C) PARM(&FIRM &KUND &KPRO &VSOR +
                          &OGRP &HGRP &UGRP &LDOR &RFMT &ENDR &DAT2 +
                          &SVAL &TVAL &XVAL &BVAL &LETY &DATO &LAGE +
                          &FTIS &FLDR &MEMB)

 END:        ENDPGM
