             PGM        PARM(&MEMBR &ONUMM &OSUFF &OTYPE &ADKOD +
                             &AKTIV &LEVUK)

/* Initiering av parametre.                                          */

             DCL        VAR(&MEMBR)    TYPE(*CHAR) LEN(10)
             DCL        VAR(&ONUMM)    TYPE(*DEC)  LEN(6 0)
             DCL        VAR(&OSUFF)    TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&OTYPE)    TYPE(*CHAR) LEN(4)
             DCL        VAR(&ADKOD)    TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&AKTIV)    TYPE(*CHAR) LEN(2)
             DCL        VAR(&LEVUK)    TYPE(*DEC)  LEN(4 0)

/* Bygger member og overstyrer                                       */

             CHKOBJ     OBJ(LWJEPF) OBJTYPE(*FILE) MBR(&MEMBR)
             MONMSG     MSGID(CPF9815) EXEC(DO)
             ADDPFM     FILE(LWJEPF) MBR(&MEMBR) TEXT('Bestilling +
                          i Webtrade-format')
             ENDDO
             OVRDBF     FILE(LWJEPF) TOFILE(LWJEPF) MBR(&MEMBR)

/* Kaller genereringsprogram                                        */

             CALL       PGM(LO750R) PARM(&ONUMM &OSUFF &OTYPE &ADKOD +
                                         &AKTIV &LEVUK)
             DLTOVR     FILE(LWJEPF)

 END:        ENDPGM
