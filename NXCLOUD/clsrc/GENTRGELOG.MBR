
/*           Gen trigger til endringslogg                        */

 strpgm:     pgm

             monmsg    msgid(cpf32c6)
             rmvpftrg  file(rkunpf) trg(Rkun_Trg_Elog_U)
             rmvpftrg  file(rlevpf) trg(Rlev_Trg_Elog_U)
             rmvpftrg  file(fohepf) trg(Fohe_Trg_Elog_U)
             rmvpftrg  file(lohepf) trg(Lohe_Trg_Elog_U)

             addpftrg file(rkunpf) trgtime(*after) +
                      trgevent(*update) pgm(rkuntru) +
                      trg(Rkun_Trg_Elog_U)

             addpftrg file(rlevpf) trgtime(*after) +
                      trgevent(*update) pgm(rlevtru) +
                      trg(Rlev_Trg_Elog_U)

             addpftrg file(fohepf) trgtime(*after) +
                      trgevent(*update) pgm(fohetru) +
                      trg(Fohe_Trg_Elog_U)

             addpftrg file(Lohepf) trgtime(*after) +
                      trgevent(*update) pgm(lohetru) +
                      trg(Lohe_Trg_Elog_U)


endpgm:      endpgm
