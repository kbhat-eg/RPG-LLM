             PGM

/* Definere parametere og variabler                                  */

             DCL        VAR(&IN03)     TYPE(*CHAR) LEN(1)
             DCL        VAR(&TYPE)     TYPE(*CHAR) LEN(1)
             DCL        VAR(&BTCH)     TYPE(*CHAR) LEN(1)

/* Call av parameterprogram                                          */

             CALL       PGM(RL622R) PARM(&IN03)

             IF         COND(&IN03 *EQ '1') THEN(GOTO CMDLBL(END))

/* Program for printerstyring                                        */

             RTVDTAARA  DTAARA(*LDA (524 1)) RTNVAR(&TYPE)
             IF         COND(&TYPE *EQ '4') THEN(DO)
             CALL       PGM(AP600R) PARM('RL623_L' '1' '0' &IN03 &BTCH)
             GOTO       CMDLBL(NESTE)
             ENDDO
             CALL       PGM(AP600R) PARM('RL623 ' '1' '0' &IN03 &BTCH)
 NESTE:

             IF         COND(&IN03 *EQ '1') THEN(GOTO CMDLBL(END))

/* Utskrift                                                          */

             IF         COND(&BTCH *EQ '1') THEN(SBMJOB CMD(CALL +
                          PGM(RL623C)) JOB(LIST_LEVER) MSGQ(*NONE))
             ELSE       CMD(CALL PGM(*LIBL/RL623C))

 END:        ENDPGM
