             PGM        PARM(&VALG)

/* Definerer parametre:                                              */

             DCL        VAR(&VALG) TYPE(*CHAR) LEN(1)
             DCL        VAR(&FEIL) TYPE(*CHAR) LEN(1)
             DCL        VAR(&MBER)  TYPE(*CHAR) LEN(10)
             DCL        VAR(&FIRM)  TYPE(*CHAR) LEN(3)

/* Bygger membernøkkel:                                              */

             RTVDTAARA  DTAARA(*LDA (944 3)) RTNVAR(&FIRM)
             CHGVAR     VAR(%SST(&MBER 1 4)) VALUE('TELE')
             CHGVAR     VAR(%SST(&MBER 5 3)) VALUE(&FIRM)

/* Tester på om member finnes i rbunpf          :                    */

             CHGVAR     VAR(&FEIL) VALUE(' ')
             CALL       PGM(GL705R) PARM(&MBER &FEIL)

             IF         COND(&FEIL *EQ '1') THEN(DO)
             CHGVAR     VAR(&VALG) VALUE('1')
             ENDDO
 ENDPGM:     ENDPGM
