
/*           Generere   HISTORISK LAGERBOK                           */
/* 110919    nho        Ny logisk fil  VHISL5                        */

 STRPGM:     PGM        PARM(&FGRP &NULL)

             DCL        VAR(&FGRP) TYPE(*CHAR) LEN(3)
             DCL        VAR(&NULL) TYPE(*CHAR) LEN(1)
             DCL        VAR(&DBIB) TYPE(*CHAR) LEN(6) VALUE('ADB400')
             DCL        VAR(&SBIB) TYPE(*CHAR) LEN(6) VALUE('ASOFAK')

             DCL        VAR(&RGBF) TYPE(*CHAR) LEN(6) VALUE('VHISBF')
             DCL        VAR(&RGPF) TYPE(*CHAR) LEN(6) VALUE('VHISPF')
             DCL        VAR(&RGLU) TYPE(*CHAR) LEN(6) VALUE('VHISLU')
             DCL        VAR(&RGLR) TYPE(*CHAR) LEN(6) VALUE('VHISLR')
             DCL        VAR(&RGL1) TYPE(*CHAR) LEN(6) VALUE('VHISL1')
             DCL        VAR(&RGL2) TYPE(*CHAR) LEN(6) VALUE('VHISL2')
             DCL        VAR(&RGL3) TYPE(*CHAR) LEN(6) VALUE('VHISL3')
             DCL        VAR(&RGL4) TYPE(*CHAR) LEN(6) VALUE('VHISL4')
             DCL        VAR(&RGL5) TYPE(*CHAR) LEN(6) VALUE('VHISL5')

/*           Setter bibliotek til current                            */

             CHGVAR     VAR(%SST(&DBIB 4 3)) VALUE(&FGRP)
             CHGCURLIB  CURLIB(&DBIB)

/*           Kalle opp fellesprogram                                 */

             CALL       PGM(AG105C) PARM(&FGRP &NULL &DBIB +
                             &SBIB &RGBF &RGPF &RGLU &RGLR +
                             &RGL1 &RGL2 &RGL3 &RGL4 &RGL5)

 ENDPGM:     ENDPGM
