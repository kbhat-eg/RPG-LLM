             PGM        PARM(&FIRM &VARE &EKOD)

             DCL        VAR(&FIRM)     TYPE(*DEC)  LEN(3 0)
             DCL        VAR(&VARE)     TYPE(*CHAR) LEN(15)
             DCL        VAR(&EKOD)     TYPE(*CHAR) LEN(3)

/* Blanker retur-variabel                                            */

             CHGVAR     VAR(&EKOD)     VALUE('   ')

/* Kaller program i lokalt vareregister                              */

             CALL       PGM(JW700R)    PARM(&FIRM &VARE &EKOD)
             MONMSG     MSGID(CPF0001)

 END:        ENDPGM
