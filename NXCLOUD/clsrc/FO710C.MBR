             PGM        PARM(&FIRM &VARE_NY &VARE_GML)

             DCL        VAR(&FIRM)     TYPE(*DEC)  LEN(3 0)
             DCL        VAR(&VARE_NY)  TYPE(*CHAR) LEN(15)
             DCL        VAR(&VARE_GML) TYPE(*CHAR) LEN(15)

/* Blanker retur-variabel                                            */

             CHGVAR     VAR(&VARE_GML) VALUE('               ')

/* Kaller program i lokalt vareregister                              */

             CALL       PGM(JO760R) PARM(&FIRM &VARE_NY &VARE_GML)
             MONMSG     MSGID(CPF0001)

 END:        ENDPGM
