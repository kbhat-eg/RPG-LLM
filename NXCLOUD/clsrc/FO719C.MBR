             PGM        PARM(&FORM)

             DCL        VAR(&FORM) TYPE(*CHAR) LEN(1)

/* Kaller program i lokalt vareregister for utskrift av rapport      */
/* over varer som ikke ble lest inn i spesialpris-registeret         */

             CALL       PGM(JV680C) PARM(&FORM)
             MONMSG     MSGID(CPF0001)

 END:        ENDPGM
