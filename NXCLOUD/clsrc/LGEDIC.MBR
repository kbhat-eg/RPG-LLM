
/*           Generere tabeller for EDI behandling                     */

 STRPGM:     PGM        PARM(&FGRP &NULL)

             DCL        VAR(&FGRP) TYPE(*CHAR) LEN(3)
             DCL        VAR(&NULL) TYPE(*CHAR) LEN(1)
             DCL        VAR(&DBIB) TYPE(*CHAR) LEN(6) VALUE('ADB400')
             DCL        VAR(&grpf) TYPE(*CHAR) LEN(10)
/* Nye tabeller                                                       */
             DCL        VAR(&fil1) TYPE(*CHAR) LEN(6) VALUE('edhepf')
             DCL        VAR(&fil2) TYPE(*CHAR) LEN(6) VALUE('eddtpf')
             DCL        VAR(&fil3) TYPE(*CHAR) LEN(6) VALUE('edtxpf')
             DCL        VAR(&fil4) TYPE(*CHAR) LEN(6) VALUE('lwebpf')
             DCL        VAR(&fil7) TYPE(*CHAR) LEN(6) VALUE('gedgpf')
/* Oppdaterte tabeller                                                */
             DCL        VAR(&fil5) TYPE(*CHAR) LEN(6) VALUE('nohepf')
             DCL        VAR(&fil6) TYPE(*CHAR) LEN(6) VALUE('nodtpf')

/*           Setter bibliotek til current                             */

             CHGVAR     VAR(%SST(&DBIB 4 3)) VALUE(&FGRP)
             CHGCURLIB  CURLIB(&DBIB)

/*           Finner gruppeprofil for sikkerhet                       */
             rtvusrprf  grpprf(&grpf)

/*           Kalle opp fellesprogram                                 */

             CALL   PGM(AG906C) PARM(&Fil1 &fgrp &grpf &null)
             CALL   PGM(AG906C) PARM(&Fil2 &fgrp &grpf &null)
             CALL   PGM(AG906C) PARM(&Fil3 &fgrp &grpf &null)
             CALL   PGM(AG906C) PARM(&Fil4 &fgrp &grpf &null)
             CALL   PGM(AG906C) PARM(&Fil5 &fgrp &grpf &null)
             CALL   PGM(AG906C) PARM(&Fil6 &fgrp &grpf &null)
             CALL   PGM(AG906C) PARM(&Fil7 &fgrp &grpf &null)


 ENDPGM:     ENDPGM
