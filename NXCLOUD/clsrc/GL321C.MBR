             DCL        VAR(&FILG) TYPE(*CHAR) LEN(3)
             DCL        VAR(&FIRM) TYPE(*CHAR) LEN(3)
             DCL        VAR(&KOMM) TYPE(*CHAR) LEN(4)

             CALL       PGM(GL321R) PARM(&FILG &FIRM &KOMM)

