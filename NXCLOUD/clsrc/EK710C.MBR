             PGM        PARM(&FIRM &KUND &PAAR &BOTY &FPER &PERI)

             DCL        VAR(&FIRM) TYPE(*CHAR) LEN(3)
             DCL        VAR(&KUND) TYPE(*CHAR) LEN(6)
             DCL        VAR(&PAAR) TYPE(*CHAR) LEN(4)
             DCL        VAR(&BOTY) TYPE(*CHAR) LEN(2)
             DCL        VAR(&PERI) TYPE(*CHAR) LEN(2)
             DCL        VAR(&FPER) TYPE(*CHAR) LEN(2)

             DCL        VAR(&WJOB) TYPE(*CHAR) LEN(10)

/* Henter jobb-nummer                                                */

             RTVJOBA    JOB(&WJOB)

/* Submitter kall til kalkulasjonsprogram                            */

             SBMJOB     CMD(CALL PGM(EK711C) PARM(&FIRM &KUND &PAAR +
                          &BOTY &FPER &PERI &WJOB)) JOB(KUNDEBONUS) +
                          MSGQ(*NONE)
/*           CALL PGM(EK711C) PARM(&FIRM &KUND &PAAR +                 */
/*                        &BOTY &FPER &PERI &WJOB)                     */

 END:        ENDPGM
