             PGM        PARM(&LPPARM)

             DCL        VAR(&LPPARM) TYPE(*CHAR) LEN(256)

             DCL        VAR(&IN03) TYPE(*CHAR) LEN(1)
             DCL        VAR(&BTCH) TYPE(*CHAR) LEN(1)

/* Program for printerstyring                                        */

             CALL       PGM(AP600R) PARM('LH600' '1' '0' &IN03 &BTCH)

             IF         COND(&IN03 *EQ '1') THEN(GOTO CMDLBL(END))

/* Generering                                                        */

             IF         COND(&BTCH *EQ '1') THEN(SBMJOB CMD(CALL +
                          PGM(LH702C) PARM(&LPPARM)) JOB(BEST_FORSL) +
                          MSGQ(*NONE))
             ELSE       CMD(CALL PGM(*LIBL/LH702C) PARM(&LPPARM))

 END:        ENDPGM
