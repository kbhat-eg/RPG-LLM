             IF         COND(&BTCH *EQ '1') THEN+
                         (SBMJOB CMD(CALL PGM(RH631C) PARM(&WMM1)) +
                          JOB(LIST_HOVBK) MSGQ(*NONE))
             ELSE       CMD(CALL PGM(*LIBL/RH631C) PARM(&WMM1))

