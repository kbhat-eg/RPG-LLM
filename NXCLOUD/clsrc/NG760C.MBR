             PGM        PARM(&NUMM &SUFF &stat)
/*                                                                   */
/* Initiering av parametre.                                          */
/*                                                                   */
             DCL        VAR(&MEMB)    TYPE(*CHAR) LEN(10)
             DCL        VAR(&NUMM)    TYPE(*DEC)  LEN(8 0)
             DCL        VAR(&SUFF)    TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&stat)    TYPE(*char) LEN(1)
/*                                                                   */
/* Kaller program for å sjekke om bestilling holder datakvalitet     */
/*                                                                   */
             CALL       PGM(NG740R) PARM(&numm &suff &stat)

             if         cond(&stat *ne ' ') then(goto cmdlbl(end))

/*                                                                   */
/* Kaller program for å lage member                                  */
/*                                                                   */
             CALL       PGM(NG762R) PARM(&MEMB)
/*                                                                   */
/* Kaller genereringsprogram                                         */
/*                                                                   */
             CALL       PGM(LO760C) PARM(&memb &NUMM &SUFF)
/*                                                                   */
/* Kaller program for å sende meldingen                              */
/*                                                                   */
             CALL       PGM(NG763C) PARM(&memb &stat)
             if         cond(&stat *ne ' ') then(goto cmdlbl(end))
/*                                                                   */
/* Kaller program for oppdatering av loggfila for sendt bestilling   */
/*                                                                   */
             CALL       PGM(NG761R) PARM(&NUMM &SUFF)
/*                                                                   */
 END:        ENDPGM
