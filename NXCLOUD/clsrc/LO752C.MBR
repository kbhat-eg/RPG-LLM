             PGM        PARM(&BNUMM &MEMBR &OTYPE &FEIL)

/* Initiering av parametre.                                          */

/*6.nu*/     DCL        VAR(&BNUMM)  TYPE(*CHAR) LEN(8)
             DCL        VAR(&MEMBR)  TYPE(*CHAR) LEN(10)
             DCL        VAR(&OTYPE)  TYPE(*CHAR) LEN(4)
             DCL        VAR(&FILN)   TYPE(*CHAR) LEN(30)
             DCL        VAR(&STAT)   TYPE(*CHAR) LEN(1)
             DCL        VAR(&SMSG)  TYPE(*CHAR) LEN(1) VALUE('1')
             DCL        VAR(&FEIL) TYPE(*CHAR) LEN(1) VALUE('0')
             DCL        VAR(&OVRF) TYPE(*CHAR) LEN(10) +
                          VALUE('JERNIABEST')

             IF         COND(&OTYPE *EQ 'FOE ') THEN(DO)
             CHGVAR     VAR(&FILN) VALUE('A' *CAT &BNUMM *CAT '.rdy')
             ENDDO
             ELSE       CMD(DO)
             CHGVAR     VAR(&FILN) VALUE('O' *CAT &BNUMM *CAT '.rdy')
             ENDDO

             RNMM       FILE(LWJEPF) MBR(&MEMBR) NEWMBR(LWJEUT)

/* Sender fil med valgt member til Jernia                            */

             CALL       PGM(AF700C) PARM(&OVRF &STAT &FILN &SMSG)

             IF         COND(&STAT *NE ' ') THEN(CHGVAR VAR(&FEIL) +
                          VALUE('1'))

/* Sletter member etter overf√∏ring                                   */

             RMVM       FILE(LWJEPF) MBR(LWJEUT)

 END:        ENDPGM
