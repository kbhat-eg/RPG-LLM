/*-------------------------------------------------------------------*/
/* ADBxxx                                                            */
/*            Dette programmet generer tabeller/indexer              */
/*  som ikke finnes i gitt bibliotek                                 */
/*                                                                   */
/* Programmet setter riktig grupperettigheter på tabell/index        */
/* Programmet starter journalføring av nye tabeller                  */
/*                                                                   */
/*-------------------------------------------------------------------*/

 STRPGM:     PGM        PARM(&FGRP &GRP &NULL)

             DCL        VAR(&FGRP)  TYPE(*CHAR) LEN(3)
             DCL        VAR(&GRP)   TYPE(*CHAR) LEN(7)
             DCL        VAR(&NULL)  TYPE(*CHAR) LEN(1)

             DCL        VAR(&TABLE_N)   TYPE(*CHAR) LEN(50)
             DCL        VAR(&COLUMN_N)  TYPE(*CHAR) LEN(50)
             DCL        VAR(&LENGDE)    TYPE(*DEC) LEN(5 0)
             DCL        VAR(&POSISJON)  TYPE(*DEC) LEN(5 0)
             DCL        VAR(&FUNNET)    TYPE(*DEC) LEN(1 0)

             DCL        VAR(&LIB)   TYPE(*CHAR) LEN(10) value('ADB ')

             MONMSG     MSGID(CPF7030) /*  already being journaled  */

             CHGVAR     VAR(%SST(&LIB 4 3)) VALUE(&FGRP)

             CHGCURLIB  CURLIB(&LIB)
             MONMSG     MSGID(CPF2110) EXEC(GOTO CMDLBL(ENDPGM))

 /* RCMRPF  */
 RCMRPF:     CHKOBJ     OBJ(&LIB/RCMRPF) OBJTYPE(*FILE)
             MONMSG     MSGID(CPF9801) EXEC(DO)
                goto       cmdlbl(NESTE1)
             ENDDO

                CHGOBJOWN  OBJ(&LIB/RCMRLU) OBJTYPE(*FILE) NEWOWN(ASP)
                CHGOBJOWN  OBJ(&LIB/RCMRL1) OBJTYPE(*FILE) NEWOWN(ASP)
                CHGOBJOWN  OBJ(&LIB/RCMRPF) OBJTYPE(*FILE) NEWOWN(ASP)

                GRTOBJAUT  OBJ(&LIB/RCMRLU) OBJTYPE(*ALL) USER(&GRP) +
                             AUT(*ALL)  REPLACE(*YES)
                GRTOBJAUT  OBJ(&LIB/RCMRL1) OBJTYPE(*ALL) USER(&GRP) +
                             AUT(*ALL)  REPLACE(*YES)
                GRTOBJAUT  OBJ(&LIB/RCMRPF) OBJTYPE(*ALL) USER(&GRP) +
                             AUT(*ALL)  REPLACE(*YES)
                GRTOBJAUT  OBJ(&LIB/RCMRLU) OBJTYPE(*ALL) USER(*PUBLIC) +
                             AUT(*EXCLUDE) REPLACE(*YES)
                GRTOBJAUT  OBJ(&LIB/RCMRL1) OBJTYPE(*ALL) USER(*PUBLIC) +
                             AUT(*EXCLUDE) REPLACE(*YES)
                GRTOBJAUT  OBJ(&LIB/RCMRPF) OBJTYPE(*ALL) USER(*PUBLIC) +
                             AUT(*EXCLUDE) REPLACE(*YES)

                STRJRNPF   FILE(&LIB/RCMRPF) JRN(&LIB/BUTIJRN) +
                             OMTJRNE(*OPNCLO)
