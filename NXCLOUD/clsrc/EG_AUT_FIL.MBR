             pgm        parm(&lib &grp)

             dcl        var(&lib)     type(*char) len(10)
             dcl        var(&clib)    type(*char) len(10)
             dcl        var(&grp)     type(*char) len(10)
             dcl        var(&asp)     type(*char) len(10)

             chgvar     var(&asp)     value('ASP_' *cat %sst(&lib 4 3))

             rtvjoba    curlib(&clib)
             dspobjd    obj(&lib/*ALL) objtype(*file) output(*outfile) outfile(&lib/dspobjdf)
             chgcurlib  curlib(&lib)
             call       pgm(eg_obj_own) parm(&lib &grp)
             chgcurlib  curlib(&clib)

             rvkobjaut obj(&lib/*all) objtype(*file) user(*all) aut(*all)

             grtobjaut obj(&lib/*all) objtype(*file) user(&grp) aut(*all)

             grtobjaut obj(&lib/*all) objtype(*file) user(*public) aut(*exclude)

             rvkobjaut obj(&lib/*all) objtype(*file) user(qdftown) aut(*all)

             rvkobjaut obj(&lib/*all) objtype(*file) user(&asp) aut(*all)

 end:        endpgm
