             PGM
/*                                                                   */
             DCL        VAR(&PARM) TYPE(*CHAR) LEN(28)
             DCL        VAR(&PF03) TYPE(*DEC)  LEN(1)
             DCL        VAR(&PF12) TYPE(*DEC)  LEN(1)
             DCL        VAR(&PROG) TYPE(*CHAR) LEN(10)
/*                                                                   */
             CHGVAR     VAR(&PF03) VALUE(0)
             CHGVAR     VAR(&PF12) VALUE(0)
/*                                                                   */
/* Kaller opp Parameter-program for utskrift  (Fakt/Kred)            */
/*                                                                   */
             CALL       PGM(LO182R) PARM(&PARM &PF03)
/*                                                                   */
/* Er det tatt F3 i parameter-bildet ?                               */
/*                                                                   */
             IF         COND(&PF03 *EQ 1) THEN(GOTO CMDLBL(ENDPGM))
/*                                                                   */
/* Kaller opp AP100R                                                 */
/*                                                                   */
             CALL       PGM(AP100R) PARM(&PF03 &PF12)
/*                                                                   */
/* Hente inn fra LDA                                                 */
/*                                                                   */
             RTVDTAARA  DTAARA(*LDA (871 10)) RTNVAR(&PROG)
/*                                                                   */
/* Kaller opp Program for utskrift  (Fakt/Kred)                      */
/*                                                                   */
             CALL       PGM(&PROG) PARM(&PARM)
/*                                                                   */
ENDPGM:      ENDPGM
