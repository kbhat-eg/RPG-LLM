
/*           Generere   VARE-REGISTER                                */

 STRPGM:     PGM        PARM(&FGRP &NULL)

             DCL        VAR(&FGRP) TYPE(*CHAR) LEN(3)
             DCL        VAR(&NULL) TYPE(*CHAR) LEN(1)
             DCL        VAR(&DBIB) TYPE(*CHAR) LEN(6) VALUE('ADB400')
             DCL        VAR(&SBIB) TYPE(*CHAR) LEN(6) VALUE('ASOFAK')

             DCL        VAR(&RGBF) TYPE(*CHAR) LEN(6) VALUE('VVARBF')
             DCL        VAR(&RGPF) TYPE(*CHAR) LEN(6) VALUE('VVARPF')
             DCL        VAR(&RGLU) TYPE(*CHAR) LEN(6) VALUE('VVARLU')
             DCL        VAR(&RGLR) TYPE(*CHAR) LEN(6) VALUE('VVARLR')
             DCL        VAR(&RGL1) TYPE(*CHAR) LEN(6) VALUE('VVARL1')
             DCL        VAR(&RGL2) TYPE(*CHAR) LEN(6) VALUE('VVARL2')
             DCL        VAR(&RGL3) TYPE(*CHAR) LEN(6) VALUE('VVARL3')
             DCL        VAR(&RGL4) TYPE(*CHAR) LEN(6) VALUE('VVARL4')
             DCL        VAR(&RGL5) TYPE(*CHAR) LEN(6) VALUE('VVARL5')
             DCL        VAR(&RGL6) TYPE(*CHAR) LEN(6) VALUE('VVARL6')
             DCL        VAR(&RGL7) TYPE(*CHAR) LEN(6) VALUE('VVARL7')
             DCL        VAR(&RGL8) TYPE(*CHAR) LEN(6) VALUE('VVARL8')
             DCL        VAR(&RGL9) TYPE(*CHAR) LEN(6) VALUE('VVARL9')
             DCL        VAR(&RGLA) TYPE(*CHAR) LEN(6) VALUE('VVARLA')
             DCL        VAR(&RGLB) TYPE(*CHAR) LEN(6) VALUE('VVARLB')

/*           Setter bibliotek til current                            */

             CHGVAR     VAR(%SST(&DBIB 4 3)) VALUE(&FGRP)
             CHGCURLIB  CURLIB(&DBIB)

/*           Kalle opp fellesprogram                                 */

             CALL       PGM(AG112C) PARM(&FGRP &NULL &DBIB +
                             &SBIB &RGBF &RGPF &RGLU &RGLR +
                             &RGL1 &RGL2 &RGL3 &RGL4 &RGL5 +
                             &RGL6 &RGL7 &RGL8 &RGL9 &RGLA +
                             &RGLB)

 ENDPGM:     ENDPGM
