             PGM        PARM(&KUNR)

/* Definere parametere og variabler                                  */

             DCL        VAR(&BTCH)     TYPE(*CHAR) LEN(1)
             DCL        VAR(&KUNR)     TYPE(*DEC)  LEN(6 0)

/* Utskrift                                                          */

             CALL       PGM(AP601C)    PARM('RL636P' &BTCH)

             CALL       PGM(RL636R)    PARM(&KUNR)

             CALL       PGM(AP606C)    PARM('RL636P')

 END:        ENDPGM
