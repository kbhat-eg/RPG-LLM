
/*           Generere   STATUS 000001                                */

 STRPGM:     PGM        PARM(&FGRP &NULL)

             DCL        VAR(&FGRP) TYPE(*CHAR) LEN(3)
             DCL        VAR(&NULL) TYPE(*CHAR) LEN(1)
             DCL        VAR(&DBIB) TYPE(*CHAR) LEN(6) VALUE('ADB400')
             DCL        VAR(&SBIB) TYPE(*CHAR) LEN(6) VALUE('ASTELE')

             DCL        VAR(&RGBF) TYPE(*CHAR) LEN(6) VALUE('GKBFBF')
             DCL        VAR(&RGPF) TYPE(*CHAR) LEN(6) VALUE('GKBFPF')
             DCL        VAR(&RGLU) TYPE(*CHAR) LEN(6) VALUE('GKBFLU')
             DCL        VAR(&RGL1) TYPE(*CHAR) LEN(6) VALUE('GKBFL1')
             DCL        VAR(&RGL2) TYPE(*CHAR) LEN(6) VALUE('GKBFL2')
             DCL        VAR(&RGL3) TYPE(*CHAR) LEN(6) VALUE('GKBFL3')
             DCL        VAR(&RGL4) TYPE(*CHAR) LEN(6) VALUE('GKBFL4')
             DCL        VAR(&RGL5) TYPE(*CHAR) LEN(6) VALUE('GKBFL5')
             DCL        VAR(&RGL6) TYPE(*CHAR) LEN(6) VALUE('GKBFL6')
             DCL        VAR(&RGL7) TYPE(*CHAR) LEN(6) VALUE('GKBFL7')

/*           Setter bibliotek til current                            */

             CHGVAR     VAR(%SST(&DBIB 4 3)) VALUE(&FGRP)
             CHGCURLIB  CURLIB(&DBIB)

/*           Kalle opp fellesprogram                                 */

             CALL       PGM(AG107C) PARM(&FGRP &NULL &DBIB +
                          &SBIB &RGBF &RGPF &RGLU &RGL1 +
                          &RGL2 &RGL3 &RGL4 &RGL5 &RGL6 +
                          &RGL7)

 ENDPGM:     ENDPGM
