             pgm

/* Definering av parametere og variable                              */

             dcl        var(&numm)     type(*dec)  len(8 0)
             dcl        var(&suff)     type(*dec)  len(2 0)
             dcl        var(&stat)     type(*char) len(1) value(' ')

             chgvar     var(&numm) value(730271)
             chgvar     var(&suff) value(0)

/* Program for setting av mailinformasjon                            */

             call       pgm(lo928c)    parm(&numm &suff &stat)

 endpgm:     endpgm
