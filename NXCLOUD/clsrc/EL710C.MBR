             PGM        PARM(&FIRM &LDOR &PAAR)

             DCL        VAR(&FIRM) TYPE(*CHAR) LEN(3)
             DCL        VAR(&LDOR) TYPE(*CHAR) LEN(6)
             DCL        VAR(&PAAR) TYPE(*CHAR) LEN(4)

             DCL        VAR(&WJOB) TYPE(*CHAR) LEN(10)

/* Henter jobb-nummer                                                */

             RTVJOBA    JOB(&WJOB)

/* Submitter kall til kalkulasjonsprogram                            */

             SBMJOB     CMD(CALL PGM(EL711C) PARM(&FIRM &LDOR &PAAR +
                          &WJOB)) JOB(LEV.BONUS) MSGQ(*NONE)

 END:        ENDPGM
