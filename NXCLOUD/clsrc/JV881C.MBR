             PGM

             DCL        VAR(&MSER)  TYPE(*CHAR) LEN(50)  VALUE(' ')
             DCL        VAR(&MTO)   TYPE(*CHAR) LEN(50)  VALUE(' ')
             DCL        VAR(&MFROM) TYPE(*CHAR) LEN(50)  VALUE(' ')
             DCL        VAR(&MVEDL) TYPE(*CHAR) LEN(50)  VALUE(' ')
             DCL        VAR(&MSUBJ) TYPE(*CHAR) LEN(200) VALUE(' ')
             DCL        VAR(&MTEXT) TYPE(*CHAR) LEN(200) VALUE(' ')
             DCL        VAR(&MSTAT) TYPE(*CHAR) LEN(1)
             DCL        VAR(&STAT)  TYPE(*CHAR) LEN(1)
             DCL        VAR(&MMEDL) TYPE(*DEC)  LEN(6 0)
             DCL        VAR(&MINIF) TYPE(*CHAR) LEN(50)  VALUE(' ')
/* sett riktig firma som brukes i VA */
/* sjekk at det er mail-adresse på dette firmaet  */
             DCL        VAR(&FIRM)  TYPE(*DEC)  LEN(3)   VALUE(000)
/* sett riktig bruker for å få riktig bibl.liste og bibliotek */
             DCL        VAR(&USER) TYPE(*CHAR) LEN(10)  VALUE('ASP_MGR')
             DCL        VAR(&DLIB) TYPE(*CHAR) LEN(10)  VALUE('ADBMGR')
/*                                              */
             DCL        VAR(&SERVER)    TYPE(*CHAR) LEN(35)
             DCL        VAR(&STATUS)    TYPE(*CHAR) LEN(1)
/* adde til nødvendlig biblioteker  */

             CALL       PGM(ASPINI/AA722C)    PARM(&USER)
             ADDLIBLE   LIB(QGPL)
             ADDLIBLE   LIB(QTEMP)
/* Setter current library                                            */

             CHGCURLIB  CURLIB(&DLIB)


/* Sjekker om sist oppd.dato er eldre enn 2 dager               */
             call       pgm(JV881R) parm(&FIRM &MFROM &STAT &MSUBJ)

/* SENDER MAIL                                                       */
 /* Henter ut mailserver                                              */
             CALL       PGM(AM705C) PARM(&SERVER &STATUS)

             IF         COND(&STAT *NE ' ') THEN(DO)
/* SENDER MAIL                                                       */

             CHGVAR     VAR(&MSER) VALUE(&SERVER)
             CHGVAR     VAR(&MTO)   VALUE('firmapost@asp-as.no')
             CHGVAR     VAR(&MVEDL) VALUE(' ')
             CHGVAR     VAR(&MTEXT) VALUE(' ')
             CHGVAR     VAR(&MSTAT) VALUE(' ')
             CHGVAR     VAR(&MINIF) VALUE('/java/asp/mail/setupfeilovf.ini')
             CALL       PGM(JÅ702R) PARM(&MSER &MTO &MFROM &MSUBJ +
                          &MTEXT &MVEDL &MINIF &MSTAT)


             CHGVAR     VAR(&MSER) VALUE(&SERVER)
             CHGVAR     VAR(&MTO)   VALUE('sigmund.waago@asp-as.no')
             CHGVAR     VAR(&MVEDL) VALUE(' ')
             CHGVAR     VAR(&MTEXT) VALUE(' ')
             CHGVAR     VAR(&MSTAT) VALUE(' ')
             CHGVAR     VAR(&MINIF) VALUE('/java/asp/mail/setupfeilovf.ini')
             CALL       PGM(JÅ702R) PARM(&MSER &MTO &MFROM &MSUBJ +
                          &MTEXT &MVEDL &MINIF &MSTAT)


             ENDDO

