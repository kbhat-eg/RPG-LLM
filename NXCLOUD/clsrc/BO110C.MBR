             PGM
/*                                                                   */
/*           BOPARM=0, Sjekk i registreringsprogram                  */
/*           BOPARM=1, Sjekk i kontroll/avstemmings-liste            */
/*           BOPARM=2, Sjekk i journal-liste                         */
/*                                                                   */
             DCL        VAR(&BOPARM) TYPE(*CHAR) LEN(1) VALUE('0')
             DCL        VAR(&WPPARM) TYPE(*CHAR) LEN(1) VALUE('0')
/*                                                                   */
/* Kontrollere skjerm/kasse/bruker -tilgang                          */
/*                                                                   */
             CALL       PGM(BOSJKR) PARM(&BOPARM)
             IF         COND(&BOPARM *EQ '9') THEN(DO)
             GOTO       CMDLBL(ENDPGM)
             ENDDO
/*                                                                   */
/* Kontrollere om filegruppe etc er lagt opp                         */
/* Fjerner dette 011220   NXCLOUD                                    */
/*           CALL       PGM(BSJEKK) PARM(&WPPARM)                    */
/*           IF         COND(&WPPARM *EQ '9') THEN(DO)               */
/*           GOTO       CMDLBL(ENDPGM)                               */
/*           ENDDO                                                   */
/*                                                                   */
/* Call til program for registrering                                 */
/* av kassabonger                                                    */
/*                                                                   */
             CALL       PGM(BO110R)
/*                                                                   */
 ENDPGM:     ENDPGM
