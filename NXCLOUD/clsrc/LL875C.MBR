             PGM       (&SCAN &VARE &STAT)

/* Initiering av parametere                                          */

             DCL        VAR(&SCAN)     TYPE(*CHAR) LEN(26)
             DCL        VAR(&VARE)     TYPE(*CHAR) LEN(15)
             DCL        VAR(&STAT)     TYPE(*CHAR) LEN(1)


/*           Sjekk om  VA brukes                                  */

             CHKOBJ     OBJ(JVARPF) OBJTYPE(*FILE)
             MONMSG     MSGID(CPF9815) EXEC(DO)
             GOTO       CMDLBL(END)
             ENDDO

/* Call av utplukksprogram  - egen modul                             */
/*                                                            */
             CALL       PGM(JV875R) PARM(&SCAN &VARE &STAT)
/*                                                            */
 END:        ENDPGM
