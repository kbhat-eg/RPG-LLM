             PGM        PARM(&LIST &FILE &FLDR)

/* Initiering av parametre.                                          */

             DCL        VAR(&LIST) TYPE(*CHAR) LEN(40)
             DCL        VAR(&FILE) TYPE(*CHAR) LEN(12)
             DCL        VAR(&FLDR) TYPE(*CHAR) LEN(20)

             DCL        VAR(&WSID) TYPE(*CHAR) LEN(10)
             DCL        VAR(&tild)  TYPE(*CHAR) LEN(25)

/* Kaller program                                                    */

             CALL       PGM(SF852R) PARM(&LIST)

/* Kopierer innhold til PC-fil i mappe                               */

/*           CPYTOPCD   FROMFILE(SONOPF) TOFLR(&FLDR) TODOC(&FILE) + */
/*                        REPLACE(*YES)                              */
             chgvar     var(&tild) value(&file)
             call       pgm(asptostmf) +
                        parm('SONOPF    ' +
                             'SONOPF    ' +
                             &FLDR +
                             &tild   +
                             '   ')

/* Sender melding                                                    */

             RTVDTAARA  DTAARA(*LDA (901 10)) RTNVAR(&WSID)
             SNDBRKMSG  MSG('ASOFAK: Statistikk til fil er ferdig') +
                          TOMSGQ(&WSID)

 END:        ENDPGM
