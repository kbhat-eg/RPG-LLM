/* Initiering av parametre.                                          */
/*                                                                   */
             DCL        VAR(&WALI)   TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&WAPO)   TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&MLD1)   TYPE(*CHAR) LEN(40)
             DCL        VAR(&MLD2)   TYPE(*CHAR) LEN(40)
/*                                                                   */
/* Varselbilde                                                       */
/*                                                                   */
             CHGVAR     VAR(&WALI) VALUE(11)
             CHGVAR     VAR(&WAPO) VALUE(30)
             CHGVAR     VAR(&MLD1) VALUE('Konvertering pågår for .... ')
             CHGVAR     VAR(&MLD2) VALUE('**** RABATTGUPPEREGISTER ****')
             CALL       PGM(AA006C)  PARM(&WALI &WAPO &MLD1 &MLD2)
/*                                                                   */
/* Henter verdier i fra LDA.                                         */
/*                                                                   */
             CHGVAR VAR(%SST(&KODER 2 3)) VALUE(&FPGRUP)
