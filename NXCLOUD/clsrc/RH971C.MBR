             PGM

             DCL        VAR(&USER)  TYPE(*CHAR) LEN(10)
             DCL        VAR(&USERB) TYPE(*CHAR) LEN(10)
             DCL        VAR(&WALI) TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&WAPO) TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&MLD1) TYPE(*CHAR) LEN(40)
             DCL        VAR(&MLD2) TYPE(*CHAR) LEN(40)

             RTVDTAARA  DTAARA(*LDA (911 10)) RTNVAR(&USER)
             CHGVAR     VAR(&USERB) VALUE('B_' *CAT &USER)

             CHGVAR     VAR(&WALI) VALUE(6)
             CHGVAR     VAR(&WAPO) VALUE(10)
             CHGVAR     VAR(&MLD1) VALUE('Dine transer kopieres til nytt')
             CHGVAR     VAR(&MLD2) VALUE('member B_USER.  USER t√∏mmes.  ')
             CALL       PGM(AA006C)  PARM(&WALI &WAPO &MLD1 &MLD2)
             DLYJOB     DLY(2)

/* Bygger membere dersom de ikke finnes B_xxxxxx                     */

             CHKOBJ     OBJ(RBUNPF) OBJTYPE(*FILE) MBR(&USERB)
             MONMSG     MSGID(CPF9815) EXEC(DO)
             ADDPFM     FILE(RBUNPF) MBR(&USERB) TEXT('Backup tra +
                          nser   (RF010C)')
             ENDDO

             CHKOBJ     OBJ(RTRAPF) OBJTYPE(*FILE) MBR(&USERB)
             MONMSG     MSGID(CPF9815) EXEC(DO)
             ADDPFM     FILE(RTRAPF) MBR(&USERB) TEXT('Backup tra +
                          nser   (RF010C)')
             ENDDO

/* Bunt kopieres                                                    */

             CPYF       FROMFILE(*LIBL/RBUNPF) TOFILE(*LIBL/RBUNPF) +
                          FROMMBR(&USER) TOMBR(&USERB) MBROPT(*REPLACE)
             MONMSG    MSGID(CPF2817 CPF2869)

 /* Transer kopieres                                                 */

             CPYF       FROMFILE(*LIBL/RTRAPF) TOFILE(*LIBL/RTRAPF) +
                          FROMMBR(&USER) TOMBR(&USERB) MBROPT(*REPLACE)
             MONMSG    MSGID(CPF2817 CPF2869)

             CLRPFM     FILE(RBUNPF) MBR(&USER)
             MONMSG    MSGID(CPF3141)

             CLRPFM     FILE(RTRAPF) MBR(&USER)
             MONMSG    MSGID(CPF3141)

 ENDPGM:     ENDPGM
