             PGM        Parm(&stat)

/* Initiering av parametre.                                          */

             dcl        var(&membr)  type(*char) len(10)
             dcl        var(&stat)  type(*char) len(1)
             chgvar     var(&membr) value('LWEDUT')

/* Sjekker om memeber er tilgjengelig                                */

             addpfm     file(LWEDPF) mbr(&membr)
             monmsg     msgid(cpf7306) exec(do)
             chgvar     var(&stat) value('L')
             goto       end
             enddo

/* Nullstiller igjen member                                          */

             rmvm       file(LWEDPF) mbr(LWEDUT)

 end:        ENDPGM
