             DCL        VAR(&BTCH)     TYPE(*CHAR) LEN(1)

/* Overstyring av file                                               */

             OVRDBF     FILE(FPSOL1)   TOFILE(*FILE) MBR(&MEMB)

/* Overstyring av printerfile                                        */

             CALL       PGM(AP601C)    PARM('FO647P' &BTCH)

/* Kaller opp program for kj√∏reliste - sammendrag                    */

             CALL       PGM(FO647R)    PARM(&PARM)

             DLTOVR     FILE(FPSOL1)

