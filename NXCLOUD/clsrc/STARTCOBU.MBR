/* STARTCOBU2                                                        */
/* Les alle forekomster og kjør jobb som behandler filen             */
/*                                                                   */
/* override */
             OVRDBF     FILE(IWBRK) TOFILE(*LIBL/IWBRK) MBR(&MEMB)
/** 6.   Endre er til riktig kundebibliotek    */
             CALL       PGM(*libl/STARTCOBU2) PARM(&MAPPF &FILT +
                          &FILS &FEIL)
/*                                                                */
             DLTOVR     file(iwbrk)

             IF         COND(&FEIL *EQ '1') THEN(GOTO CMDLBL(ENDPGM))

             rtvsysval  sysval(qtime) rtnvar(&t)
             if         cond(%substring(&T 1 4) *lt '1900') then(do)
                        dlyjob     dly(300)
                        goto  cmdlbl(crtbrk)
             enddo
