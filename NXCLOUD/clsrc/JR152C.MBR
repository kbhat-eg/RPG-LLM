             PGM        PARM(&LIST)

             DCL        VAR(&LIST) TYPE(*CHAR) LEN(32)
             DCL        VAR(&WJOB) TYPE(*CHAR) LEN(10)

/* Henter jobb-nummer                                                */

             RTVJOBA    JOB(&WJOB)

/* Submitter kall til styreprogram                                   */

             SBMJOB     CMD(CALL PGM(JR153C) PARM(&LIST &WJOB)) +
                          JOB(RAPP_VARE) MSGQ(*NONE)

 END:        ENDPGM
