             PGM        parm(&fgr)

             dcl        var(&fgr)   type(*char)   len(3)
             dcl        var(&lib)   type(*char)   len(10)
             dcl        var(&utf)   type(*char)   len(10)  value('ANSATTKJØP')

/* Endre XXX i parm til rette filgruppe */
             CALL       PGM(AA720C) PARM(&fgr)
             chgvar     var(&lib)   value('ADB' *cat &fgr)

/* Endre XXX her også, AA720C gir ikke alltid riktig filgruppe */
             CHGCURLIB  CURLIB(&lib)

             DLYJOB     DLY(10)

             callsubr   subr(SjekkReg)

             CALL PGM(SK101R)

/* Kjøre sammenstillningsquery           */
             RUNQRY     QRY(*libl/ANSATTKJØP) +
                          QRYFILE((&LIB/SSTAPF) (&LIB/SANSPF) +
                          (&LIB/RKUNPF) (&LIB/RA06pf) (&LIB/RA11pf) +
                          (&LIB/SREPpf) (&LIB/RA05pf) +
                          (&LIB/vvarpf)  (&LIB/fkprpf) +
                          (&LIB/vogrpf)  (&LIB/vhgrpf) +
                          (&LIB/ra11apf)) +
                          OUTFILE(&LIB/&UTF)
/*________________________________________________________________________*/
SUBR          SUBR(SjekkReg)
              chkobj      obj(sanspf) objtype(*file)
              monmsg      msgid(cpf9801) exec(do)
                  call    pgm(rgsansc) parm(&fgr ' ')
              enddo
              chkobj      obj(sreppf) objtype(*file)
              monmsg      msgid(cpf9801) exec(do)
                  call    pgm(rgsrepc) parm(&fgr ' ')
              enddo
              chkobj      obj(ra11apf) objtype(*file)
              monmsg      msgid(cpf9801) exec(do)
                  call    pgm(rga11ac) parm(&fgr ' ')
              enddo
ENDSUBR
