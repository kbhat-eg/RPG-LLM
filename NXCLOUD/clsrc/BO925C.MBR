 START:      PGM        PARM(&KODE &AARR &WSID &BONG)

/* Definering av parametere for butikk bong                          */

             DCL        VAR(&KODE)     TYPE(*DEC)  LEN(1 0)
             DCL        VAR(&AARR)     TYPE(*DEC)  LEN(4 0)
             DCL        VAR(&WSID)     TYPE(*CHAR) LEN(10)
             DCL        VAR(&BONG)     TYPE(*CHAR) LEN(12)

             DCL        VAR(&BTCH)     TYPE(*CHAR) LEN(1)

/* Overstyring av printerfile                                        */

             CALL       PGM(AP601C)    PARM('BO925P' &BTCH)

/* Call av utskriftsprogram                                          */

             CALL       PGM(BO925R)    PARM(&AARR &WSID &BONG +
                                            &KODE)

             CALL       PGM(AP606C)    PARM('BO925P')

 END:        ENDPGM
