             PGM

/* Definere parametere og variabler                                  */

             DCL        VAR(&WALI) TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&WAPO) TYPE(*DEC)  LEN(2 0)
             DCL        VAR(&MLD1) TYPE(*CHAR) LEN(40)
             DCL        VAR(&MLD2) TYPE(*CHAR) LEN(40)

/* Sjekk om member er i bruk?                                    */

             ALCOBJ     OBJ((RKLAPF *FILE *EXCL *FIRST)) WAIT(1)
             MONMSG     MSGID(CPF1002) EXEC(DO)
             CHGVAR     VAR(&WALI) VALUE(4)
             CHGVAR     VAR(&WAPO) VALUE(2)
             CHGVAR     VAR(&MLD1) VALUE('Notatblokk renummering kan')
             CHGVAR     VAR(&MLD2) VALUE('ikke kjøres, er i bruk.')
             CALL       PGM(AA006C)  PARM(&WALI &WAPO &MLD1 &MLD2)
             DLYJOB     DLY(5)
             GOTO       CMDLBL(END)
             ENDDO

/* Kopierer og tømmer Notatblokk                    */

             CPYF       FROMFILE(RKLAPF) TOFILE(RKLWPF) +
                          MBROPT(*REPLACE)

             CLRPFM     FILE(RKLAPF)

/* Call av program som renummererer                         */

             CALL       PGM(RK980R)
             DLCOBJ     OBJ((RKLAPF *FILE *EXCL *FIRST))


 END:        ENDPGM
