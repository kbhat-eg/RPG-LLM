             PGM        PARM(&PARM &BIBL &FIRM &USER)
             DCL        VAR(&PARM)     TYPE(*CHAR) LEN(4)
             DCL        VAR(&FIRM)     TYPE(*CHAR) LEN(3)
             DCL        VAR(&BIBL)     TYPE(*CHAR) LEN(10)
             DCL        VAR(&USER)     TYPE(*CHAR) LEN(10)
             MONMSG     MSGID(CPF0000) EXEC(GOTO CMDLBL(ERR))

             ADDLIBLE   LIB(&BIBL)     POSITION(*FIRST)
             MONMSG     MSGID(CPF2103)

/* Henter inn brukerprogram                                          */

             IF         COND(&PARM *EQ 'OFAK') THEN(DO)
             CALL       PGM(FB100R) PARM(&FIRM &USER)
             GOTO       CMDLBL(SLUTT)
             ENDDO

             IF         COND(&PARM *EQ 'LAGR') THEN(DO)
             CALL       PGM(LB100R) PARM(&FIRM &USER)
             GOTO       CMDLBL(SLUTT)
             ENDDO

             IF         COND(&PARM *EQ 'BUTI') THEN(DO)
             CALL       PGM(BB100R) PARM(&FIRM &USER)
             GOTO       CMDLBL(SLUTT)
             ENDDO

 SLUTT:      RMVLIBLE   LIB(&BIBL)
             GOTO       CMDLBL(END)

 ERR:        CHGVAR     VAR(&PARM) VALUE('FEIL')

 END:        ENDPGM
