             PGM

/* Initiering av parametre.                                          */

/*    &PROG = Programnavnet som brukes i formularfile.               */
/*    &RAPP = Navnet på rapporten i utskriftsprogram.                */
/*    &VALG = Verdi "1" dersom skjermbilde for vedlikehold av        */
/*            formularer skal vises.  Verdi "0" dersom bildet        */
/*            ikke skal vises.                                       */

             DCL        VAR(&PROG)     TYPE(*CHAR) LEN(10) +
                                       VALUE('AR022R')
             DCL        VAR(&RAPP)     TYPE(*CHAR) LEN(10) +
                                       VALUE('AR022P')
             DCL        VAR(&VALG)     TYPE(*CHAR) LEN(1) VALUE('1')
             DCL        VAR(&IN03)     TYPE(*DEC)  LEN(1 0)
             DCL        VAR(&IN12)     TYPE(*DEC)  LEN(1 0)

/*********************************************************************/

/* Oppdaterer LDA med navn på prinfile og kode for om formular-      */
/* parametre skal endres.                                            */

             CHGDTAARA  DTAARA(*LDA (800 10)) VALUE(&PROG)
             CHGDTAARA  DTAARA(*LDA (844 1))  VALUE(&VALG)
             CHGDTAARA  DTAARA(*LDA (846 10)) VALUE(&RAPP)

/* Call av program for utskriftsparametre.                           */

             CALL       PGM(AP100R)    PARM(&IN03 &IN12)

/* Avslutt dersom F12                                                */

             IF         COND(&IN12 *EQ 1) THEN(DO)
             GOTO       CMDLBL(END)
             ENDDO
/*********************************************************************/
/*SLUTT                                                              */
/*                                                                   */
/* Call av utskriftsprogram til batch.                               */
/*                                                                   */
             SBMJOB     CMD(CALL PGM(AR022C)) JOB(&PROG)
/*                                                                   */
 END:        ENDPGM
