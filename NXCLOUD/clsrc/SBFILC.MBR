             PGM        PARM(&WSPARM)

/* Initiering av parametre.                                          */

             DCL        VAR(&WSFILE) TYPE(*CHAR) LEN(10)
             DCL        VAR(&WSBIBL) TYPE(*CHAR) LEN(10)
             DCL        VAR(&WSPARM) TYPE(*CHAR) LEN(64)

             CHGVAR     VAR(&WSFILE) VALUE(%SST(&WSPARM 35 10))
             CHGVAR     VAR(&WSBIBL) VALUE(%SST(&WSPARM 45 10))
/*                                                                   */
/* Bygger membere dersom de ikke finnes                              */
/*                                                                   */
             CHKOBJ     OBJ(&WSBIBL/&WSFILE) OBJTYPE(*FILE)
             MONMSG     MSGID(CPF9815) EXEC(GOTO CMDLBL(END))
/*                                                                   */
/* Override til Ã¸nsket fil                                           */
/*                                                                   */
             OVRDBF     FILE(SINNPF) TOFILE(&WSBIBL/&WSFILE)
/*                                                                   */
/* Kaller opp program for danning av budsjett                        */
/*                                                                   */
             CALL       PGM(SBFILR)  PARM(&WSPARM)
/*                                                                   */
             DLTOVR     FILE(SINNPF)
