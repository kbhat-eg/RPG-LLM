/* Kaller opp program for utskrift   (Direkte)                       */
/*                                                                   */
             IF         COND(&LPVLG1 *NE '1') THEN(DO)
             CALL       PGM(LA914C) PARM(&LPPARM &LPVLG2 &LPVLG3)
             ENDDO
/*                                                                   */
/* Kaller opp program for utskrift   (Via jobbk√∏)                    */
/*                                                                   */
             IF         COND(&LPVLG1 *EQ '1') THEN(DO)
             SBMJOB     CMD(CALL PGM(LA914C) PARM(&LPPARM &LPVLG2 +
                          &LPVLG3)) JOB(OPPD.LAGER)
             ENDDO
