/*                                                                   */
/*           Generere   BESTILLING-SLETTET-REGISTER                  */
/*                                                                   */
 STRPGM:     PGM        PARM(&WPFGRP &WPNULL)
/*                                                                   */
             DCL        VAR(&WPFGRP) TYPE(*CHAR) LEN(3)
             DCL        VAR(&WPNULL) TYPE(*CHAR) LEN(1)
             DCL        VAR(&WPDBIB) TYPE(*CHAR) LEN(6) VALUE('ADB400')
             DCL        VAR(&WPSBIB) TYPE(*CHAR) LEN(6) VALUE('ASLAGR' )
/*                                                                   */
             DCL        VAR(&WPRGBF) TYPE(*CHAR) LEN(6) VALUE('LDELBF')
             DCL        VAR(&WPRGPF) TYPE(*CHAR) LEN(6) VALUE('LDELPF')
             DCL        VAR(&WPRGLU) TYPE(*CHAR) LEN(6) VALUE('LDELLU')
             DCL        VAR(&WPRGLR) TYPE(*CHAR) LEN(6) VALUE('LDELLR')
             DCL        VAR(&WPRGL1) TYPE(*CHAR) LEN(6) VALUE('LDELL1')
             DCL        VAR(&WPRGL2) TYPE(*CHAR) LEN(6) VALUE('LDELL2')
             DCL        VAR(&WPRGL3) TYPE(*CHAR) LEN(6) VALUE('LDELL3')
             DCL        VAR(&WPRGL4) TYPE(*CHAR) LEN(6) VALUE('LDELL4')
             DCL        VAR(&WPRGL5) TYPE(*CHAR) LEN(6) VALUE('LDELL5')
             DCL        VAR(&WPRGL6) TYPE(*CHAR) LEN(6) VALUE('LDELL6')
             DCL        VAR(&WPRGL7) TYPE(*CHAR) LEN(6) VALUE('LDELL7')
/*                                                                   */
/*           Setter bibliotek til current                            */
/*                                                                   */
             CHGVAR     VAR(%SST(&WPDBIB 4 3)) VALUE(&WPFGRP)
             CHGCURLIB  CURLIB(&WPDBIB)
/*                                                                   */
/*           Kalle opp fellesprogram                                 */
/*                                                                   */
             CALL       PGM(AG108C) PARM(&WPFGRP &WPNULL &WPDBIB +
                          &WPSBIB &WPRGBF &WPRGPF &WPRGLU &WPRGLR +
                          &WPRGL1 &WPRGL2 &WPRGL3 &WPRGL4 &WPRGL5 +
                          &WPRGL6 &WPRGL7)
/*                                                                   */
 ENDPGM:     ENDPGM
