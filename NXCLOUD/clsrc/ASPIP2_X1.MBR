             PGM

/* Eksempel 1,   på program som kan benyttes...                      */
/* -----------                                                       */

/* Venter på at server skal starte                                   */

             DLYJOB     DLY(300)

/* Starter TCP/IP                                                    */

             STRTCP
             MONMSG     MSGID(CPF0000)

/* Starter server for WIN95                                          */

             STRHOSTSVR SERVER(*ALL)
             MONMSG     MSGID(CPF0000)

/* Starter nettverksskriver                                          */

             STRRMTWTR  OUTQ(xxxxxx)   /* <-- (Legg inn riktig utkø) */
             MONMSG     MSGID(CPF0000)

             ENDPGM

