/*                                                                   */
/*           Generere   PARAMETER FOR BESTILLINGSFORSLAG             */
/*                                                                   */
 STRPGM:     PGM        PARM(&WPFGRP &WPNULL)
/*                                                                   */
             DCL        VAR(&WPFGRP) TYPE(*CHAR) LEN(3)
             DCL        VAR(&WPNULL) TYPE(*CHAR) LEN(1)
             DCL        VAR(&WPDBIB) TYPE(*CHAR) LEN(6) VALUE('ADB400')
             DCL        VAR(&WPSBIB) TYPE(*CHAR) LEN(6) VALUE('ASLOGI' )
/*                                                                   */
             DCL        VAR(&WPRGBF) TYPE(*CHAR) LEN(6) VALUE('MPPABF')
             DCL        VAR(&WPRGPF) TYPE(*CHAR) LEN(6) VALUE('MPPAPF')
             DCL        VAR(&WPRGLU) TYPE(*CHAR) LEN(6) VALUE('MPPALU')
             DCL        VAR(&WPRGL1) TYPE(*CHAR) LEN(6) VALUE('MPPAL1')
             DCL        VAR(&WPRGL2) TYPE(*CHAR) LEN(6) VALUE('MPPAL2')
             DCL        VAR(&WPRGL3) TYPE(*CHAR) LEN(6) VALUE('MPPAL3')
             DCL        VAR(&WPRGL4) TYPE(*CHAR) LEN(6) VALUE('MPPAL4')
             DCL        VAR(&WPRGLR) TYPE(*CHAR) LEN(6) VALUE('MPPALR')
/*                                                                   */
/*           Setter bibliotek til current                            */
/*                                                                   */
             CHGVAR     VAR(%SST(&WPDBIB 4 3)) VALUE(&WPFGRP)
             CHGCURLIB  CURLIB(&WPDBIB)
/*                                                                   */
/*           Kalle opp fellesprogram                                 */
/*                                                                   */
             CALL       PGM(AG105C) PARM(&WPFGRP &WPNULL &WPDBIB +
                          &WPSBIB &WPRGBF &WPRGPF &WPRGLU &WPRGL1 +
                          &WPRGL2 &WPRGL3 &WPRGL4 &WPRGLR)
/*                                                                   */
 ENDPGM:     ENDPGM
