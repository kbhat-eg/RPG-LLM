/*                                                                   */
/*           Generere   Matrise Best.mengde /Best.punkt              */
/*                                                                   */
 STRPGM:     PGM        PARM(&WPFGRP &WPNULL)
/*                                                                   */
             DCL        VAR(&WPFGRP) TYPE(*CHAR) LEN(3)
             DCL        VAR(&WPNULL) TYPE(*CHAR) LEN(1)
             DCL        VAR(&WPDBIB) TYPE(*CHAR) LEN(6) VALUE('ADB400')
             DCL        VAR(&WPSBIB) TYPE(*CHAR) LEN(6) VALUE('ASLAGR' )
/*                                                                   */
             DCL        VAR(&WPRGBF) TYPE(*CHAR) LEN(6) VALUE('MMATBF')
             DCL        VAR(&WPRGPF) TYPE(*CHAR) LEN(6) VALUE('MMATPF')
             DCL        VAR(&WPRGLU) TYPE(*CHAR) LEN(6) VALUE('MMATLU')
             DCL        VAR(&WPRGL1) TYPE(*CHAR) LEN(6) VALUE('MMATL1')
             DCL        VAR(&WPRGL2) TYPE(*CHAR) LEN(6) VALUE('MMATL2')
             DCL        VAR(&WPRGL3) TYPE(*CHAR) LEN(6) VALUE('MMATL3')
             DCL        VAR(&WPRGL4) TYPE(*CHAR) LEN(6) VALUE('MMATL4')
             DCL        VAR(&WPRGL5) TYPE(*CHAR) LEN(6) VALUE('MMATL5')
             DCL        VAR(&WPRGL6) TYPE(*CHAR) LEN(6) VALUE('MMATL6')
             DCL        VAR(&WPRGLR) TYPE(*CHAR) LEN(6) VALUE('MMATLR')
/*                                                                   */
/*           Setter bibliotek til current                            */
/*                                                                   */
             CHGVAR     VAR(%SST(&WPDBIB 4 3)) VALUE(&WPFGRP)
             CHGCURLIB  CURLIB(&WPDBIB)
/*                                                                   */
/*           Kalle opp fellesprogram                                 */
/*                                                                   */
             CALL       PGM(AG107C) PARM(&WPFGRP &WPNULL &WPDBIB +
                          &WPSBIB &WPRGBF &WPRGPF &WPRGLU &WPRGL1 +
                          &WPRGL2 &WPRGL3 &WPRGL4 &WPRGL5 &WPRGL6 +
                          &WPRGLR)
/*                                                                   */
 ENDPGM:     ENDPGM
