             PGM        PARM(&LIST &FLDR &JOBA)
/* 7.01 bof 220617 - utvidet parameter  */

             DCL        VAR(&LIST) TYPE(*CHAR) LEN(82)   /*7.01 */
             DCL        VAR(&FLDR) TYPE(*CHAR) LEN(12)
             DCL        VAR(&JOBA) TYPE(*CHAR) LEN(10)

/* Kall til styreprogram                                             */

             CALL       PGM(VP681R) PARM(&LIST &FLDR)

/* Ferdigmelding                                                     */

             SNDBRKMSG  MSG('ASOFAK: Prisfiler er opprettet') +
                          TOMSGQ(&JOBA)

 END:        ENDPGM
