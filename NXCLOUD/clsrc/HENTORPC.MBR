      PGM        parm(&pnumx  &psufx)

/*    Tilgang til denne rutina er styrt av AM11, valg 12             */


      DCL        VAR(&fil)   TYPE(*CHAR) LEN(10)  value('User      ')
      DCL        VAR(&lib)   TYPE(*CHAR) LEN(10)  value('ADBxxx    ')
      DCL        VAR(&mbr)   TYPE(*CHAR) LEN(20)  value('User      ')
      DCL        VAR(&dir)   TYPE(*char) LEN(60)
      DCL        VAR(&utf)   TYPE(*char) LEN(60)
      DCL        VAR(&fgr)   TYPE(*char) LEN(3)
      DCL        VAR(&pnumx) TYPE(*char) LEN(8)
      DCL        VAR(&psufx) TYPE(*char) LEN(2)
      DCL        VAR(&usr)   TYPE(*char) LEN(10)

      rtvdtaara  dtaara(*lda (931 3))   rtnvar(&fgr)
      chgvar     var(%sst(&lib 4 3))    value(&fgr)
      rtvdtaara  dtaara(*lda (911 10))  rtnvar(&fil)
      rtvdtaara  dtaara(*lda (911 10))  rtnvar(&mbr)
      clrpfm     file(&lib/&fil)
      monmsg     msgid(cpf0000)
/*                                                                   */
/*    Henter excel-fil fra IFS-området                               */
/*           call hentifss  (&fil &lib &mbr +        */
             call hentifs   (&fil &lib &mbr +
                             &dir &utf)

             chkobj     obj(&fil) objtype(*file)
             monmsg     msgid(cpf9801 cpf9812) exec(do)
               goto     end
             enddo

             ovrdbf file(falinj) tofile(&fil)
             call       Hentorpr   parm(&pnumx &psufx)


/*                                                                   */
/*    Oppdater excel-fil på IFS-området                              */
/*           call uppdifsc   (&dir &utf)                             */
end:  ENDPGM
