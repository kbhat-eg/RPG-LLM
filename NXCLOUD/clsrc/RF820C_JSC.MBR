             PGM        parm(&filg &kort &lagr)

             DCL        VAR(&filg) TYPE(*CHAR) LEN(3)
             DCL        VAR(&kort) TYPE(*char) LEN(1)
             DCL        VAR(&lagr) TYPE(*char) LEN(2)
             DCL        VAR(&curlib) TYPE(*CHAR) LEN(10)

/* Endre XXX i parm til rette filgruppe */
             CALL       PGM(AA720C) PARM(&filg)

/* Endre XXX her også, AA720C gir ikke alltid riktig filgruppe */

             chgvar     var(&curlib) value('ADB' *tcat &filg)
             CHGCURLIB  CURLIB(&curlib)

/* Tømmer loggfile                                            */
             CLRPFM     FILE(XMLGRLPF)

             DLYJOB     DLY(10)
/* Kjører Byggmakker kort                                     */
             CALL PGM(RF820R_SBM) parm(&kort &lagr)

END: ENDPGM
