             pgm        parm(&lib &grp)

             dcl        var(&lib)     type(*char) len(10)
             dcl        var(&grp)     type(*char) len(10)
             dcl        var(&m1)      type(*char) len(50)
             dcl        var(&m2)      type(*char) len(50)
             dcl        var(&sv)      type(*char) len(1)
             dcl        var(&f12)     type(*char) len(1)
             dcl        var(&feil)    type(*char) len(1)  value(' ')

             dclf       file(*libl/dspobjdf)

rcvf:        rcvf
             monmsg     msgid(CPF0864) exec(do)
                        goto cmdlbl(feil)
             enddo

             chgobjown  obj(&lib/&odobnm) objtype(*file) newown(&grp)
             monmsg     msgid(cpf3202 CPF22BE) exec(do)
               chgvar   var(&feil)   value('J')
             enddo

             goto       cmdlbl(rcvf)

feil:
             if         cond(&feil *eq 'J') then(do)
               chgvar   var(&m1)  value('En/flere filer fikk ikke ny eier da de var i bruk')
               chgvar   var(&m2)  value('Sjekk joblogg for hvilke ev. kjør på nytt senere')
               call     pgm(aa007r) parm(&m1 &m2 &sv &f12)
             enddo

 end:        endpgm
