             PGM        parm(&selskap)
             DCLF       FILE(Inkassov)

             dcl        var(&selskap)  type(*char)  len(10)
/*                                                                   */
/*    Hent data-area fra *curlib                                     */
/*                                                                   */
start:
             CHGVAR     VAR(&T1)       VALUE('Lindorf     ')
             CHGVAR     VAR(&T2)       VALUE('Svea        ')
             CHGVAR     VAR(&p1)       VALUE(' ')
             CHGVAR     VAR(&p2)       VALUE(' ')

             SNDRCVF    RCDFMT(K1WIN)
