             IF         COND(&IN03) THEN(do)
               chgvar   var(&selskap)  value('Avbrudd')
               goto     endpgm
             enddo
/*                                                                   */
/*                                                                   */
             if         cond(&p1 *ne 'J') then(do)
               if       cond(&p2 *ne 'J') then(do)
                 chgvar var(&meld) value('Et selskap m√• velges')
                 goto   start
               enddo
             enddo
             if         cond(&p1 *eq 'J') then(do)
               if       cond(&p2 *eq 'J') then(do)
                 chgvar var(&meld) value('Kun et selskap kan velges')
                 goto   start
               enddo
             enddo
/*                                                                   */
               if       cond(&p1 *eq 'J') then(do)
                 chgvar var(&selskap) value(&t1)
               enddo
               if       cond(&p2 *eq 'J') then(do)
                 chgvar var(&selskap) value(&t2)
               enddo
