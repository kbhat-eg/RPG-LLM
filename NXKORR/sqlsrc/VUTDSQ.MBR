CREATE OR REPLACE TABLE VUTDST(
  UID                NUMERIC(15, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
                     (START WITH 1 INCREMENT BY 1 CYCLE),
  Firma              FOR COLUMN VUDFIR NUMERIC(3, 0 ) NOT NULL DEFAULT 0,
  Varenummer         FOR COLUMN VUDVAR CHAR(15) NOT NULL DEFAULT ' ',
  Varetekst          FOR COLUMN VUDTEK CHAR(35) NOT NULL DEFAULT ' ',
  UtgarDato          FOR COLUMN VUDUDA DATE NOT NULL DEFAULT CURRENT_DATE,
  NyUtgarDato        FOR COLUMN VUDNUD DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetDato      FOR COLUMN VUDODA DATE NOT NULL DEFAULT CURRENT_DATE,
  OpprettetKl        FOR COLUMN VUDOTI TIME NOT NULL DEFAULT CURRENT_TIME,
  OpprettetAv        FOR COLUMN VUDOUS CHAR(10) NOT NULL DEFAULT ' ',

  PRIMARY KEY (UID))
  ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE VUTDST IS 'Logg for expires date';
