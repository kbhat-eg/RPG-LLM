CREATE OR REPLACE TABLE LXL6ST (
    UID NUMERIC(15, 0) NOT NULL GENERATED ALWAYS AS IDENTITY
        (START WITH 1 INCREMENT BY 1 CYCLE),
    Identnr           FOR COLUMN LX6IDE NUMERIC(10, 0) NOT NULL DEFAULT 0,
    Varenummer        FOR COLUMN LX6VAR CHAR(15) NOT NULL DEFAULT ' ',
    Lager             FOR COLUMN LX6LAG NUMERIC(2, 0) NOT NULL DEFAULT 0,
    Beskrivelse       FOR COLUMN LX6BES CHAR(50) NOT NULL DEFAULT ' ',
    PRIMARY KEY(UID))
ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE LXL6ST IS 'SlipStream kvitteringsliste';
