CREATE OR REPLACE TABLE RX02ST(
  UID              FOR COLUMN R02UID NUMERIC(18, 0) NOT NULL
                   GENERATED ALWAYS AS IDENTITY
                   (START WITH 1 INCREMENT BY 1 CYCLE),
  Identnr          FOR COLUMN R02IDE NUMERIC(10, 0) NOT NULL DEFAULT 0,
  Firm             FOR COLUMN R02FIR NUMERIC(3, 0) NOT NULL DEFAULT 0,
  CustomerNumber   FOR COLUMN R02KUN NUMERIC(6, 0) NOT NULL DEFAULT 0,
  NXProjNumber     FOR COLUMN R02NXP NUMERIC(6, 0) NOT NULL DEFAULT 0,
  SalesNumber      FOR COLUMN R02SLG NUMERIC(4, 0) NOT NULL DEFAULT 0,
  FromDate         FOR COLUMN R02FDA DATE NOT NULL DEFAULT CURRENT_DATE,
  ToDate           FOR COLUMN R02TDA DATE NOT NULL DEFAULT CURRENT_DATE,
  ProjectName      FOR COLUMN R02NAV CHAR(30) NOT NULL DEFAULT ' ',
  Adress1          FOR COLUMN R02AD1 CHAR(30) NOT NULL DEFAULT ' ',
  Adress2          FOR COLUMN R02AD2 CHAR(30) NOT NULL DEFAULT ' ',
  PostalNumber     FOR COLUMN R02PNR NUMERIC(4, 0) NOT NULL DEFAULT 0,
  ContactPerson    FOR COLUMN R02KPR CHAR(30) NOT NULL DEFAULT ' ',
  TelephoneWork    FOR COLUMN R02TLA CHAR(15) NOT NULL DEFAULT ' ',
  TelephonePriv    FOR COLUMN R02TLP CHAR(15) NOT NULL DEFAULT ' ',
  Mobile           FOR COLUMN R02TLM CHAR(15) NOT NULL DEFAULT ' ',
  Emailadresse     FOR COLUMN R02EMA CHAR(50) NOT NULL DEFAULT ' ',
  ExtprojNbr       FOR COLUMN R02EPR CHAR(15) NOT NULL DEFAULT ' ',
  InvoiceType      FOR COLUMN R02FTY NUMERIC(2, 0) NOT NULL DEFAULT 0,
  DrivingRoute     FOR COLUMN R02KJO NUMERIC(2, 0) NOT NULL DEFAULT 0,
  DrivingOrder     FOR COLUMN R02KJN NUMERIC(5, 0) NOT NULL DEFAULT 0,
  DiscountCust     FOR COLUMN R02OBU NUMERIC(6, 0) NOT NULL DEFAULT 0,
  DiscCategory     FOR COLUMN R02ORK NUMERIC(3, 0) NOT NULL DEFAULT 0,
  OverrideCust     FOR COLUMN R02OKN NUMERIC(6, 0) NOT NULL DEFAULT 0,
  OverrideProj     FOR COLUMN R02OPR NUMERIC(6, 0) NOT NULL DEFAULT 0,
  OverPriceDate    FOR COLUMN R02OPA DATE NOT NULL DEFAULT CURRENT_DATE,
  Private          FOR COLUMN R02PRI NUMERIC(1, 0) NOT NULL DEFAULT 0,
  ProjectNbr       FOR COLUMN R02PRO CHAR(6) NOT NULL DEFAULT ' ',
  Byggdoc          FOR COLUMN R02BDO NUMERIC(1, 0) NOT NULL DEFAULT 0,
  Cobuilder        FOR COLUMN R02CBO NUMERIC(1, 0) NOT NULL DEFAULT 0,
  VatCode          FOR COLUMN R02MKO NUMERIC(1, 0) NOT NULL DEFAULT 0,
  PaymTerms        FOR COLUMN R02BET NUMERIC(2, 0) NOT NULL DEFAULT 0,
  PricePacknote    FOR COLUMN R02PPA NUMERIC(1, 0) NOT NULL DEFAULT 0,
  PriceInvoice     FOR COLUMN R02NET NUMERIC(1, 0) NOT NULL DEFAULT 0,
  ExpFee           FOR COLUMN R02EKG NUMERIC(1, 0) NOT NULL DEFAULT 0,
  InvoiceSeq       FOR COLUMN R02SEK NUMERIC(2, 0) NOT NULL DEFAULT 0,
  SamleInvoice     FOR COLUMN R02SAM NUMERIC(1, 0) NOT NULL DEFAULT 0,
  InvoiceFee       FOR COLUMN R02FAG NUMERIC(1, 0) NOT NULL DEFAULT 0,
  CustomerOver     FOR COLUMN R02FKU NUMERIC(1, 0) NOT NULL DEFAULT 0,

    PRIMARY KEY(UID))

ON REPLACE PRESERVE ALL ROWS;

LABEL ON TABLE RX02ST IS 'Customer project - export';
